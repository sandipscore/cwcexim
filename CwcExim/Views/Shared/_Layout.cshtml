        function downloadFile()
        {
            var fileUrl = '/Docs/USER MANUAL.zip';
            var xhr = new XMLHttpRequest();

            xhr.open('GET', fileUrl, true);
            xhr.responseType = 'blob';

            xhr.onload = function() {
                if (xhr.status === 200) {
                    var blob = xhr.response;
                    var link = document.createElement('a');
                    link.href = window.URL.createObjectURL(blob);
                    link.download = 'USER MANUAL.zip';

                    // Create a click event on the link to trigger the download
                    var clickEvent = new MouseEvent('click', {
                        bubbles: true,
                        cancelable: true,
                        view: window
                    });

                    link.dispatchEvent(clickEvent);
                }
            };

            xhr.onprogress = function(event) {
                if (event.lengthComputable) {
                    var percentComplete = (event.loaded / event.total) * 100;
                    updateProgressBar(percentComplete);
                }
            };

            xhr.send();
        }

        function updateProgressBar(percent) {
            var progressBar = document.getElementById('downloadProgress');
            progressBar.value = percent;
        }
        // Get the modal

        $('.close').on("click", function () {
            $('#DivChangePasswordModal').hide();
            $('#DivEditProfileModal').hide();
        });

        window.onclick = function (event) {
            if (event.target == $('#DivChangePasswordModal')) {
