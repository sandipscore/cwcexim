    <script>
        function LoginWithId() {
            var ObjData = new Object();
            ObjData.LoginWithId = $('#TxtLoginId').val();
            if ($('#TxtLoginId').val() == '') {
                $('#DivLoginErr').html("Please Enter User Id");
            }
            else {
                $.ajax({
                    type: 'POST',
                    url: "/User/CheckUser",
                    contentType: 'application/json',
                    dataType: "Json",
                    data: JSON.stringify(ObjData),
                    success: function (data) {

                        if (data.Status == 1) {

                            $('#DivSignUp').load('/User/SignUp?LoginWithId=' + $('#TxtLoginId').val());
                            $('#DivSignupModal').show();
                            $('#DivLoginModal').hide();

                        }
                        else {
                            $('#DivLoginErr').html(data.Msg);
                        }
                    }
                });
            }

        }

