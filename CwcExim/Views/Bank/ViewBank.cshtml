@model CwcExim.Models.Bank
<script>
    function LoadBankPage() {
        $('#DivBody').load('/Bank/CreateBank');
    }
</script>

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>Bank Master</h4>
                </div>
            </div>
            <div class="content_wrp">

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.AccountNo, new { @class = "InputLabel" })<span class="InputLabel">:</span>
                            @Html.TextBoxFor(m => m.AccountNo, new { @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.BankName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.BankName, new { @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.IFSC, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.IFSC, new { @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.Branch, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.Branch, new { @readonly = "readonly" })
                    </div>
                </div>


                <div class="row Form_Space_top">
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.LabelFor(m => m.Address, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextAreaFor(m => m.Address, new { style = "width:100%;height:75px;", @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Phone No:</label>
                            @Html.TextBoxFor(m => m.MobileNo, new { @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.FaxNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.FaxNo, new { @readonly = "readonly" })
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.Email, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.Email, new { @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary-border btn-100px" value="Back" onclick="LoadBankPage()">
                </div>
            </div>
        </div>
    </div>
</div>



