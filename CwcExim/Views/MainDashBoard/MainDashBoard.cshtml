
@{
    ViewBag.Title = "MainDashBoard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{

    var BranchId = Session["BranchId"];
}
<style>
    body {
        background: #a3a3a3;
    }

    .body-content {
        background-color: #a3a3a3;
        height: auto;
    }

    .section, #about {
        height: auto;
        min-height: 100px;
    }

    .Land-head h4 {
        font-size: 18px;
        font-weight: 600;
        color: #feecd8;
        margin-left: 0px;
        margin-top: 0px;
    }
</style>

@*@Scripts.Render("~/bundles/jquery")*@
@*@Scripts.Render("~/bundles/unobtrusive")*@

@*<script src="~/Scripts/md5.min.js"></script>*@
@*@Scripts.Render("~/bundles/AjaxOption")*@

<div class="container-fluid">
    <div id="home">
        <div class="row">
            <div class="carousel slide" data-ride="carousel" id="myCarousel">
                <ul class="carousel-indicators">
                    <li class="active" data-target="#myCarousel" data-slide-to="0"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                    <li data-target="#myCarousel" data-slide-to="4"></li>
                    <li data-target="#myCarousel" data-slide-to="5"></li>
                </ul>
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="~/Content/Images/ship1-2.jpg" class="img-responsive">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/ship1-5.jpg" class="img-responsive">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/ship1-3.jpg" class="img-responsive">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/ship6-6.jpg" class="img-responsive">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/ship2-2.jpg" class="img-responsive">
                    </div>
                    <div class="item">
                        <img src="~/Content/Images/ship1-4.jpg" class="img-responsive">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

