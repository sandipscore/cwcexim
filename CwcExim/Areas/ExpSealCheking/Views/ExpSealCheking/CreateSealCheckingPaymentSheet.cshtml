            rawJson.TotalSGST += item.SGSTAmt;
            rawJson.TotalIGST += item.IGSTAmt;
            TotalHT += item.Total;
        });
        $('#TOTHTChrg').val((TotalHT).toFixed(2));
        //$('#TOTHTTDS').val(((TotalHT * 2) / 100).toFixed(2));
        rawJson.HTTotal = TotalHT;
        $('#AllTotal').val((TotalCWC + TotalHT).toFixed(2));
        rawJson.AllTotal = (TotalCWC + TotalHT);
        $('#PaymentSheetModelJson').val(JSON.stringify(rawJson));
        CalcInvoice();
    }

    function CalcInvoice() {
        var Total = Number($('#AllTotal').val());
        var cwcTDS = 0;// Number($('#TOTCWCTDS').val());
        var htTDS = 0;//Number($('#TOTHTTDS').val());
        var grandTDS = 0;//cwcTDS + htTDS;
        //$('#TotalTDS').val(grandTDS.toFixed(2));
        var RoundUp = Number($('#RoundUp').val());
        $('#InvoiceValue').val((Total - (grandTDS + RoundUp)).toFixed(2));
        var rawJson = JSON.parse($('#PaymentSheetModelJson').val());
        console.clear();
        //console.log(JSON.stringify(rawJson));
        //rawJson.AllTotal = Total;
        rawJson.CWCTDS = cwcTDS;
        rawJson.HTTDS = htTDS;
        rawJson.TDS = grandTDS;
        rawJson.RoundUp = RoundUp;
        rawJson.InvoiceAmt = rawJson.AllTotal - (rawJson.TDS + rawJson.RoundUp);

        rawJson.AllTotal = (rawJson.CWCTotal + rawJson.HTTotal);
        var round = Math.ceil(rawJson.AllTotal) - rawJson.AllTotal;
        rawJson.InvoiceAmt = Math.ceil(rawJson.AllTotal);

        $('#AllTotal').val(rawJson.AllTotal.toFixed(2));
        $('#RoundUp').val(round.toFixed(2));
        $('#InvoiceValue').val(rawJson.InvoiceAmt.toFixed(2));
        $('#PaymentSheetModelJson').val(JSON.stringify(rawJson));
        //$('#rest-charge-details').removeClass('hide');
    }

    function BindJson() {
        var conf = confirm("Do you want to save ?");
        if (conf) {
            try {
                debugger;
                $('#btnSave').attr('disabled', true);
                var rawJson = JSON.parse($('#PaymentSheetModelJson').val());
                rawJson.InvoiceId = 0;
                rawJson.InvoiceType = TaxType;
                rawJson.InvoiceDate = $('#InvoiceDate').val();
                rawJson.RequestId = $('#EntryId').val();
                rawJson.RequestNo = $('#TruckSlipNo').val();
                rawJson.RequestDate = $('#TruckSlipDate').val();
                rawJson.PartyId = $('#PartyId').val();
                rawJson.PartyName = $('#PartyName').val();
                rawJson.PayeeId = $('#PayeeId').val();
                rawJson.PayeeName = $('#PayeeName').val();
                rawJson.SEZ = $('#SEZ').val();
                rawJson.PartyGST = $('#GSTNo2').val();
                rawJson.Remarks = $('#Remarks').val();
                rawJson.PartyAddress = $('#hdnAddress').val();
                rawJson.PartyState = $('#hdnState').val();
                rawJson.PartyStateCode = $('#hdnStateCode').val();
                rawJson.RoundUp = parseFloat($('#RoundUp').val());
                rawJson.CompanyAddress = (rawJson.CompanyAddress).replace(new RegExp('<br/>', "gi"), '|_br_|');
                rawJson.ROAddress = (rawJson.ROAddress).replace(new RegExp('<br/>', "gi"), '|_br_|');
                //rawJson.InvoiceHtml = InvoiceHtml(rawJson);
                $.each(rawJson.lstPostPaymentChrg, function (i, item) {
                    if (item.ChargeId.toString().indexOf('_') != -1) {
                        item.ChargeId = Number(item.ChargeId.split('_')[0]);
                    }
                });
                $('#PaymentSheetModelJson').val(JSON.stringify(rawJson));
                //console.log(rawJson);
                $('form#PaymentSheetForm').submit();
            }
            catch (e) {
                $('#btnSave').removeAttr('disabled');
            }
        }
    }

