    function AllowOnlyInteger(evt) {
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }
        if ((CharCode > 47 & CharCode < 58) || CharCode == 8) {
            return true;
        }
        else {
            evt.preventDefault;
            return false;
        }
    }
    function ShowList()
    {
        $('#SearchInvoiceNo').val("");
        $('#SearchInvoiceDate').val("");
        $('#DivListOfInvoice').load('/Export/Chn_CWCExport/ListOfExpInvoiceSealChecking');
    }
    function SearchInvoice() {       
        var InvoiceNo = $('#SearchInvoiceNo').val().trim();
        var InvoiceDate = $('#SearchInvoiceDate').val().trim();
        if ($('#SearchInvoiceNo').val() == "" && $('#SearchInvoiceDate').val() == "" )
            alert("Enter Invoice No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate()==true) {
            $('#DivListOfInvoice').load('/Export/Chn_CWCExport/ListOfExpInvoiceSealChecking?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate);
        }
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Export/Chn_CWCExport/ListOfExpInvoiceSealChecking?InvoiceNo=' + InvoiceNo);
        else if(Validate()==true)
            $('#DivListOfInvoice').load('/Export/Chn_CWCExport/ListOfExpInvoiceSealChecking?InvoiceDate=' + InvoiceDate);
    }
</script>