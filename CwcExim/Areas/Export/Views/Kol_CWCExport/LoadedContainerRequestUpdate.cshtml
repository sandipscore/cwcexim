<script type="text/javascript" src="/scripts/jquery.nicescroll.min.js"></script>
<script>
    $('#LoadContReqNo').focus();


    $(document).ready(function()
    {

        $('#slim_scroll_2').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll_3').slimScroll({
            allowPageScroll: true
        });

    });

    var ArrayContDet = [];
    $(function () {

        debugger;
        ArrayContDet = @Html.Raw(Json.Encode(Model.lstContDtl));


        GenerateTable();
        $('#LoadContReqDetlId').val('0');
        $("#divRefer").hide();

        debugger;

        if ($('#PikUpFrom').find('option:selected').text() == "ICD")
            document.getElementById('ContainerNoSerach').style.visibility = "visible";
        else
            document.getElementById('ContainerNoSerach').style.visibility = "hidden";
    });
    function AddContDet() {
        if (ValidateContDet()) {

            if ($('#IsReefer').is(':checked') == true)
            {
                ReeferHr = $('#ReeferHrs').val();
                if (ReeferHr == '' || ReeferHr == '0') {
                    alert('Reefer Hour can not blank');
                    return false;
                }
            }
            else {
                ReeferHr = 0;
            }
            var ContDet = {
                "ExporterId": $('#ExporterId').val(),
                "ShippingLineId": ($('#ShippingLineId').val() == '' ? 0 : $('#ShippingLineId').val()),
                "EntryNo": $('#EntryNo').val(),
                "ContainerNo": $('#ContainerNo').val(),
                "Size": $('#Size').val(),
                "IsReefer": ($('#IsReefer').is(':checked') ? true : false),
                "ReeferHrs":ReeferHr,
                "IsInsured": ($('#IsInsured').is(':checked') ? true : false),
                "ShippingBillNo": $('#ShippingBillNo').val(),
                "ShippingBillDate": $('#ShippingBillDate').val(),
                "CommodityId": $('#CommodityId').val(),
                "CargoType": $('#CargoType').val(),
                "CargoDescription": $('#CargoDescription').val(),
                "GrossWt": $('#GrossWt').val(),
                "NoOfUnits": $('#NoOfUnits').val(),
                "FobValue": $('#FobValue').val(),
                "ExporterName": $('#ExporterName').val(),
                "ShippingLineName": $('#ShippingLineName').val(),
                "CommodityName": $('#CommodityName').val(),
                "LoadContReqDetlId":$('#LoadContReqDetlId').val(),
                "IsWashing": 0,
                "IsWeighment": 0,
                "IsReworking": 0,
                "IsInternalShifting": 0,
                "IsLiftOnEmpty":0,
                "IsLiftOffEmpty": 0,
                "RMSValue": 0,
                "BothWayEmpty":$('#BothWayEmpty').is(':checked')? true : false,
                "ShippingSeal": $('#ShippingSeal').val(),
                "ContLoadType": $('#ContLoadType').val(),
                "CustomSeal": $('#CustomSeal').val(),
                "EquipmentSealType": $('#EquipmentSealType').val(),
                "EquipmentStatus": $('#EquipmentStatus').val(),
                "EquipmentQUC": $('#EquipmentQUC').val(),
                "PackageType": $('#PackageType').val(),
                "PackUQCCode": $('#PackUQCCode').val(),
                "PackUQCDescription": $('#PackUQCDescription').val()
            };
            debugger;
            ArrayContDet.push(ContDet);
           //// $('#btnAdd').prop("disabled", true);
            $('#tblArrayContDet').html('');
            GenerateTable();
            ResetContDet();
            $('[data-valmsg-for]').html('');
        }
    }
    function GenerateTable() {
        var tablehtml = '<table id="tblArrayContDet" class="table table-striped table-bordered dataTable" style="width:100%;"><thead><tr><th class="text-center">SL No</th><th>Container No</th><th>Size</th><th>Exporter</th><th>No of Units</th><th class="text-center">Edit</th><th class="text-center">View</th></tr></thead><tbody>';
        $.each(ArrayContDet, function (i, data) {
            tablehtml += '<tr><td class="text-center">' + (i + 1) + '</td><td>' + data.ContainerNo + '</td><td>' + data.Size + '</td><td>' + data.ExporterName + '</td><td>' + data.NoOfUnits + '</td><td class="text-center"><a href="#" onclick="EditContDet(' + i + ')"><i class="fa fa-pencil-square Edit"></i></a></td><td class="text-center"><a href="#" onclick="ViewContDet(' + i + ')"><i class="fa fa-search-plus View"></i></a></td></tr>';
        });
        tablehtml += '</tbody></table>';
        $('#tblArrayContDet').html(tablehtml);
    }
    function ResetContDet() {
        $('#ExporterId,#ShippingLineId,#ContainerNo,#ShippingBillNo,#CommodityId,#CargoDescription,#NoOfUnits,#FobValue,#GrossWt,#ExporterName,#ShippingLineName,#CommodityName,#ShippingSeal,#CustomSeal,#PackUQCCode,#PackUQCDescription').val('');

        $('#IsReefer,#IsInsured,#IsWashing,#IsWeighment,#IsReworking,#IsInternalShifting,#IsLiftOnEmpty,#IsLiftOffEmpty,#BothWayEmpty').prop("checked", false);

        var SizedropDown = document.getElementById("Size");
        SizedropDown.selectedIndex = '';
        var CargoTypedropDown = document.getElementById("CargoType");
        CargoTypedropDown.selectedIndex = '';

        $('#LoadContReqDetlId').val('0');
        $('#RMSValue').val(1);
        $('#ReeferHrs').val(0);
        $('#EquipmentSealType').val('BTSL');
        $('#EquipmentStatus').val('94');
        $('#EquipmentQUC').val('PKG');
        $('#PackageType').val('P');
        $("#divRefer").hide();
        //   $('#btnAdd').prop("disabled", false);
    }
    function LoadMorePackUQC() {
        debugger;
        $.ajax({
            url: '/Export/CWCExport/LoadPackUQCList',
            type: 'GET',
            data: { PartyCode: "", Page: Page + 1 },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    $.each(data.Data.lstPackUQC, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                        tabvalue++;
                    });
                    if (Page == -1)
                        $('#tblePackUQC tbody').html(html);
                    else
                        $('#tblePackUQC tbody').append(html);
                    Page++;

                    debugger;
                    if (data.Data.PackUQCState == true)
                        $('#btnLoadPackUQC').prop('disabled', false);
                    else $('#btnLoadPackUQC').prop('disabled', true);
                }
            }
        });
    }

    function LoadPackUQC() {
        debugger;
        Page = 0;
        $.ajax({
            url: '/Export/CWCExport/LoadPackUQCList',
            type: 'GET',
            data: { PartyCode: "", Page: Page },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    $.each(data.Data.lstPackUQC, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                        tabvalue++;
                    });

                    $('#tblePackUQC tbody').html(html);

                    if (data.Data.PackUQCState == true)
                        $('#btnLoadPackUQC').prop('disabled', false);
                    else $('#btnLoadPackUQC').prop('disabled', true);
                }
            }
        });
    }

    function SearchPackUQCByCode() {
        debugger;
        if ($('#PackUQCbox').val() != '') {
            $.ajax({
                url: '/Export/CWCExport/SearchPackUQCByCode',
                type: 'GET',
                data: { PartyCode: $('#PackUQCbox').val() },
                success: function (data) {
                    debugger;
                    if (data.Status == 1) {
                        var html = '';
                        var tabvalue = 202;
                        $.each(data.Data.lstPackUQC, function (item, elem) {
                            html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                            tabvalue++;
                        });
                        $('#tblePackUQC tbody').html('');
                        $('#tblePackUQC tbody').html(html);
                        $('#btnLoadPackUQC').prop('disabled', false);
                        Page = -1;
                    }
                    else {
                        $('#tblePackUQC tbody').html('');
                        Page = -1;
                        $('#btnLoadPackUQC').prop('disabled', false);
                    }

                }
            });
        }
        else
            LoadPackUQC();
    }
    function SelectPackUQC(PackUQCId, PackUQCDescription, PackUQCCode) {
        debugger;
        $('#PackUQCDescription').val(PackUQCDescription);
        $('#PackUQCCode').val(PackUQCCode);
        $('#PackUQCId').val(PackUQCId);
        $("#PackUQCModal").modal("hide");
        $('#PackUQCbox').val('');
        $('[data-valmsg-for="PackUQCDescription"]').html('');
        LoadPackUQC();
        $('#Package').focus();
    }
    function ClosePackUQC() {
        debugger;
        $('#PackUQCbox').val('');
        $("#PackUQCModal").modal("hide");
        LoadPackUQC();
    }
    $('#MoveTo').change(function()
    {
        if($('#MoveTo').val()=='ICD')
        {
            $("#POD").rules("add", "required");
        }
        else
        {
