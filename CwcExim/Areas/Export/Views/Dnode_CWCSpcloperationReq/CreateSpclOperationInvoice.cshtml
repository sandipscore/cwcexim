            alert("Enter Invoice No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate() == true)
            $('#DivListOfInvoice').load('/Export/Dnode_CWCSpcloperationReq/ListOfExpInvoice?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate + "&Module=SO");
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Export/Dnode_CWCSpcloperationReq/ListOfExpInvoice?InvoiceNo=' + InvoiceNo + "&Module=SO");
        else if (Validate() == true)
