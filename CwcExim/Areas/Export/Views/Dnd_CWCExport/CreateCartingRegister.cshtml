            Validation = false;
        }
        if (($('#DivShippingDetails #CUM').val() != "" || $('#SQM').val() != "" || $('#ActualQty').val() != "" || $('#ActualWeight').val() != "") && ($('#DivShippingDetails').find('input:text').prop("disabled") == false)) {
            $('#DivCartingRegErrMsg').html('Please Save The Details Before Submitting');
            Validation = false;
        }
        if (ArrayOfCartingRegDtl.length <= 0) {
            $('#DivCartingRegErrMsg').html('Please Add Atleast One Carting Details Before Submitting');
            Validation = false;
        }
        if (ArrayOfCartingRegDtl.length > 0) {
            $.each(ArrayOfCartingRegDtl, function (i, item) {
                if (item.CartingAppDtlId == '') {
                    item.CartingAppDtlId = 0;
                }
            });
            $('#XMLData').val(JSON.stringify(ArrayOfCartingRegDtl));
        }


        if (ArrayOfCartingRegDtl.length > 0) {
            $.each(ArrayOfCartingRegDtl, function (i, item) {
                if (item.ActualQty == 0) {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                }
                else if (item.ActualWeight == 0) {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                }
                    /*else if (item.CUM == 0) {
                        Validation = false;
                        $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,CUM,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                    }*/
                else if (item.SQM == 0 && SpaceType == 'GeneralSpace') {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                }
                else if (item.LocationDetails == null || item.LocationDetails == '') {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                }
            });

        }

        if (Validation == true) {

            var isValid = $('#frmCartingReg').valid();
            if (isValid) {
                var Status = confirm("Are you sure you want to Save?");
                if (Status == true) {
                    $('#frmCartingReg').submit();
                }
                else {
                    $('#BtnSaveCartingReg').prop("disabled", false);
                    return false;
                }

            }
            else {
                $('#BtnSaveCartingReg').prop("disabled", false);
            }
        }
        else {
            $('#BtnSaveCartingReg').prop("disabled", false);
        }
    }

