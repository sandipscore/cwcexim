        $("#ShippingLineModal").modal("hide");
    });
    function getSpace() {
        debugger;
        var GodownId = 0;
        if ($('#hdnShippingLineList').val() != '' && $('#hdnShippingLineList').val() != undefined) {
            var parties = JSON.parse($('#hdnShippingLineList').val());
            GodownId = $.grep(parties, function (item) { return item.ShippingLineId == $('#ShippingLineId').val(); })[0].GodownId;
            $('#GodownId').val(GodownId);
            var GodownName = $.grep(parties, function (item) { return item.ShippingLineId == $('#ShippingLineId').val(); })[0].GodownName;
            $('#GodownName').val(GodownName);

            if (GodownId > 0) {
                $('#ReservedSpace').prop("checked", true);
                $('#GeneralSpace').prop("checked", false);
                OnClickSpace("ReservedSpace");
                selectGodown(GodownId);
            }
            else {
                $('#ReservedSpace').prop("checked", false);
                $('#GeneralSpace').prop("checked", true);
