    function CalculateRemaining()
    {
        debugger;

        if($('#ActualQty').val() =="")
        {
            $('#ActualQty').val(0);
        }
        if($('#ActualWeight').val() =="")
        {
            $('#ActualWeight').val(0);
        }
        if($('#FOBValue').val() =="")
        {
            $('#FOBValue').val(0);
        }
        if (($('#ActualQty').val() >= 0) && $('#ActualQty').val()!="" && $('#ActualQty').val() !=undefined)
        {
            var RemUnit = $('#NoOfUnits').val() - $('#ActualQty').val();
