        $('#ExporterId,#ShippingLineId,#ContainerNo,#ShippingBillNo,#CommodityId,#CargoDescription,#NoOfUnits,#FobValue,#GrossWt,#ExporterName,#ShippingLineName,#CommodityName,#CustomSealNo,#ShippingSealNo,#TareWt').val('');   
