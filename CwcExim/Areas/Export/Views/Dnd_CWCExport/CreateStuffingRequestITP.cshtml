    function LoadStuffingReq() {
        $('#DivBody').load('/Export/Dnd_CWCExport/CreateStuffingRequestITP');



    }
    $('#btnSubmit').on("click", function () {
        debugger;
        $('#btnSubmit').prop("disabled", true);
        if ($('#TblStuffingReq >tbody').html() == '') {
            $('#DivTblStuffingErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }

        else if ($('#TblStuffingContr >tbody').html() == '') {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
        else if ($('#ShippingBillNo').val() != '' && $('#BtnAddStuffingDtl').prop("disabled") == false) {
            $('#DivTblStuffingErrMsg').html('Please Add The Above Records');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
        else if ($('#ContainerNo').val() != '' && $('#BtnAddContainer').prop("disabled") == false) {
            $('#DivTblContrErrMsg').html('Please Add The Above Records');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
            //else if($('#CartingRegisterNo').val()==''){
            //     $('[data-valmsg-for="CartingRegisterNo"]').html('<span>Fill Out This Field</span>');
            // return false;
            //}
        else if ($('#TblStuffingContr >tbody').html() == "") {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
        if (ContainerDet == 0) {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
        if (RequestDet == 0) {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            $('#btnSubmit').prop("disabled", false);
            return false;
        }
            //else if ($('#ContainerNo').val() == '') {
            //    $('[data-valmsg-for="ContainerNo"]').html('<span>Fill Out This Field</span>');
            //    return false;
            //}
        else {
            $('#StuffingXML').val(JSON.stringify(RequestDet));
            $('#ContainerXML').val(JSON.stringify(ContainerDet));
            var isValid = $('#frmStuffingRequest').valid();
            if (isValid) {
                var Status = confirm("Are you sure you want to Save?");
                if (Status == true) {
                    $('#frmStuffingRequest').submit();
                }
                else {
                    $('#btnSubmit').prop("disabled", false);
                    return false;
                }
            }
            else {
                $('#btnSubmit').prop("disabled", false);
                return false;
            }
        }        
    });

