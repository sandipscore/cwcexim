<script>
    var RequestDet = [];
    var CheckDet = [];
    var ContainerDet = [];
    var NoOfUnits = 0, GrossWt = 0;
    var html = '';
    $(function () {
        debugger;
        $('#TblStuffingReq').hide();
        $('#TblStuffingContr').hide();
        $('#BtnAddStuffingDtl').prop("disabled", true);
        //GetStuffingReqList();
        // $('#DivCartingDetails input[type="text"]').prop("readonly", true);
        // $('#DivCartingDetails select,#DivCartingDetails textarea').prop("disabled", true);
        if ('@ViewData["IsExporter"]' == 'True') {
            $('#ExporterName').val('@ViewData["ExporterName"]');
            $('#ExporterId').val('@ViewData["EximTraderId"]');
            var EximTraderId = '@ViewData["EximTraderId"]';
            var ExporterName = '@ViewData["ExporterName"]';
            $('#LstExporter').html('<li id=' + @ViewData["EximTraderId"] +' onclick="FillExporterDet(\'' + EximTraderId + '\',\'' + ExporterName + '\')">@ViewData["ExporterName"]</li>');
        }
        if ('@Model.CHAId' > 0) {
            $('#CHANameId').val('@Model.CHAId');
            $('#CHAName').val('@Model.CHA');
            $('#CHAId').val('@Model.CHAId');
            $('#CHA').val('@Model.CHA');
            var CHAId = '@Model.CHAId';
            var CHA = '@Model.CHA';
            $('#LstCHA').html('<li id=' + @Model.CHAId +' onclick="FillCHADet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
            $('#ListOfCHAHdr').html('<li id=' + @Model.CHAId +' onclick="FillCHAHdrDet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');

            //$('#ShippingLineHdrList').html('<li id=' + @Model.CHAId +' onclick="FillCHAHdrDet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
        }
    });
    function FillExporterDet(EximTraderId, ExporterName) {
        debugger;
        $('#ExporterName').val(ExporterName);
        $('#ExporterId').val(EximTraderId);
        $("#ExporterModal").modal("hide");
    }
    function FillCHAHdrDet(CHAId, CHA) {
        debugger;
        $('#CHAId').val(CHAId);
        $('#CHA').val(CHA);
        $("#CHAHdrModal").modal("hide");
    }
    function FillCHADet(CHAId, CHA) {
        debugger;
        $('#CHANameId').val(CHAId);
        $('#CHAName').val(CHA);
        $("#CHAModal").modal("hide");
    }

    $('#ForwarderList li').on("click", function () {
        $('#Forwarder').val($(this).text());
        $('#ForwarderId').val($(this).attr('id'));
        $("#ForwarderModal").modal("hide");
    });

    $('#MainlineList li').on("click", function () {
        $('#Mainline').val($(this).text());
        $('#MainlineId').val($(this).attr('id'));
        $("#MainlineModal").modal("hide");
    });


    var ListOfVia = [];
    $('#ViaSearch').on("click", function () {
        debugger;
        $.ajax({
            url: "/Export/Dnd_CWCExport/GetViaForLoadContReq",
            type: "GET",
            dataType: "JSON",
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    var Html = '';
                    ListOfVia = data.Data;
                    $.each(data.Data, function (i, item) {
                        debugger
                        Html += '<li id=' + item.ViaId + ' onclick="FillViaBox(&quot;' + item.Via + '&quot;,&quot;' + item.ViaId + '&quot;)">' + item.Via + '</li>';
                    });
                    $('#lstVia').html(Html);
                }
            }

        });
    });


    function FillViaBox(Via, ViaId) {
        debugger;
        $('#ViaId').val(ViaId);
        $('#Via').val(Via);
        $("#ViaModal").modal("hide");
        var ViaId = $('#ViaId').val();;
        VesselDet(ViaId);
    }


    $('#Viabox').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#lstVia > li').show();
        } else {
            $('#lstVia > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });

    var ListOfForeign = [];
    $('#ForeignSearch').on("click", function () {
        debugger;
        $.ajax({
            url: "/Export/Dnd_CWCExport/GetForeignLinerForStuffingReq",
            type: "GET",
            dataType: "JSON",
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    var Html = '';
                    ListOfForeign = data.Data;
                    $.each(data.Data, function (i, item) {
                        debugger
                        Html += '<li id=' + item.StuffingReqId + ' onclick="FillForeignBox(&quot;' + item.ForeignLiner + '&quot;,&quot;' + item.StuffingReqId + '&quot;)">' + item.ForeignLiner + '</li>';
                    });
                    $('#lstForeign').html(Html);
                }
            }

        });
    });


    function FillForeignBox(ForeignLiner, StuffingReqId) {
        debugger;
        //$('#StuffingReqId').val(StuffingReqId);
        $('#ForeignLiner').val(ForeignLiner);
        $("#ForeignModal").modal("hide");

    }


    $('#Foreignbox').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#lstForeign > li').show();
        } else {
            $('#lstForeign > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });


    function VesselDet(ViaId) {
        debugger;
        $.ajax({
            url: "/Export/Dnd_CWCExport/GetVesselInfoForLoadContReq",
            type: "GET",
            dataType: "JSON",
            data: { ViaId: ViaId },
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    $('#ViaId').val(data.Data.ViaId);
