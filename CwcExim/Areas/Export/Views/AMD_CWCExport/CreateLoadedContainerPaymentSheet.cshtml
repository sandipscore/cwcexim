    function ContainerSelect() {
        ArrayContainer = [];
        $('#lstcfsCode tbody tr').each(function () {
            var Chk = $(this).find('input[type="checkbox"]');
            if ($(Chk).is(':checked')) {
                var Container = {
                    "CFSCode": $(Chk).attr('id')
                }
                ArrayContainer.push(Container);
            }
        });

        var Weighment = 0;
        if ($('#Weighment').is(':checked')) {
            Weighment = 1;

        }
        var CustomBottleSealCharge = 0;
        if ($('#CustomBottleSealCharge').is(':checked')) {
            CustomBottleSealCharge = 1;

        }
        var Sweeping = 0;
        if ($('#Sweeping').is(':checked')) {
            Sweeping = 1;

        }
        var Fumigation = 0;
        if ($('#Fumigation').is(':checked')) {
            Fumigation = 1;

        }

        var _Insured = 0;
        if ($('#Insured').is(':checked')) {
            _Insured = 1;

        }
                

        if (ArrayContainer.length > 0 && $('#InvoiceDate').val() != "") {
            $.ajax({
                url: '/Export/AMD_CWCExport/GetLoadedContainerPaymentSheet',
                type: 'POST',
                data: {
                    InvoiceDate: $('#InvoiceDate').val(),
                    InvoiceType: TaxType,
                    StuffingReqId: parseInt($('#StuffingReqId').val()),
                    lstPaySheetContainer: ArrayContainer,
                    PayeeId: $('#PayeeId').val(),
                    PartyId: $('#PartyId').val(),
                    InvoiceId: 0,
                    OTHour: $('#OTHour').val(),
                    OnWheelHours: $('#OnWheelHours').val(),
                    ReeferHours: $('#ReeferHours').val(),
                    ExportUnder: $('#SEZ').val(),
                    CfsToFactory: $('#CfsToFactory').val() == "" ? 0 : $('#CfsToFactory').val(),
                    Weighment: Weighment,
                    CustomBottleSealCharge: CustomBottleSealCharge,
                    Sweeping: Sweeping,
                    Fumigation: Fumigation,                   
                    CartStuffPer: $('#CartStuffPer').val() == "" ? 100 : $('#CartStuffPer').val(),
                    Insured: _Insured
                },
                dataType: 'JSON',
                success: function (data) {
                    if (data != null && data != 'undefined') {
                        $('#PaymentSheetModelJson').val(JSON.stringify(data));
                        $('#SEZ').prop("disabled", true);
                        

                        var html = '';
                        var html1 = '';
                        //All H&T binding in modal
                        
                        var HTCharge = $.grep(data.lstPostPaymentChrg, function (item) { return item.ChargeType == "HT"; });
                        $.each(HTCharge, function (i, item) {
                            
                            var result = $.grep(data.ActualApplicable, function (e) { return e == item.Clause; });
                            if (result.length == 0) {
                                html += '<li id="liAHT' + i + '"><div class="boolean-container"><input id="chkAHT' + i + '" type="checkbox" data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkAHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                            }
                            else {
                                html += '<li id="liAHT' + i + '"><div class="boolean-container"><input id="chkAHT' + i + '" type="checkbox" checked data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkAHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                            }
                            html1 += '<li id="liNHT' + i + '"><div class="boolean-container"><input id="chkNHT' + i + '" type="checkbox" data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkNHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                        });
                        $('#lstHT').html(html);
                        $('#lstNewHT').html(html1);
                        //$('#btnSave').removeAttr('disabled');
                        //Container Binding
                        html = '';
                        
                        $.each(data.lstPSCont, function (i, item) {
                            html += '<tr><td>' + (i + 1) + '</td><td>' + item.CFSCode + '</td><td>' + item.ContainerNo + '</td><td>' + item.Size + '</td><td>' + (item.Insured == 0 ? 'No' : 'Yes') + '</td>';//
                        });
                        $('#tblContainer tbody').html(html);

                        //CWC Charge Binding
                        debugger;
                        var CWCCharge = $.grep(data.lstPostPaymentChrg, function (item) { return item.ChargeType == "CWC"; });
                        html = '';
                        var TotalCWC = 0;
                        $.each(CWCCharge, function (i, item) {
                            TotalCWC += item.Total;
                            //html += '<tr><td>' + item.Clause + '. ' + item.ChargeName + '</td>'
                            html += '<tr><td>' + item.ChargeName + '</td>'
                                    + '<td><input class="text-right" id="' + item.OperationId + '" type="text" value="' + item.Amount.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="IGSTP' + item.OperationId + '" type="text" value="' + item.IGSTPer + '" readonly /></td>'
                                    + '<td><input class="text-right" id="IGSTA' + item.OperationId + '" type="text" value="' + item.IGSTAmt.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="CGSTP' + item.OperationId + '" type="text" value="' + item.CGSTPer + '" readonly /></td>'
                                    + '<td><input class="text-right" id="CGSTA' + item.OperationId + '" type="text" value="' + item.CGSTAmt.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="SGSTP' + item.OperationId + '" type="text" value="' + item.SGSTPer + '" readonly /></td>'
                                    + '<td><input class="text-right" id="SGSTA' + item.OperationId + '" type="text" value="' + item.SGSTAmt.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="TOT' + item.OperationId + '" type="text" value="' + item.Total.toFixed(2) + '" readonly /></td></tr>';
                        });
                        $('#tblCWCCharges tbody').html(html);
                        $('#TOTCWCChrg').val((TotalCWC).toFixed(2));

                        $('.search').css('display', 'none');
                        $('#InvoiceDate').parent().find('img').css('display', 'none');

                        $('#OnWheelHours').attr("disabled", 'disabled');
                        $('#ReeferHours').attr("disabled", 'disabled');
                        $('#CfsToFactory').attr("disabled", 'disabled');
                        $('#CartStuffPer').attr("disabled", 'disabled');

                        $('#Weighment').prop("disabled", true);
                        $('#CustomBottleSealCharge').prop("disabled", true);
                        $('#Sweeping').prop("disabled", true);
                        $('#Fumigation').prop("disabled", true);
                        $('#Insured').prop("disabled", true);
                        //$('#btnAddExtraHT').removeAttr('disabled');
                        //CalcTax(1);
                        BindHnTCharges();
                    }
                    else {
