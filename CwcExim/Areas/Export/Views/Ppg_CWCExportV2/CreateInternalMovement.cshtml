<div id="PartyModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" data-dismiss="modal" onclick="LoadPartyList()"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">Choose Payee Name</h4>
