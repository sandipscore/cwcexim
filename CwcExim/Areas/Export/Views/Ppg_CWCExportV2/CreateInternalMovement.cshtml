
@model CwcExim.Areas.Export.Models.PPG_ContainerMovementV2

<style>
    .ang_srch {
        top: 3%;
        right: 7px;
        height: 19px;
        padding: 0 4px;
        display: block;
        background: #ebebe4;
        position: absolute;
        cursor: pointer;
    }

    #tbleParty.dataTable tbody > tr > td {
        cursor: pointer;
        text-align: left;
    }

    #tblePayee.dataTable tbody > tr > td {
        cursor: pointer;
        text-align: left;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }

    #tblSHP.dataTable tbody > tr > td {
        cursor: pointer;
    }
</style>

<div>
    <div class="row">
        <div class="col-md-12">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>Container / CBT Movement-Export</h4>
                    </div>
                </div>
                @using (Ajax.BeginForm("AddEditInternalPaymentSheet", "Ppg_CWCExportV2", new AjaxOptions { HttpMethod = "POST", OnSuccess = "MovementOnSuccess", OnFailure = "MovementOnFailure" }, new { @Id = "PaymentSheetForm" }))
                {
                    @Html.Hidden("ContainerJson")
                    @Html.Hidden("BOEJson")
                    @Html.Hidden("hdnAddress")
                    @Html.Hidden("hdnState")
                    @Html.Hidden("hdnStateCode")
                    @Html.Hidden("hdnAddress2")
                    @Html.Hidden("hdnState2")
                    @Html.Hidden("hdnStateCode2")
                    @Html.Hidden("hdnAddress3")
                    @Html.Hidden("hdnState3")
                    @Html.Hidden("hdnStateCode3")
                    @Html.Hidden("GST1")
                    @Html.Hidden("GST2")
                    @Html.Hidden("GST3")
                    @Html.Hidden("PaymentSheetModelJson")
                    @Html.Hidden("PaymentSheetModelJsonn")
                    @Html.Hidden("PaymentSheetModelJsonnn")
                    @Html.Hidden("lstShipbillwiseAmountV2")
                    @Html.Hidden("ServiceType")
                    @Html.Hidden("SEZ1")
                    if (ViewBag.RightsList != null)
                    {
                        <input type="hidden" id="hdnRights" value="@ViewBag.RightsList" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnRights" value="" />
                    }

                    if (ViewBag.LstContainerNo != null)
                    {
                        <input type="hidden" id="hdnContainer" value="@ViewBag.LstContainerNo" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnContainer" value="" />
                    }
                    if (ViewBag.PaymentParty != null)
                    {
                        <input type="hidden" id="hdnPartyPayee" value="@ViewBag.PaymentParty" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnPartyPayee" value="" />
                    }

                    if (ViewBag.ShippingLineList != null)
                    {
                        <input type="hidden" id="hdnsline" value="@ViewBag.ShippingLineList" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnsline" value="" />
                    }

                    @Html.AntiForgeryToken()
                    <div class="content_wrp">
                        <div id="DivAddMovement">

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Movement No:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.MovementNo, new { @placeholder = "Movement No", @readonly = true })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Movement Date:</label>
                                    </div>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.MovementDate, new { @placeholder = "Movement Date", @readonly = true })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Container / CBT No:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.ContainerStuffingDtlId)
                                        @Html.HiddenFor(m => m.ContainerStuffingId)
                                        @Html.TextBoxFor(m => m.Container, new { @placeholder = "Search Container / CBT No", @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="ShowContainerNo();" id="ContModal" data-target="#BOENoModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.Container, "", new { @class = "Error_Msg" })
                                        <span style="color:red" id="ErrContainer"></span>
                                    </div>
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>ICD Code:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.CFSCode, new { @placeholder = "Enter ICD Code", @readonly = true })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Move To:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.LocationId)
                                        @Html.HiddenFor(m => m.Cargo)
                                        @Html.TextBoxFor(m => m.LocationName, new { @placeholder = "Search Move To", @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="ShowLocation();" id="ModalLocation" data-target="#LocationModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.LocationName)
                                        <span style="color:red" id="ErrLocation"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Reworking of Container / CBT at Gateway Port:</label>
                                    </div>
                                        @Html.Hidden("port")
                                        @Html.DropDownList("hdnport", new List<SelectListItem>{new SelectListItem { Text="Yes",Value="1"},new SelectListItem { Text="No",Value="2"}}, "----Select----", new { @onchange = "$('#port').val($('#hdnport').val())" })
                                        @Html.ValidationMessage("port", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>SLA Seal No:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.SealNo, new { @placeholder = "Enter SLA Seal No" })
                                </div>
                                <div class="col-md-3 col-md-offset-6" style="display:none">
                                    <div class="form-group">
                                        <label>Date:</label>
                                    </div>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.SealDate, new { @placeholder = "Select Date", @readonly = true })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Custom Seal No.:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.CustomSealNo, new { @placeholder = "Enter Custom Seal No" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Date:</label>
                                    </div>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.CustomSealDate, new { @placeholder = "Select Date", @readonly = true })
                                    </div>
                                </div>
                            </div>


                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>POD:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.POD, new { @placeholder = "Enter POD" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Transport Mode:</label>
                                    </div>
                                    @Html.TextBox("TranMode", null, new { id = "TranMode", placeholder = "Enter Transport Mode", @readonly = "readonly" })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Shipping Line:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.sid)
                                        @Html.TextBoxFor(m => m.Sline, new { @placeholder = "Search Shipping Line", @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreSHP();" data-target="#ShippingLineModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.Sline, "", new { @class = "Error_Msg" })
                                        <span style="color:red" id="ErrShipping"></span>
                                    </div>
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Shipping Bill No.:</label>
                                    </div>
                                    @Html.TextBox("Shipbillno",null, new { @id = "Shipbillno", @placeholder = "Enter Shipping Bill No", @readonly = true })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Tare Weight (Kg):</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.TareWeight, new { @placeholder = "Enter Tare Weight(Kg.)" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Cargo Weight (Kg):</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.CargoWeight, new { @readonly = true })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Tare Weight (ELWB):</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.ElwbTareWeight, new { @readonly = true })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Cargo Weight (ELWB):</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.ElwbCargoWeight, new { @readonly = true })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Party:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.partyId)
                                        @Html.TextBoxFor(m => m.Party, new { @placeholder = "Search Party", @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="PartyBinding();" id="ModalParty" data-target="#PartyModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.Party, "", new { @class = "Error_Msg" })
                                        <span style="color:red" id="ErrParty"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Payee:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.PayeeID)
                                        @Html.TextBoxFor(m => m.PayeeName, new { @placeholder = "Search Payee", @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="PayeeBinding();" id="ModalPayee" data-target="#PayeeModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.PayeeName, "", new { @class = "Error_Msg" })
                                        <span style="color:red" id="ErrParty"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row SignUp_space_bottom SignUp_space_top"></div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Invoice No.:</label>
                                    </div>
                                    @Html.TextBoxFor(m => m.Invoice, new { @placeholder = "Enter Invoice No", @readonly = true })
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>SEZ</label>
                                    </div>
                                        @Html.DropDownList("SEZ", new List<SelectListItem>
                                        {
                                        new SelectListItem { Text="SEZWP",Value="SEZWP"},
                                        new SelectListItem {Text="SEZWOP",Value="SEZWOP" }

                                        }, "---Select---", new {})
                                        @Html.ValidationMessage("SEZ", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label></label>
                                        </div>
                                        <input type="button" value="Calculate Charges" id="btnCalc" onclick="GetInternalInvoice()" class="btn btn-primary mr-1 btn-100px" />
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <table id="tblWeighCharges" class="table table-bordered table-striped dataTable Table_center">
                                        <thead>
                                            <tr>
                                                <th rowspan="2">Charges</th>
                                                <th rowspan="2">Value</th>
                                                <th colspan="2" class="text-center">IGST</th>
                                                <th colspan="2" class="text-center">CGST</th>
                                                <th colspan="2" class="text-center">SGST</th>
                                                <th rowspan="2" style="width:15%;">Total</th>
                                            </tr>
                                            <tr>
                                                <th>%</th>
                                                <th>Amt</th>
                                                <th>%</th>
                                                <th>Amt</th>
                                                <th>%</th>
                                                <th style="border-right:1px solid #ddd;">Amt</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                        <tfoot>
                                            <tr>
                                                <td colspan="8" style="text-align:right;">Total:</td>
                                                <td class="Table_right">
                                                    <input type="text" id="AllTotal" value="0" readonly>
                                                </td>
                                            </tr>
                                            @*<tr>
                                                    <td colspan="8" style="text-align:right;">TDS %:</td>
                                                    <td class="Table_right"><input type="text" id="CWCTDSPer" value="0" onblur="CalcInvoice()"></td>
                                                </tr>
                                                <tr>
                                                    <td colspan="8" style="text-align:right;">TDS Amount:</td>
                                                    <td class="Table_right"><input type="text" id="TOTCWCTDS" value="0" readonly></td>
                                                </tr>*@
                                        </tfoot>
                                    </table>
                                </div>
                            </div>



                            <div class="row SignUp_space_bottom SignUp_space_top">
                                <div class="stoke"></div>
                            </div>

                        <div class="d-flex justify-content-left">
                            <input type="button" id="savbtn" class="btn btn-primary mr-1 btn-100px" value="Save" onclick="BindJson()">
                            <input type="button" id="btnReset" class="btn btn-primary-border mr-1 btn-100px" value="Reset" onclick="ResetMovement()">
                            <input type="button" id="BtnGenerateIRN" class="btn btn-primary mr-1 btn-100px" value="Generate IRN" onclick="GenerateIRN()">
                            <input type="button" id="Print" class="btn btn-primary mr-1 btn-100px" value="Print" onclick="PrintInvoice()">
                        </div>

                            <div class="d-flex justify-content-left Form_Space_top">
                                <input type="button" id="btnShowList" class="btn btn-primary mr-1 btn-100px" value="Show List" onclick="$('#DivMovementList').load('/Export/Ppg_CWCExportV2/ListOfMovementInv'); $('#btnShowList').prop('disabled', true);">
                            </div>
                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div id="DivMovementList"></div>
                            </div>
                        </div>

                            <div class="logSuccMsg" id="DivSucMsg" style="background-color:transparent"></div>
                            <div class="logErrMsg Form_Space_top" id="DivErrMsg" style="background-color:transparent"></div>
                        </div>
                }
            </div>
        </div>
    </div>
</div>

