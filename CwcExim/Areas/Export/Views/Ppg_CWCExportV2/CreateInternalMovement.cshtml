                </table>
                <div id="slim_scrollSHP">
                    <table class="table dataTable table-bordered table-striped slim_tble" id="tblSHP">
                        <tbody></tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-6">
                        <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreSHP()" class="btn btn-primary mr-1 btn-100px" id="btnSHP">Load More Data</button>
                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn btn-primary-border btn-100px" onclick="CloseSHP()">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

