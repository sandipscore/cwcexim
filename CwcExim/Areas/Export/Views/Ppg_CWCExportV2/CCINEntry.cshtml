@model CwcExim.Areas.Export.Models.CCINEntryV2
<style>
    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }
    #tblSHP.dataTable tbody > tr > td,
    #tblEXP.dataTable tbody > tr > td,
    #tblCHA.dataTable tbody > tr > td
    {
        cursor: pointer;
    }
    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }
</style>
<script>
    //function PaymentMode() {
    //    var PaymentMode = $('#hdnPaymentMode').val();
    //    alert(PaymentMode);
    //}
</script>

    <div class="row">
        <div class="col-md-12">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>CCIN Entry</h4>
                    </div>
                    @*<input type="hidden" id="hdnPaymentMode" value="@ViewBag.PaymentMode" />*@
                </div>
                @using (Ajax.BeginForm("AddEditCCINEntry", "Ppg_CWCExportV2", new AjaxOptions
                {

                    HttpMethod = "POST",
                    OnSuccess = "OnSuccess",
                    OnFailure = "OnFailure"
                }, new { id = "frmCCINEntry" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.Id)
                    @Html.HiddenFor(m => m.InvoiceId)
                    @Html.HiddenFor(m => m.PartySDBalance)
                    @Html.HiddenFor(m => m.IsInGateEntry)
                    @Html.HiddenFor(m => m.OTEHr)
                    if (ViewBag.check != null)
                    {
                        <input type="hidden" id="hdnShippingLineList" value="@ViewBag.check" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnShippingLineList" value="" />
                    }
                    if (ViewBag.ListOfSBNo != null)
                    {
                        <input type="hidden" id="hdnShippingBillList" value="@ViewBag.ListOfSBNo" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnShippingBillList" value="" />
                    }

                    if (ViewBag.ListOfPort != null)
                    {
                        <input type="hidden" id="hdnPortName" value="@ViewBag.PortName" />
                        <input type="hidden" id="hdnPortId" value="@ViewBag.PortId" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnPortName" value="" />
                        <input type="hidden" id="hdnPortId" value="" />
                        <input type="hidden" id="hdnListOfPort" value="" />
                    }

                    <div class="content_wrp">
                        <div class="panel with-nav-tabs panel-primary cwc_panel" style="margin-top: 15px;">

                            <div class="panel-heading cwc_panel_heading">
                                <ul class="nav nav-tabs" id="DivTabs">
                                    <li class="active"><a href="#DivCCIN" data-toggle="tab" onclick="SetActiveTab('CCIN')">CCIN</a></li>

                                </ul>
                            </div>

                            <div class="panel-body">
                                <div class="tab-content">
                                    <div class="tab-pane fade in active" id="DivCCIN">
                                        <div id="DivJOHdr" class="Form_Space_bottom">

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>CCIN App No.:</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.CCINNo, new {@placeholder="", @readonly = true })
                                                </div>
                                                <div class="col-md-3 col-md-offset-6">
                                                    <div class="form-group">
                                                        <label>CCIN App Date:</label>
                                                    </div>
                                                    <div class="position-relative Date_Img">
                                                        <input type="hidden" id="CCINAppDate" />
                                                        @Html.TextBoxFor(m => m.CCINDate, new { @placeholder = "Select CCIN App. Date", @readonly = true })
                                                        <span style="color:red" id="ErrAppCCINAppDate"></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>SB No:</label>
                                                    </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.SBId)
                                                        @Html.TextBoxFor(m => m.SBNo,new {@placeholder="Search SB No." })
                                                        @Html.ValidationMessageFor(m => m.SBNo, "", new { @class = "Error_Msg" })
                                                        <span class="input_icon" id="ShippingBillLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="ShippingBillSerach();" data-target="#SBNoModal"></i></span>
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label></label>
                                                    </div>
                                                        @Html.CheckBoxFor(m => m.IsSEZ)
                                                        <label for="IsSEZ"><i class="square"></i><span>SEZ</span></label>
                                                </div>

                                                <div class="col-md-3 col-md-offset-4">
                                                    <div class="form-group">
                                                        <label>SB Date:</label>
                                                    </div>
                                                    <div class="position-relative Date_Img">
                                                        <input type="hidden" id="CCINAppDate" />
                                                        @Html.TextBoxFor(m => m.SBDate, new {@placeholder= "Select SB Date", @readonly = true })
                                                        @Html.ValidationMessageFor(m => m.SBDate, "", new { @class = "Error_Msg" })
                                                        <span style="color:red" id="ErrAppCCINAppDate"></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>SB Type:</label>
                                                        @Html.DropDownListFor(m => m.SBType, new List<SelectListItem>
                                                    {
                                                    new SelectListItem {Text="Baggage",Value="1" },
                                                    new SelectListItem {Text="Duty Free Goods",Value="2" },
                                                    new SelectListItem {Text="Cargo in Drawback",Value="3" }
                                                    }, "----Select----", new { })
                                                        @Html.ValidationMessageFor(m => m.SBType, "", new { @class = "Error_Msg" })
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        <label>Exporter:</label>
                                                    </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.ExporterId)
                                                        @Html.TextBoxFor(m => m.ExporterName, new {@placeholder= "Search Exporter", @readonly = true })
                                                        @Html.ValidationMessageFor(m => m.ExporterName, "", new { @class = "Error_Msg" })
                                                        <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreEXP();" data-target="#ExporterModal"></i></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                         <label>Shipping Line:</label>
                                                    </div>
                                                    <div class="position-relative">
                                                            @Html.HiddenFor(m => m.ShippingLineId)
                                                            @Html.TextBoxFor(m => m.ShippingLineName, new {@placeholder= "Search Shipping Line", @readonly = true })
                                                            <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreSHP();" data-target="#ShippingLineModal"></i></span>
                                                     </div>
                                                 </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        <label>CHA:</label>
                                                    </div>
                                                    <div class="position-relative">
                                                            @Html.HiddenFor(m => m.CHAId)
                                                            @Html.TextBoxFor(m => m.CHAName, new {@placeholder="Search CHA", @readonly = true })
                                                            @Html.ValidationMessageFor(m => m.CHAName, "", new { @class = "Error_Msg" })
                                                            <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreCHA();" data-target="#CHAModal"></i></span>
                                                     </div>
                                                </div>
                                             </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        <label>Consignee:</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.ConsigneeName, new { @placeholder = "Enter consignee name" })
                                                    @Html.ValidationMessageFor(m => m.ConsigneeName, "", new { @class = "Error_Msg" })
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        <label>Consignee Address:</label>
                                                    </div>
                                                        @Html.TextAreaFor(m => m.ConsigneeAdd, new { @placeholder = "Enter consignee address" })
                                                        @Html.ValidationMessageFor(m => m.ConsigneeAdd, "", new { @class = "Error_Msg" })
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        @Html.LabelFor(m => m.CountryId)
                                                        @if (ViewBag.Country != null)
                                                        {
                                                            @Html.DropDownListFor(m => m.CountryId, new SelectList(ViewBag.Country, "CountryId", "CountryName"), "---Select---", new { @onchange = "GetStateList()" })
                                                        }
                                                        else
                                                        {
                                                            @Html.DropDownListFor(m => m.CountryId, new SelectList("", "Value", "Text"), "---Select---", new { })
                                                        }
                                                        @Html.ValidationMessageFor(m => m.CountryId, "", new { @class = "Error_Msg" })
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-12">
                                                    <div class="form-group">
                                                        @Html.HiddenFor(m => m.SelectStateId)
                                                        @Html.LabelFor(m => m.StateId, new { @class = "InputLabel" })
                                                    </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.PortOfDestId)
                                                        @Html.TextBoxFor(m => m.PortOfDestName, new {@placeholder="Search Port of Destination", @readonly = true })
                                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="PortOfDestSerach();" data-target="#PortDestModal"></i></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                <div class="form-group">
                                                        <label>Commodity:</label>
                                                 </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.CommodityId)
                                                        @Html.TextBoxFor(m => m.CommodityName, new { @placeholder = "Search Commodity", @readonly = true })
                                                        <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="CommoditySerach();" data-target="#CommodityModal"></i></span>
                                                    </div>
                                                </div>

                                                <div class="col-md-3 col-md-offset-6">
                                                    <div class="form-group">
                                                        <label>Pack UQC :</label>
                                                    </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.PackUQCId)
                                                        @Html.HiddenFor(m => m.PackUQCCode)
                                                        @Html.TextBoxFor(m => m.PackUQCDescription, new {@placeholder= "Search Pack UQC", @readonly = true, tabindex = 18, onkeydown = "PopupActivate('PackUQCModal');" })
                                                        @Html.ValidationMessageFor(m => m.PackUQCDescription, "", new { @class = "Error_Msg" })
                                                        <span class="input_icon" id="PackUQCSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PackUQCModal"></i></span>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>Cargo Type:</label>
                                                        @Html.DropDownListFor(m => m.CargoType, new List<SelectListItem>
                                                    {
                                                        new SelectListItem {Text="HAZ",Value="1" },
                                                        new SelectListItem {Text="NON-HAZ",Value="2" }

                                                    }, "----Select----", new { @disabled = "disabled" })
                                                        @Html.HiddenFor(m => m.CargoTypeID)
                                                        @Html.ValidationMessageFor(m => m.CargoType, "", new { @class = "Error_Msg" })
                                                    </div>
                                                </div>

                                                <div class="col-md-3" style="display:none">
                                                    <div class="form-group">
                                                        @Html.HiddenFor(m => m.SelectCityId)
                                                        @Html.LabelFor(m => m.CityId, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                                                    </div>
                                                    @Html.DropDownListFor(m => m.CityId, new SelectList("", "Value", "Text"), "---Select---", new { @class = "form-control input-sm dropdown" })
                                                    @*@Html.ValidationMessageFor(m => m.CityId, "", new { @class = "Error_Msg" })*@
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>Port of Loading:</label>
                                                    </div>
                                                    <div class="position-relative">
                                                        @Html.HiddenFor(m => m.PortOfLoadingId)
                                                        @Html.TextBoxFor(m => m.PortOfLoadingName, new {@placeholder= "Search Port of Loading", @readonly = true })
                                                        @Html.ValidationMessageFor(m => m.PortOfLoadingName, "", new { @class = "Error_Msg" })
                                                        <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="PortOfDestSerach();" data-target="#PortModal"></i></span>
                                                    </div>
                                                </div>
                                                <div class="col-md-3 col-md-offset-6">
                                                    <div class="form-group">
                                                        <label>Port of Discharge:</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.PortOfDischarge, new {@placeholder= "Enter Port of Discharge" })
                                                    @Html.ValidationMessageFor(m => m.PortOfDischarge, "", new { @class = "Error_Msg" })
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>No of Package:</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.Package, new {@placeholder= "Enter No of Package" })
                                                </div>
                                                <div class="col-md-3 col-md-offset-6">
                                                    <div class="form-group">
                                                        <label>Weight (Kg):</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.Weight, new {@placeholder= "Enter Weight", @onblur = "ResetInvoice()" })
                                                    @Html.ValidationMessageFor(m => m.Weight, "", new { @class = "Error_Msg" })
                                                </div>
                                            </div>

                                            <div class="row Form_Space_top">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label>FOB:</label>
                                                    </div>
                                                    @Html.TextBoxFor(m => m.FOB, new { @placeholder="Enter FOB",@onblur = "ResetInvoice()" })
                                                    @Html.ValidationMessageFor(m => m.FOB, "", new { @class = "Error_Msg" })
                                                </div>

                                                <div class="col-md-3 col-md-offset-6">
                                                    <div class="form-group">
                                                        <label>Package Type:</label>
                                                    </div>

                                                    @Html.DropDownListFor(m => m.PackageType, new List<SelectListItem>
                                                    {
                                                        new SelectListItem { Text="Package",Value="P"},
                                                        new SelectListItem { Text="Liquid Bulk",Value="LB"},
                                                        new SelectListItem { Text="Dry Bulk",Value="DB"}
                                                    }, new { })
                                                    @Html.ValidationMessage("PackageType", new { @class = "Error_Msg" })
                                                </div>
                                            </div>


                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                        </div>
                        <div class="d-flex justify-content-left">
                            <input type="button" id="btnSave" class="btn btn-primary mr-1 btn-100px" value="Save" onclick="return validate();">
                            <input type="button" id="btnReset" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetAllField()">
                        </div>

                        @*<div class="row SignUp_space_top2">
                            <div class="col-md-offset-3 col-md-2">
                                <input type="button" id="btnSave" class="btn log_Btn_sm" onclick="return validate();" value="Save" />
                            </div>
                            <div class="col-md-2">
                                <input type="button" id="btnReset" class="btn log_Btn_sm" value="Reset" onclick="ResetAllField()" />
                            </div>
                            <div class="col-md-2">
                                <input type="button" id="btnPrint" class="btn log_Btn_sm" value="Print" onclick="PrintInvoice()" disabled />
                            </div>
                        </div>*@

                        <div class="logSuccMsg Form_Space_top" id="DivSucMsg" style="background-color:transparent"></div>
                        <div class="logErrMsg Form_Space_bottom" id="DivErrMsg" style="background-color:transparent"></div>
                        <div class="row">
                            <div class="col-md-2 SignUp_space_bottom">
                                <input type="button" id="BtnCCINEntryLoad" value="Show List" class="btn btn-primary mr-1 btn-100px" onclick="LoadCCINEntryList()" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div id="tblEntryDet"></div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div id="CCINEntryList"></div>
                            </div>
                        </div>
                    </div>
                }
                        
                    </div>

        </div>
    </div>

