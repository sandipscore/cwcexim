@model CwcExim.Areas.Export.Models.Ppg_CargoShifting

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row">
                <div class="col-md-12">
                    <h4>Cargo Shifting</h4>
                </div>
            </div>
            <div class="content_wrp">
                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Shifting No:</label>
                        </div>
                        @Html.TextBoxFor(m => m.ShiftingNo, new { @readonly = true })
                    </div>
                    <div class="col-md-3 col-md-offset-6">
                        <div class="form-group">
                            <label>Shifting Date:</label>
                        </div>
                        <div class="position-relative Date_Img">
                            @Html.TextBoxFor(m => m.ShiftingDt, new { @readonly = true })
                        </div>
                    </div>
                </div>
                <div class="row Form_Space_top" style="display:none;">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Invoice No :</label>
                        </div>
                        <div class="col-md-2-5">
                            @Html.TextBoxFor(m => m.InvoiceNo, new { @readonly = true })
                        </div>
                    </div>
                </div>
                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Shifting Type:</label>
                        </div>
                        <select disabled="true" id="ShiftingType">
                            <option value="2">Godown To Godown</option>
                            <option value="3">Both</option>
                        </select>
                    </div>
                </div>
                <div class="row Form_Space_top">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>From Shipping:</label>
                        </div>
                        @Html.TextBoxFor(m => m.FromShippingName, new { @readonly = true })
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>To Shipping:</label>
                        </div>
                        @Html.TextBoxFor(m => m.ToShippingName, new { @readonly = true })
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>From Godown:</label>
                        </div>
                        @Html.TextBoxFor(m => m.FromGodownName, new { @readonly = true })
                    </div>
                    <div class="col-md-3 col-md-offset-6">
                        <div class="form-group">
                            <label>To Godown:</label>
                        </div>
                        @Html.TextBoxFor(m => m.ToGodownName, new { @readonly = true })
                    </div>
                </div>

                <div class="row Form_Space_top Form_Space_bottom">
                    <div class="col-md-12">
                        <table class="table table-striped table-bordered dataTable bigger_table" id="tblShippingDetails" style="width:100%;">
                            <thead>
                                <tr>
                                    <th class="text-center" style="width:8%;">@Html.DisplayName("Entry No")</th>
                                    <th class="text-center">@Html.DisplayName("Carting Date")</th>
                                    <th class="text-center">@Html.DisplayName("Ship Bill No")</th>
                                    <th class="text-center">@Html.DisplayName("Ship Bill Date")</th>
                                    <th class="text-center">@Html.DisplayName("Package")</th>
                                    <th class="text-center">@Html.DisplayName("Gross Weight (Kg)")</th>
                                    <th class="text-center">@Html.DisplayName("SQM")</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                foreach (var data in Model.lstCartingRegisterDtl)
                                {
                                <tr>
                                    <td class="text-center">@data.CartingRegisterNo</td>
                                    <td class="text-center">@data.RegisterDate</td>
                                    <td class="text-center">@data.ShippingBillNo</td>
                                    <td class="text-center">@data.ShippingBillDate</td>
                                    <td class="text-center">@data.ActualQty</td>
                                    <td class="text-center">@data.ActualWeight</td>
                                    <td class="text-center">@data.SQM</td>
                                </tr>
                                }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="d-flex justify-content-left Form_Space_top">
                    <input type="button" id="BtnMOveCargoShp" class="btn btn-primary mr-1 btn-100px" value="Back" onclick="$('#DivBody').load('/Export/Ppg_CWCExportV2/CreateCargoShifting')">
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function ()
    {
        $('#IsApproved').prop("checked", '@Convert.ToBoolean(Model.IsApproved)');
        $('#ShiftingType').val(@Model.ShiftingType);
        
    });
</script>