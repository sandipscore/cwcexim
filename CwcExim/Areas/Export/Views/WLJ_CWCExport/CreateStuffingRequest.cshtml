    var Wtperunit = 0;
    function ClearErrMsg(elem) {
        debugger;
        if ($('#StuffQuantity').val() != "") {
            var CommodityName = $("#CommodityName option:selected").text();
            CommodityName = CommodityName.split("-");
            $.each(RequestDet, function (i, item1) {
                debugger;
                if (item1.CartingRegisterId == $('#CartingRegisterId').val() && item1.ShippingBillNo == CommodityName[1]) {
                    Wtperunit = RequestDet[i].GrossWeight / RequestDet[i].NoOfUnits
                    $('#StuffWeight').val((Wtperunit * $('#StuffQuantity').val()).toFixed(2));
                }
            });
        }
        else
        {
            $('#StuffWeight').val('');
        }
        $('[data-valmsg-for=" ' + elem.id + ' "]').html('<span></span>');
    }
    function CalculateFOBnGW(elem)
    {
        debugger;
        if (CheckDet != [] && $('#CartingRegisterDtlId').val()!='')
        {
            var Units = GW = Fob= 0;
            Units = (CheckDet.filter(function (item) { return item.CartingRegisterDtlId == $('#CartingRegisterDtlId').val() })[0]).NoOfUnits;
            GW = (CheckDet.filter(function (item) { return item.CartingRegisterDtlId == $('#CartingRegisterDtlId').val() })[0]).GrossWeight;
            Fob = (CheckDet.filter(function (item) { return item.CartingRegisterDtlId == $('#CartingRegisterDtlId').val() })[0]).Fob;
            if (Units > 0)
            {
                $('#GrossWeight').val(Number(($('#NoOfUnits').val() * (GW / Units)).toFixed(2)));
                $('#Fob').val(Number(($('#NoOfUnits').val() * (Fob / Units)).toFixed(2)));
                $('[data-valmsg-for=" ' + elem.id + ' "]').html('<span></span>');
            }
        }
    }

