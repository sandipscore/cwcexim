                            </div>
                            <div class="col-md-10-5 col_cus_10">
                                <span class="ReQSimbol"></span>
                                @Html.TextBoxFor(m => m.ShippingHdrLine, new { @class = "form-control input-sm", @readonly = "readonly" })
