        if ($('#CartingAppId').val() == '') {
            $('#CartingAppId').val(0);
        }
        if ($('#CartingAppDtlId').val() == '') {
            $('#CartingAppDtlId').val(0);
        }
        if($('#ApplicationNo').val()==''){
            $('#ApplicationNo').val('0')
        }
        if($('#ApplicationDate').val()==''){
            $('#ApplicationDate').val('@DateTime.Now.ToString("dd/MM/yyyy")')
        }

        if ($('#CartingAppId').val() == 0) {
            $('[data-valmsg-for="ApplicationNo"]').html('<span>CCIN No. Required.</span>');
            Validation = false;
        }
        if ($('#CartingType').val() == '' ) {
            $('[data-valmsg-for="CartingType"]').html('<span>Carting Type Required.</span>');
            Validation = false;
        }
        if ($('#CommodityType').val() == '') {
            $('[data-valmsg-for="CommodityType"]').html('<span>Commodity Type Required.</span>');
            Validation = false;
        }
        if ($('#hdnCHAId').val() == '' || $('#hdnCHAId').val() == '0') {
            $('[data-valmsg-for="CHAName"]').html('<span>CHA Name Required.</span>');
            Validation= false;
        }
        if ($('#hdnGodownId').val() == '' || $('#hdnGodownId').val() == '0') {
            $('[data-valmsg-for="GodownName"]').html('<span>Godown Required.</span>');
            Validation= false;
        }
        if (($('#DivShippingDetails #CUM').val() != "" || $('#SQM').val() != "" || $('#ActualQty').val() != "" || $('#ActualWeight').val() != "" ) && ($('#DivShippingDetails').find('input:text').prop("disabled") == false)) {
            $('#DivCartingRegErrMsg').html('Please Save The Details Before Submitting');
            Validation= false;
        }
        if (ArrayOfCartingRegDtl.length<=0) {
            $('#DivCartingRegErrMsg').html('Please Add Atleast One Carting Details Before Submitting');
            Validation= false;
        }
        if(ArrayOfCartingRegDtl.length>0) {
            $.each(ArrayOfCartingRegDtl, function (i, item) {
                if (item.CartingAppDtlId == '') {
                    item.CartingAppDtlId = 0;
                }
            });
            $('#XMLData').val(JSON.stringify(ArrayOfCartingRegDtl));
        }


        if (ArrayOfCartingRegDtl.length > 0) {
            $.each(ArrayOfCartingRegDtl, function (i, item) {
                if (item.ActualQty == 0) {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
                }
                else if (item.ActualWeight == 0) {
                    Validation = false;
                    $('#DivCartingRegErrMsg').html('In Carting Details ActualQty,ActualWeight,SQM can not be Zero. And Location can not be blank. Edit Carting Details.');
