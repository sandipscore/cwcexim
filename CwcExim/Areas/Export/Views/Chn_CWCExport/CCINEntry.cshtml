    function CalcTax(elem) {
        debugger;
        var rawJson = JSON.parse($('#PaymentSheetModelJson').val());
        
        var selectedItem = $.grep(rawJson, function (item) { return item.ChargeId == elem; })[0];

        var Hour = Number($('#OTHr').val());
        $('#OTEHr').val(Hour);
        var MAmt = Hour * selectedItem.Rate;
        $('#' + elem).val(MAmt);
        var IP = 0;
        var CP = 0;
        var SP = 0;

        //if (JSON.parse($('#IsPartyStateInCompState').val())) {
        if ($('#IsPartyStateInCompState').val()) {
            CP = Number($('#CGSTP' + elem).val());
            SP = Number($('#SGSTP' + elem).val());
        }
        else {
            IP = Number($('#IGSTP' + elem).val());
        }

        $('#IGSTA' + elem).val(Math.ceil((MAmt * (IP / 100)).toFixed(2)).toFixed(2)); //ceil added
        $('#CGSTA' + elem).val(Math.ceil((MAmt * (CP / 100)).toFixed(2)).toFixed(2)); //ceil added
        $('#SGSTA' + elem).val(Math.ceil((MAmt * (SP / 100)).toFixed(2)).toFixed(2)); //ceil added
        // $('#TOT' + elem).val((MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100))).toFixed(2));  // decimal value
        //$('#TOT' + elem).val(Math.ceil(MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100))));     // ceilling value


        selectedItem.Quantity = Hour;
        selectedItem.Amount = MAmt;
        selectedItem.Taxable = MAmt;
        //selectedItem.IGSTPer = IP;
        selectedItem.IGSTAmt =Math.ceil((MAmt * (IP / 100)).toFixed(2)); //ceil added
        //selectedItem.CGSTPer = CP;
        selectedItem.CGSTAmt = Math.ceil((MAmt * (CP / 100)).toFixed(2)); //ceil added
        //selectedItem.SGSTPer = SP;
        selectedItem.SGSTAmt = Math.ceil((MAmt * (SP / 100)).toFixed(2)); //ceil added
        // selectedItem.Total = (MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100)));          // decimal value
        //selectedItem.Total = Math.ceil(MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100)));  // ceilling value
        selectedItem.Total = selectedItem.Amount + selectedItem.IGSTAmt + selectedItem.CGSTAmt + selectedItem.SGSTAmt;
        $('#TOT' + elem).val(selectedItem.Total);

        $('#PaymentSheetModelJson').val(JSON.stringify(rawJson));

        CalcInvoice();
    }

