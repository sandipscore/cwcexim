    function checkdat() {
        //debugger;
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth() + 1; //January is 0!
        var yyyy = today.getFullYear();

        if (dd < 10) {
            dd = '0' + dd;
        }
        if (mm < 10) {
            mm = '0' + mm;
        }

        var date = dd + '/' + mm + '/' + yyyy;
        var dateTime = date;

        if ($('#RegisterDate').val() != '') {
            var result = moment($('#RegisterDate').val(), "DD/MM/YYYY", true).isValid();
            if (!result) {
                $('#Msges').html('Incorrect Date');
                setTimeout(function () { $('#Msges').html(''); }, 3000);
                $('#RegisterDate').val(dateTime);                
            }
        }

    }
