    function PopulatedContainerDetails(ExpoterId)
    {
        $('#tblContainerDetails').html("");
        var html = '';
        $.ajax({
            url: '/Export/WFLD_CWCExport/GetContainerDetailsForBTT',
            type: 'GET',
            dataType: 'JSON',
            data: { 'ExpoterId': ExpoterId },
            success: function (data) {
                debugger;
                if (data.Status != 0) {
                    $('#ContainerDetailsXml').val(JSON.stringify(data.Data));
                    $('#tblContainerDetails').html('');
                    var html = '';
                    var k = 1;
                    $.each(data.Data, function (i, item) {
                        var odcval = 0;
                        if (item.IsOdc == "ODC")
                        {
                            odcval = 1;
                        }

                        var haz = item.HazNonHaz == 1 ? 'HAZ' : item.HazNonHaz == 2 ? 'NON HAZ' : 'ODC';

                        if (odcval == 1) {
                            html += "<tr><td>" + k + "</td><td>" + item.ContrainerNo + "</td><td>" + item.Size + "</td><td>" + item.SbNo + "</td><td>" + item.SbDate + "</td><td>" + item.CommodityName + "</td><td>" + haz + "</td><td><input type='checkbox' class='checkBoxodcClass' checked id=od" + k + " /><label for=od" + k + "><span><i class='square'></i></span></label></td><td><input type='checkbox' class='checkBoxClass' id=Ch" + k + " /><label for=Ch" + k + "><span><i class='square'></i></span></label></td><td style='display:none;'>" + item.CFSCode + "</td><td style='display:none;'>" + item.EntryDate + "</td><td style='display:none;'>" + item.isLoadedContrainer + "</td><td style='display:none;'>" + item.IsOdc + "</td><td style='display:none;'>" + item.Weight + "</td><td style='display:none;'>" + item.ContainerStuffingId + "</td><td style='display:none;'>" + item.NoofPkg + "</td><td style='display:none;'>" + item.ShippingLineId + "</td><td style='display:none;'>" + item.ChargesFor + "</td><tr>";
                        }
                        else
                        {
                            html += "<tr><td>" + k + "</td><td>" + item.ContrainerNo + "</td><td>" + item.Size + "</td><td>" + item.SbNo + "</td><td>" + item.SbDate + "</td><td>" + item.CommodityName + "</td><td>" + haz + "</td><td><input type='checkbox' class='checkBoxodcClass'  id=od" + k + " /><label for=od" + k + "><span><i class='square'></i></span></label></td><td><input type='checkbox' class='checkBoxClass' id=Ch" + k + " /><label for=Ch" + k + "><span><i class='square'></i></span></label></td><td style='display:none;'>" + item.CFSCode + "</td><td style='display:none;'>" + item.EntryDate + "</td><td style='display:none;'>" + item.isLoadedContrainer + "</td><td style='display:none;'>" + item.IsOdc + "</td><td style='display:none;'>" + item.Weight + "</td><td style='display:none;'>" + item.ContainerStuffingId + "</td><td style='display:none;'>" + item.NoofPkg + "</td><td style='display:none;'>" + item.ShippingLineId + "</td><td style='display:none;'>" + item.ChargesFor + "</td><tr>";
                        }
                        k++;
                    });
                    $('#tblContainerDetails').html(html);

                    $(".checkBoxClass").click(function () {

                        if (!$(this).prop("checked")) {
                            $("#chBttContainerDetails").prop("checked", false);
                        }
                    });

                }
            }
        });
    }

