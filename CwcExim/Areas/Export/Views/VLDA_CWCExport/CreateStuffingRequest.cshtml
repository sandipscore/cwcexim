    function ResetStuffingReq() {
        debugger;
        $('#CommodityName').html('');
        $('#CommodityName').append($('<option></option>').val('').html('---Select---'));
        $('#DivCreateStuffingReq').find('input:text').val('');
        $('#DivCreateStuffingReq').find('input:hidden').val('');
        $('#TblStuffingReq >tbody').html('');
        $('#DivCreateStuffingReq #TblStuffingContr >tbody').html('');
        $('#TblStuffingReq').hide('');
        $('#TblStuffingReqTotal').hide();
        $('#TblStuffingContr').hide('');
        $('[data-valmsg-for]').html('<span></span>');
        $('#DivStuffingMsg').html('');
        $('#DivStuffingErrMsg').html('');
        RequestDet = [];
        ContainerDet = [];
        if ('@ViewData["IsExporter"]' == 'True') {
            $('#ExporterName').val('@ViewData["ExporterName"]');
            $('#ExporterId').val('@ViewData["EximTraderId"]');
            var EximTraderId = '@ViewData["EximTraderId"]';
            var ExporterName = '@ViewData["ExporterName"]';
            $('#LstExporter').html('<li id=' + @ViewData["EximTraderId"] +' onclick="FillExporterDet(\'' + EximTraderId + '\',\'' + ExporterName + '\')">@ViewData["ExporterName"]</li>');
        }
        if ('@Model.CHAId' > 0) {
            $('#CHANameId').val('@Model.CHAId');
            $('#CHAName').val('@Model.CHA');
            $('#CHAId').val('@Model.CHAId');
            $('#CHA').val('@Model.CHA');
            var CHAId = '@Model.CHAId';
            var CHA = '@Model.CHA';
            $('#LstCHA').html('<li id=' + @Model.CHAId +' onclick="FillCHADet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
            $('#ListOfCHAHdr').html('<li id=' + @Model.CHAId +' onclick="FillCHAHdrDet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
        }
        var remsize = 0;
        var remweight = 0;
        editflag = 0;
        $('#DivBody').load('/Export/VLDA_CWCExport/CreateStuffingRequest');
    }


    function StuffingReqOnSuccess(data) {
        $('#DivStuffingErrMsg').html('');
        if (data.Status == 1) {
            $('#DivStuffingMsg').html(data.Message);
            // $("#ContainerModal ul li:contains(" + $('#ContainerNo').val() + ")").remove();
            $("#myModal ul li:contains(" + $('#CartingRegisterNo').val() + ")").remove();

            setTimeout(LoadStuffingReq, 2000);
            debugger;
            var ContainerJson = JSON.parse($('#hdnContainer').val());
            var html = '';
            var tab = 401;
            $.each(ContainerJson, function (i, item) {
                html += '<li tabindex=' + tab + '_' + i + ' id="' + item.CFSCode + '" onkeypress="GetContainerDet(' + item.CFSCode + ');" onclick="GetContainerDet(' + item.CFSCode + ');">' + item.ContainerNo + '</li>';
            });

            $('#ContainerList').html(html);

            editflag = 0;




            //GetStuffingReqList();
        }
        else {
            $('#DivStuffingErrMsg').html(data.Message);
        }
    }
    function StuffingReqOnFailure() { }
    function LoadStuffingReq() {
        $('#DivBody').load('/Export/VLDA_CWCExport/CreateStuffingRequest');
    }


    function GetAllContainerList() {
        debugger;
        var data = $('#hdnContainer').val();
        $('#ContainerList').html('');
        if (data == '' || data == null) {

        }
        else {
            var jsonData = JSON.parse(data);
            var html = '';
            var tab = 401;
            $.each(jsonData, function (i, item) {
                html += '<li tabindex=' + tab + '_' + i + ' id="' + item.CFSCode + '">' + item.ContainerNo + '-' + item.ShippingLine + '</li>';
            });

            $('#ContainerList').html(html);
        }

    }

    function ShippingLine_Click()
    {
       // debugger;
       // alert(0);
        $.ajax({
            url: '/Export/VLDA_CWCExport/ShippingDetail',
            type: 'GET',
            dataType: 'JSON',
           // data: { },
            success: function (data) {
                debugger;
                if(data.Status==1)
                {
                    var html = '';
                    var tabvalue = 401;
                    var u = '_';
                    var i = 0;
                    $.each(data.Data, function (key, val) {
                        html += '<li tabindex="' + tabvalue + '_' + i + '" onkeypress="SelectShippingLine(' + val.ShippingLineId + ',&quot;' + val.ShippingLineName + '&quot;)" onclick="SelectShippingLine(' + val.ShippingLineId + ',&quot;' + val.ShippingLineName + '&quot;)" id=' + val.ShippingLineId + '>' + val.ShippingLineName + '</li>';
                        tabvalue++;
                        i++;

                    });
                    $('#ShippingLineList').html(html);
                    $('#ShippingLineModal').modal('show');
                }
               
            }
        });
    }

