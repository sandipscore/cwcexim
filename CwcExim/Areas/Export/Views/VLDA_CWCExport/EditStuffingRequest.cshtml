    function CartingRQtyChange(id)
    {
        debugger;
        var RemainingQty = $('#RemainingPkg' + id).val() == '' ? 0 : $('#RemainingPkg' + id).val();
        var StuffQty = $('#StuffPkg' + id).val() == '' ? 0 : $('#StuffPkg' + id).val();
        if (parseFloat(RemainingQty) < parseFloat(StuffQty))
        {
            alert('Entered Stuffing Qty exceed remaining Qty');
            $('#StuffPkg' + id).val('');
        }
        else if (parseFloat(StuffQty)==0)
        {
            $('#StuffPkg' + id).focus();
            return;
        }
        else {
            RequestDet[id].StuffQty = StuffQty;
            var FOB = $('#StuffFOB' + id).val() == '' ? 0 : $('#StuffFOB' + id).val();

            var RemFOB = (parseFloat(FOB) / parseFloat(RemainingQty)) * parseFloat(StuffQty);
            $('#StuffFOB' + id).val(RemFOB);
            RequestDet[id].StuffQty = StuffQty;
            RequestDet[id].Fob = RemFOB;
        }
        
        
    }
    
