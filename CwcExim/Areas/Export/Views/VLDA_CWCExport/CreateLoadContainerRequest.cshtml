<script type="text/javascript" src="/scripts/jquery.nicescroll.min.js"></script>
<script>
    $(function () {
        $('#slim_scroll_2').slimScroll({
            allowPageScroll: true
        });
    });
    $(function () {
        $('#slim_scroll_3').slimScroll({
            allowPageScroll: true
        });
    });

    $(function () {
        //LoadList();
        $('#ShippingBillDate').val('@ViewBag.Currentdt');
        $('#LoadContReqId').val('0');
        $('#LoadContReqDate').focus();
    });

    function AllowOnlyDecimal(evt) {
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }
        if ((CharCode > 47 & CharCode < 58) || (CharCode == 46) || CharCode == 8) {
            return true;
        }
        else {
            evt.preventDefault;
            return false;
        }
    }
    function AllowOnlyInteger(evt) {
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }
        if ((CharCode > 47 & CharCode < 58) || CharCode == 8) {
            return true;
        }
        else {
            evt.preventDefault;
            return false;
        }
    }

    var ArrayContDet = [];
    function AddContDet() {
        if (ValidateContDet()) {
            debugger;
            var ContDet = {
                "ExporterId": $('#ExporterId').val(),
                "ShippingLineId": ($('#ShippingLineId').val() == '' ? 0 : $('#ShippingLineId').val()),
                "ContainerNo": $('#ContainerNo').val(),
                "Size": $('#Size').val(),
                "IsReefer": ($('#IsReefer').is(':checked') ? true : false),
                "IsInsured": ($('#IsInsured').is(':checked') ? true : false),
                "ShippingBillNo": $('#ShippingBillNo').val(),
                "ShippingBillDate": $('#ShippingBillDate').val(),
                "CommodityId": $('#CommodityId').val(),
                "CargoType": $('#CargoType').val(),
                "CargoDescription": $('#CargoDescription').val(),
                "GrossWt": $('#GrossWt').val(),
                "NoOfUnits": $('#NoOfUnits').val(),
                "FoBValue": $('#FobValue').val(),
                "ExporterName": $('#ExporterName').val(),
                "ShippingLineName": $('#ShippingLineName').val(),
                "CommodityName": $('#CommodityName').val(),
                "ExamType": $('#ExamType').val(),
                "ContClass": $('#ContClass').val(),
                "OutGauge": ($('#IsOutGauge').is(':checked') ? true : false),
                "PODId": ($('#PODId').val()==''?0:$('#PODId').val()),
                "POLid": ($('#ContPOL').val()==''?0:$('#ContPOL').val()),
                "POD": $('#POD').val(),
                "CFSCode": $('#CFSCode').val(),
                "EquipmentSealType": $('#EquipmentSealType').val(),
                "EquipmentStatus": $('#EquipmentStatus').val(),
                "EquipmentQUC": $('#EquipmentQUC').val(),
                "PackageType": $('#PackageType').val(),
                "ContLoadType": $('#ContLoadType').val(),
                "CustomSeal": $('#CustomSeal').val(),
                "PackUQCCode": $('#PackUQCCode').val(),
                "PackUQCDescription": $('#PackUQCDescription').val(),
                "SEZ": ($('#IsSEZ').is(':checked') ? 1 : 0),
            };
            ArrayContDet.push(ContDet);
            $('#tblArrayContDet').html('');
            GenerateTable();
            ResetContDet();
            $('[data-valmsg-for]').html('');
        }
    }
    function GenerateTable() {
        var tablehtml = '<table id="tblArrayContDet" class="table table-striped table-bordered dataTable" style="width:100%;"><thead><tr><th class="text-center">SL No</th><th>Container / CBT No</th><th>Size</th><th>Exporter</th><th>No of Units</th><th class="text-center">Edit</th><th class="text-center">View</th></tr></thead><tbody>';
        $.each(ArrayContDet, function (i, data) {
            tablehtml += '<tr><td class="text-center">' + (i + 1) + '</td><td>' + data.ContainerNo + '</td><td>' + data.Size + '</td><td>' + data.ExporterName + '</td><td>' + data.NoOfUnits + '</td><td class="text-center"><a href="#" onclick="EditContDet(' + i + ')"><i class="fa fa-pencil-square Edit"></i></a></td><td class="text-center"><a href="#" onclick="ViewContDet(' + i + ')"><i class="fa fa-search-plus View"></i></a></td></tr>';
        });
        tablehtml += '</tbody></table>';
        $('#tblArrayContDet').html(tablehtml);
    }
    function ResetContDet() {
        //$('#ExporterId,#ShippingLineId,#ContainerNo,#ShippingBillNo,#CommodityId,#CargoDescription,#NoOfUnits,#FobValue,#GrossWt,#ExporterName,#ShippingLineName,#CommodityName').val('');
        //$('#Size').val('---Select---');
        //$('#CargoType').val('---Select---');
        //$('#PODId').val(''),
        //$('#ContPOL').val(''),
        //$('#POD').val('')

        $('#ShippingBillNo,#NoOfUnits,#FobValue,#GrossWt,#CustomSeal').val('');
        $('#EquipmentSealType').val('BTSL');
        $('#EquipmentStatus').val('94');
        $('#EquipmentQUC').val('PKG');
        $('#PackageType').val('P');
        $('#ContLoadType').val('---Select---');
        $('#IsInsured,#IsOutGauge').prop("checked", false);
        $('#ShippingBillDate').val('@ViewBag.Currentdt');
        $('#CargoType').val('2');
        $('#btnAdd').prop("disabled", false);
        $('#IsSEZ').prop('checked', false);
       // $('#CFSCode').val('')


    }
    function EditContDet(i) {
        debugger;
        $('#btnAdd').prop("disabled", false);
        $('#ExporterId').val(ArrayContDet[i].ExporterId);
        $('#ExporterName').val(ArrayContDet[i].ExporterName);
        $('#ShippingLineId').val(ArrayContDet[i].ShippingLineId);
        $('#ShippingLineName').val(ArrayContDet[i].ShippingLineName);
        $('#ContainerNo').val(ArrayContDet[i].ContainerNo);
        $('#Size').val(ArrayContDet[i].Size);
        $('#IsReefer').prop("checked", ArrayContDet[i].IsReefer);
        $('#IsInsured').prop("checked", ArrayContDet[i].IsInsured);
        $('#IsOutGauge').prop("checked", ArrayContDet[i].OutGauge);
        $('#ShippingBillNo').val(ArrayContDet[i].ShippingBillNo);
        $('#ShippingBillDate').val(ArrayContDet[i].ShippingBillDate);
        $('#CommodityName').val(ArrayContDet[i].CommodityName);
        $('#CommodityId').val(ArrayContDet[i].CommodityId);
        $('#CargoType').val(ArrayContDet[i].CargoType);
        $('#CargoDescription').val(ArrayContDet[i].CargoDescription);
        $('#NoOfUnits').val(ArrayContDet[i].NoOfUnits);
        $('#GrossWt').val(ArrayContDet[i].GrossWt);
