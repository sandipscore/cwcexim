@model CwcExim.Areas.Export.Models.WFLD_LoadContReq
<style>
    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }

</style>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                @using (Ajax.BeginForm("AddEditLoadContReq", "VLDA_CWCExport", new AjaxOptions
                {
                    Confirm = "Are you sure you want to Save?",
                    HttpMethod = "POST",
                    OnSuccess = "contReqOnSuccess",
                    OnFailure = "contReqFailure",
                    InsertionMode = InsertionMode.InsertAfter
                }))
                {
                    if (ViewBag.RightsList != null)
                    {
                        <input type="hidden" id="hdnRights" value="@ViewBag.RightsList" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnRights" value="" />
                    }

                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.LoadContReqId)
                    @Html.HiddenFor(m => m.StringifyData)
                    @Html.Hidden("CFSCode")
                    @Html.Hidden("PODId")
                    <div class="row">
                        <div class="col-md-12">
                            <h4>Loaded Container / CBT Request</h4>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2 padding_rt">
                                <label>Container / CBT Request No:</label>
                            </div>
                            <div class="col-md-2-5">
                                @Html.TextBoxFor(m => m.LoadContReqNo, new { @class = "form-control input_sm", @readonly = true })
                            </div>
                            <div class="col-md-1-5 col-md-offset-4-5 col_cus_offset_3 col_cus_2 padding_rt">
                                <label>Container / CBT Request Date:</label>
                            </div>
                            <div class="col-md-2 col_cus_2_5 Date_Img">
                                @Html.TextBoxFor(m => m.LoadContReqDate, new { @class = "form-control input_sm", @readonly = true })
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>CHA:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">
                                @Html.HiddenFor(m => m.CHAId)
                                @Html.TextBoxFor(m => m.CHAName, new { @class = "form-control input_sm", @readonly = true, @onkeydown = "PopupActivate('CHAModal');" })
                                <span class="search" id="CHASerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CHAModal"></i></span>
                                @Html.ValidationMessageFor(m => m.CHAName, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Forwarder:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">
                                @Html.HiddenFor(m => m.ForwarderId)
                                @Html.TextBoxFor(m => m.Forwarder, new { @class = "form-control input_sm", @readonly = true, @onkeydown = "PopupActivate('ForwarderModal');" })
                                <span class="search" id="ForwarderSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ForwarderModal"></i></span>
                                @Html.ValidationMessageFor(m => m.Forwarder, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Main line:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">                              
                                @Html.TextBoxFor(m => m.MainLine, new { @class = "form-control input_sm", @maxlength = 45 })                               
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Foreign Liner:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">                                
                                @Html.TextBoxFor(m => m.ForeignLiner, new { @class = "form-control input_sm", @readonly = true, @maxlength = 45, @onkeydown = "PopupActivate('ForeignLinerModal');" })
                                <span class="search" id="ForeignLinerSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ForeignLinerModal"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2 padding_rt">
                                <label>Vessel:</label>
                            </div>
                            <div class="col-md-2-5">
                                @Html.TextBoxFor(m => m.Vessel, new { @class = "form-control input_sm", @maxlength = 45 })                     
                            </div>
                            <div class="col-md-1-5 col_cus_2 col_cus_offset_3 col-md-offset-4-5 padding_rt">
                                <label>Voyage:</label>
                            </div>
                            <div class="col-md-2 col_cus_2_5 Date_Img">
                                @Html.TextBoxFor(m => m.Voyage, new { @class = "form-control input_sm", @maxlength = 45 })
                            </div>
                        </div>
                    </div>
                    <div id="divDestination" class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Final Destination location:</label>
                            </div>
                            <div class="col-md-4-5">
                                @*<span class="ReQSimbol">*</span>*@
                                @Html.HiddenFor(m => m.FinalDestinationLocationID)
                                @Html.TextBoxFor(m => m.FinalDestinationLocation, new { @class = "form-control input_sm", @readonly = true, @onkeydown = "PopupActivate('DestinationModal');" })
                                <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#DestinationModal" onclick="GetFinalDestination()"></i></span>
                                @Html.ValidationMessageFor(m => m.FinalDestinationLocation, "", new { @class = "Error_Msg" })
                            </div>

                        </div>
                    </div>
                    <div class="row Form_Space_top Form_Space_bottom">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                @Html.LabelFor(m => m.Remarks, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            </div>
                            <div class="col-md-10-5 col_cus_10">
                                @Html.TextAreaFor(m => m.Remarks, new { @class = "form-control input_sm", @maxlength = 500, @style = "width:100%;height:100px;" })
                                @Html.ValidationMessageFor(m => m.Remarks, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Movement:</label>
                            </div>
                            <div class="col-md-2-5">
                                @Html.DropDownList("Movement", new List<SelectListItem> {
                                    new SelectListItem { Text="CWC",Value="CWC"},
                                    new SelectListItem { Text="Own Transport(FR)",Value="Own Transport(FR)"}}, "---Select---", new { @class = "form-control input-sm dropdown" })
                                @Html.ValidationMessage("Movement", new { @class = "Error_Msg" })
                            </div>
                            


                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2">
                                        <label>Custom Examination Type:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.DropDownList("CustomExaminationType", new List<SelectListItem>
                                        {
                                            new SelectListItem { Text="Open",Value="Open"},
                                            new SelectListItem { Text="Seal Checking",Value="SealChecking"},

                                        }, "----Select----", new { @class = "form-control input-sm dropdown" })
                                        @Html.ValidationMessage("CustomExaminationType", new { @class = "Error_Msg" })
                                    </div>
                                   

                                </div>
                            </div>
                      

                    <div id="DivShipping" class="Head_h5 Form_Space_top">
                        <div class="row">
                            <div class="col-md-12">
                                <h5>Container / CBT Details</h5>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Exporter:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.Hidden("ExporterId")
                                    @Html.TextBox("ExporterName", "", new { @class = "form-control input-sm", @readonly = true, @onkeydown = "PopupActivate('ExporterModal');" })
                                    <span class="search" id="ExporterSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ExporterModal"></i></span>
                                    @Html.ValidationMessage("ExporterId", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Shipping Line:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.Hidden("ShippingLineId")
                                    @Html.TextBox("ShippingLineName", "", new { @class = "form-control input-sm", @readonly = true, @onkeydown = "PopupActivate('ShippingLineModal');" })
                                    <span class="search" id="ShippingLineSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                    @Html.ValidationMessage("ShippingLineId", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Container / CBT No:</label>
                                </div>
                                <div class="col-md-2-5 col_cus_10">
                                    @Html.TextBox("ContainerNo", "", new { @class = "form-control input-sm", @maxlength = 15, @onkeydown = "PopupActivate('ContainerModal');" })
                                    <span class="search" id="ContainerSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ContainerModal"></i></span>
                                    @Html.ValidationMessage("ContainerNo", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Size:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownList("Size", new List<SelectListItem> {
                                    new SelectListItem { Text="20",Value="20"},
                                    new SelectListItem { Text="40",Value="40"}}, "---Select---", new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("Size", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1 col_cus_1_5">
                                    <div class="boolean-container">
                                        @Html.CheckBox("IsReefer")
                                        <label for="IsReefer"><i class="square"></i><span>Reefer</span></label>
                                    </div>
                                </div>
                                <div class="col-md-1 col_cus_1_5">
                                    <div class="boolean-container">
                                        @Html.CheckBox("IsInsured")
                                        <label for="IsInsured"><i class="square"></i><span>Insured</span></label>
                                    </div>
                                </div>
                                <div class="col-md-1 col_cus_1_5">
                                    <div class="boolean-container">
                                        @Html.CheckBox("IsSEZ")
                                        <label for="IsSEZ"><i class="square"></i><span>SEZ</span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Shipping Bill No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("ShippingBillNo", "", new { @class = "form-control input-sm", @onblur = "ClearErrMsg(this)", maxlength = "7" })
                                    @Html.ValidationMessage("ShippingBillNo", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col_cus_2 col_cus_offset_3 col-md-offset-4-5 padding_rt">
                                    <label>Shipping Bill Date:</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5 Date_Img">
                                    @Html.TextBox("ShippingBillDate", "", new { @class = "form-control input-sm", @readonly = true })
                                    @Html.ValidationMessage("ShippingBillDate", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Commodity:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.Hidden("CommodityId")
                                    @Html.TextBox("CommodityName", "", new { @class = "form-control input-sm", @readonly = true, @onkeydown = "PopupActivate('CommodityModal');" })
                                    <span class="search" id="SearchCommodity"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CommodityModal"></i></span>
                                    @Html.ValidationMessage("CommodityId", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Cargo Type:</label>
                                </div>
                                <div class="col-md-1-5">
                                    @Html.DropDownList("CargoType", new List<SelectListItem>
                                {
                                   new SelectListItem { Text="Non HAZ",Value="2"},
                                    new SelectListItem { Text="HAZ",Value="1"},
                                    new SelectListItem {Text="ODC",Value="3"}
                                }, "---Select---", new { @class = "form-control input-sm dropdown", @onblur = "ClearErrMsg(this)" })
                                    @Html.ValidationMessage("CargoType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Examination Type:</label>
                                </div>
                                <div class="col-md-1-5">
                                    @Html.DropDownList("ExamType", new List<SelectListItem>
                                {
                                   new SelectListItem { Text="Factory",Value="Factory"},
                                    new SelectListItem { Text="On Wheel",Value="Wheel"}
                                }, new { @class = "form-control input-sm dropdown", @onblur = "ClearErrMsg(this)" })
                                    @Html.ValidationMessage("ExamType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Container Class:</label>
                                </div>
                                <div class="col-md-1-5">
                                    @Html.DropDownList("ContClass", new List<SelectListItem>
                                {
                                    new SelectListItem { Text="General",Value="General"},
                                   new SelectListItem { Text="Open Top",Value="Open Top"},
                                    new SelectListItem { Text="Flat Rec",Value="Flat Rec"},
                                    new SelectListItem { Text="ODC",Value="ODC"}
                                }, new { @class = "form-control input-sm dropdown", @onblur = "ClearErrMsg(this)" })
                                    @Html.ValidationMessage("ContClass", new { @class = "Error_Msg" })
                                </div>

                                <div class="col-md-1 col_cus_1_5">
                                    <div class="boolean-container">
                                        @Html.CheckBox("IsOutGauge")
                                        <label for="IsOutGauge"><i class="square"></i><span>OutGauge</span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="DivShippingSeal" class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Container Load Type:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownList("ContLoadType", new List<SelectListItem> {
                                    new SelectListItem { Text="FCL",Value="FCL"},
                                    new SelectListItem { Text="LCL",Value="LCL"}}, "---Select---", new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("ContLoadType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>Custom Seal:</label>
                                </div>
                                <div class="col-md-2-5 col_cus_2_5">
                                    @Html.TextBox("CustomSeal", "", new
                               {
                                   @class = "form-control input-sm",
                                   @maxlength = "30"
                               })
                                    @Html.ValidationMessage("CustomSeal", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div id="divPackage" class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Package Type:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @*<span class="ReQSimbol">*</span>*@
                                    @Html.DropDownList("PackageType", new List<SelectListItem>
                                                    {
                                                        new SelectListItem { Text="Package",Value="P"},
                                                        new SelectListItem { Text="Liquid Bulk",Value="LB"},
                                                        new SelectListItem { Text="Dry Bulk",Value="DB"}
                                                    }, new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("PackageType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>Equipment Seal Type:</label>
                                </div>
                                <div class="col-md-2-5 col_cus_2_5">
                                    @*<span class="ReQSimbol">*</span>*@
                                    @Html.DropDownList("EquipmentSealType", new List<SelectListItem>
                                                {
                                                   new SelectListItem { Text="Bottle-Seal",Value="BTSL"},
                                                   new SelectListItem { Text="E-Seal",Value="ESEAL"},
                                                   new SelectListItem { Text="Others",Value="OTH"},
                                                }, new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("EquipmentSealType", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div id="divEquipment" class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Equipment Status:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @*<span class="ReQSimbol">*</span>*@
                                    @Html.DropDownList("EquipmentStatus", new List<SelectListItem>
                                                {
                                                   new SelectListItem { Text="Intact",Value="94"},
                                                   new SelectListItem { Text="Damaged",Value="218"}
                                                }, new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("EquipmentStatus", new { @class = "Error_Msg" })
                                </div>

                                <div class="col-md-1-5 col-md-offset-0-5 col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>Pack UQC:</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5">
                                    @Html.Hidden("PackUQCId")
                                    @Html.Hidden("PackUQCCode")
                                    @Html.TextBox("PackUQCDescription","", new { @class = "form-control input_sm", @readonly = true, onkeydown = "PopupActivate('PackUQCModal');" })
                                    @Html.ValidationMessage("PackUQCDescription", new { @class = "Error_Msg" })
                                    <span class="search" id="PackUQCSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PackUQCModal"></i></span>
                                </div>                               

                                <div class="col-md-1-5 col-md-offset col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>Equipment QUC:</label>
                                </div>
                                <div class="col-md-2-5 col_cus_2_5">
                                    @*<span class="ReQSimbol">*</span>*@
                                    @Html.DropDownList("EquipmentQUC", new List<SelectListItem>
                                                {
                                                    new SelectListItem { Text="Package",Value="PKG"},
                                                    new SelectListItem { Text="Kilogram",Value="KGS"},
                                                }, new { @class = "form-control input-sm dropdown" })
                                    @Html.ValidationMessage("EquipmentQUC", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Cargo Description:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextArea("CargoDescription", "", new { @class = "form-control input-sm", @style = "width:100%;height:100px;", @onblur = "ClearErrMsg(this)", maxlength = "500" })
                                    @Html.ValidationMessage("CargoDescription", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>POD:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("POD", "", new { @class = "form-control input-sm", @readonly = true, @onkeydown = "PopupActivate('PortOfDestinationModal');" })
                                    <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PortOfDestinationModal" onclick="GetAllPortOfDestination()"></i></span>
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2">
                                    <label>POL:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownListFor(m => m.ContPOL, new SelectList(ViewBag.ListOfPOD, "Value", "Text"), "----Select----")
                                    @Html.ValidationMessage("ContPOL", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>No of Units:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("NoOfUnits", "", new { @class = "form-control input-sm", @onblur = "ClearErrMsg(this)", maxlength = "10", @onkeypress = "return AllowOnlyInteger(event)" })
                                    @Html.ValidationMessage("NoOfUnits", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2">
                                    <label>Gross Weight (Kg):</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("GrossWt", "", new { @onblur = "ClearErrMsg(this)", maxlength = "12", @onkeypress = "return AllowOnlyDecimal(event)" })
                                    @Html.ValidationMessage("GrossWt", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>FOB Value:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("FobValue", "", new { @class = "form-control input-sm", @onblur = "ClearErrMsg(this)", maxlength = "12", @onkeypress = "return AllowOnlyDecimal(event)" })
                                    @Html.ValidationMessage("FobValue", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
