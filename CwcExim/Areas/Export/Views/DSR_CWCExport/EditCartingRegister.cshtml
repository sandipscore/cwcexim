        }
    }
    $('#BtnSaveShortCargo').on("click",function()
    {
        //debugger;
        if($('#mdl_CartingDate').val()=='')
        {
            alert('Select Carting Date');
            return false;
        }
        else if($('#mdl_Qty').val()=='')
        {
            alert('Enter Quantity');
            return false;
        }
        else if($('#mdl_Wt').val()=='')
        {
            alert('Enter Weight');
            return false;
        }
        else if($('#mdl_SQM').val()=='')
        {
            alert('Enter SQM');
            return false;
        }
        else if($('#mdl_FOB').val()=='')
        {
            alert('Enter FOB');
            return false;
        }
        else
        {
            var conf=confirm("Are you sure you want to Save?");
            if(conf==true)
            {
                $('#BtnSaveCartingReg,#BtnSaveShortCargo').prop('disabled',true);
                var ModelData=
                    {
                        "ShortCargoDtlId":0,
                        "CartingDate":$('#mdl_CartingDate').val(),
                        "Qty":$('#mdl_Qty').val(),
                        "Weight":$('#mdl_Wt').val(),
                        "ManualWT":$('#mdl_ManualWT').val(),
                        "MechanicalWT":$('#mdl_MechanicalWT').val(),
                        "SQM":$('#mdl_SQM').val(),
                        "ReservedSQM":$('#mdl_ReservedSQM').val(),
                        "TotalSQM":$('#mdl_TotalSQM').val(),
                        "FOB":$('#mdl_FOB').val(),
                        "modalGodownId":$('#modalGodownId').val(),
                        "modalGodownName":$('#modalGodownName').val()
                    };
                var CartingRegisterDtlId=ArrayOfCartingRegDtl[0].CartingRegisterDtlId;
                var ShippingBillNo=ArrayOfCartingRegDtl[0].ShippingBillNo;
                $.ajax({
                    url:'/Export/DSR_CWCExport/AddShortCargoDetail',
                    type:"POST",
                    data:{
                        "objSC":ModelData,
                        "CartingRegisterId":$('#CartingRegisterId').val(),
                        "CartingRegisterDtlId":CartingRegisterDtlId,
                        "ShippingBillNo":ShippingBillNo,
                    },
                    dataType:"JSON",
                    success:function(data)
                    {
                        if(data.Status==1)
                        {
                            alert(data.Message);
                            $('#tblShortCargo tbody').append('<tr><td>'+$('#mdl_CartingDate').val()+'</td><td>'+$('#mdl_Qty').val()+'</td><td>'+$('#mdl_Wt').val()+'</td><td>'+$('#mdl_SQM').val()+'</td><td>'+$('#mdl_ReservedSQM').val()+'</td><td>'+$('#mdl_TotalSQM').val()+'</td><td>'+$('#mdl_FOB').val()+'</td><td>'+$('#modalGodownName').val()+'</td></tr>');
                            $('#mdl_CartingDate,#mdl_Qty,#mdl_ManualWT,#mdl_MechanicalWT,#mdl_Wt,#mdl_SQM,#mdl_ReservedSQM,#mdl_TotalSQM,#mdl_FOB,#mdl_ReservedSpace').val('');
                        }
                        else alert(data.Message);

                    }
                });
            }
            else return false;
        }
    });

    $('#mdl_Qty').on('blur', function () {
        debugger;
        if ($('#NoOfUnits').val() != '' && $('#Weight').val() != '' && $('#ActualQty').val() != '') {
            var n = $('#NoOfUnits').val();
            var w = $('#Weight').val();
            var q = $('#ActualQty').val();

            $('#mdl_ManualWT').val(((w / n) * q).toFixed(2));
        }


        if ($('#ActualQty1').val() != '' && $('#FOBValue1').val() != '' && $('#ActualQty').val() != '') {
            var n = $('#ActualQty1').val();
            var f = $('#FOBValue1').val();
            var q = $('#ActualQty').val();

            $('#mdl_FOB').val(((f / n) * q).toFixed(2));
        }

    })

    function ChangeResvSpace()
    {
        //debugger;
        var GodownId = 0;
        var ExporterId = 0;
        GodownId=$('#hdnGodownId').val();
        ExporterId=$('#hdnExporterId').val();

        if (GodownId>0 && ExporterId>0)
        {
            getResSpace();
        }
    }

    function getResSpace() {
        //debugger
        var GodownId = 0;
        var ExporterId = 0;
        var RegisterDate = '';
        GodownId = $('#hdnGodownId').val();
        ExporterId = $('#hdnExporterId').val();
        RegisterDate = $('#mdl_CartingDate').val();
        $.ajax({
            url: '/Export/DSR_CWCExport/GetAvailableReservedSpace',
            type: 'GET',
            data: { GodownId: GodownId, ExporterId: ExporterId, RegisterDate:RegisterDate },
            dataType: 'JSON',
            success: function (data) {
                //console.log(data);
                //debugger;
                
                var ResSpace=data.Data.ReservedSpace;              
                $('#mdl_ReservedSpace').val(ResSpace);
            }
        });
    }


</script>