            $('#SearchExporter').css('display', 'none');
            $('#LocationSearch').css('display', 'none');
            $('#RegisterDate').parent().find('img').css('display', 'none');
        }
        else{
            $('#ActualQty,#ActualWeight,#SQM,#ReservedSQM,#TotalSQM,#FOBValue').prop('readonly',false);
            $('#SpanGodownSearch').css('display', '');
            $('#SearchExporter').css('display', '');
            $('#LocationSearch').css('display', '');
            $('#RegisterDate').parent().find('img').css('display', '');
        }
    });
    $('#RegisterDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='RegisterDate']").html('');
        }
    });
    $('#ShippingBillDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='RegisterDate']").html('');
        }
    });

    $('#mdl_CartingDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {

        }
    });


    $("#mdl_CartingDate").datepicker("setDate", new Date());

    function CallModalShortCargo()
    {
     
        $("#ShortCargoModal").modal("show");
        $('#mdl_Qty').focus();
    }
    //*********************************************************************
    $('#ActualQty').on('blur', function () {
        if ($('#NoOfUnits').val() != '' && $('#Weight').val() != '' && $('#ActualQty').val() != '') {
            var n = $('#NoOfUnits').val();
            var w = $('#Weight').val();
            var q = $('#ActualQty').val();

            $('#ActualWeight').val(((Number(w) /Number(n)) * Numver(q)).toFixed(2));
        }


        if ($('#ActualQty1').val() != '' && $('#FOBValue1').val() != '' && $('#ActualQty').val() != '') {
            var n = $('#ActualQty1').val();
            var f = $('#FOBValue1').val();
            var q = $('#ActualQty').val();

            $('#FOBValue').val(((Number(f) /Number(n)) * Number(q)).toFixed(2));
        }

    })

    //*********************************************************************
   
    $('#ShippingLinebox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstShippingLine > li').show();
        else {
            $('#lstShippingLine > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });



    $('#lstShippingLine > li').on("click", function () {
        $('#ShippingLineName').val($(this).text());
        $('#ShippingLineId').val($(this).attr('id'));
        //debugger;
        $("#ShippingLineModal").modal("hide");
    });

    function getSpace() {
        //debugger
        var GodownId = 0;
        var ExporterId = 0;
        var RegisterDate = '';
        GodownId = $('#hdnGodownId').val();
        ExporterId = $('#hdnExporterId').val();
        RegisterDate = $('#RegisterDate').val();

        $.ajax({

            url: '/Export/DSR_CWCExport/GetAvailableReservedSpace',
            type: 'GET',
            data: { GodownId: GodownId, ExporterId: ExporterId, RegisterDate:RegisterDate },
            dataType: 'JSON',
            success: function (data) {
                //console.log(data);
                //debugger;
                //$('#ReservedSpace').val(data.Data.ReservedSpace);

                var ResSpace=data.Data.ReservedSpace;
                var eResSpace=0;
                var fResSpace=0;
                if($('#ReservedSQM').val()!='')
                {
                    eResSpace=$('#ReservedSQM').val();
                }
                fResSpace=(Number(ResSpace)+Number(eResSpace));
                $('#ReservedSpace').val(fResSpace);
            }
        });

    }
    function OnChangeGetSpace() {
        //debugger
        var GodownId = 0;
        var ExporterId = 0;
        var RegisterDate = '';
        GodownId = $('#hdnGodownId').val();
        ExporterId = $('#hdnExporterId').val();
        RegisterDate = $('#RegisterDate').val();

        $.ajax({

            url: '/Export/DSR_CWCExport/GetAvailableReservedSpace',
            type: 'GET',
            data: { GodownId: GodownId, ExporterId: ExporterId, RegisterDate:RegisterDate },
            dataType: 'JSON',
            success: function (data) {
                //console.log(data);               
                var ResSpace=data.Data.ReservedSpace;
               
                $('#ReservedSpace').val(ResSpace);
            }
        });

    }
    $('#box').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#CartingRegList > li').show();
        } else {
            $('#CartingRegList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });

    $('#Godownbox').keyup(function () {
        var valThis = $(this).val().toLowerCase();
