                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });


    function GodownBinding() {
        //debugger;
        if ($('#hdnGodownList').val() != '') {
            var GodownJson = JSON.parse($('#hdnGodownList').val());
            var html = '';
            tabvalue = 32;
          
            $.each(GodownJson, function (i, item) {
                html += '<li tabindex=' + tabvalue + ' id="' + item.GodownId + '" onkeypress="selectGodown(' + item.GodownId + ');" onclick="selectGodown(' + item.GodownId + ');">' + item.GodownName + '</li>';
                tabvalue++;
            });
            $('#lstGodown').html(html);

        }
    }
    function selectGodown(gid) {
        debugger;
        $('#Location').val('');
        $('#modalGodownId').val(gid);
        var GodownJson = JSON.parse($('#hdnGodownList').val());
        //console.log(GodownJson);
        $("#modalGodownName").val($.grep(GodownJson, function (item) { return item.GodownId == gid; })[0].GodownName);
        $.ajax({
            url: '/Export/DSR_CWCExport/GetLocationDetailsByGodownId',
            type: 'GET',
            data: { GodownId: gid },
            dataType: 'JSON',
            success: function (data) {
                LocationDetails=[];
                LocationDetails = data;
                //console.log(data);
                OnChangeLocationDet(LocationDetails);
            }
        });
        if ($('#Exporter').val() != '')
        {
            OnChangeGetSpace();
        }
        else {
            $('#ReservedSpace').val(0);
        }

        $('#godownModal').modal('hide');
    }
    function selectCHA(CHAId,CHAName){
        $('#CHAName').val(CHAName);
        $('#hdnCHAId').val(CHAId);
        $('#CHAModal').modal('hide');
    }

    function selectExporter(eid,ename){
        $('#Exporter').val(ename);
        $('#hdnExporterId').val(eid);
        $('#ExporterModal').modal('hide');
        OnChangeGetSpace();
    }
    function selectcommodity(cid,cname){
        $('#Commodity').val(cname);
        $('#hdnCommodityId').val(cid);
        $('#CommodityModal').modal('hide');
    }

    function LoadCartingRegPage(){
        $('#DivBody').load( '/Export/DSR_CWCExport/CreateCartingRegister');
    }
    function RegisterOnSuccess(data)
    {
        $('#DivCartingRegErrMsg').html('');
        if(data.Status==2||data.Status==1)
