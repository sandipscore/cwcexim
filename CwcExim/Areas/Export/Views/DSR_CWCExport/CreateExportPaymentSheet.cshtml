<script>

    var hazidx = 0;
    function ContainerSelect() {
        debugger;
        ArrayContainer = [];
        $('#lstcfsCode tbody tr').each(function () {
            var Chk = $(this).find('input[type="checkbox"]');
            if ($(Chk).is(':checked')) {
                var Container = {
                    "CFSCode": $(Chk).attr('id')
                }
                ArrayContainer.push(Container);
            }
        });
        var CWCMovement = 0;
        var PrivateMovement = 0;
        if ($('#PrivateMovement').is(':checked')) {
            PrivateMovement = 1;
        }
        if ($('#CWCMovement').is(':checked')) {
            CWCMovement = 1;
        }

        $('#GREPaidById').val($('#GREPaidBy').val());

        if (ArrayContainer.length > 0 && $('#DeliveryDate').val() != "") {
            $.ajax({
                url: '/Export/DSR_CWCExport/GetContainerPaymentSheet',
                type: 'POST',
                data: {
                    InvoiceDate: $('#InvoiceDate').val(),
                    DeliveryDate: $('#DeliveryDate').val(),
                    "TaxType": TaxType,
                    AppraisementId: parseInt($('#StuffingReqId').val()),
                    lstPaySheetContainer: ArrayContainer,
                    PartyId: $('#PartyId').val(),
                    PayeeId: $('#PayeeId').val(),
                    Insured: $('#Insured').val(),
                    cwcMovement: CWCMovement,
                    distance: $('#distance').val(),
                    movetoport: $('#PortId').val(),
                    PortOfLoading: $('#PolId').val(),
                    ownMovement: $('#Movement').val()==''?0:$('#Movement').val(),
                    ISGREPaid: $('#GREPaidBy').val(),
                    InvoiceId: 0,
                    IntercartingApplicable: $('#IntercartingApplicable').is(":checked") == true ? 1 : 0,
                    ICDDestuffing: $('#ICDDestuffing').is(":checked") == true ? 1 : 0,
                    ExportUnder: $('#ExportUnder').val()
                },
                dataType: 'JSON',
                success: function (data) {
                    if (data != null && data != 'undefined') {
                        $('#PaymentSheetModelJson').val(JSON.stringify(data));
                        $('#IntercartingApplicable').prop("disabled", true);
                        $('#ICDDestuffing').prop("disabled", true);

                        //ADD CDD-29
                        $("#PrivateMovement").prop("disabled", true);
                        $("#CWCMovement").prop("disabled", true);
                        $('#GREPaidBy').prop("disabled", true);
                        $('#Movement').prop("disabled", true);
                        $('#ExportUnder').prop('disabled', true);
                        
                        $('#IsPartyStateInCompState').val(data.IsPartyStateInCompState);
                        var html = '';
                        var html1 = '';
                        //All H&T binding in modal
                        debugger;
                        var HTCharge = $.grep(data.lstPostPaymentChrg, function (item) { return item.ChargeType == "HT"; });

                        if (HTCharge.length > 0) {
                            for (var i = 0; i < HTCharge.length; i++) {
                                if(HTCharge[i].Clause=='Haz')
                                {
                                    hazidx = HTCharge[i].ChargeId;
                                }
                            }
                        }

                        $.each(HTCharge, function (i, item) {
                            debugger;
                            //conditionally done by Sweta
                            //begin
                            if (item.Clause != 'XXI-12' && item.ChargeName != 'Lift on Empty' && item.ChargeName != 'Lift off Empty')
                                var result = $.grep(data.ActualApplicable, function (e) { return e == item.Clause; });
                            else
                                var result = $.grep(data.ActualApplicable, function (e) { return e.split('#')[0] == item.Clause && e.split('#')[1] == item.ChargeName });
                            //end
                            if (result.length == 0) {
                                html += '<li id="liAHT' + i + '"><div class="boolean-container"><input id="chkAHT' + i + '" type="checkbox" data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkAHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                            }
                            else {
                                html += '<li id="liAHT' + i + '"><div class="boolean-container"><input id="chkAHT' + i + '" type="checkbox" checked data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkAHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                            }
                            if (item.Clause != 'Haz')
                            {
                                html1 += '<li id="liNHT' + i + '"><div class="boolean-container"><input id="chkNHT' + i + '" type="checkbox" data-val="' + btoa(JSON.stringify(item)) + '"/><label for="chkNHT' + i + '"><i class="square"></i> &nbsp; &nbsp; &nbsp;' + item.Clause + ' (<span style="font-size:8pt;">' + item.ChargeName + '</span>)' + '</label></div></li>';
                            }
                            
                        });
                        $('#lstHT').html(html);
                        $('#lstNewHT').html(html1);
                        //$('#btnSave').removeAttr('disabled');
                        //Container Binding
                        html = '';
                        debugger;
                        $.each(data.lstPSCont, function (i, item) {
                            html += '<tr><td>' + (i + 1) + '</td><td>' + item.CFSCode + '</td><td>' + item.ContainerNo + '</td><td>' + item.Size + '</td><td>'+ (item.Insured == 0 ? 'No' : 'Yes')+'</td>';//
                        });
                        $('#tblContainer tbody').html(html);

                        //CWC Charge Binding
                        debugger;
                        var CWCCharge = $.grep(data.lstPostPaymentChrg, function (item) { return item.ChargeType == "CWC"; });
                        html = '';
                        var TotalCWC = 0;
                        $.each(CWCCharge, function (i, item) {
                            TotalCWC += item.Total;
