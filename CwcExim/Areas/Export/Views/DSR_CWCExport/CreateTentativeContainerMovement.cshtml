    function CalcTax(elem) {
        var MAmt = Number($('#fmcamt').val());
        var IP = Number($('#IGSTP').val());
        var CP = Number($('#CGSTP').val());
        var SP = Number($('#SGSTP').val());
        $('#IGSTA').val((MAmt * (IP / 100)).toFixed(2));
        $('#CGSTA').val((MAmt * (CP / 100)).toFixed(2));
        $('#SGSTA').val((MAmt * (SP / 100)).toFixed(2));
        $('#TOT').val((MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100))).toFixed(2));
        $('#TOTRCChrg').val($('#TOT').val());
        var rawJson = JSON.parse($('#PaymentSheetModelJsonnn').val());
        // var rawJson = JSON.parse($('#ChargesJson').val());
        //  var selectedItem = $.grep(rawJson.lstChargesType, function (item) { return item.ChargeId == elem; })[0];
        rawJson.IGSTPer = IP;
        rawJson.IGSTAmt = MAmt * (IP / 100);
        rawJson.CGSTPer = CP;
        rawJson.CGSTAmt = MAmt * (CP / 100);
        rawJson.SGSTPer = SP;
        rawJson.SGSTAmt = MAmt * (SP / 100);
        rawJson.Total = (MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100)));
        rawJson.RoundUp = 0;
        rawJson.InvoiceAmt = rawJson.Total;
        $('#TOTRCChrg').val($('#TOT').val());
        $('#AllTotal').val($('#TOT').val());
        $('#InvoiceValue').val($('#TOT').val());

        $.each(rawJson.lstPostPaymentChrg, function (i, item) {
            item.ChargeName = "FMC"
            item.Rate = MAmt;
            item.Amount = MAmt;
            item.Discount = 0;
            item.Taxable = MAmt;
            item.IGSTPer = IP;
            item.IGSTAmt = $('#IGSTA').val((MAmt * (IP / 100)).toFixed(2));
            item.CGSTPer = CP;
            item.CGSTAmt = $('#CGSTA').val((MAmt * (CP / 100)).toFixed(2));
            item.SGSTPer = SP;
            item.SGSTAmt = $('#SGSTA').val((MAmt * (SP / 100)).toFixed(2));
            item.Total = (MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100)));
        });







        $('#PaymentSheetModelJsonnn').val(JSON.stringify(rawJson));
        //  CalcInvoice();
    }


