                    var Sbill = data.Data.ShipBill;
                    var sbillno = "";
                    $.each(Sbill, function (i, item) {
                        sbillno = sbillno + item.shippingBillNo + ",";
                    });
                    sbillno = sbillno.substring(0, sbillno.length - 1);
                    $('#Shipbillno').val(sbillno);
                    if (data.Data.TransportMode == 1) {
                        $('#TranMode').val("By Train");
                        $('#RRtype').innerHTML = "RR";
                        document.getElementById("RRtype").innerHTML = "RR";


                    }
                    else {
                        $('#TranMode').val("By Road");
                        $('#RRtype').innerHTML = "FMC";
                        document.getElementById("RRtype").innerHTML = "FNC";
                    }



                }
            }
        });
