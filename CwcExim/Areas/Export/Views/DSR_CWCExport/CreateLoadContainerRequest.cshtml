        $('#ExporterId,#ShippingLineId,#ContainerNo,#EntryNo,#ShippingBillNo,#CommodityId,#CargoDescription,#NoOfUnits,#FobValue,#GrossWt,#ExporterName,#ShippingLineName,#CommodityName,#ShippingSeal,#CustomSeal,#PackUQCCode,#PackUQCDescription').val('');
