    function ResetStuffingReq() {
        debugger;
        $('#CommodityName').html('');
        $('#CommodityName').append($('<option></option>').val('').html('---Select---'));
        $('#DivCreateStuffingReq').find('input:text').val('');
        $('#DivCreateStuffingReq').find('input:hidden').val('');
        $('#TblStuffingReq >tbody').html('');
        $('#DivCreateStuffingReq #TblStuffingContr >tbody').html('');
        $('#TblStuffingReq').hide('');
        $('#TblStuffingContr').hide('');
        $('[data-valmsg-for]').html('<span></span>');
        $('#DivStuffingMsg').html('');
        $('#DivStuffingErrMsg').html('');
        RequestDet = [];
        ContainerDet = [];
        if ('@ViewData["IsExporter"]' == 'True') {
            $('#ExporterName').val('@ViewData["ExporterName"]');
            $('#ExporterId').val('@ViewData["EximTraderId"]');
            var EximTraderId= '@ViewData["EximTraderId"]';
            var ExporterName = '@ViewData["ExporterName"]';
            $('#LstExporter').html('<li id=' + @ViewData["EximTraderId"] +' onclick="FillExporterDet(\'' + EximTraderId + '\',\'' + ExporterName + '\')">@ViewData["ExporterName"]</li>');
        }
        if ('@Model.CHAId' > 0) {
            $('#CHANameId').val('@Model.CHAId');
            $('#CHAName').val('@Model.CHA');
            $('#CHAId').val('@Model.CHAId');
            $('#CHA').val('@Model.CHA');
            var CHAId='@Model.CHAId';
            var CHA='@Model.CHA';
            $('#LstCHA').html('<li id=' + @Model.CHAId +' onclick="FillCHADet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
            $('#ListOfCHAHdr').html('<li id=' + @Model.CHAId +' onclick="FillCHAHdrDet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
        }
        var remsize = 0;
        var remweight = 0;
        editflag = 0;
        $('#DivBody').load('/Export/DSR_CWCExport/CreateStuffingRequest');
    }


    function StuffingReqOnSuccess(data) {
        $('#DivStuffingErrMsg').html('');
        if (data.Status == 1) {
            $('#DivStuffingMsg').html(data.Message);
           // $("#ContainerModal ul li:contains(" + $('#ContainerNo').val() + ")").remove();
            $("#myModal ul li:contains(" + $('#CartingRegisterNo').val() + ")").remove();

            setTimeout(LoadStuffingReq, 5000);
            debugger;
            var ContainerJson = JSON.parse($('#hdnContainer').val());
            var html = '';
            $.each(ContainerJson, function (i, item) {
                html += '<li id="' + item.CFSCode + '" onclick="GetContainerDet(' + item.CFSCode + ');">' + item.ContainerNo + '</li>';
            });

            $('#ContainerList').html(html);

            editflag = 0;




            //GetStuffingReqList();
        }
        else
        {
            $('#DivStuffingErrMsg').html(data.Message);
        }
    }
    function StuffingReqOnFailure() { }
    function LoadStuffingReq() {
        $('#DivBody').load('/Export/DSR_CWCExport/CreateStuffingRequest');



    }
    $('#btnSubmit').on("click", function ()
    {
        debugger;
        if ($('#TblStuffingReq >tbody').html() == '') {
            $('#DivTblStuffingErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            return false;
        }
        else if ($('#TblStuffingContr >tbody').html() == '') {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            return false;
        }
        else if ($('#ShippingBillNo').val() != '' && $('#BtnAddStuffingDtl').prop("disabled") == false) {
            $('#DivTblStuffingErrMsg').html('Please Add The Above Records');
            return false;
        }
        else if ($('#ContainerNo').val() != '' && $('#BtnAddContainer').prop("disabled") == false) {
            $('#DivTblContrErrMsg').html('Please Add The Above Records');
            return false;
        }
        else
        {
            $('#StuffingXML').val(JSON.stringify(RequestDet));
            $('#ContainerXML').val(JSON.stringify(ContainerDet));
        }
    });
    function PopupActivate(Id) {
        debugger;
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }

        if (CharCode == 32) {



            if (Id == "CHAModal") {
                ChaNameSerach();
            }



            $('#' + Id).modal('show');
            if (Id == "myModal") {
                debugger;
                $('#box').focus();
            }

            if (Id == "ShippingLineModal") {
                debugger;
                $('#ShippingLineBox').focus();
            }
            if (Id == "ShippingLineHdrModal") {
