            if (Id == "CHAModal") {
                debugger;
                $('#SearchCHA').focus();
            }

            if (Id == "PortDestModal") {
                debugger;
                $('#PortDestbox').focus();
            }
            if (Id == "ForwarderModal") {
                debugger;
                $('#ForwarderBox').focus();
            }

            if (Id == "PortModal") {
                debugger;
                $('#Portbox').focus();
            }

            if (Id == "CommodityModal") {
                debugger;
                $('#CommodityBox').focus();
            }
            if (Id == "SBNoModal") {
                debugger;
                $('#SBNoBox').focus();
            }
            if (Id == "ExporterModal") {
                debugger;
                $('#SearchExporter').focus();
            }
            if (Id == "ContainerModal") {
                debugger;
                $('#ContainerBox').focus();
            }
        }




    }

