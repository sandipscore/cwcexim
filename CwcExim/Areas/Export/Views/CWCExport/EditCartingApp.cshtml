@Scripts.Render("~/bundles/jqueryval")
<script>
    var ArrayDet = [];
    $(function(){
        $('#btnAddShippingDet').prop("disabled",true);
        ArrayDet=@Html.Raw(Json.Encode(Model.lstShipping));
        var html='';
        $.each(ArrayDet,function(i,data)
        {
            html+='<tr><td class="text-center">'+(i+1)+'</td><td>'+data.ShippingBillNo+'</td><td>'+data.ShippingDate+'</td><td>'+data.Exporter+'</td><td>'+data.NoOfUnits+'</td><td class="text-center"><a href="#" onclick="EditShippingDet('+i+')"><i class="fa fa-pencil-square Edit"></i></a></td></tr>';
        });
        $('#tblShippingDet tbody').html(html);

        $('#slim_scroll_2').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll_1').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll_3').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll').slimScroll({
            allowPageScroll: true
        });
    });
    function LoadCartingApp()
    {
        $('#DivBody').load( '/Export/CWCExport/CreateCartingApplication');
    }
    function AddShippingDet() {
        if (ValidateShippingDet()) {
            var ShippingDet = {
                "CartingAppDtlId":($('#CartingAppDtlId').val()==""||$('#CartingAppDtlId').val()==null)?0:$('#CartingAppDtlId').val(),
                "ShippingBillNo": $('#ShippingBillNo').val(),
                "ShippingDate": $('#ShippingBillDate').val(),
                "CommInvcNo": $('#CommInvNo').val(),
                "PackingListNo": $('#PackingListNo').val(),
                "ExporterId": $('#ExporterId').val(),
                "Exporter": $('#Exporter').val(),
                "CargoDescription": $('#CargoDescription').val(),
                "CommodityId": $('#CommodityId').val(),
                "Commodity": $('#Commodity').val(),
                "CargoType": $('#CargoType').val(),
                "MarksAndNo": $('#MarksAndNo').val(),
                "NoOfUnits": $('#NoOfUnits').val(),
                "Weight": $('#Weight').val(),
                "FoBValue": $('#FobValue').val(),
                "SCMTRPackageType": $('#SCMTRPackageType').val(),
                "PackUQCCode": $('#PackUQCCode').val(),
                "PackUQCDescription": $('#PackUQCDescription').val(),
                "IsSEZ": ($('#IsSEZ').is(':checked') ? true : false)
            };
            ArrayDet.push(ShippingDet);
            $('#tblShippingDet').html('');
            GenerateTable();
            ResetShippingDet();
            $('[data-valmsg-for]').html('');
            $('#btnAddShippingDet').prop("disabled",true);
        }
    }
    function EditShippingDet(i) {
        if(CheckExistingData())
        {
            debugger;
            $('#btnAddShippingDet').prop("disabled",false);
            $('#CartingAppDtlId').val(ArrayDet[i].CartingAppDtlId);
            $('#ShippingBillNo').val(ArrayDet[i].ShippingBillNo);
            $('#ShippingBillDate').val(ArrayDet[i].ShippingDate);
            $('#CommInvNo').val(ArrayDet[i].CommInvcNo);
            $('#PackingListNo').val(ArrayDet[i].PackingListNo);
            $('#Exporter').val(ArrayDet[i].Exporter);
            $('#ExporterId').val(ArrayDet[i].ExporterId);
            $('#CargoDescription').val(ArrayDet[i].CargoDescription);
            $('#Commodity').val(ArrayDet[i].CommodityName);
            $('#CommodityId').val(ArrayDet[i].CommodityId);
            $('#MarksAndNo').val(ArrayDet[i].MarksAndNo);
            $('#NoOfUnits').val(ArrayDet[i].NoOfUnits);
            $('#Weight').val(ArrayDet[i].Weight);
            $('#FobValue').val(ArrayDet[i].FoBValue);
            $('#CargoType').val(ArrayDet[i].CargoType);
            $('#SCMTRPackageType').val(ArrayDet[i].SCMTRPackageType);
            $('#PackUQCCode').val(ArrayDet[i].PackUQCCode);
            $('#PackUQCDescription').val(ArrayDet[i].PackUQCDescription);
            $('#IsSEZ').prop("checked", ArrayDet[i].IsSEZ);
            ArrayDet.splice(i,1);
            $('#tblShippingDet').html('');
            GenerateTable();
        }
        setTimeout(function(){$('#DivMsgEdit').html('');},5000);
    }
    function CheckExistingData()
    {
        if($('#ShippingBillNo').val()!="" || $('#CommInvNo').val()!="" || $('#PackingListNo').val()!="" ||/* $('#Exporter').val()!="" ||*/
            $('#CargoDescription').val()!="" || $('#Commodity').val()!="" ||$('#MarksAndNo').val()!="" || $('#NoOfUnits').val()!="" ||
            $('#Weight').val()!="" ||$('#FobValue').val()!="" || $('#CargoType').val()!="")
        {
            $('#DivMsgEdit').html('Add Earlier Record To Edit New Record');
            return false;
        }
        else
            return true;
    }
    function GenerateTable()
    {
        var tablehtml = '<table id="tblShippingDet" class="table table-striped table-bordered" style="width:100%;"><thead><tr><th>SL No</th><th>Shipping Bill No</th><th>Shipping Bill Date</th><th>Exporter</th><th>No of Units</th><th>Edit</th></tr></thead><tbody>';
        $.each(ArrayDet, function (i, data) {
            tablehtml += '<tr><td>' + (i + 1) + '</td><td>' + data.ShippingBillNo + '</td><td>' + data.ShippingDate + '</td><td>' + data.Exporter + '</td><td>' + data.NoOfUnits + '</td><td><a href="#" onclick="EditShippingDet(' + i + ')"><i class="fa fa-pencil-square Edit"></i></a></td></tr>';
        });
        tablehtml += '</tbody></table>';
        $('#tblShippingDet').html(tablehtml);
    }
    function ResetShippingDet()
    {
        if ('@ViewData["IsExporter"]' == 'False'){
                $('#ShippingBillNo,#ShippingBillDate,#CommInvNo,#PackingListNo,#Exporter,#ExporterId,#CargoDescription,#Commodity,#CommodityId,#MarksAndNo,#NoOfUnits,#Weight,#FobValue,#CargoType,#PackUQCCode,#PackUQCDescription').val('');
            $('#IsSEZ').prop("checked", false);
        }            
        else
        {
                                    $('#ShippingBillNo,#ShippingBillDate,#CommInvNo,#PackingListNo,#CargoDescription,#Commodity,#CommodityId,#MarksAndNo,#NoOfUnits,#Weight,#FobValue,#CargoType,#PackUQCCode,#PackUQCDescription').val('');
        $('#SCMTRPackageType').val('P');
        $('#IsSEZ').prop("checked", false);
        }
            
    }
    $('#btnUpdate').on("click",function() {
        var Status=true;
        if($('#ShippingBillNo').val()!="" || $('#CommInvNo').val()!="" || $('#PackingListNo').val()!="" ||/* $('#Exporter').val()!="" ||*/
            $('#CargoDescription').val()!="" || $('#Commodity').val()!="" ||$('#MarksAndNo').val()!="" || $('#NoOfUnits').val()!="" ||
            $('#Weight').val()!="" ||$('#FobValue').val()!="" || $('#CargoType').val()!="")
        {
            $('#DivMsgEdit').html('Add The Record To Save');
            Status=false;
        }
        else
        {
            if($('#tblShippingDet>tbody>tr').html()=='')
            {
                $('#DivMsgEdit').html('Add The Record To Save');
                Status=false;
            }         
        }
        if(Status==true)
            $('#StringifyData').val(JSON.stringify(ArrayDet));
        setTimeout(function(){$('#DivMsgEdit').html('');},5000);
        return Status;
    });
    function CartingOnSuccess(data)
    {
        if(data.Status==2)
        {
            $('#DivMsg').html(data.Message);
            setTimeout(function(){$('#DivBody').load( '/Export/CWCExport/CreateCartingApplication');},5000);
        }
        else
        {
            $('#DivMsgEdit').html(data.Message);
        }
    }
    function CartingOnFailure(){}
    function ValidateData(evt){
        var CharCode;
        if(window.event){
            CharCode=window.event.keyCode;
        }
        else{
            CharCode=evt.which;
        }
        if(CharCode==32){
            return true;
        }
        if((CharCode>31&CharCode<48)||(CharCode>57&CharCode<65)||(CharCode>90&CharCode<97)||(CharCode>122)){
            evt.preventDefault;
            return false;
        }
    }
    function AllowOnlyDecimal(evt){
        var CharCode;
        if(window.event){
            CharCode=window.event.keyCode;
        }
        else{
            CharCode=evt.which;
        }
        if((CharCode>47&CharCode<58)||(CharCode==46)||CharCode==8){
            return true;
        }
        else{
            evt.preventDefault;
            return false;
        }
    }
    function AllowOnlyInteger(evt){
        var CharCode;
        if(window.event){
            CharCode=window.event.keyCode;
        }
        else{
            CharCode=evt.which;
        }
        if(CharCode>47&CharCode<58||CharCode==8){
            return true;
        }
        else{
            evt.preventDefault;
            return false;
        }
    }
    function ValidateShippingDet() {
        var Status = true;
        if ($('#ExporterId').val() == "") {
            $('[data-valmsg-for="ExporterId"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#CargoDescription').val() == "") {
            $('[data-valmsg-for="CargoDescription"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#CommodityId').val() == "") {
            $('[data-valmsg-for="CommodityId"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#CargoType').val() == "") {
            $('[data-valmsg-for="CargoType"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#NoOfUnits').val() == "") {
            $('[data-valmsg-for="NoOfUnits"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#Weight').val() == "") {
            $('[data-valmsg-for="Weight"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#Weight').val() != "") {
            if (!DecimalValidationGW("Weight")) {
                $('[data-valmsg-for="Weight"]').html('<span>Weight must be a decimal up to 2 places and less then 99999999.99<span>');
                Status = false;
            }
        }
        if ($('#FobValue').val() == "") {
            $('[data-valmsg-for="FobValue"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#FobValue').val() != "") {
            if (!DecimalValidation("FobValue")) {
                $('[data-valmsg-for="FobValue"]').html('<span>FobValue must be a decimal up to 2 places and less then 99999999.99</span>');
                Status = false;
            }
        }
        return Status;
    }
    function DecimalValidation(elem) {
        var State = true;
        var Value = $('#' + elem).val();
        if (!Value.match(/^[\d]{1,8}\.\d{0,2}$/) && !Value.match(/^[\d]{1,8}$/))
            State = false;
        return State;
    }
    function DecimalValidationGW(elem) {
        var State = true;
        var Value = $('#' + elem).val();
        if (!Value.match(/^[\d]{1,8}\.\d{0,2}$/) && !Value.match(/^[\d]{1,8}$/))
            State = false;
        return State;
    }

    $('#ShippingBillSerach').on("click", function () {
        GetSBNo();
    });

    function GetSBNo() {
        debugger;
        $.ajax({
            url: '/Export/CWCExport/GeSBNoForCartingApp',
            type: 'GET',
            dataType: 'JSON',
            success: function (data) {
                debugger;
                if (data != '') {
                    var EcJson = JSON.parse(data);
                    var html = '';
                    var tabvalue = 401;
                    debugger;
                    $.each(EcJson, function (i, item) {
                        debugger;
                        html += '<li tabindex=' + tabvalue + ' id="' + item.ShippingBillId + '" onkeypress="SelectSBNo(' + item.ShippingBillId + ',&quot;' + item.ShippingBillNo + '&quot;);" onclick="SelectSBNo(' + item.ShippingBillId + ',&quot;' + item.ShippingBillNo + '&quot;);">' + item.ShippingBillNo + '</li>';
                        tabvalue++;
                    });
                    $('#lstSBNo').html(html);

                }
            }
        });
    }
    function SelectSBNo(ShippingBillId, ShippingBillNo) {
        debugger;
        $("#ShippingBillId").val(ShippingBillId);
        $("#ShippingBillNo").val(ShippingBillNo);
        $("#SBNoModal").modal("hide");
        $('#SBNobox').val('');
        $('[data-valmsg-for="ShippingBillNo"]').html('');
        GetSBDetails(ShippingBillId);
    }

    function GetSBDetails(ShippingBillId) {
        if (ShippingBillId > 0) {

            $.ajax({
                url: '/Export/CWCExport/GetSBDetailsBySBId',
                type: 'Get',
                data: { SBId: ShippingBillId },
                async: false,
                success: function (data) {
                    if (data != null) {
                        //debugger;
                        $('#ShippingBillNo').val(data.ShippingBillNo);
                        $('#ShippingBillDate').val(data.ShippingDate);
                        $('#Exporter').val(data.Exporter);
                        $('#ExporterId').val(data.ExporterId);
                        $('#FobValue').val(data.FoBValue);
                        $('#NoOfUnits').val(data.NoOfUnits);
                    }
                }
            });
        }
    }

    $('#SBNobox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstSBNo > li').show();
        else {
            $('#lstSBNo > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    
    
    
    function LoadMorePackUQC() {
        debugger;
        $.ajax({
            url: '/Export/CWCExport/LoadPackUQCList',
            type: 'GET',
            data: { PartyCode: "", Page: Page + 1 },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    $.each(data.Data.lstPackUQC, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                        tabvalue++;
                    });
                    if (Page == -1)
                        $('#tblePackUQC tbody').html(html);
                    else
                        $('#tblePackUQC tbody').append(html);
                    Page++;

                    debugger;
                    if (data.Data.PackUQCState == true)
                        $('#btnLoadPackUQC').prop('disabled', false);
                    else $('#btnLoadPackUQC').prop('disabled', true);
                }
            }
        });
    }

    function LoadPackUQC() {
        debugger;
        Page = 0;
        $.ajax({
            url: '/Export/CWCExport/LoadPackUQCList',
            type: 'GET',
            data: { PartyCode: "", Page: Page },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    $.each(data.Data.lstPackUQC, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                        tabvalue++;
                    });

                    $('#tblePackUQC tbody').html(html);

                    if (data.Data.PackUQCState == true)
                        $('#btnLoadPackUQC').prop('disabled', false);
                    else $('#btnLoadPackUQC').prop('disabled', true);
                }
            }
        });
    }

    function SearchPackUQCByCode() {
        debugger;
        if ($('#PackUQCbox').val() != '') {
            $.ajax({
                url: '/Export/CWCExport/SearchPackUQCByCode',
                type: 'GET',
                data: { PartyCode: $('#PackUQCbox').val() },
                success: function (data) {
                    debugger;
                    if (data.Status == 1) {
                        var html = '';
                        var tabvalue = 202;
                        $.each(data.Data.lstPackUQC, function (item, elem) {
                            html += '<tr tabindex=' + tabvalue + ' onclick="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)" onkeypress="SelectPackUQC(' + elem.PackUQCId + ',&quot;' + elem.PackUQCDescription + '&quot,&quot;' + elem.PackUQCCode + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.PackUQCDescription + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PackUQCCode + '</td></tr>';
                            tabvalue++;
                        });
                        $('#tblePackUQC tbody').html('');
                        $('#tblePackUQC tbody').html(html);
                        $('#btnLoadPackUQC').prop('disabled', false);
                        Page = -1;
                    }
                    else {
                        $('#tblePackUQC tbody').html('');
                        Page = -1;
                        $('#btnLoadPackUQC').prop('disabled', false);
                    }

                }
            });
        }
        else
            LoadPackUQC();
    }
    function SelectPackUQC(PackUQCId, PackUQCDescription, PackUQCCode) {
        debugger;
        $('#PackUQCDescription').val(PackUQCDescription);
        $('#PackUQCCode').val(PackUQCCode);
        $('#PackUQCId').val(PackUQCId);
        $("#PackUQCModal").modal("hide");
        $('#PackUQCbox').val('');
        $('[data-valmsg-for="PackUQCDescription"]').html('');
        LoadPackUQC();
        $('#Package').focus();
    }
    function ClosePackUQC() {
        debugger;
        $('#PackUQCbox').val('');
        $("#PackUQCModal").modal("hide");
        LoadPackUQC();
    }
</script>