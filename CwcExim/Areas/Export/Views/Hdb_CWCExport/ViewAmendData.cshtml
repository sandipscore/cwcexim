</div>
<div class="row Form_Space_top">
    <div class="form-group Form_Input">
       
        <div class="col-md-1-5">
            <label>Pack UOM:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.PackUQCDescription, new { @readonly = true })
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-offset-5 col-md-2  SignUp_space_bottom">
        <input type="button" id="btnBack" class="btn log_Btn_sm" value="Back" onclick="$('#DivBody').load('/Export/Hdb_CWCExport/Amendment')" />
    </div>
</div>

