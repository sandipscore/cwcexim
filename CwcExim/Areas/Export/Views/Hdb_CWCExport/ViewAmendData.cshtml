@model CwcExim.Areas.Export.Models.Hdb_AmendmentViewModel

<div class="row Form_Space_top">
    <div class="form-group Form_Input">
        <div class="col-md-1-5 col_cus_2" style="display:none;">
            <label>Invoice No:</label>
        </div>
        <div class="col-md-2-5" style="display:none;">
            @Html.TextBoxFor(m => m.InvoiceNo, new { @readonly = true })
        </div>
        <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt" style="display:none;">
            <label>Invoice Date:</label>
        </div>
        <div class="col-md-2 col_cus_2_5" style="display:none;">
            @Html.TextBoxFor(m => m.InvoiceDate, new { @readonly = true })
        </div>
    </div>
</div>
<div class="row Form_Space_top">
    <div class="form-group Form_Input">
        <div class="col-md-1-5 col_cus_2">
            <label>Ship Bill No:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.ShipBillNo, new { @readonly = true })
        </div>
        <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
            <label>Date:</label>
        </div>
        <div class="col-md-2 col_cus_2_5 Date_Img2">
            @Html.TextBoxFor(m => m.Date, new { @readonly = true })
        </div>
    </div>
</div>
<div class="row">
    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
</div>
<div class="row Form_Space_top">
    <div class="form-group Form_Input">
        <div class="col-md-1-5 col_cus_2">
            <label>Exporter:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.ExporterName, new { @readonly = true })
        </div>
        <div class="col-md-2-5 col_cus_2">
            <label>ShippingLineName:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.ShippingLineName, new { @readonly = true })
        </div>
    </div>
</div>
<div class="row Form_Space_top">
    <div class="form-group Form_Input">
        <div class="col-md-1-5 col_cus_2">
            <label>Commodity:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.Cargo, new { @readonly = true })
        </div>
        <div class="col-md-2-5 col_cus_2">
            <label>SB Type:</label>
        </div>
        <div class="col-md-2-5">
            @Html.TextBoxFor(m => m.SBTypeName, new { @readonly = true })
        </div>
    </div>
</div>
