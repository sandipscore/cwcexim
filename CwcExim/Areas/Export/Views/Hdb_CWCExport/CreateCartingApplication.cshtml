        debugger;
        if (CheckExistingData()) {
            debugger;
            if (!isEditOpened) {
            $('#ShippingBillNo').val(ArrayShippingDet[i].ShippingBillNo);
            $('#ShippingBillDate').val(ArrayShippingDet[i].ShippingDate);
            $('#CommInvNo').val(ArrayShippingDet[i].CommInvcNo);
