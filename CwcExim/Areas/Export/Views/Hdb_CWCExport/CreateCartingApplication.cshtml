<script>
    $('#ApplicationDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='ApplicationDate']").html('');
        }
    });

    $('#CommInvDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='CommInvDate']").html('');
        }
    });


    $('#PackingListDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='PackingListDate']").html('');
        }
    });

    $('#ShippingBillDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='ShippingBillDate']").html('');
        }
    });
    $('#CHAbox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstCHA > li').show();
        else {
            $('#lstCHA > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    $('#lstCHA > li').on("click", function () {
        $('#CHAName').val($(this).text());
        $('#CHAEximTraderId').val($(this).attr('id'));
        var ss = '';
      //  $("#CHAbox").val('');
      //  $("#CHAbox").val(ss.trim());
      //  $("#lstCHA").empty();
        $("#CHAModal").modal("hide");

    });
    $('#Exporterbox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstExporter > li').show();
        else {
            $('#lstExporter > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    $('#lstExporter > li').on("click", function () {
        $('#Exporter').val($(this).text());
        $('#ExporterId').val($(this).attr('id'));
        $("#ExporterModal").modal("hide");
        GetGstNoForCartApp($(this).attr('id'));
    });
    $('#Commoditybox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstCommodity > li').show();
        else {
            $('#lstCommodity > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    $('#lstCommodity > li').on("click", function () {
        $('#Commodity').val($(this).text());
        $('#CommodityId').val($(this).attr('id'));
        $("#CommodityModal").modal("hide");
    });
    function GetGstNoForCartApp(ExpEximTraderId) {
        debugger;
        $.ajax({
            url: '/Export/Hdb_CWCExport/GetGstNoForCartApp',
            type: 'GET',
            dataType: 'JSON',
            async: false,
            data: { ExpEximTraderId: ExpEximTraderId },
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                  
                   // $('#BranchId').val(data.Data.BranchId);
                    $('#GSTNo').val(data.Data.GSTNo);
                    data.Data.AppraisementStatus == 1 ? $('#Pass').prop("checked", true) : $('#Fail').prop("checked", true);
                    // ArrayOfDestuffingEntry = data.Data.LstDestuffing;
                    //  GenerateDestuffingTable();
                }
            }
        });
    }


</script>
