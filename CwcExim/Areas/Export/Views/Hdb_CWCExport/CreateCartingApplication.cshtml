            Status = false;
        }
        if ($('#Weight').val() != "") {
            if (!DecimalValidationGW("Weight")) {
                $('[data-valmsg-for="Weight"]').html('<span>Weight must be a decimal up to 2 places and less then 99999999.99<span>');
                Status = false;
            }
        }
        if ($('#FobValue').val() == "") {
            $('[data-valmsg-for="FobValue"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#FobValue').val() != "") {
            if (!DecimalValidation("FobValue")) {
                $('[data-valmsg-for="FobValue"]').html('<span>FobValue must be a decimal up to 2 places and less then 99999999.99</span>');
                Status = false;
            }
        }
   
        if ($('#PackageType').val() == "") {
            $('[data-valmsg-for="PackageType"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#PackageType').val() == "Others" && $('#OtherPkgType').val()=="") {
            $('[data-valmsg-for="OtherPkgType"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        if ($('#PackUQCDescription').val() == "") {
            $('[data-valmsg-for="PackUQCDescription"]').html('<span>Fill Out This Field</span>');
            Status = false;
        }
        return Status; 
    }
    function DecimalValidation(elem) {
        var State = true;
        var Value = $('#' + elem).val();
        if (!Value.match(/^[\d]{1,9}\.\d{0,2}$/) && !Value.match(/^[\d]{1,9}$/))
            State = false;
        return State;
    }
    function DecimalValidationGW(elem) {
        var State = true;
        var Value = $('#' + elem).val();
        if (!Value.match(/^[\d]{1,8}\.\d{0,2}$/) && !Value.match(/^[\d]{1,8}$/))
            State = false;
        return State;
    }
