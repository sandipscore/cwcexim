            $('#NoOfUnits').val(ArrayShippingDet[i].NoOfUnits);
            $('#Weight').val(ArrayShippingDet[i].Weight);
            $('#FobValue').val(ArrayShippingDet[i].FoBValue);
            $('#CargoType').val(ArrayShippingDet[i].CargoType);
            $('#CustomHouseCode').val(ArrayShippingDet[i].CustomHouseCode);
            $('#IECCode').val(ArrayShippingDet[i].IECCode);
            $('#GSTNo').val(ArrayShippingDet[i].GSTNo);
            $('#PortOfDestination').val(ArrayShippingDet[i].PortOfDestination);
            $('#PackageType').val(ArrayShippingDet[i].PackageType);
                $('#SCMTRPackageType').val(ArrayShippingDet[i].SCMTRPackageType);
