    $('#SBNo').on("click", function () {
        debugger;
        $.ajax({
            url: "/Export/Hdb_CWCExport/GetIceGateSBList",
            type: "GET",
            dataType: "JSON",
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    var Html = '';
                    $.each(data.Data, function (i, item) {
                        debugger
                        Html += '<li id=' + item.Id + ' onclick="FillTSbBox(&quot;' + item.SBNo + '&quot;,' + item.Id + ')">' + item.SBNo + '</li>';
                    });
                    $('#lstSB').html(Html);
                }
            }

        });
    });

    $('#SBbox').keyup(function () {
        debugger;
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstSB > li').show();
        else {
            $('#lstSB > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });

    function FillTSbBox(SBNo, Id) {
        $('#ShippingBillNo').val(SBNo);
        $('#SBId').val(Id);
         GetIcegateSBDet(SBNo, Id);
    }

    function GetIcegateSBDet(SBNo,Id) {
        $("#SbModal").modal("hide");
        $.ajax({
            url: "/Export/Hdb_CWCExport/GetIceGateSBDet",
            dataType: 'JSON',
            type: 'GET',
            data: { Id: Id,SBNo:SBNo },
            success: function (data) {
                if (data != null) {
                    $.each(data, function (i, elem) {
                        $('#ShippingBillNo').val(elem.ShippingBillNo);
                        $('#ShippingBillDate').val(elem.ShippingDate).toString("dd/MMM/yyyy");
                        $('#CustomHouseCode').val(elem.CustomHouseCode);
                        $('#IECCode').val(elem.IECCode);
                        $('#PortOfDestination').val(elem.PortOfDestination);
                        $('#FobValue').val(elem.FoBValue);

                    });
                }
            }
        });

    }

    function GenerateTable() {
        var tablehtml = '<table id="tblShippingDet" class="table table-striped table-bordered dataTable" style="width:100%;"><thead><tr><th class="text-center">SL No</th><th>Shipping Bill No</th><th>Shipping Bill Date</th><th>Exporter</th><th>No of Units</th><th class="text-center">Edit</th><th class="text-center">View</th></tr></thead><tbody>';
        $.each(ArrayShippingDet, function (i, data) {
            tablehtml += '<tr><td class="text-center">' + (i + 1) + '</td><td>' + data.ShippingBillNo + '</td><td>' + data.ShippingDate + '</td><td>' + data.Exporter + '</td><td>' + data.NoOfUnits + '</td><td class="text-center"><a href="#" onclick="EditShippingDet(' + i + ')"><i class="fa fa-pencil-square Edit"></i></a></td><td class="text-center"><a href="#" onclick="ViewShippingDet(' + i + ')"><i class="fa fa-search-plus Edit"></i></a></td></tr>';
        });
        tablehtml += '</tbody></table>';
        $('#tblShippingDet').html(tablehtml);
    }
    function ResetShippingDet() {
        if ('@ViewData["IsExporter"]' == 'False') {
            $('#ShippingBillNo,#ShippingDate,#CommInvNo,#CommInvDate,#PackingListNo,#PackingListDate,#Exporter,#ExporterId,#CargoDescription,#Commodity,#CommodityId,#MarksAndNo,#NoOfUnits,#Weight,#FobValue,#CargoType,#CustomHouseCode,#IECCode,#GSTNo,#PortOfDestination,#PackageType,#OtherPkgType,#SBId').val(''),
                 $("#OtherPkgType").hide();
            $('#IsReefer').prop("checked", false);
            $('#IsSEZ').prop("checked", false);
        }
        else
            $('#ShippingBillNo,#ShippingDate,#CommInvNo,#CommInvDate,#PackingListNo,#PackingListDate,#CargoDescription,#Commodity,#CommodityId,#MarksAndNo,#NoOfUnits,#Weight,#FobValue,#CargoType,#CustomHouseCode,#IECCode,#GSTNo,#PortOfDestination,#PackageType,#IsReefer,#OtherPkgType,#SBId,#PackUQCDescription').val(''),
                $('#IsReefer').prop("checked", false);
        $('#IsSEZ').prop("checked", false);

       
        $('#SCMTRPackageType').val('P');

    }
