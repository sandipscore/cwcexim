@model CwcExim.Areas.Export.Models.Hdb_StuffingRequest
<script type="text/javascript" src="/scripts/jquery.nicescroll.min.js"></script>

<script>

    $(function () {
        $('#slim_scroll_2').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll_1').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll_3').slimScroll({
            allowPageScroll: true
        });
        $('#slim_scroll').slimScroll({
            allowPageScroll: true
        });
    });

    $('#ShippingDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='ShippingBillDate']").html('');
        }
    });
    $('#RequestDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $("[data-valmsg-for='ShippingBillDate']").html('');
        }
    });
    $('#box').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#myModal .navList > li').show();
        } else {
            $('#myModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    var cid = 0;
    var editflag = 0;
    $('#CommodityName').on("change", function () {
        debugger;
        //  $('#CartingRegisterNo').val($(this).text().split('-')[0]);
        
        cid = parseInt($('#CommodityName').val());

        //  alert(cid);
        //  var CartingRegisterId = $('#CartingRegisterId').val();
        // $("#myModal").modal("hide");
        // removed  $('#DivTblStuffingErrMsg').html('');
        //  GetStuffingDetails(CartingRegisterId);
        var elem = RequestDet.filter(x=>x.CartingRegisterDtlId == cid)[0];
        $('#StuffQuantity').val(elem.NoOfUnits);
        $('#StuffWeight').val(elem.GrossWeight);
        $('#StuffFOBValue').val(elem.Fob);

        $('#CartingRegisterId').val(elem.CartingRegisterId);
    });

    $('#CartingRegList > li').on("click", function () {
        debugger;
        //  $('#CartingRegisterNo').val($(this).text().split('-')[0]);
        $('#CartingRegisterId').val($(this).attr('id'));
        var CartingRegisterId = $('#CartingRegisterId').val();
        $("#myModal").modal("hide");
        // removed  $('#DivTblStuffingErrMsg').html('');
        GetStuffingDetails(CartingRegisterId);
    });
    $('#ShippingLineBox').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#ShippingLineModal .navList > li').show();
        } else {
            $('#ShippingLineModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    $('#ShippingLineList li').on("click", function () {
        $('#ShippingLine').val($(this).text());
        $('#ShippingLineId').val($(this).attr('id'));
        $("#ShippingLineModal").modal("hide");
    });
    $('#ContainerBox').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#ContainerModal .navList > li').show();
        } else {
            $('#ContainerModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    $('#ContainerList  li').on("click", function () {
        $('#ContainerNo').val($(this).text());
        $('#CFSCode').val($(this).attr('id'));
        var CFSCode = $('#CFSCode').val();
        $("#ContainerModal").modal("hide");
        GetContainerDet(CFSCode);
        $('#BtnAddContainer').prop("disabled", false);
    });
    $('#LstExporter li').on("click", function () {
        $('#ExporterName').val($(this).text());
        $('#ExporterId').val($(this).attr('id'));
        $("#ExporterModal").modal("hide");
    });
    $('#SearchExporter').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#ExporterModal .navList > li').show();
        } else {
            $('#ExporterModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    $('#ListOfShippingNo li').on("click", function () {
        $('#ShippingBillNo').val($(this).text());
        $("#ShippingModal").modal("hide");
    });
    $('#SearchShippingNo').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#ShippingModal .navList > li').show();
        } else {
            $('#ShippingModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    $('#ListOfCHAHdr li').on("click", function () {
        $('#CHA').val($(this).text());
        $('#CHAId').val($(this).attr('id'));
        $("#CHAHdrModal").modal("hide");
    });
    $('#SearchCHAHdr').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#CHAHdrModal .navList > li').show();
        } else {
            $('#CHAHdrModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    $('#LstCHA li').on("click", function () {
        $('#CHAName').val($(this).text());
        $('#CHANameId').val($(this).attr('id'));
        $("#CHAModal").modal("hide");
    });
    $('#SearchCHA').keyup(function () {
        var valThis = $(this).val().toLowerCase();
        if (valThis == "") {
            $('#CHAModal .navList > li').show();
        } else {
            $('#CHAModal .navList > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(valThis) >= 0) ? $(this).show() : $(this).hide();
            });
        };
    });
    function ResetStuffingReq() {
        debugger;
        $('#CommodityName').html('');
        $('#CommodityName').append($('<option></option>').val('').html('---Select---'));
        $('#DivCreateStuffingReq').find('input:text').val('');
        $('#DivCreateStuffingReq').find('input:hidden').val('');
        $('#TblStuffingReq >tbody').html('');
        $('#DivCreateStuffingReq #TblStuffingContr >tbody').html('');
        $('#TblStuffingReq').hide('');
        $('#TblStuffingContr').hide('');
        $('[data-valmsg-for]').html('<span></span>');
        $('#DivStuffingMsg').html('');
        $('#DivStuffingErrMsg').html('');
        RequestDet = [];
        ContainerDet = [];
        if ('@ViewData["IsExporter"]' == 'True') {
            $('#ExporterName').val('@ViewData["ExporterName"]');
            $('#ExporterId').val('@ViewData["EximTraderId"]');
            var EximTraderId = '@ViewData["EximTraderId"]';
            var ExporterName = '@ViewData["ExporterName"]';
            $('#LstExporter').html('<li id=' + @ViewData["EximTraderId"] +' onclick="FillExporterDet(\'' + EximTraderId + '\',\'' + ExporterName + '\')">@ViewData["ExporterName"]</li>');
        }
        if ('@Model.CHAId' > 0) {
            $('#CHANameId').val('@Model.CHAId');
            $('#CHAName').val('@Model.CHA');
            $('#CHAId').val('@Model.CHAId');
            $('#CHA').val('@Model.CHA');
            var CHAId = '@Model.CHAId';
            var CHA = '@Model.CHA';
            $('#LstCHA').html('<li id=' + @Model.CHAId +' onclick="FillCHADet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
            $('#ListOfCHAHdr').html('<li id=' + @Model.CHAId +' onclick="FillCHAHdrDet(\'' + CHAId + '\',\'' + CHA + '\')">@Model.CHA</li>');
        }
        var remsize = 0;
        var remweight = 0;
        editflag = 0;
        $('#DivBody').load('/Export/Hdb_CWCExport/CreateStuffingRequest');
    }
    function StuffingReqOnSuccess(data) {
        $('#DivStuffingErrMsg').html('');
        if (data.Status == 1 || data.Status == 2 || data.Status == 3 ) {
            $('#DivStuffingMsg').html(data.Message);
            // $("#ContainerModal ul li:contains(" + $('#ContainerNo').val() + ")").remove();
            $("#myModal ul li:contains(" + $('#CartingRegisterNo').val() + ")").remove();
            setTimeout(LoadStuffingReq, 5000);
            editflag = 0;
            //GetStuffingReqList();
        }
        else if(data.Status == 4)
        {
            var conf = confirm('Stuffed FOB value is more than Party bond value.Shall we allow?');
            if(conf)
            {
                $('#IsConfirm').val('YES');
                $('#btnSubmit').prop('disabled',true);
                $('#submitform').submit();
            }
        }
        else {
            $('#DivStuffingErrMsg').html(data.Message);
        }
    }
    function StuffingReqOnFailure() { }
    function LoadStuffingReq() {
        $('#DivBody').load('/Export/Hdb_CWCExport/CreateStuffingRequest');
    }
    $('#btnSubmit').on("click", function () {
        $('#DivTblContrErrMsg').html('');
        debugger;
        if ($('#TblStuffingReq >tbody').html() == '') {
            $('#DivStuffingErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            return false;
        }
        else if ($('#TblStuffingContr >tbody').html() == '') {
            $('#DivTblContrErrMsg').html('Please Add Atleast One Record In Order To Save Stuffing Request Detail');
            return false;
        }
        else if ($('#ShippingBillNo').val() != '' && $('#BtnAddStuffingDtl').prop("disabled") == false) {
            $('#DivStuffingErrMsg').html('Please Add The Above Records');
            return false;
        }
        else if ($('#ContainerNo').val() != '' && $('#BtnAddContainer').prop("disabled") == false) {
            $('#DivTblContrErrMsg').html('Please Add The Above Records');
            return false;
        }
        
        else {
            for (var i = 0; i < RequestDet.length; i++) {
                if (RequestDet[i].PackUQCCode == '') {
                    $('#DivTblContrErrMsg').html('Please Add All Shipping Bill No PackUQC');
                    return false;
                }
            }
            var conf = confirm('Are You Sure You Want To Save?');
            if (conf) {
                $('#StuffingXML').val(JSON.stringify(RequestDet));
                $('#ContainerXML').val(JSON.stringify(ContainerDet));
            }
            else return false;

        }
    });
</script>
@{
    AjaxOptions options = new AjaxOptions(); 
    options.HttpMethod = "POST";
    //options.Confirm = "Are You Sure You Want To Save?";
    options.OnSuccess = "StuffingReqOnSuccess";
    options.OnFailure = "StuffingReqOnFailure";
    options.InsertionMode = InsertionMode.InsertAfter;
}
<div id="DivCreateStuffingReq" class="container-fluid">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
