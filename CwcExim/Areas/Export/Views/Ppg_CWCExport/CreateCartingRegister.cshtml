@model CwcExim.Areas.Export.Models.Ppg_CartingRegister

@if (ViewBag.GodownList != null)
{
    <input type="hidden" id="hdnGodownList" value="@ViewBag.GodownList" />
}
else
{
    <input type="hidden" id="hdnGodownList" value="" />
}
@if (ViewBag.RightsList != null)
{
    <input type="hidden" id="hdnRights" value="@ViewBag.RightsList" />
}
else
{
    <input type="hidden" id="hdnRights" value="" />
}
<style>
    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }

    #tblSHP.dataTable tbody > tr > td,
    #tblEXP.dataTable tbody > tr > td,
    #tblCHA.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row">
                <div class="col-md-12">
                    <h4>Carting Register</h4>
                </div>
            </div>
            <div class="content_wrp">
                <div id="DivAddCartingRegister">
                    @using (Ajax.BeginForm("AddEditCartingRegister", "Ppg_CWCExport", new AjaxOptions { HttpMethod = "POST", OnSuccess = "RegisterOnSuccess", OnFailure = "RegisterOnFailure" }, new { @Id = "frmCartingReg" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.CartingAppId)
                    @Html.HiddenFor(m => m.XMLData)

                    if (ViewBag.check != null)
                    {
                    <input type="hidden" id="hdnShippingLineList" value="@ViewBag.check" />
                    }
                    else
                    {
                    <input type="hidden" id="hdnShippingLineList" value="" />
                    }

                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Carting No:</label>
                            </div>
                            @Html.TextBoxFor(m => m.CartingRegisterNo, new { @placeholder = "Carting No", @readonly = true })
                        </div>
                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>Register Date:</label>
                            </div>
                            <div class="position-relative Date_Img">
                                @Html.TextBoxFor(m => m.RegisterDate, new { @placeholder = "Register Date", @readonly = true })
                                @Html.ValidationMessageFor(m => m.RegisterDate, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>CCIN No:</label>
                            </div>
                            <div class="position-relative">
                                @Html.TextBoxFor(m => m.ApplicationNo, new { @placeholder = "Search CCIN No", @readonly = true })
                                <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#myModal"></i></span>
                                @Html.ValidationMessageFor(m => m.ApplicationNo, "", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>CCIN Date:</label>
                            </div>
                            <div class="position-relative Date_Img">
                                @Html.TextBoxFor(m => m.ApplicationDate, new { @placeholder = "Select CCIN Date", @readonly = true })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Carting Type:</label>
                            </div>
                            @Html.DropDownListFor(m => m.CartingType, new List<SelectListItem>
                                {
                                    new SelectListItem { Text="Manual",Value="1"},
                                }, new { })
                            @Html.ValidationMessageFor(m => m.CartingType, "", new { @class = "Error_Msg" })
                        </div>
                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>Commodity Type:</label>
                            </div>
                            @Html.DropDownListFor(m => m.CommodityType, new List<SelectListItem>
                                {
                                    new SelectListItem { Text="General",Value="1"},
                                    new SelectListItem { Text="Heavy/Scrape",Value="2"}
                                }, "----Select----", new { })
                            @Html.ValidationMessageFor(m => m.CommodityType, "", new { @class = "Error_Msg" })
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(m => m.CHAName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            </div>
                            <div class="position-relative">
                                @Html.TextBoxFor(m => m.CHAName, new { @placeholder = "Search CHA", @readonly = true })
                                <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreCHA();" data-target="#CHAModal"></i></span>
                                @Html.HiddenFor(m => m.CHAId, new { @Id = "hdnCHAId" })
                                @Html.ValidationMessageFor(m => m.CHAName, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Shipping Line:</label>
                            </div>
                            <div class="position-relative">
                                @Html.HiddenFor(m => m.ShippingLineId)
                                @Html.TextBoxFor(m => m.ShippingLineName, new { @placeholder = "Search Shipping Line", @readonly = true })
                                <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreSHP();" id="shipping" data-target="#ShippingLineModal"></i></span>
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Godown Name:</label>
                            </div>
                            <div class="position-relative">
                                @Html.TextBoxFor(m => m.GodownName, new { @placeholder = "Search Godown Name", @readonly = true })
                                <span class="input_icon" id="SpanGodownSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#godownModal"></i></span>
                                @Html.HiddenFor(m => m.GodownId, new { @Id = "hdnGodownId" })
                                @Html.ValidationMessageFor(m => m.GodownName, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top Form_Space_bottom">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(m => m.Remarks, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            </div>
                            @Html.TextBoxFor(m => m.Remarks, new { @placeholder = "Enter remarks", @maxlength = "1000", @style = "width:100%;" })
                            @Html.ValidationMessageFor(m => m.Remarks, "", new { @class = "Error_Msg" })
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="Radiocontainer">
                                    Reserved Space
                                    @Html.RadioButton("Space", "ReservedSpace", new { id = "ReservedSpace", @onclick = "OnClickSpace('ReservedSpace')" })
                                    <span class="checkmark"></span>
                                </label>
                                <label class="Radiocontainer">
                                    General Space
                                    @Html.RadioButton("Space", "GeneralSpace", new { id = "GeneralSpace", @onclick = "OnClickSpace('GeneralSpace')" })
                                    @Html.HiddenFor(m => m.SpaceType, new { @readonly = true })
                                    <span class="checkmark"></span>
                                </label>
                            </div>
                        </div>
                    </div>

                    <div id="DivShippingDetails" class="Head_h5">
                        <div class="row">
                            <div class="col-md-12">
                                <h5>Carting Details</h5>
                            </div>
                        </div>
                        @Html.Hidden("CartingAppDtlId")
                        @Html.Hidden("LocationDetails")

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Shipping Bill No:</label>
                                </div>
                                @Html.TextBox("ShippingBillNo", "", new { @placeholder = "Enter Shipping Bill No" })
                                @Html.ValidationMessage("ShippingBillNo", new { @class = "Error_Msg" })
                            </div>
                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Shipping Bill Date:</label>
                                </div>
                                <div class="position-relative Date_Img">
                                    @Html.TextBox("ShippingBillDate", "", new { @placeholder = "Select Shipping Bill Date", @readonly = true })
                                    @Html.ValidationMessage("ShippingBillDate", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Exporter:</label>
                                </div>
                                <div class="position-relative">
                                    @Html.TextBox("Exporter", "", new { @placeholder = "Search Exporter", @readonly = true })
                                    <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="LoadMoreEXP();" data-target="#ExporterModal"></i></span>
                                    <input type="hidden" id="hdnExporterId" />
                                    @Html.ValidationMessage("Exporter", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Cargo Description:</label>
                                </div>
                                @Html.TextBox("CargoDescription", "", new { @placeholder = "Enter Cargo Description", @maxlength = 1000 })
                                @Html.ValidationMessage("CargoDescription", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Commodity:</label>
                                </div>
                                <div class="position-relative">
                                    @Html.TextBox("Commodity", "", new { @placeholder = "Search Commodity", @readonly = true })
                                    <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CommodityModal"></i></span>
                                    <input type="hidden" id="hdnCommodityId" />
                                    @Html.ValidationMessage("Commodity", new { @class = "Error_Msg" })
                                </div>
                            </div>
                            <div class="col-md-3 col-md-offset-5">
                                <div class="form-group">
                                    <label>Cargo Type:</label>
                                </div>
                                @Html.DropDownList("CargoType", new List<SelectListItem>
                                {

                                    new SelectListItem { Text="HAZ",Value="1"},
                                    new SelectListItem { Text="Non HAZ",Value="2"}
                                }, "----Select----", new { @onblur = "ClearErrMsg(this)" })
                                @Html.ValidationMessage("CargoType", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Marks & No:</label>
                                </div>
                                @Html.TextBox("MarksAndNo", "", new { @placeholder = "Enter Marks & No" })
                                @Html.ValidationMessage("MarksAndNo", new { @class = "Error_Msg" })
                            </div>
                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>No of Units:</label>
                                </div>
                                @Html.TextBox("NoOfUnits", "", new { @placeholder = "Enter No of Units", @maxlength = "9", onkeypress = "return AllowOnlyInteger(event)", @readonly = true })
                                @Html.ValidationMessage("NoOfUnits", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Weight (Kg):</label>
                                </div>
                                @Html.TextBox("Weight", "", new { @placeholder = "Enter Weight", maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)", @readonly = true })
                                @Html.ValidationMessage("Weight", new { @class = "Error_Msg" })
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>CUM:</label>
                                </div>
                                @Html.TextBox("CUM", "", new { @placeholder = "Enter CUM", maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)" })
                                @Html.ValidationMessage("CUM", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Actual Unit:</label>
                                </div>
                                @Html.TextBox("ActualQty", "", new { @placeholder = "Enter Actual Unit", @maxlength = "9", onkeypress = "return AllowOnlyInteger(event)" })
                                @Html.ValidationMessage("ActualQty", new { @class = "Error_Msg" })
                            </div>
                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>SQM:</label>
                                </div>
                                @Html.TextBox("SQM", "", new { @placeholder = "Enter SQM", maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)" })
                                @Html.ValidationMessage("SQM", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>FOB Value:</label>
                                </div>
                                @Html.TextBox("FOBValue", "", new { @placeholder = "Enter FOB Value", maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)" })
                                @Html.ValidationMessage("FOBValue", new { @class = "Error_Msg" })
                                @Html.Hidden("FOBValue1")
                            </div>
                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Gross Weight (Kg):</label>
                                </div>
                                @Html.TextBox("ActualWeight", "", new { @placeholder = "Enter Gross Weight (Kg)", maxlength = "11", @onkeypress = "return AllowOnlyDecimal()" })
                                @Html.ValidationMessage("ActualWeight", new { @class = "Error_Msg" })
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>Location:</label>
                                </div>
                                <div class="position-relative">
                                    @Html.TextBox("Location", "", new { @placeholder = "Search Location", @readonly = true })
                                    <span id="LocationSearch" class="input_icon"><i class="fa fa-search" aria-hidden="true"></i></span>
                                    @Html.ValidationMessage("Location", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                        </div>

                        <div class="d-flex justify-content-left">
                            <input type="button" id="BtnAddCartingReg" class="btn btn-primary mr-1 btn-100px" value="Add" onclick="SaveRegDtl()()">
                            <input type="button" id="BtnResetCartingReg" class="btn btn-primary-border btn-100px" value="Reset" onclick="SaveRegDtl()">
                        </div>

                        <div id="DivCartingRegTblErrMsg" class="logErrMsg" style="background-color:transparent"></div>
                        <div class="row Form_Space_top Form_Space_bottom">
                            <div class="col-md-12">
                                <table class="table table-striped table-bordered dataTable" id="tblShippingDetails" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th class="text-center">@Html.DisplayName("Edit")</th>
                                            <th class="text-center">@Html.DisplayName("View")</th>
                                            <th class="text-center" style="width:8%;">@Html.DisplayName("SL No")</th>
                                            <th>@Html.DisplayName("Shipping Bill No")</th>
                                            <th>@Html.DisplayName("Shipping Bill Date")</th>
                                            <th>@Html.DisplayName("Exporter")</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
                    </div>

                    <div class="d-flex justify-content-left">
                        <input type="button" id="BtnSaveCartingReg" class="btn btn-primary mr-1 btn-100px" value="Save" onclick="SubmitForm()()">
                        <input type="button" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetCartingRegister()">
                    </div>


                    <div id="DivCartingRegMsg" class="logSuccMsg" style="background-color:transparent"></div>
                    <div id="DivCartingRegErrMsg" class="logErrMsg" style="background-color:transparent"></div>
                        }
                    <div class="row">
                        <div class="col-md-2">
                            <input type="button" id="btnCRList" value="Show List" class="btn btn-primary mr-1 btn-100px" onclick="LoadRegisterList()" />
                        </div>
                        <div class="col-md-12">
                            <div id="DivCartingRegisterList"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content popup_modal">
                    <div class="modal-header popup_header">
                        <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                        <h4 class="modal-title">List of Application No</h4>
                    </div>
                    <div class="modal-body popup_body">
                        <input placeholder="Search Me" id="box" type="text" />
                        <ul class="navList" id="CartingRegList">
                            @{
                                foreach (var item in Model.lstAppNo)
                                {
                    <li id="@item.CartingAppId">@item.ApplicationNo</li>
                        }
                            }
                        </ul>
                    </div>
                    <div class="modal-footer">
                        <div class="row">
                            <div class="col-md-offset-6 col-md-6">
                                <button type="button" class="btn btn-primary-border btn-100px" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="LocationModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content popup_modal">
                    <div class="modal-header popup_header">
