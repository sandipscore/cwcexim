@model CwcExim.Areas.Export.Models.PPG_LoadContReq
<style>
    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }
</style>


<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row">
                <div class="col-md-12">
                    <h4>Loaded Container / CBT Request</h4>
                </div>
            </div>
            <div class="content_wrp">
                @using (Ajax.BeginForm("AddEditLoadContReq", "Ppg_CWCExport", new AjaxOptions
                {
                    Confirm = "Are you sure you want to Save?",
                    HttpMethod = "POST",
                    OnSuccess = "contReqOnSuccess",
                    OnFailure = "contReqFailure",
                    InsertionMode = InsertionMode.InsertAfter
                }))
                {
                    if (ViewBag.RightsList != null)
                    {
                        <input type="hidden" id="hdnRights" value="@ViewBag.RightsList" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnRights" value="" />
                    }

                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.LoadContReqId)
                    @Html.HiddenFor(m => m.StringifyData)

                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Container / CBT Request No:</label>
                            </div>
                            @Html.TextBoxFor(m => m.LoadContReqNo, new { @readonly = true })
                        </div>
                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>Container / CBT Request Date:</label>
                            </div>
                            <div class="position-relative Date_Img">
                                @Html.TextBoxFor(m => m.LoadContReqDate, new { @readonly = true })
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>CHA:</label>
                            </div>
                            <div class="position-relative">
                                @Html.HiddenFor(m => m.CHAId)
                                @Html.TextBoxFor(m => m.CHAName, new { @placeholder = "Search CHA", @readonly = true })
                                <span class="input_icon" id="CHASerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CHAModal"></i></span>
                                @Html.ValidationMessageFor(m => m.CHAName, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>
                    <div id="divDestination" class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Final Destination location:</label>
                            </div>
                            <div class="position-relative">
                                @Html.HiddenFor(m => m.FinalDestinationLocationID)
                                @Html.TextBoxFor(m => m.FinalDestinationLocation, new { @placeholder = "Search Final Destination Location", @readonly = true })
                                <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#DestinationModal" onclick="GetFinalDestination()"></i></span>
                                @Html.ValidationMessageFor(m => m.FinalDestinationLocation, "", new { @class = "Error_Msg" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top Form_Space_bottom">
                        <div class="col-md-12">
                            <div class="form-group">
                                @Html.LabelFor(m => m.Remarks, new { @class = "InputLabel" })
                            </div>
                            @Html.TextAreaFor(m => m.Remarks, new { @placeholder = "Enter Remarks", @maxlength = 500, @style = "width:100%;height:100px;" })
                            @Html.ValidationMessageFor(m => m.Remarks, "", new { @class = "Error_Msg" })
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Movement:</label>
                            </div>
                            @Html.DropDownListFor(m => m.Movement, new List<SelectListItem> {
                                    new SelectListItem { Text="CWC",Value="CWC"},
                                    new SelectListItem { Text="Own Transport(FR)",Value="Own Transport(FR)"}}, "---Select---", new { })
                            @Html.ValidationMessageFor(m => m.Movement, "", new { @class = "Error_Msg" })
                        </div>

                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>Custom Examination Type:</label>
                            </div>
                            @Html.DropDownListFor(m => m.CustomExaminationType, new List<SelectListItem>
                                        {
                                            new SelectListItem { Text="Open",Value="Open"},
                                            new SelectListItem { Text="Seal Checking",Value="SealChecking"},
                                        }, "----Select----", new { })
                            @Html.ValidationMessageFor(m => m.CustomExaminationType, "", new { @class = "Error_Msg" })
                        </div>
                    </div>
                    <div id="DivContainerSize" class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Origin:</label>
                            </div>
                            @Html.DropDownList("Origin", new List<SelectListItem>
                                        {
                                            new SelectListItem { Text="FAC",Value="FAC"},
                                            new SelectListItem { Text="NON FAC",Value="NON FAC"},

                                        }, "----Select----", new {})
                            @Html.ValidationMessage("Origin", new { @class = "Error_Msg" })
                        </div>
                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                <label>Via:</label>
                            </div>
                            @Html.DropDownList("Via", new List<SelectListItem>
                                        {
                                            new SelectListItem { Text="ACTL",Value="ACTL"},
                                            new SelectListItem { Text="TKD",Value="TKD"},
                                              new SelectListItem { Text="ROAD",Value="ROAD"},

                                        }, "----Select----", new {  })
                            @Html.ValidationMessage("Via", new { @class = "Error_Msg" })
                        </div>
                    </div>

                        <div id="DivShipping" class="Head_h5 Form_Space_top">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>Container / CBT Details</h5>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Exporter:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.Hidden("ExporterId")
                                        @Html.TextBox("ExporterName", "", new {@placeholder="Search Exporter", @readonly = true })
                                        <span class="input_icon" id="ExporterSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ExporterModal"></i></span>
                                        @Html.ValidationMessage("ExporterId", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                                <div class="row Form_Space_top">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Shipping Line:</label>
                                        </div>
                                        <div class="position-relative">
                                            @Html.Hidden("ShippingLineId")
                                            @Html.TextBox("ShippingLineName", "", new { @placeholder = "Search Shipping Line", @readonly = true })
                                            <span class="input_icon" id="ShippingLineSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                            @Html.ValidationMessage("ShippingLineId", new { @class = "Error_Msg" })
                                        </div>
                                    </div>
                                </div>
                                <div class="row Form_Space_top">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Container / CBT No:</label>
                                        </div>
                                        @Html.TextBox("ContainerNo", "", new { @placeholder = "Enter Container No", @maxlength = 15 })
                                        @Html.ValidationMessage("ContainerNo", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label>Size:</label>
                                        </div>
                                        @Html.DropDownList("Size", new List<SelectListItem> {
                                    new SelectListItem { Text="20",Value="20"},
                                    new SelectListItem { Text="40",Value="40"}}, "---Select---", new { })
                                        @Html.ValidationMessage("Size", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label></label>
                                        </div>
                                        @Html.CheckBox("IsInsured")
                                        <label for="IsInsured"><i class="square"></i><span>  Insured</span></label>
                                    </div>
                                </div>
                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Shipping Bill No:</label>
                                    </div>
                                    @Html.TextBox("ShippingBillNo", "", new { @placeholder = "Enter Shipping Bill No", @onblur = "ClearErrMsg(this)", maxlength = "30" })
                                    @Html.ValidationMessage("ShippingBillNo", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label></label>
                                    </div>
                                   @Html.CheckBox("IsSEZ")
                                    <label for="IsSEZ"><i class="square"></i><span> SEZ</span></label>
                                </div>
                                <div class="col-md-3 col-md-offset-3">
                                    <div class="form-group">
                                        <label>Shipping Bill Date:</label>
                                    </div>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBox("ShippingBillDate", "", new { @placeholder = "Select Shipping Bill Date", @readonly = true })
                                        @Html.ValidationMessage("ShippingBillDate", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Commodity:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.Hidden("CommodityId")
                                        @Html.TextBox("CommodityName", "", new { @placeholder = "Search Commodity", @readonly = true })
                                        <span class="input_icon" id="SearchCommodity"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CommodityModal"></i></span>
                                        @Html.ValidationMessage("CommodityId", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Cargo Type:</label>
                                    </div>
                                    @Html.DropDownList("CargoType", new List<SelectListItem>
                                {
                                   new SelectListItem { Text="Non HAZ",Value="2"},
                                    new SelectListItem { Text="HAZ",Value="1"}
                                }, "---Select---", new {  @onblur = "ClearErrMsg(this)" })
                                    @Html.ValidationMessage("CargoType", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div id="DivShippingSeal" class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Container Load Type:</label>
                                    </div>
                                    @Html.DropDownList("ContLoadType", new List<SelectListItem> {
                                    new SelectListItem { Text="FCL",Value="FCL"},
                                    new SelectListItem { Text="LCL",Value="LCL"}}, "---Select---", new { })
                                    @Html.ValidationMessage("ContLoadType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Custom Seal:</label>
                                    </div>
                                    @Html.TextBox("CustomSeal", "", new {@placeholder= "Enter Custom Seal", @maxlength = "30" })
                                    @Html.ValidationMessage("CustomSeal", new { @class = "Error_Msg" })
                                </div>
                            </div>



                            <div id="divPackage" class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Package Type:</label>
                                    </div>
                                    @Html.DropDownList("PackageType", new List<SelectListItem>
                                                    {
                                                        new SelectListItem { Text="Package",Value="P"},
                                                        new SelectListItem { Text="Liquid Bulk",Value="LB"},
                                                        new SelectListItem { Text="Dry Bulk",Value="DB"}
                                                    }, new {  })
                                    @Html.ValidationMessage("PackageType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Equipment Seal Type:</label>
                                    </div>
                                    @Html.DropDownList("EquipmentSealType", new List<SelectListItem>
                                                {
                                                   new SelectListItem { Text="Bottle-Seal",Value="BTSL"},
                                                   new SelectListItem { Text="E-Seal",Value="ESEAL"},
                                                   new SelectListItem { Text="Others",Value="OTH"},
                                                }, new { })
                                    @Html.ValidationMessage("EquipmentSealType", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div id="divEquipment" class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Equipment Status:</label>
                                    </div>
                                    @Html.DropDownList("EquipmentStatus", new List<SelectListItem>
                                                {
                                                   new SelectListItem { Text="Intact",Value="94"},
                                                   new SelectListItem { Text="Damaged",Value="218"}
                                                }, new { })
                                    @Html.ValidationMessage("EquipmentStatus", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Equipment QUC:</label>
                                    </div>
                                    @Html.DropDownList("EquipmentQUC", new List<SelectListItem>
                                                {
                                                    new SelectListItem { Text="Package",Value="PKG"},
                                                    new SelectListItem { Text="Kilogram",Value="KGS"},
                                                }, new { })
                                    @Html.ValidationMessage("EquipmentQUC", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div id="divEquipment" class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Pack UQC:</label>
                                    </div>
                                    <div class="position-relative">
                                        @Html.Hidden("PackUQCCode")
                                        @Html.TextBox("PackUQCDescription", "", new { @placeholder = "Enter Pack UQC", @readonly = true, onkeydown = "PopupActivate('PackUQCModal');" })
                                        @Html.ValidationMessage("PackUQCDescription", new { @class = "Error_Msg" })
                                        <span class="input_icon" id="PackUQCSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PackUQCModal"></i></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Cargo Description:</label>
                                    </div>
                                    @Html.TextArea("CargoDescription", "", new { @placeholder = "Enter Cargo Description", @style = "width:100%;height:100px;", @onblur = "ClearErrMsg(this)", maxlength = "500" })
                                    @Html.ValidationMessage("CargoDescription", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>No of Units:</label>
                                    </div>
                                    @Html.TextBox("NoOfUnits", "", new { @placeholder = "Enter No of Units", @onblur = "ClearErrMsg(this)", maxlength = "10", @onkeypress = "return AllowOnlyInteger(event)", })
                                    @Html.ValidationMessage("NoOfUnits", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Gross Weight (Kg):</label>
                                    </div>
                                    @Html.TextBox("GrossWt", "", new { @placeholder = "Enter Gross Weight(Kg.)", @onblur = "ClearErrMsg(this)", maxlength = "12", @onkeypress = "return AllowOnlyDecimal(event)" })
                                    @Html.ValidationMessage("GrossWt", new { @class = "Error_Msg" })
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>FOB Value:</label>
                                    </div>
                                    @Html.TextBox("FobValue", "", new { @placeholder = "Enter FOB Value", @onblur = "ClearErrMsg(this)", maxlength = "12", @onkeypress = "return AllowOnlyDecimal(event)" })
                                    @Html.ValidationMessage("FobValue", new { @class = "Error_Msg" })
                                </div>
                            </div>

                                <div class="row">
                                    <div class="stoke SignUp_space_bottom SignUp_space_top SignUp_space_bottom"></div>
                                </div>
                            <div class="d-flex justify-content-left Form_Space_bottom">
                                <input type="button" id="btnAdd" class="btn btn-primary mr-1 btn-100px" value="Add" onclick="AddContDet()">
                                <input type="button" id="btnReset" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetContDet()">
                            </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <div id="tblArrayContDet"></div>
                                    </div>
                                </div>
                            </div>
                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top Form_Space_top"></div>
                        </div>

                        <div class="d-flex justify-content-left Form_Space_top">
                            <input type="submit" id="btnSave" class="btn btn-primary mr-1 btn-100px" value="Save">
                            <input type="button" id="btnReset" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetAllFields()">
                        </div>
                        <div class="logSuccMsg" id="DivMsg" style="background-color:transparent"></div>
                }
                <div class="row">
                    <div class="col-md-12">
                        <div id="DivLoadContList"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="CHAModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of CHA Names</h4>
            </div>
            <div class="modal-body popup_body">
                <input placeholder="Search Me" id="CHAbox" type="text" />
                <ul class="navList" id="lstCHA">
                    @{
                        if (ViewBag.ListOfCHA != null)
                        {
                            foreach (var item in ViewBag.ListOfCHA)
                            {
                                <li id="@item.CHAEximTraderId">@item.CHAName</li>
                            }
                        }
                    }
                </ul>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-offset-6 col-md-6">
                        <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade mTabindex" id="PackUQCModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" onclick="ClosePackUQC()"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List Of Pack UOM</h4>
            </div>
            <div class="modal-body popup_body" style="position:relative;">
                <input placeholder="Search By Pack UQC Name Or Pack UQC Code " id="PackUQCbox" type="text" tabindex="200" autofocus />
                <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchPackUQCByCode()"></i></span>

                <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                    <thead>
                        <tr>
                            <th colspan="8" width="80%">Pack UOM Description</th>
                            <th colspan="4" width="20%">Pack UOM Code</th>
                        </tr>
                    </thead>
                </table>

                <div id="slim_scroll_3">
                    <table tabindex="201" class="table dataTable table-bordered table-striped slim_tble" id="tblePackUQC">
                        @{
                            if (ViewBag.lstPackUQC != null)
                            {
                                var tabvalue = 202;
                                foreach (var item in ViewBag.lstPackUQC)
                                {
                                    <tr tabindex="@tabvalue" onclick="SelectPackUQC(@item.PackUQCId,'@item.PackUQCDescription','@item.PackUQCCode')" onkeypress="SelectPackUQC(@item.PackUQCId,'@item.PackUQCDescription','@item.PackUQCCode')">
                                        <td colspan="8" width="80%" class="txt_wrp">@item.PackUQCDescription</td>
                                        <td colspan="4" width="20%" class="txt_wrp">@item.PackUQCCode</td>
                                    </tr>
                                    tabvalue++;
                                }
                            }
                        }

                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-6">
                        @if (ViewBag.PackUQCState != null && Convert.ToBoolean(ViewBag.PackUQCState) == true)
                        {
                            <button type="button" style="width:100%; margin-left:0;" onclick="LoadMorePackUQC()" class="btn log_Btn_sm" id="btnLoadPackUQC">Load More Data</button>
                        }
                        else
                        {
                            <button type="button" style="width:100%; margin-left:0;" onclick="LoadMorePackUQC()" class="btn log_Btn_sm" id="btnLoadPackUQC" disabled>Load More Data</button>
                        }
                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn log_Btn_sm" onclick="ClosePackUQC()">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="ExporterModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of Exporter</h4>
            </div>
            <div class="modal-body popup_body">
                <input placeholder="Search Me" id="Exporterbox" type="text" />
                <ul class="navList" id="lstExporter">
                    @{
                        if (ViewBag.ListOfExporter != null)
                        {
                            foreach (var item in ViewBag.ListOfExporter)
                            {
                                <li id="@item.EXPEximTraderId">@item.ExporterName</li>
                            }
                        }
                    }
                </ul>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-offset-6 col-md-6">
                        <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="CommodityModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content popup_modal" style="width:80%;">
            <div class="modal-header popup_header">
                <button type="button" class="close" onclick="CloseCommodity()"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of Commodity</h4>
            </div>
            <div class="modal-body popup_body" style="position:relative;">
                <input placeholder="Search By Commodity Name" id="CommodityBox" type="text" />
                <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchCommodityByPartyCode()"></i></span>
                <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                    <thead>
                        <tr>
                            <th colspan="8" width="80%">Commodity Name</th>

                        </tr>
                    </thead>
                </table>
                <div id="slim_scroll_2">
                    <table class="table dataTable table-bordered table-striped" id="tbleCommodity">
                        @{
                            if (ViewBag.LstCommodity != null)
                            {
                                foreach (var item in ViewBag.LstCommodity)
                                {
                                    <tr onclick="SelectCommodity(@item.CommodityId,'@item.CommodityName')">
                                        <td colspan="8" width="80%">@item.CommodityName</td>

                                    </tr>
                                }
                            }
                        }
                    </table>
                </div>
            </div>
            <div class="modal-footer">
