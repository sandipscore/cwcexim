<script>
    function SelectCartingNo(value, text) {
        debugger;
        var flag = "C";
        if (text.substring(0, 3) == 'EXP')
        {
            flag = "L";
        }

       
        if (RequestDet.filter(x=> x.CartingRegisterId == value).length > 0)
            alert("Data already added with this Carting No.");
        else GetStuffingDetails(value, flag);
        $("#myModal").modal("hide");
    }
    var caridx = -1;
    function SelectCartingIndex(i) {
        caridx = i;
    }
    function ExistingCartingNo(value, text) {
        debugger;
        var CarRegNo = text.split('-')[0];
        var SbNo = text.split('-')[1];

        if (RequestDet.filter(x=>x.CartingRegisterId == value).length > 0)
            alert("Data already added with this Carting No.");
        else if ($('#CommodityName option:selected').text().split('-')[3] == RequestDet[caridx].CartingRegisterNo
            && $('#CommodityName option:selected').text().split('-')[1] == RequestDet[caridx].ShippingBillNo)
            alert("Commodity with this Carting is selected in Shipping Bill");
        else if (ContainerDet.filter(x=>x.CartRegDtlId == RequestDet[caridx].CartingRegisterDtlId).length > 0)
            alert("Commodity with this Carting already added in Container");
        else GetStuffingDetailsExisting(value, caridx);
        caridx = -1;
        $("#myModalExisting").modal("hide");
    }
    function CartingDetChange(i) {
        debugger;
        //Date validation
        var check = 0;
        var splitdate = $('#ShippingBillDt' + i).val().split('/');
        if (splitdate.length > 0)
        {
            splitdate[0] = parseInt(splitdate[0]);
            splitdate[1] = parseInt(splitdate[1]);
            splitdate[2] = parseInt(splitdate[2]);
            var dateregex = /^(0?[1-9]|[12][0-9]|3[01])[\/](0?[1-9]|1[012])[\/]\d{2}$/;
            if ((splitdate[1]) >= 1 && (splitdate[1]) <= 12)
            {
                var Days30 = [4, 6, 9, 11];
                var Days31 = [1, 3, 5, 7, 8, 10, 12];
                if ($('#ShippingBillDt' + i).val().match(dateregex) == null)
                {
                    alert("Please enter a correct Shipping date in dd/mm/yy format");
                    $('#ShippingBillDt' + i).val(RequestDet[i].ShippingDate);
                    return false;
                }
                else if (Days31.indexOf(splitdate[1]) > -1 && splitdate[0] >= 1 && splitdate[0] <= 31) {
                    check = 1;
                }
                else if (Days30.indexOf(splitdate[1]) > -1 && splitdate[0] >= 1 && splitdate[0] <= 30) {
                    check = 1;
                }
                else if (splitdate[1] == 2) {
                    var year = ('20' + splitdate[2]);
                    if (year % 4 == 0 && splitdate[0] >= 1 && splitdate[0] <= 29) {
                        check = 1;
                    }
                    else if (year % 4 != 0 && splitdate[0] >= 1 && splitdate[0] <= 28) {
                        check = 1;
                    }
                }
                if (check == 0) {
                    alert('Please enter a valid Shipping date');
                    $('#ShippingBillDt' + i).val(RequestDet[i].ShippingDate);
                }
            }
            else
            {
                alert("Please enter a valid Shipping date");
                $('#ShippingBillDt' + i).val(RequestDet[i].ShippingDate);
            }
        }
        else {
            alert("Please enter a valid Shipping date");
            $('#ShippingBillDt' + i).val(RequestDet[i].ShippingDate);
        }
        RequestDet[i].ShippingBillNo = $('#ShippingBillNo' + i).val();
        RequestDet[i].ShippingDate = $('#ShippingBillDt' + i).val();
        RequestDet[i].CargoDescription = $('#CargoDescription' + i).val();
        RequestDet[i].CargoType = $('#CargoType' + i).val();
    }

    function GetStuffingDetailsExisting(CartingRegisterId, idx) {
        CheckDet = [];
        $.ajax({
            url: '/Export/Loni_CWCExportV2/GetCartRegDetForStuffingReq',
            type: 'GET',
            dataType: 'JSON',
            data: { CartingRegisterId: CartingRegisterId },
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    $.each(data.Data.ContainerDetails, function (i, item) {
                        // $('#CommodityName').append($('<option></option>').val(item.CartRegDtlId).html(item.CommodityName));
                        CommodityJson[idx] = item;
                    });
                    $.each(data.Data.LstStuffing, function (i, item) {
                        var flag = 0;
                        $.each(RequestDet, function (i, item1) {
                            if (item.CartingRegisterId == item1.CartingRegisterId &&
                                item1.ShippingBillNo == item.ShippingBillNo) {
                                flag = 1;
                            }
                        });
                        if (flag == 0) {
                            RequestDet[idx] = item;
                            CheckDet = RequestDet;
                        }
                    });
                    GenerateTable();
                    CommodityBind();
                }
            }
        });
    }
    function CommodityBind() {
        var html = '';
        html += '<option value="">---Select---</option>';
        $.each(CommodityJson, function (i, elem) {
            html += '<option value="' + elem.CartRegDtlId + '">' + elem.CommodityName + '</option>';
        });
        $('#CommodityName').html(html);
    }



    function PullContainerDtl()
    {
        if ($('#ContainerNo').val() == "" || ($('#ShippingHdrLine').val() == "" && $('#ShippingLine').val() == "") || RequestDet.length == 0)
        {
            alert("Please select Container No. and Header ShippingLine and Stuffing Details");
        }
        else

        {
           
            $.each(CommodityJson, function (i, elem) {
               
                var Data = {
                    CartRegDtlId: elem.CartRegDtlId,
                    CommodityName: elem.CommodityName,
                    ContainerNo: $('#ContainerNo').val(),
                    Size: $('#Size').val(),
                    ShippingLineId: $('#ShippingLine').val() == "" ? $('#ShippingHdrLineId').val() : $('#ShippingLineId').val(),
                    ShippingLine:  $('#ShippingLine').val() == "" ?$('#ShippingHdrLine').val(): $('#ShippingLine').val(),
                    StuffQuantity: RequestDet[i].RQty,
                    StuffWeight: RequestDet[i].RWt,
                    CFSCode: $('#CFSCode').val(),
                    FOB: RequestDet[i].Fob
                };
                ContainerDet.push(Data);
                // $("#ContainerModal ul li:contains(" + $('#ContainerNo').val() + ")").remove();
               
            });


            GenerateContrTable();
            $('#DivContainerDet').find('input:text,select,textarea').val('');
            $('#DivContainerDet').find('input:hidden').val('');
            $('#BtnAddContainer').prop("disabled", true);
            // $('#DivContainerDet input[type="text"]').prop("readonly", true);
            // $('#DivCartingDetails select,#DivCartingDetails textarea').prop("disabled", true);

            $('#BtnPullContainer').prop("disabled", true);
        }
    }
</script>