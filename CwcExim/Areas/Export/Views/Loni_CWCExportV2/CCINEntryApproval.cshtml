                        ClearStateDropdown();
                        $.each(data.Data, function (i, item) {
                            //debugger;
                            var SelectStateId = parseInt($('#SelectStateId').val());
                            $('#StateId').append($('<option></option>').val(item.StateId).html(item.StateName));
