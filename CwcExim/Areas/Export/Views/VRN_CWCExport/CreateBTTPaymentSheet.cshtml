<script>
    function ShowList()
    {
        $('#SearchInvoiceNo').val("");
        $('#SearchInvoiceDate').val("");
        $('#DivListOfInvoice').load('/Export/VRN_CWCExport/ListOfExpInvoiceBTT');
    }
    function SearchInvoice() {
        var InvoiceNo = $('#SearchInvoiceNo').val().trim();
        var InvoiceDate = $('#SearchInvoiceDate').val().trim();
        if ($('#SearchInvoiceNo').val() == "" && $('#SearchInvoiceDate').val() == "")
            alert("Enter Invoice No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate() == true)
            $('#DivListOfInvoice').load('/Export/VRN_CWCExport/ListOfExpInvoiceBTT?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate);
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Export/VRN_CWCExport/ListOfExpInvoiceBTT?InvoiceNo=' + InvoiceNo);
        else if (Validate() == true)
            $('#DivListOfInvoice').load('/Export/VRN_CWCExport/ListOfExpInvoiceBTT?InvoiceDate=' + InvoiceDate);
    }
</script>

