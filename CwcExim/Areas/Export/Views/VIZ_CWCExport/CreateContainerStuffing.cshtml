               $('#EventDate'+id).val('').focus();
            }
            else {
               // $('#ErrddlValidType_' + id).html('');
                return true;
            }
        }
        else {
           // $('#ErrddlValidType_' + id).html('');
        }
    }

    function SendSF() {
        debugger;
        var ContainerStuffingId = $('#ContrainerStuffingId').val();
        var Token = $('input[name="__RequestVerificationToken"]').val();
        $.ajax({
            url: "/Export/VIZ_CWCExport/SendSF",
