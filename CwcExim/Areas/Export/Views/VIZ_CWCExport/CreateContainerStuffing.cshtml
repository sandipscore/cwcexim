    function LoadStuffingRequest() {
        Page = 0;
        $('#LstRequestNo').html('');
        $.ajax({
            url: '/Export/VIZ_CWCExport/GetReqNoForContainerStuffing',
            type: 'GET',
            dataType: 'JSON',
            success: function (data) {
                debugger;
                if (data != '' && data != null) {
                    var html = '';
                    var tab = 401;
                    var jsondata = JSON.parse(JSON.stringify(data.Data));
                    if (jsondata.length > 0) {
                        $.each(jsondata, function (i, item) {
                            html += '<li tabindex=' + tab + '_' + i + ' onkeypress="StuffRequest(' + item.StuffingReqId + ',&quot;' + item.StuffingReqNo + '&quot;,&quot;' + item.RequestDate + '&quot;)" onclick="StuffRequest(' + item.StuffingReqId + ',&quot;' + item.StuffingReqNo + '&quot;,&quot;' + item.RequestDate + '&quot;)">' + item.StuffingReqNo + '</li>';
                        });
                    }
                    $('#LstRequestNo').html(html);
                }

            }
        });
    }

