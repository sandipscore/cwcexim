                                @Html.TextAreaFor(m => m.Naration, new { @style = "height:75px;", @maxlength = "500" })
                            </div>
                        </div>
                    </div>
                  
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label> Amount:</label>
                            </div>
                            <div class="col-md-4">
