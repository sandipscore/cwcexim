                            <div class="col-md-1-5 col_cus_2">
                                <label> Round Up:</label>
                            </div>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.Round_up, "", new { @readonly = true })
                            </div>

                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label> Total:</label>
                            </div>
                            <div class="col-md-4">
                                @Html.TextBoxFor(m => m.NetAmt, "", new { @readonly = true })
