<div id="DivCreateStuffingReq" class="container-fluid">
    @using (Ajax.BeginForm("AddEditPestControl", "WLJ_Pest", new AjaxOptions { HttpMethod = "POST", InsertionMode = InsertionMode.InsertAfter, OnSuccess = "PaymentOnSuccess", OnFailure = "PaymentOnFailure" }, new { @Id = "FumiInvoice" }))
