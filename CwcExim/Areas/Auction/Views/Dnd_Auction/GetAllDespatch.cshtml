@{
    Layout = null;
}
@model IEnumerable<CwcExim.Areas.Auction.Models.Dnd_Despatch>
<table id="TblAuctionNotice" class="table table-striped table-bordered nowrap dataTable" cellspacing="0" width="100%">

    <thead>
            <tr>
                <th>SL No.</th>
                <th>Notice No</th>
                <th>Despatch No</th>
                <th>Despatch Date</th>
            </tr>       
    </thead>
    <tbody>
        @{

          

                int i = 0;
                foreach (var item in Model)
                {
                    i = i + 1;
                    <tr>
                        <td>@i</td>
                        <td>@Html.DisplayFor(m => item.AuctionNo)</td>
                        <td>@Html.DisplayFor(m => item.DespatchNo)</td>
                        <td>@Html.DisplayFor(m => item.DespatchDate)</td>
                       
                    </tr>
                }
           
            
        }

    </tbody>

</table>
<script>
    $(document).ready(function () {
        var TblAuctionNotice = $('#TblAuctionNotice').DataTable({
            "bFilter": false,
            "bInfo": false,
            "aoColumns": [
                    { "sName": "SLNo", "width": "7%" },
                    { "sName": "Notice No", "width": "15%" },
                    { "sName": "DespatchNo" },
                    { "sName": "Party" }
                   
            ]
        });
        new $.fn.dataTable.FixedHeader(TblAuctionNotice);
    });
</script>