<script>
    var TimeInSeconds;
    $(document).ready(function () {
        TimeInSeconds = 5000;
        $("#InvoiceDate").datepicker({
            dateFormat: "dd/mm/yy",
            showOn: "button",
            buttonImage: "/Content/images/calendar.png",
            buttonImageOnly: true,
            buttonText: "Select date",
            changeMonth: true,
            changeYear: true,
            showAnima: "fadein",
            onClose: function () {
                $(".Date_Img .Error_Msg").text("");
                $('[data-valmsg-for="Data"]').html('<span></span>');
            }
        });

        //StuffingReqBinding();
        //PartyBinding();
        //PayeeBinding();
    });

    $('#lstInvoice > li').on("click", function () {

        $('#BIDId').val($(this).attr('id'));
        var bidid = $('#BIDId').val();
        $("#BIDNoModal").modal("hide");

        if (bidid > 0) {
            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetBidDetails", "AuctionInvoice")',
                data: { BIDId: bidid },
                success: function (data) {
                    if (data != undefined || data != null) {
                        setTimeout(function () {
                            $("#BIDNo").val(data.BIDNo);
                            $("#ReceiptDate").val(data.ReceiptDate);
                            $("#PartyName").val(data.PartyName);
                            $("#GSTNo").val(data.GSTNo);
                            $("#Sate").val(data.Sate);

                            $("#PartyId").val(data.PartyId);
                            $("#hdnAddress").val(data.PartyAddress);
                            $("#hdnState").val(data.State);
                            $("#hdnStateCode").val(data.StateCode);
                            $("#InvoiceType").val(data.InvoiceType);


                        }, 600);
                    }
                    else {
                        $('#BIDId').val(0);
                        $("#BIDNo").val("");
                        $("#ReceiptDate").val("");
                        $("#PartyName").val("");
                        $("#GSTNo").val("");
                        $("#Sate").val("");
                    }
                },
                error: function () {
                }
            });
        }
    });


</script>
