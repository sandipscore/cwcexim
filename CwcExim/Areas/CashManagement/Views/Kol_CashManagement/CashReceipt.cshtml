                                                    <td class="text-right">@Model.HTChargeType[a].Amount</td>
                                                    <td class="text-right">@Model.HTChargeType[a].IGSTPer</td>
                                                    <td class="text-right">@Model.HTChargeType[a].IGSTAmt</td>
                                                    <td class="text-right">@Model.HTChargeType[a].CGSTPer</td>
                                                    <td class="text-right">@Model.HTChargeType[a].CGSTAmt</td>
                                                    <td class="text-right">@Model.HTChargeType[a].SGSTPer</td>
                                                    <td class="text-right">@Model.HTChargeType[a].SGSTAmt</td>
