                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-offset-7 col-md-2">
                                <label>Total Value:</label>
                            </div>
                            <div class="col-md-3">
                                @Html.TextBoxFor(m => m.TotalValue, new { @readonly = true, @style = "text-align:right;" })
