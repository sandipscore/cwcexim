                            </div>
                            <div class="col-md-offset-2-5 col_cus_offset_2 col-md-2">
                                <label>Invoice Value:</label>
                            </div>
                            <div class="col-md-3">
                                @Html.TextBoxFor(m => m.InvoiceValue, new { @class = "Numeric", @readonly = true, @style = "text-align:right;" })
