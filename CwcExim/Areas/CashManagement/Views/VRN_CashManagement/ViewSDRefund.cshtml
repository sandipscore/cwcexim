@model CwcExim.Areas.CashManagement.Models.VRN_AddMoneyToPDModelRefund


<div class="container-fluid">
    <div class="col-md-11 col-md-offset-0-5">
        <div class="Head_h4">

            <div class="row">
                <div class="col-md-12">
                    <h4>Refund From SD</h4>
                </div>
            </div>

            @Html.HiddenFor(m => m.PartyId, new { Value = "0000000000" })


            @Html.HiddenFor(m => m.UnPaidAmount)
            @Html.HiddenFor(m => m.PName)
            @Html.HiddenFor(m => m.PartyAddress)

            <div class="row Form_Space_top">
                <div class="Form_Input form-group">
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Journal No.:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2">
                        @Html.TextBoxFor(m => m.JournalNo)

                    </div>
                    <div class="col-md-1-5 col_cus_2 col-md-offset-4 col_cus_offset_4 padding_rt">
                        <label>Journal Date.:</label>
                    </div>
                    <div class="col-md-2 col_cus_2 Date_Img">
                        @Html.TextBoxFor(m => m.JournalDate, new { @readonly = true })
                        @Html.ValidationMessageFor(m => m.JournalDate)
                    </div>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input form-group">
                    <div class="col-md-1-5 col_cus_2">
                        @Html.LabelFor(m => m.PartyName)
                    </div>
                    <div class="col-md-10-5 col_cus_10">
                        <span class="ReQSimbol">*</span>
                        @Html.TextBoxFor(m => m.PartyName, new { @readonly = true })
                        @Html.ValidationMessageFor(m => m.PartyName)

                    </div>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input form-group">
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Amount:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2">
                        @Html.TextBoxFor(m => m.OpBalance, new { @readonly = true })
                        @Html.ValidationMessageFor(m => m.OpBalance)
                    </div>
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Refund Amount:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2 Error_Msg">
                        <span class="ReQSimbol">*</span>
                        @Html.TextBoxFor(m => m.RefundAmount, new { onkeypress = "return AllowOnlyDecimal(event)" })
                        @Html.ValidationMessageFor(m => m.RefundAmount)
                    </div>
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Closing Balance:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2">
                        @Html.TextBoxFor(m => m.closingBalance, new { @readonly = true })
                        @Html.ValidationMessageFor(m => m.closingBalance)
                    </div>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input">
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Folio No.:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2">
                        @Html.TextBoxFor(m => m.FolioNo)

                    </div>
                    <div class="col-md-1-5 col_cus_2 col-md-offset-4 col_cus_offset_4 padding_rt">
                        <label>Ro Sanction Order No.:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2">
                        @Html.TextBoxFor(m => m.RoSanctionOrderNo)

                    </div>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input">
                    <div class="col-md-1-5 col_cus_2 padding_rt">
                        <label>Ro Sanction Date.:</label>
                    </div>
                    <div class="col-md-2-5 col_cus_2 Date_Img">
                        @Html.TextBoxFor(m => m.RoSanctionOrderDate)

                    </div>

                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input form-group">
                    <div class="col-md-1-5 col_cus_2">
                        <label>Remarks:</label>

                    </div>
                    <div class="col-md-10-5 col_cus_10">
                        @Html.TextAreaFor(m => m.Remarks)
                    </div>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="col-md-12">
                    <table class="table table-bordered table-striped dataTable">
                        <thead>
                            <tr>
                                <th>Mode</th>
                                <th>Drawee Bank</th>
                                <th>Instrument No.</th>
                                <th>Date</th>
                                <th>Amount</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var i in Model.Details)
                            {
                                <tr>
                                    <td>@i.Type</td>
                                    <td>@i.Bank </td>
                                    <td>@i.InstrumentNo</td>
                                    <td class="Datetab">@i.Date</td>
                                    <td>@i.Amount</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row Form_Space_top">
                <div class="Form_Input form-group">
                    <div class="col-md-offset-8 col-md-1-5">
                        <label>Total:</label>
                    </div>
                    <div class="col-md-2-5">
                        <input type="text" id="txtTotal" value="@Model.RefundAmount" readonly />
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
            </div>
            <div class="row">
                <div class="col-md-offset-4 col-md-2 SignUp_space_bottom">
                    <input type="button" class="btn log_Btn_sm" value="Back" id="BtnSaveCustomAppraisement" onclick="return Back()" />
                </div>
                <div class="col-md-2">
                    <input type="button" class="btn log_Btn_sm" id="printButton" value="Print" onclick="Print()" />
                </div>
            </div>



        </div>
    </div>
</div>


<div class="modal fade" id="PartyList" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of Party Names</h4>
            </div>
            <div class="modal-body popup_body">
                <input placeholder="Search Me" id="CHAbox" type="text" />
                <ul class="navList" id="lstCHA"></ul>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-offset-6 col-md-6">
                        <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $('#JournalDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });

    $('#RoSanctionOrderDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });

    //$('#JournalDate').datepicker("setDate", new Date());
    //$("#RoSanctionOrderDate").datepicker("setDate", new Date());


    function AllowOnlyDecimal(evt) {
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }
        if ((CharCode > 47 & CharCode < 58) || (CharCode == 46) || CharCode == 8) {
            return true;
        }
        else {
            evt.preventDefault;
            return false;
        }
    }

    function Back()
    {
        $('#DivBody').load('/CashManagement/VRN_CashManagement/RefundFromPDA');
    }

    function LoadSDRefundList() {
        debugger;
        $('#DivSDRefundList').load('/CashManagement/VRN_CashManagement/SDRefundList');
    }

    var date = new Date();
    var curDate = date.getDate();
    function Print() {

        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetSDRefundReceipt", "VRN_ReportCWC", new { area = "Report" })',
            dataType: 'JSON',
            type: 'POST',
            data: {
                JournalNo: $('#JournalNo').val(),

            },
            success: function (data) {
                debugger;
                if (data.Status == 1)
                    window.open(data.Message + "?_t=" + (new Date().getTime()), "_blank");
                else
                    alert(data.Message);
            }
        });
    }

</script>
@Scripts.Render("~/bundles/jqueryval")
