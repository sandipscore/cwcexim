                    @Html.HiddenFor(m => m.CRNoteId)
                    @Html.HiddenFor(m => m.InvoiceId)
                    @Html.HiddenFor(m => m.Module)
                    @Html.HiddenFor(m => m.InvoiceType)
                    @Html.HiddenFor(m => m.InvoiceDate)
                    @Html.HiddenFor(m => m.PayeeId)
