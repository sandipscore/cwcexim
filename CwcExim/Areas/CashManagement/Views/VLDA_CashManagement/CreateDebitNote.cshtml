@model CwcExim.Areas.CashManagement.Models.WFLDCreditNote
<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>Debit Note</h4>
                    </div>
                </div>
                @using (Ajax.BeginForm("AddDebitNote", "VLDA_CashManagement", new AjaxOptions
                {
                  //  Confirm = "Are you sure you want to Save?",
                    HttpMethod = "POST",
