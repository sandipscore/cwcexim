                    });
                    if (Page == -1)
                        $('#tblParty tbody').html(html);
                    else
                        $('#tblParty tbody').append(html);
                    Page++;

                    if (data.Data.State == true)
                        $('#btnLoadMoreParty').prop('disabled', false);
                    else $('#btnLoadMoreParty').prop('disabled', true);
                }
            }
        });
    }

















