                                    + '<td><input class="text-right" id="IGSTP' + i + '" type="text" value="' + item.IGSTPer + '" readonly /></td>'
                                    + '<td><input class="text-right" id="IGSTA' + i + '" type="text" value="' + item.IGST.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="CGSTP' + i + '" type="text" value="' + item.CGSTPer + '" readonly /></td>'
                                    + '<td><input class="text-right" id="CGSTA' + i + '" type="text" value="' + item.CGST.toFixed(2) + '" readonly /></td>'
                                    + '<td><input class="text-right" id="SGSTP' + i + '" type="text" value="' + item.SGSTPer + '" readonly /></td>'
