                            $('#InvoiceAmt').val((item.InvoiceAmt).toFixed(2));
                            $('#Total').val((item.Amount));
                            $('#CGST').val((item.CGST));
                            $('#IGST').val((item.IGST));
                            $('#SGST').val((item.SGST));
                            $('#InvoiceValue').val((item.InvoiceAmt).toFixed(2));
                            
                        });
                        
                      

                        $('#tblCWCCharges tbody').html(html);
                        $('#TOTCWCChrg').val((TotalCWC).toFixed(2));
