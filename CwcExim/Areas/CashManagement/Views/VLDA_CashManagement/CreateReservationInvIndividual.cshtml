            alert("Please select Godown");
        }
        else if ($('#PartyName').val() == "") {
            alert("Please select Party Name");
        }
        else if (($('#SQM').val() == "" || $('#SQM').val() < 25) && $('#GodownType').val()=='Bond') {
            alert("Please select SQM or Mimimum put 25 for Bond Godown");
        }

