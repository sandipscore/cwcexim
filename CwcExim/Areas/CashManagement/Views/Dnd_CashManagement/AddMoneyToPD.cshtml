@using (Ajax.BeginForm("AddMoneyToPD", "Dnd_CashManagement", new AjaxOptions { HttpMethod = "POST", OnSuccess = "OnSuccess", LoadingElementDuration = 1000, LoadingElementId = "divProgress" }))
            {
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-11 col-md-offset-0-5">
                <div class="Head_h4">
                    <div class="row">
                        <div class="col-md-12">
                            <h4>Add Money to SD </h4>
                        </div>
                    </div>
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.PartyId, new { Value = "0000000000" })

                    @if (ViewBag.RightsList != null)
                    {
                        <input type="hidden" id="hdnRights" value="@ViewBag.RightsList" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnRights" value="" />
                    }

                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-1-5">
                                @Html.LabelFor(m => m.ReceiptNo)
                            </div>
                            <div class="col-md-2-5">
                                @Html.TextBoxFor(m => m.ReceiptNo, new { Value = "000000", @readonly = true })
                                @Html.ValidationMessageFor(m => m.ReceiptNo)
                            </div>
                            <div class="col-md-offset-4 col-md-1-5">
                                @Html.LabelFor(m => m.TransDate)
                            </div>
                            <div class="col-md-2 Date_Img">
                                @Html.TextBoxFor(m => m.TransDate, new { Value = DateTime.Today.ToString("dd/MM/yyyy") })
                                @Html.ValidationMessageFor(m => m.TransDate)
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-1-5">
                                @Html.LabelFor(m => m.PartyName)
                            </div>
                            <div class="col-md-10-5">
                                @Html.TextBoxFor(m => m.PartyName)
                                @Html.ValidationMessageFor(m => m.PartyName)
                                <span class="search" onclick="partyBinding()"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="" data-target="#InvoiceNoModal"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-1-5">
                                @Html.LabelFor(m => m.Address)
                            </div>
                            <div class="col-md-10-5">
                                @Html.TextAreaFor(m => m.Address)
                                @Html.ValidationMessageFor(m => m.Address)
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-1-5">
                                @Html.LabelFor(m => m.FolioNo)
                            </div>
                            <div class="col-md-2-5">
                                @Html.TextBoxFor(m => m.FolioNo, new { @readonly = true })
                                @Html.ValidationMessageFor(m => m.FolioNo)
                            </div>
                            <div class="col-md-offset-4 col-md-1-5">
                                @Html.LabelFor(m => m.OpBalance)
                            </div>
                            <div class="col-md-2-5">
                                @Html.TextBoxFor(m => m.OpBalance, new { @readonly = true })
                                @Html.ValidationMessageFor(m => m.OpBalance)
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <table class="table table-bordered table-striped dataTable">
                                <thead>
                                    <tr>
                                        <th>Mode</th>
                                        <th>Drawee Bank</th>
                                        <th>Instrument No.</th>
                                        <th>Date</th>
                                        <th>Amount</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (var i = 0; i < 5; i++)
                                    {
                                        <tr>
                                            <td>@Html.DropDownListFor(m => m.Details[i].Type, (SelectList)ViewBag.Type)</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].Bank)</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].InstrumentNo)</td>
                                            <td class="Datetab Datetab_Img">@Html.TextBoxFor(m => Model.Details[i].Date, new { @placeholder = "dd/mm/yyyy" })</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].Amount, new { @onblur = "Calculate()" })</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-offset-8 col-md-1-5">
                                <label>Total:</label>
                            </div>
                            <div class="col-md-2-5">
                                <input type="text" id="txtTotal" value="0" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-offset-8 col-md-1-5">
                                <label>Closing:</label>
                            </div>
                            <div class="col-md-2-5">
                                <input type="text" id="txtClosing" value="" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
                    </div>
                    <div class="row">
                        <div class="col-md-offset-3 col-md-2 SignUp_space_bottom">
                            <input type="submit" class="btn log_Btn_sm" value="Save" id="BtnSaveCustomAppraisement" />
                        </div>
                        <div class="col-md-2">
                            <input type="button" class="btn log_Btn_sm" value="Print" id="btnPrint" onclick="Print()" />
                        </div>
                        <div class="col-md-2">
                            <input type="button" class="btn log_Btn_sm" value="Reset" id="btnReset" onclick="Reset()" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

<div class="modal fade" id="InvoiceNoModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" onclick="CloseParty()"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of Party</h4>
            </div>
            <div class="modal-body popup_body" style="position:relative;">
                <input placeholder="Search By Party Name Or Party Code" id="Partybox" type="text" />
                <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchPartyByCode()"></i></span>
                <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                    <thead>
                        <tr>
                            <th colspan="8" width="80%">Party Name</th>
                            <th colspan="4" width="20%">Party Code</th>
                        </tr>
                    </thead>
                </table>
                <div id="slim_scroll">
                    <table class="table dataTable table-bordered table-striped slim_tble" id="tbleParty">
                        <tbody id="tblPartyDetails"></tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-6">

                        <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreParty()" class="btn log_Btn_sm" id="btnLoadMore">Load More Data</button>

                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn log_Btn_sm" onclick="CloseParty()">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
