    function ReCalculateInvoice() {
        debugger;
        var rawJson = JSON.parse($('#PaymentSheetModelJson').val());
        var IP = 0;
        var CP = 0;
        var SP = 0;
        $.each(rawJson.lstPostPaymentChrg, function (i, item) {
            if (rawJson.InvoiceType == "Tax") {
                if (rawJson.PartyStateCode == rawJson.CompStateCode) {
                    CP = Number(item.CGSTPer);
                    SP = Number(item.SGSTPer);
                }
                else {
                    IP = Number(item.IGSTPer);
                }
            }
            item.IGSTAmt = Number((item.Amount * (IP / 100)).toFixed(2));
            item.CGSTAmt = Number((item.Amount * (CP / 100)).toFixed(2));
            item.SGSTAmt = Number((item.Amount * (SP / 100)).toFixed(2));
            item.Total = Number((item.Amount + item.IGSTAmt + item.CGSTAmt + item.SGSTAmt).toFixed(2));
        });
        $.each(rawJson.lstPostPaymentChrg, function (i, item) {
            $('#' + item.ChargeId).val(item.Amount);
            $('#IGSTP' + item.ChargeId).val(item.IGSTPer);
            $('#IGSTA' + item.ChargeId).val(item.IGSTAmt);
            $('#CGSTP' + item.ChargeId).val(item.CGSTPer);
            $('#CGSTA' + item.ChargeId).val(item.CGSTAmt);
            $('#SGSTP' + item.ChargeId).val(item.SGSTPer);
            $('#SGSTA' + item.ChargeId).val(item.SGSTAmt);
            $('#TOT' + item.ChargeId).val(item.Total);
        });
    }
