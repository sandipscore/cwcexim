@model CwcExim.Areas.CashManagement.Models.HDB_AddMoneyToPDModelRefund
<script>


    var TimeInSeconds = 5000;
    function OnSuccess(data) {
        var a = data.Message;
        var b = data.Data;
        var c;
        if (b != null && b != "" && b != undefined) {
            c = a + ". " + "Receipt No.: " + b;
        }
        else {

            c = a;
        }

        alert(c);
        if (data.Status != null && data.Data != "" && data.Data != undefined) {
            $('#JournalNo').val(data.Data);
            $('#printButton').prop('disabled', false);
            $('#BtnSaveCustomAppraisement').prop('disabled', true);
        }
        else {
            $('#printButton').prop('disabled', true);
            $('#BtnSaveCustomAppraisement').prop('disabled', false);
        }
        //  setTimeout(RefreshPage, TimeInSeconds);
    }
    function OnFailure() {
        alert("Something went wrong , Please check data");
        $('#printButton').prop('disabled', true);

    }
    //$(document).ready(function () {
    //    $('#printButton').prop('disabled', true);

    //});

   
    for (var i = 0; i < 7; i++) {
        var date = new Date();
        var currentMonth = date.getMonth();
        var currentDate = date.getDate();
        var currentYear = date.getFullYear();
        $('#Details_' + i + '__Date').datepicker({
            dateFormat: "dd/mm/yy",
            showOn: "button",
            buttonImage: "data:image/png;base64,",
            buttonImageOnly: true,
            buttonText: "Select date",
            changeMonth: true,
            changeYear: true,
            // maxDate: new Date(currentYear, currentMonth, currentDate),
            // minDate: 0,
            showAnima: "fadein",
            onClose: function () {
            }
        });
    }


    function RefreshPage() {
        $('#DivBody').load('/CashManagement/HDB_CashManagement/RefundFromPDA');
    }


    GenerateParties();
    function GenerateParties() {
        var parties = JSON.parse($('#Parties').val());
        debugger;
        var html = '';
        $.each(parties, function (i, item) {
            html += '<li id="' + item.Id + '" onclick="GetPartyDetails(this)">' + item.Name + '</li>';
        });
        $('#lstCHA').html(html);
    }
    function GetPartyDetails(elem) {
        debugger;

        $('#PartyName').val('');
        $('#PartyId').val('');
        $('#OpBalance').val('');
        $('#RefundAmount').val('');
        $('#closingBalance').val('');
        $('#Bank').val('');
        $('#Branch').val('');
        $('#ChequeNo').val('');
        $('#ChequeDate').val('');

        $('#PartyName').val($(elem).text());
        $('#PartyId').val($(elem).attr('id'));
        var parties = JSON.parse($('#Parties').val());
        var party = $.grep(parties, function (item) { return item.Id == $(elem).attr('id'); })[0];
        //$('#FolioNo').val(party.Folio);
        //$('#Address').val(party.Address);
        $('#OpBalance').val(party.Balance);
        $('#UnPaidAmount').val(party.UnPaidAmount);
        $('#PName').val(party.PName);
        $('#FolioNo').val(party.Folio);
        $('#PartyAddress').val(party.PartyAddress);
        $("#PartyList").modal("hide");
    }

    function Calculate() {
        var amt = 0;
        for (var i = 0; i < 5; i++) {
            amt += Number($('#Details_' + i + '__Amount').val());
        }
        $('#txtTotal').val(amt);
        $('#txtClosing').val(Number($('#OpBalance').val()) + amt);
    }
    var date = new Date();
    var curDate = date.getDate();
    function Print() {

        $.ajax({
            type: 'POST',
            url: '@Url.Action("GetSDRefundReceipt", "Hdb_ReportCWC", new { area = "Report" })',
            dataType: 'JSON',
            type: 'POST',
            data: {
                JournalNo: $('#JournalNo').val(),
               
            },
            success: function (data) {
                if (data.Status == 1)
                    window.open(data.Message + "?_t=" + (new Date().getTime()), "_blank");
                else
                    alert(data.Message);
            }
        });
    }

    function RemoveMessage() {
        alert("in");
        $('[data-valmsg-for="RefundAmount"]').html('Fill Out This Field');
    }
    $(window).keypress(function (event) {
        if (event.which == 13) {
            $(this).next().focus();
        }
    });

    function checkAmount() {
        var RefundAmount = Number($("#RefundAmount").val());
        debugger;
        if (RefundAmount == "" || RefundAmount == null) {

            $('[data-valmsg-for="RefundAmount"]').html('<span>Fill Out This Field</span>');
            //setTimeout(LoadGateEntryView, TimeInSeconds);
            return false;

        }
        if (RefundAmount!=Number($("#txtTotal").val()))
        {
            alert('Refund Amount Should be Same As Total Amount ');
            return false;
        }
        if ( $('#JournalDate').val()=='') {
            alert('Please Select JV Voucher SD Date ');
            return false;
        }
        if ($('#RoSanctionOrderDate').val()=='') {
            alert('Please Select Ro Sanction Order Date ');
            return false;
        }
        var opening = Number($("#OpBalance").val());
        var closing = Number($("#OpBalance").val()) - Number(RefundAmount);
        var UnPaid = Number($("#UnPaidAmount").val());
        $("#closingBalance").val(closing);
        if (RefundAmount > opening || RefundAmount < 0) {
            alert("Refund Amount Can Not Be Greater Than  Balance ");
            $("#BtnSaveCustomAppraisement").attr("disabled", true);
            $("#printButton").attr("disabled", true);
            return false;
        }
        else if (UnPaid > 0) {
            alert("SD Refund Can Not Be Possible due to UnPaid Bill");
            $("#BtnSaveCustomAppraisement").attr("disabled", true);
            $("#printButton").attr("disabled", true);
            return false;
        }


        else {
            $("#BtnSaveCustomAppraisement").attr("disabled", false);
            // $("#printButton").attr("disabled", false);
        }
    }
    $("#RefundAmount").blur(function () {
        if ($("#OpBalance").val() == null || $("#OpBalance").val() == "" || $("#OpBalance").val() == undefined || Number($("#OpBalance").val()) <= 0) {
           // alert("Opening Balance Should be >0");
            // $("#BtnSaveCustomAppraisement").attr("disabled", true);
            //  $("#printButton").attr("disabled", true);
        }
        else if ($("#RefundAmount").val() == null || $("#RefundAmount").val() == "" || $("#RefundAmount").val() == undefined || Number($("#RefundAmount").val()) <= 0) {
            alert("Refund Amount Should Be Greater Than 0");
            $("#BtnSaveCustomAppraisement").attr("disabled", true);
            $("#printButton").attr("disabled", true);

        }
        else {
            $("#BtnSaveCustomAppraisement").attr("disabled", false);
            //  $("#printButton").attr("disabled", false);
            var closing = Number($("#OpBalance").val()) - Number($("#RefundAmount").val());
            $("#closingBalance").val(closing);
        }
    });

    $('#CHAbox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstCHA > li').show();
        else {
            $('#lstCHA > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
</script>
<input type="hidden" id="Parties" value="@ViewBag.Parties.ToString()" />

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            @using (Ajax.BeginForm("SaveRefundFromPDA", "HDB_CashManagement", new AjaxOptions { HttpMethod = "POST", Confirm = "Are you sure you want to Save?", OnFailure = "OnFailure", OnSuccess = "OnSuccess", LoadingElementDuration = 1000, LoadingElementId = "divProgress" }))
            {
                <div class="row">
                    <div class="col-md-12">
                        <h4>Refund From SD</h4>
                    </div>
                </div>
                <div class="content_wrp">
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.PartyId, new { Value = "0000000000" })


                    @Html.HiddenFor(m => m.UnPaidAmount)
                    @Html.HiddenFor(m => m.PName)
                    @Html.HiddenFor(m => m.PartyAddress)
                    <input type="hidden" id="currentDate" name="currentDate" value="@ViewBag.currentDate" />
                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Journal No.:</label>
                            </div>
                            @Html.TextBoxFor(m => m.JournalNo, new { @readonly = true })

                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Journal Date.:</label>
                            </div>
                            @Html.TextBoxFor(m => m.JournalDate, new { @readonly = true })
                            @Html.ValidationMessageFor(m => m.JournalDate)
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <span class="ReQSimbol">*</span>
                                @Html.LabelFor(m => m.PartyName)
                            </div>
                            <div class="position-relative">
                                @Html.TextBoxFor(m => m.PartyName, new { @placeholder = "Party Name",@readonly = true })
                                @Html.ValidationMessageFor(m => m.PartyName)
                                <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PartyList"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Amount:</label>
                            </div>
                            @Html.TextBoxFor(m => m.OpBalance, new { @readonly = true })
                            @Html.ValidationMessageFor(m => m.OpBalance)
                        </div>
                        <div class="col-md-3 col-md-offset-1">
                            <div class="form-group">
                                <span class="ReQSimbol">*</span>
                                <label>Refund Amount:</label>
                            </div>
                            @Html.TextBoxFor(m => m.RefundAmount, new { @placeholder = "Refund Amount", onkeypress = "return AllowOnlyDecimal(event)" })
                            @Html.ValidationMessageFor(m => m.RefundAmount)
                        </div>
                        <div class="col-md-3 col-md-offset-1">
                            <div class="form-group">
                                <label>Closing Balance:</label>
                            </div>
                            @Html.TextBoxFor(m => m.closingBalance, new { @readonly = true })
                            @Html.ValidationMessageFor(m => m.closingBalance)
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Folio No.:</label>
                            </div>
                            @Html.TextBoxFor(m => m.FolioNo, new { @placeholder = "Folio No"})

                        </div>
                        <div class="col-md-3 col-md-offset-1">
                            <div class="form-group">
                                <label>Ro Sanction Order No.:</label>
                            </div>
                            @Html.TextBoxFor(m => m.RoSanctionOrderNo, new { @placeholder = "Ro Sanction Order No."})

                        </div>
                        <div class="col-md-3 col-md-offset-1">
                            <div class="form-group">
                                <label>Ro Sanction Date.:</label>
                            </div>
                            <div class="position-relative Date_Img">
                                @Html.TextBoxFor(m => m.RoSanctionOrderDate, new { @placeholder = "dd/mm/yyyy"})
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Remarks:</label>
                            </div>
                            @Html.TextAreaFor(m => m.Remarks, new { @placeholder = "Enter Remarks"})
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <table class="table table-bordered table-striped dataTable">
                                <thead>
                                    <tr>
                                        <th>Mode</th>
                                        <th>Drawee Bank</th>
                                        <th>Instrument No.</th>
                                        <th>Date</th>
                                        <th>Amount</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @for (var i = 0; i < 5; i++)
                                    {
                                        <tr>
                                            <td>@Html.DropDownListFor(m => m.Details[i].Type, (SelectList)ViewBag.Type)</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].Bank)</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].InstrumentNo)</td>
                                            <td class="Datetab Datetab_Img">@Html.TextBoxFor(m => Model.Details[i].Date, new { @placeholder = "dd/mm/yyyy" })</td>
                                            <td>@Html.TextBoxFor(m => Model.Details[i].Amount, new { @onblur = "Calculate()" })</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="col-md-3 col-md-offset-9">
                            <div class="form-group">
                                <label>Total:</label>
                            </div>
                            <input type="text" id="txtTotal" value="0" readonly />
                        </div>
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
                    </div>
                    <div class="SignUp_space_bottom">
                        <div class="d-flex justify-content-left">
                            <input type="submit" class="btn btn-primary mr-1 btn-100px" value="Save" id="BtnSaveCustomAppraisement" onclick="return checkAmount()" />
                            <input type="button" class="btn btn-primary mr-1 btn-100px" id="printButton" disabled value="Print" onclick="Print()" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2 SignUp_space_bottom">
                            <input type="button" class="btn btn-primary mr-1 btn-100px" value="Show List" id="BtnShowList" onclick="return LoadSDRefundList()" />
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="DivSDRefundList"></div>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</div>


<div class="modal fade" id="PartyList" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">List of Party Names</h4>
            </div>
            <div class="modal-body popup_body">
                <input placeholder="Search Me" id="CHAbox" type="text" />
                <ul class="navList" id="lstCHA"></ul>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-offset-6 col-md-6">
                        <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
