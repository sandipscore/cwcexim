                                                <th class="text-center" style="width:5%;">Amount</th>
                                                <th class="text-center" style="width:5%;">%</th>
                                                <th class="text-center" style="width:5%;">Amount</th>
                                                <th class="text-center" style="width:5%;">%</th>
                                                <th class="text-center" style="width:5%;">Amount</th>
