@model CwcExim.Areas.CashManagement.Models.DSR_CashColAgnBncChq
@if (ViewBag.ImpExpCha != null)
{
    <input type="hidden" id="hdnImpExpCha" value="@ViewBag.ImpExpCha" />
}
else
{
    <input type="hidden" id="hdnImpExpCha" value="" />
}
<script>
    $('#BncChequeDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#BncChequeBounceDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#ChequeDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#ChqDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#ChequeDepositDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "data:image/png;base64,",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#CHAbox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstCHA > li').show();
        else {
            $('#lstCHA > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    $('#lstCHA > li').on("click", function () {
        $('#ImpExpChaName').val($(this).text());
        $('#ImpExpChaId').val($(this).attr('id'));
        $("#CHAModal").modal("hide");
    });
    $('#BncChequeAmt').on("blur", function () {
        //debugger;
        if($('#BncChequeAmt').val()<=0)
        {
            $('#DivSucMsg').html("Negetive value can not be added");
            return false;
        }
        else
        {
            $('#DivSucMsg').html("");
        }
        $('#ChequeAmt').val($('#BncChequeAmt').val());
        var BncCharge=@ViewBag.ImpExpCha.bouncecheck.BouncedCheque;

        $('#OtherCharges').val(BncCharge);
        //if (parseFloat($('#BncChequeAmt').val()) > 25000) {
        //    $('#OtherCharges').val('500');
        //}
        //else {
        //    $('#OtherCharges').val('300');
        //}
    });
    $('#ChequeAmt').on("blur", function () {
        if($('#ChequeAmt').val()<=0)
        {
            $('#DivSucMsg').html("Negetive value can not be added");
            return false;
        }
        else
        {
            $('#DivSucMsg').html("");
        }

    });

    function OnBegin() {
        if ($('#CashColAgnBncChqId').val() != "0") {
            alert('Please click on reset.');
            return false;
        }
        else {
            return true;
        }
    }
    function OnSuccess(data) {
        debugger;
        if (data.Status == 1) {

            debugger;
            $('#CashColAgnBncChqId').val('1');
            $('#PaymentSheetModelJson').val(JSON.stringify(data.Data));
            var rno = data.Data.lstCashColAgnBncChqHeader[0].ReceiptNo;
            $('#ReceiptNo').val(rno);
            $('#DivSucMsg').html("Data Saved Successfully");

            $('#btnSave').attr('disabled', 'disabled');
            $('#PrnInvbtn').removeAttr('disabled');
            $('#PrnRecbtn').removeAttr('disabled');
            $('#btnGeneratedIRN').removeAttr('disabled');
            
           // alert(data.Message);
        }
        else {
            alert(data.Message);
        }
    }
    function OnFailure() {
        alert("Error");
    }
    function Reset() {
        $('#PrnInvbtn').attr('disabled', 'disabled');
        $('#PrnRecbtn').attr('disabled', 'disabled');
        $('#btnSave').removeAttr('disabled');

        $('#DivBody').load('/CashManagement/DSR_CashManagement/CashColAgnBncCheque');
    }

    function ShowList() {
        GetBncChequeList();
    }

    function GetBncChequeList() {
        debugger;
        $('#DivBncChequeList').load('/CashManagement/DSR_CashManagement/GetBounceChequeInvoiceList');
    }

</script>
