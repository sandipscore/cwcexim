                            len,
                            [item.InvoiceDate],
                            [item.PartyName],
                            [item.amount],
                            [item.GSTNo],
                            [item.cgst],
                            [item.sgst],
                            [item.igst],
                            [item.round_up],
                            [item.total],
                            [item.InvoiceNo],

                            ['<a href="#" onclick=PrintRentInvoice("' + [item.InvoiceNo] + '")><i class="fa fa-print"></i></a>'],
                            ]);
                        });
                        if (data.State == "0") {
                            $('#btnLoadMore').attr('disabled', true);
                        }
                        else {
                            $('#btnLoadMore').attr('disabled', false);
                        }
                    }
                    else alert('No more data available');
                }
            });
