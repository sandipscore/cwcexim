@model CwcExim.Areas.CashManagement.Models.PPGInterBalanceTransfer


<div class="container-fluid">
    <div class="col-md-11 col-md-offset-0-5">
        <div class="Head_h4">
          
                <div class="row">
                    <div class="col-md-12">
                        <h4>Inter Balance Transfer From SD Party to another SD Party</h4>
                    </div>
                </div>
             
                <div class="row Form_Space_top">
                    <div class="Form_Input form-group">
                        <div class="col-md-2">

                            <label>From SD Party Name:</label>

                        </div>
                        <div class="col-md-6">
                            <span class="ReQSimbol">*</span>
                            @Html.TextBoxFor(m => m.FromPartyName, new { @readonly = true })
                          
                        </div>
                    </div>
                </div>

                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-2">

                                <label>To SD Party Name:</label>

                            </div>
                            <div class="col-md-6">
                                <span class="ReQSimbol">*</span>
                                @Html.TextBoxFor(m => m.ToPartyName, new { @readonly = true })
                              
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="Form_Input form-group">
                            <div class="col-md-2">
                                <label>Transfer Amount:</label>
                            </div>
                            <div class="col-md-2">
                                <span class="ReQSimbol">*</span>
                                @Html.TextBoxFor(m => m.TransferBalance, new { @readonly = true })
                               
                            </div>

                            <div class="col-md-2 padding_rt">
                                <label>Transfer Date:</label>
                            </div>
                            <div class="col-md-2 Date_Img">
                                @Html.TextBoxFor(m => m.TransferDate, new { @readonly = true })
                                
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
                    </div>
                    <div class="row">
                        <div class="col-md-offset-4 col-md-2 SignUp_space_bottom">
                            <input type="button" class="btn log_Btn_sm" value="Back" id="BtnSDRefundBack" onclick="RefreshPage()" />
                        </div>

                    </div>

          
            
        </div>
    </div>
</div>




@Scripts.Render("~/bundles/jqueryval")

<script>




    function RefreshPage() {
        $('#DivBody').load('/CashManagement/Ppg_CashManagement/InterBalanceTransferSD');
    }



</script>
