            <script>

                var TimeInSeconds;
                $(document).ready(function () {
                    TimeInSeconds = 5000;
                    $('#InvoiceDate').datepicker({
                        dateFormat: "dd/mm/yy",
                        showOn: "button",
                        buttonImage: "/Content/images/calendar.png",
                        buttonImageOnly: true,
                        buttonText: "Select date",
                        changeMonth: true,
                        changeYear: true,
                        showAnima: "fadein",
                        onClose: function () {
                        }
                    });
                  
                });
              
                function AllowOnlyDecimal(evt) {
                    var CharCode;
                    if (window.event) {
                        CharCode = window.event.keyCode;
                    }
                    else {
                        CharCode = evt.which;
                    }
                    if ((CharCode > 47 & CharCode < 58) || (CharCode == 46) || CharCode == 8) {
                        return true;
                    }
                    else {
                        evt.preventDefault;
                        return false;
                    }
                }


                //calculate total amount
                function caltotal() {
                    debugger;

                    if ($('#SEZ').val() == 'SEZWP')
                    {
                        var amt = $('#amount').val();
                        var taxvalue = Math.ceil((amt * 18) / 100);
                        var cgmt = 0;
                        var sgst = 0;
                        var totamt = parseFloat(amt) + parseFloat(cgmt) + parseFloat(sgst);
                        var round = Math.ceil(totamt) - totamt;
                        var rounduptot = Math.ceil(totamt);
                    }
                    else if ($('#SEZ').val() == 'SEZWOP')
                    {
                        var amt = $('#amount').val();
                        var taxvalue = Math.ceil((amt * 18) / 100);
                        var cgmt = 0;
                        var sgst = 0;
                        var totamt = parseFloat(amt) + parseFloat(cgmt) + parseFloat(sgst);
                        var round = Math.ceil(totamt) - totamt;
                        var rounduptot = Math.ceil(totamt);
                    }
                    else
                    {
                        var amt = $('#amount').val();
                        var taxvalue = Math.ceil((amt * 18) / 100);
                        var cgmt = taxvalue / 2;
                        var sgst = taxvalue / 2;
                        var totamt = parseFloat(amt) + parseFloat(cgmt) + parseFloat(sgst);
                        var round = Math.ceil(totamt) - totamt;
                        var rounduptot = Math.ceil(totamt);
                    }

                    $('#round_up').val(round.toFixed(2));
                    $('#total').val(rounduptot);
                    $('#cgst').val(cgmt);
                    $('#sgst').val(sgst);
                    $("#cgst").attr("disabled", "disabled");
                    $("#sgst").attr("disabled", "disabled");
                    $('#igst').val('0.00');
                    $('#SEZ').prop('disabled',true)
                }

                //calculate tax

                function caltax() {
                    var cgst = $('#cgst').val();
                    var sgst = $('#sgst').val();
                    var igst = $('#igst').val();
                    var amt = $('#amount').val();
                    if (amt != '') {
                        var totamt = 0;
                        if (cgst == '')
                            cgst = 0;


                        if (sgst == '')
                            sgst = 0;

                        if (igst == '')
                            igst = 0;


                        totamt = parseFloat(amt) + parseFloat(cgst) + parseFloat(sgst) + parseFloat(igst);

                        var round = Math.ceil(totamt) - totamt;
                        var rounduptot = Math.ceil(totamt);

                        $('#round_up').val(round.toFixed(2));
                        $('#total').val(rounduptot);

                        if (cgst > 0 || sgst > 0) {
                            $('#igst').prop('readonly', true);
                        }

                        if (igst > 0) {
                            cgst = 0;
                            sgst = 0;
                            $('#cgst').prop('readonly', true);
                            $('#sgst').prop('readonly', true);
                        }

                    }
                }

                function ResetField() {
                    debugger;
                    $('#DivBody').load('/CashManagement/Ppg_CashManagement/CreateRentInvoice');
                }



                function PrintInvoice(i) {
                    debugger;
                    var invoiceno = $('#hdnRentDet').val();
                    var inv = invoiceno.split(",");

                    if (i < inv.length) {
                        invv = inv[i].split('-')[0];
                        PrintInvoiceNext(invv,i);
                    }
                }
                    //if (invoiceno == '')
                    //{
                    //    alert('No Invoice No');
                    //    return;
                    //}
                function PrintInvoiceNext(invno,j)
                {
                    debugger;
                    var k1 = j;
                    var Token = $('input[name="__RequestVerificationToken"]').val();
                    $.ajax({
                        /*url: 'Url.Action("YardInvoicePrint", "Ppg_CWCImport", new { Area = "Import" }) ',*/
                        url: '@Url.Action("GetBulkInvoiceReport", "Ppg_ReportCWC", new { Area = "Report" }) ',
                        headers: { "__RequestVerificationToken": Token },
                        dataType: 'JSON',
                        type: 'POST',
                        data: {
                            InvoiceNumber: invno,
                            InvoiceModule: 'Rent',
                            PeriodFrom: '@DateTime.Now.ToString("dd/MM/yyyy")',
                            PeriodTo: '@DateTime.Now.ToString("dd/MM/yyyy")',
                            InvoiceModuleName: 'Rent',
                        },
                        success: function (data) {
                            if (data.Status == 1) {
                                
