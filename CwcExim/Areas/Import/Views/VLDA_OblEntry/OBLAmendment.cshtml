@model CwcExim.Areas.Import.Models.WFLDOBLAmendment
<script src="~/Areas/Import/Scripts/App/Services/VLDAOBLAmendmentService.js"></script>
<script src="~/Areas/Import/Scripts/App/Controllers/VLDAOBLAmendmentCtrl.js"></script>
<script>
    angular.bootstrap(document.getElementById('OBLAmendmentAppDiv'), ['CWCApp']);
</script>
<style>
    #DivFormOneDet {
        height: 270px;
        overflow-x: hidden;
    }

       .slimScrollDiv { height:300px; }
    .table.tblOBL{
        border-bottom:0;
    }
    .slim_tble{
        width:100%;
        table-layout: fixed;
    }
    .txt_wrp{
         overflow:hidden; 
         word-wrap:break-word;
    }
     #tblOBL.dataTable tbody > tr > td {
        cursor: pointer;
    }

</style>
<div class="container-fluid" id="OBLAmendmentAppDiv" ng-controller="VLDAOBLAmendmentCtrl">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>OBL/HBL Amendment(OBL/HBL Correction Module)</h4>
                    </div>
                </div>
              
                @{
                @Html.AntiForgeryToken()
                    <div class="row">
                        <div class="SignUp_space_top"></div>
                    </div>
                    <div class="Head_h4">
                        <div class="row">
                            <div class="col-md-12">
                                <h4>Old OBL/HBL Display</h4>
                            </div>
                        </div>
                        <div id="DivOldOBL" class="Form_Space_bottom">
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>OBL/HBL No:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldOBLNo, new { @class = "form-control input_sm", @onkeydown = "PopupActivate('OBLModal');",@tabindex=1 })
                                        <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#OBLModal"></i></span>

                                    </div>
                                    <div class="col-md-offset-0-5 col-md-2-5">
                                        <input type="button" id="btnShowDetails" class="btn log_Btn_sm" ng-model="btnGet" ng-hide="Action" value="Show Details" ng-click="GetOBLDetails()" />
                                    </div>
                                    <div class="col-md-offset-1 col-md-1-5 col_cus_2 padding_rt">
                                        <label>OBL/HBL Date:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OBLDate, new { @class = "form-control input_sm", @readonly = true,@tabindex = 2 })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>Container / CBT No:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.ContainerNo, new { @class = "form-control input_sm", @readonly = true, @tabindex = 3 })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>CFS Code:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.CFSCode, new { @class = "form-control input_sm", @readonly = true, @tabindex = 4 })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>IGM No:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.IGMNo, new { @class = "form-control input_sm", @readonly = true, @tabindex = 5 })
                                        @Html.ValidationMessageFor(m => m.IGMNo, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>IGM Date:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.IGMDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 6 })
                                        @Html.ValidationMessageFor(m => m.IGMDate, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>No Of Pkg.:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldNoOfPkg, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 7 })
                                        @Html.ValidationMessageFor(m => m.OldNoOfPkg, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>Gross Wt(Kg).:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldGRWT, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 8 })
                                        @Html.ValidationMessageFor(m => m.OldGRWT, "", new { @class = "Error_Msg" })
                                    </div>

                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Cargo Type:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                       @Html.DropDownListFor(m => m.OldCargoType, new List<SelectListItem>
                                   {
                                       new SelectListItem {Text="HAZ",Value="1" },
                                       new SelectListItem {Text="Non-HAZ",Value="2" },
                                        new SelectListItem {Text="ODC",Value="3" }
                                   }, "----Select----", new { @class = "form-control input_sm dropdown", tabindex = 9 })
                                    @Html.ValidationMessageFor(m => m.OldCargoType, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>TSA No.:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldTSA, new { @class = "form-control input_sm", @readonly = "true" , @tabindex = 10})
                                        @Html.ValidationMessageFor(m => m.OldTSA, "", new { @class = "Error_Msg" })
                                    </div>

                                </div>
                            </div>


                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Line No.:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldLine, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 11 })
                                        @Html.ValidationMessageFor(m => m.OldLine, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>CBM:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.OldCBM, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 12 })
                                        @Html.ValidationMessageFor(m => m.OldCBM, "", new { @class = "Error_Msg" })
                                    </div>

                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label> Importer Name :</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.ODLImporterName, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 13 })
                                       @Html.HiddenFor(m=>m.ODLImporterID)
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>Shipping Line:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.ODLShippinglineName, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 14 })
                                        @Html.HiddenFor(m => m.ODLShippingLineId)
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Cargo Desc:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.ODLCargoDesc, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 15 })
                                      
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                     
                                    </div>
                                    <div class="col-md-2-5">
                                     
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="SignUp_space_top"></div>
                    </div>

                    <div class="Head_h4">
                        <div class="row">
                            <div class="col-md-12">
                                <h4>New OBL/HBL No.</h4>
                            </div>
                        </div>
                        <div id="DivNewOBL" class="Form_Space_bottom">
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>New OBL/HBL No:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewOBLNo, new { @class = "form-control input_sm", @tabindex = 16 })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>No Of Pkg.:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewNoOfPkg, new { @class = "form-control input_sm", @tabindex = 17 })
                                        @Html.ValidationMessageFor(m => m.NewNoOfPkg, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>Gross Wt(Kg).:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewGRWT, new { @class = "form-control input_sm", @tabindex = 18})
                                        @Html.ValidationMessageFor(m => m.NewGRWT, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>Cargo Type:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.DropDownListFor(m => m.NewCargoType, new List<SelectListItem>
                                   {
                                       new SelectListItem {Text="HAZ",Value="1" },
                                       new SelectListItem {Text="Non-HAZ",Value="2" },
                                        new SelectListItem {Text="ODC",Value="3" }
                                   }, "----Select----", new { @class = "form-control input_sm dropdown", tabindex = 19 })
                                        @Html.ValidationMessageFor(m => m.NewCargoType, "", new { @class = "Error_Msg" })
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>TSA No.:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewTSA, new { @class = "form-control input_sm", tabindex = 20 })
                                        @Html.ValidationMessageFor(m => m.NewTSA, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>Line No.:</label>
                                    </div>
                                   
                                        <div class="col-md-2-5">
                                            @Html.TextBoxFor(m => m.NewLine, new { @class = "form-control input_sm", tabindex =21 })
                                            @Html.ValidationMessageFor(m => m.NewLine, "", new { @class = "Error_Msg" })
                                        </div>
                                   
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>CBM:</label>
                                    </div>

                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewCBM, new { @class = "form-control input_sm", tabindex = 22 })
                                        @Html.ValidationMessageFor(m => m.NewCBM, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label> Importer Name :</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewImporterName, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 23, @onkeydown = "PopupActivate('ImporterModal');" })
                                        <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ImporterModal"></i></span>
                                        @Html.HiddenFor(m => m.NewImporterID)
                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                        <label>Shipping Line:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewShippinglineName, new { @class = "form-control input_sm", @readonly = "true", @tabindex = 24, @onkeydown = "PopupActivate('ShippingLineModal');" })
                                        <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                        @Html.HiddenFor(m => m.NewShippingLineId)
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Cargo Desc:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.NewCargoDesc, new { @class = "form-control input_sm", @tabindex = 25})

                                    </div>
                                    <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">

                                    </div>
                                    <div class="col-md-2-5">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                  
                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                            <div class="logErrMsg" id="DivTblContrErrMsg" style="background-color:transparent"></div>
                        </div>
                        <div class="row">
                            <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                        </div>
                        <div class="row SignUp_space_top2">
                            <div class="col-md-offset-4 col-md-2">
                                <input type="submit" id="btnSave" class="btn log_Btn_sm" value="Save" ng-click="OnOBLEntrySave()" tabindex = 26/>
                            </div>
                            <div class="col-md-2">
                                <input type="button" id="btnReset" class="btn log_Btn_sm" value="Reset" ng-click="ResetJODet()" tabindex = 27 />
                            </div>
                        </div>
                        <div class="logSuccMsg Form_Space_top" id="DivSucMsg" style="background-color:transparent"></div>
                        <div class="logErrMsg Form_Space_bottom" id="DivErrMsg" style="background-color:transparent"></div>
                        }
                        <div class="row">
                            <div class="col-md-12">
                                <div id="OblEntryList"></div>
                            </div>
                        </div>
                    </div>

        </div>
    </div>

    <div class="modal fade" id="OBLModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content popup_modal" style="width:50%;">
                <div class="modal-header popup_header">
                    <button type="button" class="close" onclick="CloseOBL()"><i class="fa fa-times" aria-hidden="true"></i></button>
                    <h4 class="modal-title">List of OBL/HBL No</h4>
                </div>
                <div class="modal-body popup_body" style="position:relative;">
                    <input placeholder="Search By OBL/HBL No" id="OBLBox" type="text" tabindex="100" />
                    <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchOBL()"></i></span>
                    <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                        <thead>
                            <tr>
                                <th>OBL/HBL No</th>
                            </tr>
                        </thead>
                    </table>
                    <div id="slim_scroll">
                        <table tabindex="101" class="table dataTable table-bordered table-striped" id="tblOBL">
                            @{
                                if (ViewBag.LstObl != null)
                                {
                                    var tabvalue = 102;
                                    foreach (var item in ViewBag.LstObl)
                                    {
                                        <tr tabindex="@tabvalue" onclick="SelectOBL('@item.OBLNo')" onkeypress="SelectOBL('@item.OBLNo')">
                                            <td>@item.OBLNo</td>
                                        </tr>
                                        tabvalue++;
                                    }
                                }
                            }
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-md-5">
                            @if (ViewBag.State != null && Convert.ToBoolean(ViewBag.State) == true)
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreOBL()" class="btn log_Btn_sm" id="btnLoadMoreOBL">Load More</button>
                            }
                            else
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreOBL()" class="btn log_Btn_sm" id="btnLoadMoreOBL" disabled>Load More</button>
                            }
                        </div>
                        <div class="col-md-offset-3 col-md-4">
                            <button type="button" class="btn log_Btn_sm" onclick="CloseOBL()">Close</button>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="modal fade" id="ShippingLineModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content popup_modal">
                <div class="modal-header popup_header">
                    <button type="button" class="close" onclick="CloseEximTrader()"><i class="fa fa-times" aria-hidden="true"></i></button>
                    <h4 class="modal-title">List of Shipping Line Names</h4>
                </div>
                <div class="modal-body popup_body">
                    <input placeholder="Search By Shipping Line Name Or Party Code" id="ShpngLinebox" type="text" tabindex="300" />
                    <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchByPartyCode()"></i></span>

                    <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                        <thead>
                            <tr>
                                <th colspan="8" width="80%">Shipping Line Name</th>
                                <th colspan="4" width="20%">Party Code</th>
                            </tr>
                        </thead>
                    </table>
                    <div id="slim_scroll_2">
                        <table tabindex="301" class="table dataTable table-bordered table-striped slim_tble" id="tbleSline">
                            @{
                                if (ViewBag.lstShippingLine != null)
                                {
                                    var tabvalue = 302;
                                    foreach (var item in ViewBag.lstShippingLine)
                                    {
                                        <tr tabindex="@tabvalue" onclick="SelectShippingLine(@item.ShippingLineId,'@item.ShippingLineName')" onkeypress="SelectShippingLine(@item.ShippingLineId,'@item.ShippingLineName')">
                                            <td colspan="8" width="80%" class="txt_wrp">@item.ShippingLineName</td>
                                            <td colspan="4" width="20%" class="txt_wrp">@item.PartyCode</td>
                                        </tr>
                                        tabvalue++;
                                    }
                                }
                            }
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-md-6">
                            @if (ViewBag.ShippingLineState != null && Convert.ToBoolean(ViewBag.ShippingLineState) == true)
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreEximTrader()" class="btn log_Btn_sm" id="btnLoadMore">Load More Data</button>
                            }
                            else
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreEximTrader()" class="btn log_Btn_sm" id="btnLoadMore" disabled>Load More Data</button>
                            }
                        </div>
                        <div class="col-md-6">
                            <button type="button" class="btn log_Btn_sm" onclick="CloseEximTrader()">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="ImporterModal" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content popup_modal">
                <div class="modal-header popup_header">
                    <button type="button" class="close" onclick="CloseImporter()"><i class="fa fa-times" aria-hidden="true"></i></button>
                    <h4 class="modal-title">List of Importer Name</h4>
                </div>
                <div class="modal-body popup_body" style="position:relative;">
                    <input placeholder="Search By Importer Name Or Party Code" id="Impbox" type="text" tabindex="200" />
                    <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchImporterByPartyCode()"></i></span>
                    <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                        <thead>
                            <tr>
                                <th colspan="8" width="80%">Importer Name</th>
                                <th colspan="4" width="20%">Party Code</th>
                            </tr>
                        </thead>
                    </table>
                    <div id="slim_scroll_1">
                        <table tabindex="201"  class="table dataTable table-bordered table-striped slim_tble" id="tbleImp">
                            @{
                                if (ViewBag.lstImporter != null)
                                {
                                    var tabvalue = 202;
                                    foreach (var item in ViewBag.lstImporter)
                                    {
                                        <tr tabindex="@tabvalue" onclick="SelectImporter(@item.ImporterId,'@item.ImporterName')" onkeypress="SelectImporter(@item.ImporterId,'@item.ImporterName')">
                                            <td colspan="8" width="80%" class="txt_wrp">@item.ImporterName</td>
                                            <td colspan="4" width="20%" class="txt_wrp">@item.PartyCode</td>
                                        </tr>
                                        tabvalue++;
                                    }
                                }
                            }
                        </table>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-md-6">
                            @if (ViewBag.ImpState != null && Convert.ToBoolean(ViewBag.ImpState) == true)
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreImporter()" class="btn log_Btn_sm" id="btnLoadImp">Load More Data</button>
                            }
                            else
                            {
                                <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreImporter()" class="btn log_Btn_sm" id="btnLoadImp" disabled>Load More Data</button>
