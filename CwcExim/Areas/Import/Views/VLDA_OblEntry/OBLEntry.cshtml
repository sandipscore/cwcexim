                    $('#TPNo').focus();
            }
            });
        }

    });


    function SelectCommodity(CommodityId, Commodity,CommodityType) {
        angular.element(document.querySelector('[ng-controller="VLDA_OBLEntryCtrl"]')).scope().
         SelectCommodity(CommodityId, Commodity,CommodityType);
