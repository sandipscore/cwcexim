@model CwcExim.Areas.Import.Models.Wfld_OblEntry
<script src="~/Areas/Import/Scripts/App/Services/VLDA_OBLEntryService.js"></script>
<script src="~/Areas/Import/Scripts/App/Controllers/VLDA_OBLEntryCtrl.js"></script>
<script>
    angular.bootstrap(document.getElementById('OBLEntryAppDiv'), ['CWCApp']);
</script>
<style>
    .ang_srch {
        top: 25%;
        right: 6px;
        height: 20px;
        padding: 0 4px;
        display: block;
        background: #ebebe4;
        position: absolute;
        cursor: pointer;
    }

    #DivFormOneDet {
        height: 584px;
        overflow-x: hidden;
    }

    .slimScrollDiv {
        height: 300px;
    }

    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }

    #tbleImp.dataTable tbody > tr > td {
        cursor: pointer;
    }
     #ObltbleSline.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .table.tblhd {
        border-bottom: 0;
    }
</style>
<style>
    #tbleSline.dataTable tbody > tr > td {
        cursor: pointer;
    }
</style>
<div class="container-fluid" id="OBLEntryAppDiv" ng-controller="VLDA_OBLEntryCtrl">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>OBL Entry</h4>
                    </div>
                </div>
                @*@using (Ajax.BeginForm("AddEditOBLEntry", "Wfld_OblEntry", new AjaxOptions
                    {
                        Confirm = "Are you sure you want to save?",
                        HttpMethod = "POST",
                        OnSuccess = "JobOrderOnSuccess",
                        OnFailure = "JobOrderOnFailure"
                    }))*@
                @{
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.StringifiedText)
                    @Html.HiddenFor(m => m.Id)
                    @Html.HiddenFor(m => m.IsAlreadyUsed)
                    if (ViewBag.ListOfContainerNo != null)
                    {
                        <input type="hidden" id="hdnListOfContainerNo" value="@ViewBag.ListOfContainerNo" />
                    }
                    else
                    {
                        <input type="hidden" id="hdnListOfContainerNo" value="" />
                    }
                    <div id="DivJOHdr" class="Form_Space_bottom">
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Container / CBT:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownListFor(m => m.CONTCBT, new List<SelectListItem>
                                      {
                                      new SelectListItem { Text="CBT",Value="CBT"},
                                      new SelectListItem { Text="Container",Value="CONT"},
                                      }, new { @class = "form-control input-sm",@onchange="OnTypeChange()", tabindex = 1})
                                </div>
                                @*<div class="col-md-2-5">
                                    @Html.DropDownListFor(m => m.CONTCBT, new List<SelectListItem>
                                      {
                                      new SelectListItem { Text="Container",Value="CONT"},
                                      new SelectListItem { Text="CBT",Value="CBT"},
                                      }, "----Select----", new { @class = "form-control input-sm", @onchange = "OnTypeChange()" })
                                </div>*@
                                
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Container / CBT No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.HiddenFor(m => m.CFSCode)
                                    @Html.TextBoxFor(m => m.ContainerNo, new { @class = "form-control input_sm",tabindex = 2, onkeydown = "callmodal()" })
                                    <span class="search" id="ContainerNoSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ContainerNoModal"></i></span>
                                </div>
                                <div class="col-md-offset-0-5 col-md-2-5">
                                    <input type="button" id="btnShowDetails" class="btn log_Btn_sm" value="Show Details" tabindex="3" onclick="ShowDetails()" />
                                </div>
                                <div class="col-md-offset-1 col-md-1-5 col_cus_2 padding_rt">
                                    <label>Container / CBT Size:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownListFor(m => m.ContainerSize, new List<SelectListItem>
                                    {
                                        new SelectListItem { Text="20",Value="20"},
                                        new SelectListItem { Text="40",Value="40"},
                                    }, "----Select----",new {  tabindex = 4 })
                                    @Html.ValidationMessageFor(m => m.ContainerSize, "", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>IGM No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBoxFor(m => m.IGM_No, new { @class = "form-control input_sm", tabindex = 5 })
                                    @*@Html.ValidationMessageFor(m => m.IGM_No, "", new { @class = "Error_Msg" })*@
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>IGM Date:</label>
                                </div>
                                <div class="col-md-2-5 Date_Img">
                                    @Html.TextBoxFor(m => m.IGM_Date, new { @size = 10, @maxlength = 10, @onkeyup = "checks()", @onblur = "checkdates()", tabindex = 6})
                                    <div class="field-validation-error" id="DivMsges" style="background-color:transparent"></div>
                                    @*@Html.ValidationMessageFor(m => m.IGM_Date, "", new { @class = "Error_Msg" })*@
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>TP No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBoxFor(m => m.TPNo, new { @class = "form-control input_sm", tabindex = 7})
                                    @Html.ValidationMessageFor(m => m.TPNo, "", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt">
                                    <label>TP Date:</label>
                                </div>
                                <div class="col-md-2-5 Date_Img">
                                    @Html.TextBoxFor(m => m.TPDate, new { @size = 10, @maxlength = 10, @onkeyup = "chec()", @onblur = "checkdat()", tabindex = 8})
                                    <div class="field-validation-error" id="DivMs" style="background-color:transparent"></div>
                                    @Html.ValidationMessageFor(m => m.TPDate, "", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Movement Type:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.DropDownListFor(m => m.MovementType, new List<SelectListItem>
                               {
                                   new SelectListItem {Text="FCL",Value="FCL" },
                                   new SelectListItem {Text="LCL",Value="LCL" }
                               }, "----Select----", new { @class = "form-control input_sm dropdown", tabindex = 9})
                                    @Html.ValidationMessageFor(m => m.MovementType, "", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>Port:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.HiddenFor(m => m.PortId)
                                    @Html.TextBoxFor(m => m.PortName, new { @class = "form-control input_sm", @readonly = true,onkeydown = "CallPortmodal()", tabindex = 10 })
                                    <span class="search" id="PortSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PortModal"></i></span>
                                    @Html.ValidationMessageFor(m => m.PortId, "", new { @class = "Error_Msg" })
                                    @Html.HiddenFor(m => m.SelectPortId)
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">

                                    <label>Country:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.HiddenFor(m => m.CountryId)
                                    @Html.TextBoxFor(m => m.CountryName, new { @class = "form-control input_sm", @readonly = true, tabindex = 11 , onkeydown = "CallCountrymodal()" })
                                    <span class="search" id="CountrySearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CountryModal"></i></span>
                                    @Html.ValidationMessageFor(m => m.CountryId, "", new { @class = "Error_Msg" })
                                    @Html.HiddenFor(m => m.SelectCountryId)
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>Shipping Line:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.HiddenFor(m => m.ShippingLineId)
                                    @Html.TextBoxFor(m => m.ShippingLineName, new { @class = "form-control input_sm", @readonly = true, onkeydown = "CallShippingLineModalmodal()", tabindex = 12 })
                                    <span class="search" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div id="DivJobOrderDtl" class="Head_h5 Form_Space_top" ng-hide="true">
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>OBL / HBL No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("OBL_No", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                    @Html.ValidationMessage("OBL_No", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>OBL / HBL Date:</label>
                                </div>
                                <div class="col-md-2-5 Date_Img">
                                    @Html.TextBox("OBL_Date", new { @size = 10, @maxlength = 10, @onkeyup = "check()", @onblur = "checkdate()" })
                                    <div class="field-validation-error" id="DivMsgs" style="background-color:transparent"></div>
                                    @Html.ValidationMessage("OBL_Date", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Line:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("LineNo", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>SMTP No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("SMTPNo", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>OBL / HBL Date:</label>
                                </div>
                           
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>No Of Package:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("NoOfPkg", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>Cargo Type:</label>
                                </div>
                                <div class="col-md-2-5 Date_Img">
                                    @Html.DropDownList("CargoType", new List<SelectListItem>
                                    {
                                        new SelectListItem {Text="HAZ",Value="1" },
                                        new SelectListItem {Text="NON-HAZ",Value="2" },
                                         new SelectListItem {Text="ODC",Value="3" }
                                    }, "----Select----", new { @class = "form-control input_sm dropdown" })
                                    @Html.ValidationMessage("CargoType", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Cargo Description:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextArea("CargoDescription", "", new { @class = "form-control input_sm", @style = "width:100%;height:50px;" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Package type:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("PkgType", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                    @Html.ValidationMessage("PkgType", new { @class = "Error_Msg" })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col_cus_2 padding_rt col-md-1-5">
                                    <label>GR WT(Kg):</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBox("GR_WT", "", new { @class = "form-control input_sm", @maxlength = 30 })
                                    @Html.ValidationMessage("GR_WT", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Importer Name:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    <input type="hidden" id="ImporterId" value="0" />
                                    @Html.TextBox("ImporterName", "", new { @class = "form-control input_sm", @readonly = true })
                                    <span class="search" id="ImporterSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ImporterModal"></i></span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <div id="DivFormOneDet" ng-hide="OblEntryDetails.length==0">
                                <table class="table table-striped table-bordered dataTable">
                                    <thead>
                                        <tr>
                                            <th>Sl No</th>
                                            <th width="10%">OBL / HBL No</th>
                                            <th width="8%">OBL / HBL Date</th>
                                            <th width="10%">Original Container No</th>
                                            <th width="8%">Arrival Date</th>
                                            <th width="4%" ng-show="true">Line No.</th>
                                            <th width="8%">TSA No.</th>
                                            <th width="8%">TSA Date</th>
                                            <th width="8%">SMTP No.</th>
                                            <th width="8%">SMTP Date</th>
                                            <th width="10%">Cargo Desc</th>
                                            <th width="8%">Commodity</th>
                                            <th width="10%">Cargo Type</th>
                                            <th width="5%">No. of PKG</th>
                                            <th width="5%">PKG Type</th>
                                            <th width="5%">GR WT(Kg)</th>
                                            <th width="3%">CBM</th>
                                            <th width="5%">CIF Value</th>
                                            <th>Importer Name</th>
                                            <th>Shipping Line</th>
                                            <th>IGM Importer Name</th>
                                            <th width="3%" class="text-center" ng-hide="Action">Action</th>
                                            @*<th class="text-center" style="width:10%;">View</th>*@
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyFocus">
                                        <tr ng-repeat="j in OblEntryDetails">
                                            <td>{{$index+1}}
                                            <input type="hidden" ng-model="j.icesContId" />
                                            <input type="hidden" ng-model="j.ID" />
                                            <input type="hidden" ng-model="j.AddID" /></td>
                                            <td width="10%"><input type="text" tabindex="" ng-model="j.OBL_No" ng-blur="onchangetext(j);" id="focusInput_{{$index+1}}" autofocus/> </td>
                                            @*<td width="8%">
                                                    <input type="text" ng-model="j.OBL_Date"
                                                           name="date"
                                                           placeholder="dd/mm/yyyy"
                                                           onkeyup="var v = this.value;
                                                    if (v.match(/^\d{2}$/) !== null) {
                                                    this.value = v + '/';
                                                    } else if (v.match(/^\d{2}\/\d{2}$/) !== null) {
                                                    this.value = v + '/';
                                                    }"
                                                           maxlength="10" />
                                                </td>*@
                                            <td>
                                                <input type="text" id="date{{$index}}" ng-model="j.OBL_Date" name="date" placeholder="dd/mm/yyyy" 
                                                       onkeyup = "check(this.id)" onblur = "checkdate(this.id)"  />
                                                <span class="field-validation-error" id="ErrddlValidType_date{{$index}}"></span>
                                                                                            
                                            </td>
                                            <td width="10%"><input type="text" tabindex="" ng-model="j.ContainerNo" /> </td>
                                            <td>
                                                <input type="text" id="ArrivalDate{{$index}}" ng-model="j.ArrivalDate" name="date" placeholder="dd/mm/yyyy"
                                                       onkeyup="check(this.id)" onblur="checkdate(this.id)" />
                                                <span class="field-validation-error" id="ErrddlValidType_ArrivalDate{{$index}}"></span>

                                            </td>
                                            <td width="4%" ng-show="true"><input type="text" ng-model="j.LineNo" /></td>
                                            <td width="8%"><input type="text" ng-model="j.TSANo" /></td>
                                            <td style="position:relative;">
                                                <input type="text" id="TSA_Date{{$index}}" ng-model="j.TSA_Date" name="date" placeholder="dd/mm/yyyy"                                               
                                                       onkeyup="check(this.id)" onblur="checkdate(this.id)" />
                                                <span class="field-validation-error" id="ErrddlValidType_TSA_Date{{$index}}"></span>
                                                @*<span class="ang_srch"><i class="fa fa-calendar-o" aria-hidden="true" onclick="TSADatepicker(this.id)"></i></span>*@

                                            </td>
                                            <td width="8%"><input type="text" ng-model="j.SMTPNo" /></td>
                                            <td>
                                                <input type="text" id="SMTP_Date{{$index}}" ng-model="j.SMTP_Date" name="date" placeholder="dd/mm/yyyy"
                                                       onkeyup="check(this.id)"  />
                                                <span class="field-validation-error" id="ErrddlValidType_SMTP_Date{{$index}}"></span>


                                            </td>
                                            <td width="10%">
                                                <textarea class="col-xs-12"  id="Cargo{{$index}}" style="padding:3px; resize:none;" ng-model="j.CargoDescription" cols="2" onblur="setfocus(this.id);"></textarea>
                                            </td>
                                            <td style="position:relative;">
                                                <input type="text" ng-model="j.Commodity" id="Commodity_{{$index}}" readonly required ng-keydown = "CallCommodityModal($index)"  />
                                                <input type="text" ng-hide="true" ng-model="j.CommodityId" />
                                                <span class="ang_srch" id="CommoditySerach"><i class="fa fa-search" aria-hidden="true" ng-model="j.Commoditymodal" ng-click="onCommodityChange($index)" data-toggle="modal" data-target="#CommodityModal"></i></span>
                                            </td>
                                            <td width="10%">
                                                <select id="CargoType{{$index}}" ng-model="j.CargoType" required onchange="setpkgfocus(this.id);">
                                                    <option value="0">--Select--</option>
                                                    <option value="{{s.id}}" ng-repeat="s in CargoTypeList">{{s.CargoType}}</option>
                                                </select>
                                            </td>
                                            <td width="5%">
                                                <input type="text" ng-model="j.NoOfPkg" onkeypress="return isNumber(event)" id="Pkg{{$index}}"/>
                                            </td>
                                            <td>
                                                <input type="text" ng-model="j.PkgType" />
                                            </td>
                                            <td width="5%"><input type="text" ng-model="j.GR_WT" onkeypress="return isDecimalNumber(event)" id="GR_WT{{$index}}" /></td>
                                            <td width="3%"><input type="text" id="cbm{{$index}}" value="0" ng-model="j.AreaCBM" onkeypress="return isDecimalNumber(event)" /></td>
                                            <td width="5%"><input type="text" ng-model="j.CIFValue" onkeypress="return isDecimalNumber(event)" /></td>
                                            <td style="position:relative;">
                                                <input type="text" ng-model="j.ImporterName" id="ImporterName_{{$index}}" ng-readonly="true" required ng-keydown = "CallImporterModal($index)" />
                                                <input type="text" ng-hide="true" ng-model="j.ImporterId" />
                                                <span class="ang_srch" id="ImporterSerach"><i class="fa fa-search" aria-hidden="true" ng-model="j.Slmodal" ng-click="onImporterChange($index)" data-toggle="modal" data-target="#ImporterModal"></i></span>
                                            </td>
                                            <td style="position:relative;">
                                                <input type="text" ng-model="j.ShippingLineName" id="ShippingLineName_{{$index}}" ng-readonly="true" required ng-keydown = "CallShippingModal($index)" />
                                                <input type="text" ng-hide="true" ng-model="j.ShippingLineId" />
                                                <span class="ang_srch" id="OblShippingLineSerach"><i class="fa fa-search" aria-hidden="true" ng-model="j.Shiplmodal" ng-click="onShippingLineChange($index)" data-toggle="modal" data-target="#OBLShippingLineModal"></i></span>
                                            </td>
                                            <td>
                                                <input type="text" ng-model="j.IGM_IMPORTER" ng-readonly="true" />
                                            </td>
                                            <td width="3%" style="text-align:center;" ng-hide="Action">
                                                <input type="hidden" ng-model="j.IsProcessed" />
                                                <a href="#" ng-click="Delete($index,j)" ><i class="fa fa-window-close-o Delete" aria-hidden="true"></i></a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                        <div class="logErrMsg" id="DivTblContrErrMsg" style="background-color:transparent"></div>
                    </div>
                    <div class="row SignUp_space_bottom">
                        <div class="col-md-offset-4 col-md-2">
                            <input type="button" id="btnAddJO" class="btn log_Btn_sm" value="Add" tabindex="13" ng-click="AddOblEntry()" />
                        </div>
                        <div class="col-md-2">
                            <input type="button" id="btnResetJO" class="btn log_Btn_sm" value="Reset" tabindex="14" ng-click="ResetJODet()" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                    </div>
                    <div class="row SignUp_space_top2">
                        <div class="col-md-offset-4 col-md-2">
                            <input type="submit" id="btnSave" class="btn log_Btn_sm" value="Save" tabindex="15" ng-click="OnOBLEntrySave()" />
                        </div>
                        <div class="col-md-2">
                            <input type="button" id="btnReset" class="btn log_Btn_sm" value="Reset" tabindex="16" onclick="ResetAllField()" />
                        </div>
                    </div>
                    <div class="logSuccMsg Form_Space_top" id="DivSucMsg" style="background-color:transparent"></div>
