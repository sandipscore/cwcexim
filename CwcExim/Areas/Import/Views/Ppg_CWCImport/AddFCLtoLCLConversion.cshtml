@using (Ajax.BeginForm("AddFCLtoLCLConversion", "Ppg_CWCImport", new AjaxOptions { HttpMethod = "POST", OnSuccess = "SealCuttingOnSuccess", OnFailure = "SealCuttingeOnFailure" }))
{
    @Html.AntiForgeryToken()
    @Html.Hidden("PaymentSheetModelJson")
    @Html.Hidden("ServiceType")
