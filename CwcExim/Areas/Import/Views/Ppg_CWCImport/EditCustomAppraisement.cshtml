    AjaxOptions options = new AjaxOptions();
    options.Confirm = "Are You Sure You Want To Save?";
    options.OnSuccess = "CstmAppraiseOnSuccess";
    options.OnFailure = "CstmAppraiseOnFailure";
    options.InsertionMode = InsertionMode.InsertAfter;
}

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row">
                <div class="col-md-12">
                    <h4>Custom Appraisement Application</h4>
                </div>
            </div>

            <div class="content_wrp">
                @using (Ajax.BeginForm("AddEditCustomAppraisement", "Ppg_CWCImport", options))
                {
                    @Html.AntiForgeryToken()
                    <div id="DivAddCustomAppraisement">
                        @Html.HiddenFor(m => m.CustomAppraisementXML)
                        @Html.HiddenFor(m => m.CustomAppraisementId)
                        @Html.HiddenFor(m=>m.CAOrdDtlXml)
                        @Html.Hidden("CustomAppraisementDtlId")

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Appraisement No:</label>
                                    @Html.TextBoxFor(m => m.AppraisementNo, new { @readonly = true })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Appraisement Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.AppraisementDate, new { @readonly = true })
                                        @Html.ValidationMessageFor(m => m.AppraisementDate, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div id="DivShippingLine">
                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Shipping Line:</label>
                                        <div class="position-relative">
                                            @Html.TextBoxFor(m => m.ShippingLine, "", new { @readonly = true })
                                            <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                            @Html.HiddenFor(m => m.ShippingLineId)
                                            @Html.ValidationMessageFor(m => m.ShippingLine, "", new { @class = "Error_Msg" })                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>CHA:</label>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.CHAId)
                                        @Html.TextBoxFor(m => m.CHAName, new { @readonly = true })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CHAHdrModal"></i></span>
                                        @Html.ValidationMessageFor(m => m.CHAName, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Vessel:</label>
                                    <div class="position-relative">
                                        @Html.TextBoxFor(m => m.Vessel, new { placeholder = "Enter Vessel" })
                                        @Html.ValidationMessageFor(m => m.Vessel, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div id="DivRotation">
                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Voyage:</label>
                                        @Html.TextBoxFor(m => m.Voyage, "", new { placeholder = "Enter Voyage" })
                                        @Html.ValidationMessageFor(m => m.Voyage, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3 col-md-offset-6">
                                    <div class="form-group">
                                        <label>Rotation:</label>
                                        @Html.TextBoxFor(m => m.Rotation, "", new { placeholder = "Enter Rotation" })
                                        @Html.ValidationMessageFor(m => m.Rotation, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>                            
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="Radiocontainer">
                                        Cargo Delivery
                                       @Html.RadioButton("DeliveryType", "1", new { @class = "", @id = "CargoDelivery", @checked = true })
                                        <span class="checkmark"></span>
                                    </label>
                                    <label class="Radiocontainer">
                                        Container / CBT Delivery
                                        @Html.RadioButton("DeliveryType", "2", new { @class = "", @id = "ContainerDelivery" })
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="Radiocontainer">
                                        With DO
                                        @Html.RadioButton("IsDO", "1", new { @class = "", @id = "WithDo", @checked = true })
                                        <span class="checkmark"></span>
                                    </label>
                                    <label class="Radiocontainer">
                                        Without DO
                                        @Html.RadioButton("IsDO", "2", new { @class = "", @id = "WithoutDO" })
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                            </div>
                        </div>                        

                        <div id="DivCustomAppraisementDet" class="Head_h5">
                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Container / CBT No:</label>
                                        @Html.TextBox("ContainerNo", "", new { @readonly = true })
                                        @Html.ValidationMessage("ContainerNo", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>ICD Code:</label>
                                        @Html.TextBox("CFSCode", "", new { @readonly = true })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Size:</label>
                                       @Html.DropDownList("Size", new List<SelectListItem> {
                                       new SelectListItem { Text="20",Value="20"},
                                       new SelectListItem {Text="40",Value="40" },
                                       new SelectListItem {Text="ODC",Value="ODC" }
                                       }, "---Select---", new { @class = "dropdown", @disabled = true })
                                    @Html.ValidationMessage("Size", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>FCL / LCL:</label>
                                        @Html.DropDownList("LCLFCL", new List<SelectListItem>{
                                        new SelectListItem {Text="FCL",Value="FCL" },
                                        new SelectListItem {Text="LCL",Value="LCL" }
                                      }, "---Select---", new { @class = "dropdown", @disabled = true })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Container / CBT Type:</label>
                                        @Html.DropDownList("ContainerType", new List<SelectListItem> {
                                       new SelectListItem { Text="Empty Container / CBT",Value="1"},
                                       new SelectListItem {Text="Loaded Container / CBT",Value="2" ,Selected=true}
                                       }, "---Select---", new { @class = "dropdown", @disabled = true })
                                        @Html.ValidationMessage("ContainerType", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Cargo Type:</label>
                                        @Html.DropDownList("CargoType", new List<SelectListItem>{
                                        new SelectListItem {Text="HAZ",Value="1" },
                                        new SelectListItem {Text="Non HAZ",Value="2" }
                                      }, "---Select---", new { @class = "dropdown", @disabled = true })
                                        @Html.ValidationMessage("CargoType", "", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>RMS:</label>
                                        @Html.DropDownList("RMSValue", new List<SelectListItem>{
                                         new SelectListItem {Text="Exim",Value="1" ,Selected=true},
                                        new SelectListItem {Text="RMS Exim",Value="2" },
                                        new SelectListItem {Text="RMS Non Exim",Value="3" }
                                      }, "---Select---", new { @class = "dropdown" })
                                        @Html.ValidationMessage("RMSValue", "", new { @class = "Error_Msg" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Line No:</label>
                                        @Html.TextBox("LineNo", "", new { @readonly = true })
                                        @Html.ValidationMessage("LineNo", "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>OBL No:</label>
                                        @Html.TextBox("OBLNo", "", new { placeholder = "Enter OBL No" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>OBL Date:</label>
                                        <div class="position-relative Date_Img">
                                            @Html.TextBox("OBLDate", "", new { @readonly = true })
                                            @Html.ValidationMessage("OBLDate", "", new { @class = "Error_Msg" })
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>BOE No:</label>
                                        <div class="position-relative">
                                            @Html.TextBox("BOENo", "", new { @maxlength = "45", @onblur = "return GetCfcValue();", placeholder = "Enter BOE No" })
                                            <span class="input_icon" onclick="return GetCfcValue();"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target=""></i></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>BOE Date:</label>
                                        <div class="position-relative Date_Img">
                                            @Html.TextBox("BOEDate", "", new { @readonly = true })
                                        </div>                                        
                                    </div>
                                </div>
                            </div>
                          
                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>CHA Name And Address:</label>
                                        <div class="position-relative">
                                            @Html.TextBox("CHA", "", new { @readonly = true })
                                            <span class="input_icon" id="CHADtlSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CHADtlModal"></i></span>
                                            @Html.Hidden("CHANameId")
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Importer Name And Address:</label>
                                        <div class="position-relative">
                                            @Html.TextBox("Importer", "", new { @readonly = true })
                                            <span class="input_icon" id="ImporterSearch" onclick="hideMsg(this);"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ImporterModal"></i></span>
                                            @Html.Hidden("ImporterId")
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label>Cargo Description:</label>
                                        @Html.TextArea("CargoDescription", "", new { @style = "width:100%;height:100px", @disabled = true })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>No of Packages:</label>
                                        @Html.TextBox("NoOfPackages", "", new{ placeholder = "Enter No of Packages" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Gross Weight(Kg):</label>
                                       @Html.TextBox("GrossWeight", "", new{ placeholder = "Enter Gross Weight(Kg)" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>CIF Value:</label>
                                       @Html.TextBox("CIFValue", "", new { @maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)", placeholder = "Enter CIF Value" })
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Duty:</label>
                                        @Html.TextBox("Duty", "", new { @maxlength = "11", @onkeypress = "return AllowOnlyDecimal(event)", placeholder = "Enter Duty" })
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label>Without DO Seal No:</label>
                                        @Html.TextBox("WithoutDOSealNo", "", new { @maxlength = "45", placeholder = "Enter Without DO Seal No" })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="stoke SignUp_space_top SignUp_space_bottom"></div>
                            </div>

                            <div class="d-flex justify-content-left">
                                <input type="button" id="BtnAddCustomAppraisement" class="btn btn-primary mr-1 btn-100px" value="Add" onclick="SaveCstmAppraiseDtl()">
                                <input type="button" id="" class="btn btn-primary-border btn-100px" value="Reset">
                            </div>

                            <div id="DivCustomAppraisementTblMsg" class="logErrMsg Form_Space_top Form_Space_bottom" style="background-color:transparent"></div>

                            <div class="row Form_Space_top">
                                <div class="col-md-12">
                                    <table class="table table-striped table-bordered dataTable" id="TblCustomAppraisementDet" style="width:100%;">
                                        <thead>
                                            <tr>
                                                <th class="text-center" style="width:8%;">@Html.DisplayName("SL No")</th>
                                                <th>@Html.DisplayName("ICD No")</th>
                                                <th>@Html.DisplayName("Container / CBT No")</th>
                                                <th>@Html.DisplayName("OBL No")</th>
                                                <th>@Html.DisplayName("BOE No")</th>
                                                <th class="text-center" style="width:10%;">@Html.DisplayName("Edit")</th>
                                                <th class="text-center" style="width:10%;">@Html.DisplayName("View")</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="DivDeliveryOrderDetails" class="Head_h5">
                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Do Issued By:</label>
                                    @Html.TextBox("IssuedBy", "", new { placeholder = "Enter Do Issued By" })
                                    @Html.ValidationMessage("IssuedBy", "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Cargs. Delivered To:</label>
                                    @Html.TextBox("CargsDelivered", "", new { placeholder = "Enter Cargs. Delivered To" })
                                    @Html.ValidationMessage("CargsDelivered", "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Valid Type:</label>
                                    @Html.DropDownList("ddlValidType", new List<SelectListItem>
                                        {
                                        new SelectListItem { Text="Open",Value="Open"},
                                        new SelectListItem { Text="Closed",Value="Closed"},

                                        }, "----Select----", new { @class = "", @onchange = "ValidDt(this);" })
                                    <span class="field-validation-error" id="ErrddlValidType"></span>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Do Valid Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBox("ValidDate", "", new { @readonly = true, Value = DateTime.Now.ToString("dd/MM/yyyy") })
                                    </div>                                    
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="stoke SignUp_space_top SignUp_space_bottom"></div>
                        </div>

                        <div class="d-flex justify-content-left">
                            <input type="button" id="BtnAddDeliveryOrd" class="btn btn-primary mr-1 btn-100px" value="Add" onclick="SaveDeliveryOrdDtl()">
                            <input type="button" id="" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetDeliveryOrdDet()">
                        </div>

                        <div id="DivDeliveryOrdTblErrMsg" class="logErrMsg Form_Space_top Form_Space_bottom" style="background-color:transparent"></div>

                        <table class="table table-striped table-bordered dataTable" id="TblDeliveryAppOrd" style="width:100%;">
                            <thead>
                                <tr>
                                    <th class="text-center">@Html.DisplayName("SL No")</th>
                                    <th>@Html.DisplayName("Issued By")</th>
                                    <th>@Html.DisplayName("Cargo. Delivered")</th>
                                    <th class="text-center">@Html.DisplayName("Edit")</th>
                                    <th class="text-center">@Html.DisplayName("View")</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom2 SignUp_space_top2"></div>
                    </div>

                    <div class="d-flex justify-content-left">
                        <input type="submit" id="BtnSaveCustomAppraisement" class="btn btn-primary mr-1 btn-100px" value="Save">
                        <input type="button" id="" class="btn btn-primary-border btn-100px" value="Cancel" onclick="ReloadCstmAppraise()">
                    </div>
                    
                    <div class="logSuccMsg" style="background-color:transparent" id="DivCustomAppraisementMsg"></div>
                    <div class="logErrMsg Form_Space_bottom" style="background-color:transparent" id="DivCustomAppraisementErrMsg"></div>

                }
                @Scripts.Render("~/bundles/jqueryval")
            </div>
        </div>
    </div>
</div>




