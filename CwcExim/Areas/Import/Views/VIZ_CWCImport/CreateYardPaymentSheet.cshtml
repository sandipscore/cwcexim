        if ($('#SearchInvoiceNo').val() == "" && $('#SearchInvoiceDate').val() == "")
            alert("Enter Invoice No./Container No./Assessment No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate() == true)
            $('#DivListOfInvoice').load('/Import/VIZ_CWCImport/ListOfExpInvoice?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate + "&Module=IMPYard");
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Import/VIZ_CWCImport/ListOfExpInvoice?InvoiceNo=' + InvoiceNo + "&Module=IMPYard");
        else if (Validate() == true)
