    $('#ContainerList li').on("click", function () {
        debugger;
        var SplitContainerNo = ($(this).text()).split('-');
        var ContainerNo = SplitContainerNo[0];
        var LineNo = SplitContainerNo[1];
        var CFSCode = $(this).attr('id');
        $('#Container').val(ContainerNo);
        $("#ContainerModal").modal("hide");
        $('#DivDestuffingDet').find('input:text,textarea,select').val('');
        $('#DivDestuffingDet').find('input:hidden').val('');
        $('#IsInsured').prop("checked", false);
        $('#DivDestuffingTblMsg').html('');
        DestuffType = 0;
        $('#LineNo').val(LineNo);
        GetContainerDet(CFSCode, LineNo);
    });

    function GetContainerDet(CFSCode, LineNo) {
        debugger;
        $.ajax({
            url: '/Import/CWCImport/GetCntrDetForDestuffing',
            type: 'GET',
            dataType: 'JSON',
            data: { CFSCode: CFSCode, LineNo: LineNo },
            success: function (data) {
                debugger;
                if (data.Status == 1) {
                    DestuffType = 1;
                    $('#CustomAppraisementId').val(data.Data.CustomAppraisementId);
                    $('#ShippingLineId').val(data.Data.ShippingLineId);
                    $('#CHAId').val(data.Data.CHAId);
                    $('#Rotation').val(data.Data.Rotation);
                    $('#Fob').val(data.Data.Fob);
                    $('#GrossDuty').val(data.Data.GrossDuty);
                    data.Data.DeliveryType == 1 ? $('#CargoDelivery').prop("checked", true) : $('#ContainerDelivery').prop("checked", true);
                    data.Data.IsDO == 1 ? $('#WithDo').prop("checked", true) : $('#WithoutDO').prop("checked", true);
                    $('#ShippingLine').val(data.Data.ShippingLine);
                    $('#CHAName').val(data.Data.CHAName);
                    //added:
                    $.each(data.Data.LstDestuffing, function (i, item) {
                        debugger;
                        item.IsInsured = 0;
                        ArrayOfDestuffing.push(item);
                        $("#ContainerModal ul li:contains(" + item.ContainerNo + "-" + item.LineNo + ")").remove();
                    });

                    //removed  ArrayOfDestuffing = data.Data.LstDestuffing;
                    GenerateDestuffingTable();
                }
                else if (data.Status == 2) {

                    DestuffType = 2;
                    $('#BtnAddDestuffing').prop("disabled", false);
                    $('#ShippingLine').val(data.Data.ShippingLine);
                    $('#ShippingLineId').val(data.Data.ShippingLineId);
                    $('#CustomAppraisementId').val(0);
                    $('#CustomAppraisementDtlId').val(0);
                    $('#Rotation').val(data.Data.Rotation);
                    $('#ContainerNo').val(data.Data.ContainerNo);
                    $('#CFSCode').val(data.Data.CFSCode);
                    $('#Size').val(data.Data.Size);
                    $('#LCLFCL').val(data.Data.LCLFCL);
                    // $('#LineNo').val(data.Data.LineNo);
                    $('#ImporterId').val(data.Data.ImporterId);
                    $('#Importer').val(data.Data.Importer);
                    $('#CHANameId').val(data.Data.CHANameId);
                    $('#CHA').val(data.Data.CHA);
                    $('#CargoDescription').val(data.Data.CargoDescription);
                    $('#NoOfPackages').val(data.Data.NoOfPackages);
                    $('#GrossWeight').val(data.Data.GrossWeight);
                    $('#Vessel').val(data.Data.Vessel);
                    $('#Voyage').val(data.Data.Voyage);
                    $("#ContainerModal ul li:contains(" + data.Data.ContainerNo + "-" + data.Data.LineNo + ")").remove();
                    // ArrayOfDestuffing.push(data.Data);  //added
                }
            }
        });
    }


    function SaveDestuffingDtl() {
        debugger;
        $('#DivDestuffingTblMsg').html('');
        if ($('#ContainerNo').val() != "") {
            var DestuffingDtl = {
                "DestuffingDtlId": $('#DestuffingDtlId').val() == "" ? 0 : $('#DestuffingDtlId').val(),
                "CustomAppraisementDtlId" : $('#CustomAppraisementDtlId').val(),
                "ContainerNo": $('#ContainerNo').val(),
                "CFSCode": $('#CFSCode').val(),
                "Size": $('#Size').val(),
                "LCLFCL": $('#LCLFCL').val(),
                "LineNo": $('#LineNo').val(),
                "BOENo": $('#BOENo').val(),
                "BOEDate": $('#BOEDate').val(),
                "CHANameId": $('#CHANameId').val(),
                "CHA": $('#CHA').val(),
                "ImporterId": $('#ImporterId').val(),
                "Importer": $('#Importer').val(),
                "CargoDescription": $('#CargoDescription').val(),
                "NoOfPackages": $('#NoOfPackages').val() == "" ? 0 : $('#NoOfPackages').val(),
               // "NoOfPackages": $('#NoOfPackages').val(),
                "GrossWeight": $('#GrossWeight').val() == "" ? 0 : $('#GrossWeight').val(),
                "Duty": $('#Duty').val() == "" ? 0 : $('#Duty').val(),
                "CIFValue": $('#CIFValue').val() == "" ? 0 : $('#CIFValue').val(),
               // "CIFValue": $('#CIFValue').val(),
                // "WithoutDOSealNo": $('#WithoutDOSealNo').val(),
                "WithoutDOSealNo": $('#WithoutDOSealNo').val() == "" ? 0 : $('#WithoutDOSealNo').val(),
                "Vessel": $('#Vessel').val(),
                "Voyage": $('#Voyage').val(),
                "IsInsured": $('#IsInsured').prop("checked") == false ? 0 : 1
            }
            ArrayOfDestuffing.push(DestuffingDtl);
            IsEdit = 0;
            //$('#ContainerList #' + $('#ContainerNo').val()).remove();
            ResetDestuffingDet();
            GenerateDestuffingTable();
        }
        else {
            $('#DivDestuffingTblMsg').html('Please Select A Container No In Order To Add Details');
            // $('[data-valmsg-for="ContainerNo"]').html('<span>Fill Out This Field</span>');
        }
    }

    function ResetDestuffingDet() {
        debugger;
        $('#DivDestuffingDet').find('input:text,textarea,select').val('');
        $('#DivDestuffingDet').find('input:hidden').val('');
        $('#IsInsured').prop("checked", false);
    }

    function GenerateDestuffingTable() {
        debugger;
        $('#TblDestuffingDet>tbody').html('');
        $('#TblDestuffingDet').hide();
        var Html = '';
        if (ArrayOfDestuffing.length != 0) {
            $.each(ArrayOfDestuffing, function (i, item) {
                debugger;
                //Html += '<tr><td class="text-center">' + (i + 1) + '</td><td>' + item.CFSCode + '</td><td>' + item.ContainerNo + '</td><td class="text-center"><a href="#" onclick="EditDestuffing(' + i + ')"><i class="fa fa fa-pencil-square Edit" aria-hidden="true"></i></a></td><td class="text-center"><a href="#" onclick="ViewDestuffing(' + i + ')"><i class="fa fa-search-plus Edit" aria-hidden="true"></i></a></td><td class="text-center"><a href="#" onclick="DeleteDestuffing(' + i + ')"><i class="fa fa-window-close-o Delete" aria-hidden="true"></i></a></td></tr>';
                Html += '<tr><td class="text-center">' + (i + 1) + '</td><td>' + item.CFSCode + '</td><td>' + item.ContainerNo + '</td><td class="text-center"><a href="#" onclick="EditDestuffing(' + i + ')"><i class="fa fa fa-pencil-square Edit" aria-hidden="true"></i></a></td><td class="text-center"><a href="#" onclick="ViewDestuffing(' + i + ')"><i class="fa fa-search-plus Edit" aria-hidden="true"></i></a></td></tr>';
            });
            $('#TblDestuffingDet>tbody').html(Html);
            $('#TblDestuffingDet').show();
        }
    }

    function EditDestuffing(Index) {
        debugger;
        if (IsEdit == 0) {
            $('#BtnAddDestuffing').prop("disabled", false);
            $('#CHADtlSearch').css("display", "block");
            $('#ContainerNo').val(ArrayOfDestuffing[Index].ContainerNo);
            $('#CFSCode').val(ArrayOfDestuffing[Index].CFSCode);
            $('#Size').val(ArrayOfDestuffing[Index].Size);
            $('#LCLFCL').val(ArrayOfDestuffing[Index].LCLFCL);
            $('#LineNo').val(ArrayOfDestuffing[Index].LineNo);
