                        @Html.Hidden("DestuffingDtlId")
                        @Html.HiddenFor(m=>m.DestuffingId)
                        @Html.HiddenFor(m => m.CustomAppraisementId)
                        @Html.Hidden("CustomAppraisementDtlId")
                        @*@Html.Hidden("ContainerCFSCode")*@
