    function SelectLocation() {
        debugger;
        var data = '';
        var inf = '';
        $('#lstYardWiseLctn').find('input:checked').each(function () {
            debugger;
            if (data == '')
                data = $(this).prop('id');
            else
                data = data + ',' + $(this).prop('id');
            if (inf == '')
                inf = $('label[for="' + $(this).prop('id') + '"]').text();
