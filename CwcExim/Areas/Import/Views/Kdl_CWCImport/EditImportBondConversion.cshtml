@model CwcExim.Areas.Import.Models.KDL_ImportBondConversion

@if (ViewBag.GodownList != null)
{
    <input type="hidden" id="hdnGodownList" value="@ViewBag.GodownList" />
}
else
{
    <input type="hidden" id="hdnGodownList" value="" />
}

<style>
    .sec_dvsn_area{
        border: 1px solid #f7a654;
    }
    .sec_dvsn {
        border-left: 1px solid #f7a654;
        width: 50%;
        float:left;
    }
    .sec_dvsn h4 {
        border-bottom: 1px solid #f7a654;
        background: none;
        padding: 10px;
        margin: 0;
        color: #fe9b35;
    }
    .sec_dvsn_wrp {
       padding: 8px;
    }
</style>

@{
    AjaxOptions options = new AjaxOptions();
    options.HttpMethod = "POST";
    options.Confirm = "Are You Sure You Want To Save?";
    options.OnSuccess = "BondConvOnSuccess";
    options.OnFailure = "BondConvOnFailure";
    options.InsertionMode = InsertionMode.InsertAfter;
}

@using (Ajax.BeginForm("AddEditImportBondConversion", "Kdl_CWCImport", options))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.MovementId)
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-11 col-md-offset-0-5">
                <div class="container-fluid">
                    <div class="Head_h4">
                        <div class="row SignUp_space_bottom">
                            <div class="col-md-12">
                                <h4>Transfer of Import Stock to Bond</h4>
                            </div>
                        </div>

                        <div class="row Form_Space_top SignUp_space_bottom">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 padding_rt">
                                    <label>Transfer No :</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5">
                                    @Html.TextBoxFor(m=>m.MovementNo, new { @class = "form-control input-sm", @readonly = true })
                                </div>
                                <div class="col-md-1-5 col-md-offset-5">
                                    <label>Date :</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5 Date_Img">
                                    <span class="ReQSimbol">*</span>
                                    @*<input class="form-control" type="text" id="MovementDate" readonly="readonly" />*@
                                    @Html.TextBoxFor(m=>m.MovementDate, new { @class = "form-control input-sm", @readonly = true })
                                </div>
                            </div>
                        </div>

                        <div class="sec_dvsn_area">
                            <div class="sec_dvsn" style="border-left:0;">
                                <div class="row">
                                    <div class="col-md-12">
                                        <h4>FROM</h4>
                                    </div>
                                </div>
                                <div class="sec_dvsn_wrp">

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>OBL / HBL NO :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m => m.DestuffingId)
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.OBLNo, new { @class = "form-control input_sm", @readonly = true })
                                                <span class="search" id="OBLSerach"><i aria-hidden="true" data-toggle="modal" id="OBL"></i></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>OBL / HBL Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.OBLDate, new { @class = "form-control input_sm", @readonly = true })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Godown Name :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m => m.FromGodownId)
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.FromGodownName, new { @class = "form-control input_sm", @readonly = true })
                                            </div>
                                        </div>
                                    </div>

                                   
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Location :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m=>m.FromLocationId)
                                                @Html.TextBoxFor(m => m.FromLocationName, new { @class = "form-control input_sm", @readonly = true })                                                
                                            </div>
                                        </div>
                                    </div>

                                   
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>No of Pkg :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.NoOfPkg, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                                
                                                @Html.HiddenFor(m=>m.RemainingNoOfPkg, new { @id= "hdnNoOfPkg" })
                                                @Html.HiddenFor(m => m.NoOfPkg, new { @id = "hdnNoOfPkgOriginal" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Weight(kg) :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.Weight, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                                
                                                @Html.HiddenFor(m => m.RemainingGrWeight, new { @id = "hdnWeight" })
                                                @Html.HiddenFor(m => m.Weight, new { @id = "hdnWeightOriginal" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>SQM :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.SQM, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                                
                                                @Html.HiddenFor(m => m.RemainingSQM, new { @id = "hdnSQM" })
                                                @Html.HiddenFor(m => m.SQM, new { @id = "hdnSQMOriginal" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top" style="display:none;">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>CUM :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CUM, new { @class = "form-control input_sm", @readonly = true })                                                
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>CIF :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CIF, new { @class = "form-control input_sm",  onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                                
                                                @Html.HiddenFor(m => m.RemainingCIF, new { @id = "hdnCIF" })
                                                @Html.HiddenFor(m => m.CIF, new { @id = "hdnCIFOriginal" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>DUTY :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.Duty, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                                
                                                @Html.HiddenFor(m => m.RemainingDuty, new { @id = "hdnDuty" })
                                                @Html.HiddenFor(m => m.Duty, new { @id = "hdnDutyOriginal" })
                                            </div>
                                        </div>
                                    </div>                                    
                                </div>
                            </div>

                            <div class="sec_dvsn">
                                <div class="row">
                                    <div class="col-md-12">
                                        <h4>TO</h4>
                                    </div>
                                </div>
                                <div class="sec_dvsn_wrp">
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">                                                
                                                <label>SAC No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                @Html.HiddenFor(m => m.SACId)
                                                @Html.TextBoxFor(m => m.SACNo, "", new { @class = "form-control input-sm", @readonly = true })
                                                <span style="display:none;" class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#SacModal"></i></span>
                                                @Html.ValidationMessage("SACNo", new { @class = "Error_Msg" })

                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">                                                
                                                <label>SAC Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                @Html.TextBoxFor(m => m.SACDate, "", new { @class = "form-control input-sm", @readonly = true })

                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Godown Name :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.GodownName, new { @class = "form-control input-sm", @readonly = true ,@onkeydown = "PopupActivate('godownModal');" , @tabindex=1 })
                                                <span class="search" id="SpanGodownSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="FocusOnSacGodown()" data-target="#godownModal"></i></span>
                                                @Html.HiddenFor(m => m.GodownId)
                                                @Html.ValidationMessageFor(m => m.GodownName, "", new { @class = "Error_Msg" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Location :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6 Date_Img">
                                                @Html.HiddenFor(m => m.LocationId)
                                                @Html.TextBoxFor(m => m.Location, "", new { @class = "form-control input-sm", @readonly = true, onkeydown = "PopupActivate('LocationModal');" ,@tabindex = 2 })
                                                <span id="LocationSearch" class="search"><i class="fa fa-search" aria-hidden="true"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>WR No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.WRNo, new { @class = "form-control input_sm", @tabindex = 3 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>WR Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                @Html.TextBoxFor(m => m.WRDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 4 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Bond No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.BondNo, new { @class = "form-control input_sm" , @tabindex = 5 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Bond Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.BondDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 6 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Bond No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CustomBondNo, new { @class = "form-control input_sm", @tabindex = 7 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Bond Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                @Html.TextBoxFor(m => m.CustomBondDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 8 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Seal No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CustomSealNo, new { @class = "form-control input_sm", @tabindex = 9 })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="ClearFix"></div>
                        </div>                        

                        <div class="row SignUp_space_top SignUp_space_bottom"></div>


                        <div class="row SignUp_space_bottom">
                            <div class="col-md-offset-4 col-md-2">
                                <input type="submit" value="Save" name="Command" class="btn log_Btn_sm" id="BtnSave" />
                            </div>
                            <div class="col-md-2">
                                <input type="button" id="btnReset" class="btn log_Btn_sm" value="Reset" onclick="ResetMovement()" />
                            </div>
                        </div>
                        <div class="logSuccMsg" id="DivSucMsg" style="background-color:transparent"></div>
                        <div class="logErrMsg Form_Space_top" id="DivErrMsg" style="background-color:transparent"></div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-2">
                                <input type="button" value="Show List" name="Command" class="btn log_Btn_sm" id="BtnSave" onclick="GetMovementList()" tabindex=13 />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="DivMovementList"></div>
                        </div>
                    </div>                   
                </div>                
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jqueryval")
}

