        rawJson.TDSCol = Number($('#TotalTDSCol').val());
        rawJson.AllTotal = (rawJson.CWCTotal + rawJson.HTTotal + Number($('#TotalTDSCol').val())) - (Math.round(CurCWCTds + CurHTTds));
