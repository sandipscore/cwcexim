                    $('#InvoiceValue').val(data.InvoiceAmt.toFixed(2));
                    data.TotalAmt = tmat;
                    //CalcInvoice();
                    $('.search').css('display', 'none');
                    $('#InvoiceDate').parent().find('img').css('display', 'none');
