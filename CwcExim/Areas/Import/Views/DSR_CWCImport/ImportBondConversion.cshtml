@model CwcExim.Areas.Import.Models.DSR_ImportBondConversion

@if (ViewBag.GodownList != null)
{
    <input type="hidden" id="hdnGodownList" value="@ViewBag.GodownList" />
}
else
{
    <input type="hidden" id="hdnGodownList" value="" />
}

<style>
    .sec_dvsn_area{
        border: 1px solid #f7a654;
    }
    .sec_dvsn {
        border-left: 1px solid #f7a654;
        width: 50%;
        float:left;
    }
    .sec_dvsn h4 {
        border-bottom: 1px solid #f7a654;
        background: none;
        padding: 10px;
        margin: 0;
        color: #fe9b35;
    }
    .sec_dvsn_wrp {
       padding: 8px;
    }
</style>

@{
    AjaxOptions options = new AjaxOptions();
    options.HttpMethod = "POST";
    options.Confirm = "Are You Sure You Want To Save?";
    options.OnSuccess = "BondConvOnSuccess";
    options.OnFailure = "BondConvOnFailure";
    options.InsertionMode = InsertionMode.InsertAfter;
}

@using (Ajax.BeginForm("AddEditImportBondConversion", "DSR_CWCImport", options ))
{
    @Html.AntiForgeryToken()
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-11 col-md-offset-0-5">
                <div class="container-fluid">
                    <div class="Head_h4">
                        <div class="row SignUp_space_bottom">
                            <div class="col-md-12">
                                <h4>Transfer of Import Stock to Bond</h4>
                            </div>
                        </div>

                        <div class="row Form_Space_top SignUp_space_bottom">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 padding_rt">
                                    <label>Transfer No :</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5">
                                    @Html.TextBoxFor(m=>m.MovementNo, new { @class = "form-control input-sm", @readonly = true })
                                </div>
                                <div class="col-md-1-5 col-md-offset-5">
                                    <label>Date :</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5 Date_Img">
                                    <span class="ReQSimbol">*</span>
                                    @*<input class="form-control" type="text" id="MovementDate" readonly="readonly" />*@
                                    @Html.TextBox("MovementDate", "", new { @class = "form-control input-sm", @readonly = true })
                                    @Html.ValidationMessageFor(m => m.MovementDate, "", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div class="sec_dvsn_area">
                            <div class="sec_dvsn" style="border-left:0;">
                                <div class="row">
                                    <div class="col-md-12">
                                        <h4>FROM</h4>
                                    </div>
                                </div>
                                <div class="sec_dvsn_wrp">

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>OBL / HBL NO :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m => m.DestuffingId)
                                                @Html.HiddenFor(m => m.CHAId)
                                                @Html.HiddenFor(m => m.ImporterId)
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.OBLNo, new { @class = "form-control input_sm", @readonly = true , @tabindex = 1, @onkeydown = "PopupActivate('OBLModal');" })
                                                <span class="search" id="OBLSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" id="OBL"  data-target="#OBLModal"></i></span>
                                                @Html.ValidationMessageFor(m=>m.OBLNo)
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>OBL / HBL Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.OBLDate, new { @class = "form-control input_sm", @readonly = true })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Godown Name :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m => m.FromGodownId)
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.FromGodownName, new { @class = "form-control input_sm", @readonly = true, onkeydown = "PopupActivate('godownModalOBL');" })
