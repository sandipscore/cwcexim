                                            </div>
                                        </div>
                                    </div>

                                   
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Location :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.HiddenFor(m=>m.FromLocationId)
                                                @Html.TextBoxFor(m => m.FromLocationName, new { @class = "form-control input_sm", @readonly = true })                                                
                                            </div>
                                        </div>
                                    </div>

                                   
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>No of Pkg :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @*@Html.TextBoxFor(m => m.NoOfPkg, new { @class = "form-control input_sm", @readonly = true })*@  
                                                @Html.TextBoxFor(m => m.NoOfPkg, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })  
                                                <input type="hidden" id="hdnNoOfPkg" value="0"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>Weight(kg) :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @*@Html.TextBoxFor(m => m.Weight, new { @class = "form-control input_sm", @readonly = true })*@   
                                                @Html.TextBoxFor(m => m.Weight, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })  
                                                <input type="hidden" id="hdnWeight" value="0" />                                           
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>SQM :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @*@Html.TextBoxFor(m => m.SQM, new { @class = "form-control input_sm",@readonly = true })*@ 
                                                @Html.TextBoxFor(m => m.SQM, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })
                                                <input type="hidden" id="hdnSQM" value="0" />                                                
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top" style="display:none;">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>CUM :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CUM, new { @class = "form-control input_sm", @readonly = true })                                                
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>CIF :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @*@Html.TextBoxFor(m => m.CIF, new { @class = "form-control input_sm", @readonly = true })*@  
                                                @Html.TextBoxFor(m => m.CIF, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })
                                                <input type="hidden" id="hdnCIF" value="0" />                                               
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-3 padding_rt">
                                                <label>DUTY :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @*@Html.TextBoxFor(m => m.Duty, new { @class = "form-control input_sm", @readonly = true })*@   
                                                @Html.TextBoxFor(m => m.Duty, new { @class = "form-control input_sm", onkeypress = "return AllowOnlyDecimal(event)", onchange = "ValidOBLTextBoxes()" })                                               
                                                <input type="hidden" id="hdnDuty" value="0" />
                                            </div>
                                        </div>
                                    </div>                                    
                                </div>
                            </div>

                            <div class="sec_dvsn">
                                <div class="row">
                                    <div class="col-md-12">
                                        <h4>TO</h4>
                                    </div>
                                </div>
                                <div class="sec_dvsn_wrp">
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>SAC No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                @Html.HiddenFor(m => m.SACId)
                                                @Html.TextBoxFor(m => m.SACNo, "", new { @class = "form-control input-sm", @readonly = true, @tabindex = 2,onkeydown = "PopupActivate('SacModal');" })
                                                <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="FocusOnSacNoSearch()" data-target="#SacModal"></i></span>
                                                @Html.ValidationMessageFor(m => m.SACNo, "", new { @class = "Error_Msg" })
                                                
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>SAC Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                @Html.TextBoxFor(m => m.SACDate, "", new { @class = "form-control input-sm", @readonly = true ,@tabindex = 3})
                                                
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Godown Name :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.GodownName, new { @class = "form-control input-sm", @readonly = true, @tabindex = 4 , onkeydown = "PopupActivate('godownModal');" })
                                                <span class="search" id="SpanGodownSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="FocusOnSacGodown()" data-target="#godownModal"></i></span>
                                                @Html.HiddenFor(m => m.GodownId)
                                                @Html.ValidationMessageFor(m => m.GodownName, "", new { @class = "Error_Msg" })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Location :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_6 Date_Img">
                                                @*<input class="form-control input-sm" id="location" name="location" readonly="true" type="text" value="">
    <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#LocationModal"></i></span>*@
                                                @Html.HiddenFor(m => m.LocationId)
                                                @Html.TextBoxFor(m => m.Location, "", new { @class = "form-control input-sm", @readonly = true, @tabindex = 5, @onkeydown = "PopupActivate('LocationModal');" })
                                                <span id="LocationSearch" class="search"><i class="fa fa-search" aria-hidden="true"></i></span>
                                                @*@Html.ValidationMessage("Location", new { @class = "Error_Msg" })*@
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>WR No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.WRNo, new { @class = "form-control input_sm" , @tabindex = 6, })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>WR Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                @Html.TextBoxFor(m => m.WRDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 7  })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Bond No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.BondNo, new { @class = "form-control input_sm" , @tabindex = 8 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Bond Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                <span class="ReQSimbol">*</span>
                                                @Html.TextBoxFor(m => m.BondDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 9 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Bond No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CustomBondNo, new { @class = "form-control input_sm", @tabindex = 10 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Bond Date :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5 Date_Img">
                                                @Html.TextBoxFor(m => m.CustomBondDate, new { @class = "form-control input_sm", @readonly = true, @tabindex = 11 })
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row Form_Space_top">
                                        <div class="form-group Form_Input">
                                            <div class="col-md-4 padding_rt">
                                                <label>Custom Seal No :</label>
                                            </div>
                                            <div class="col-md-6 col_cus_5">
                                                @Html.TextBoxFor(m => m.CustomSealNo, new { @class = "form-control input_sm", @tabindex = 12 })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="ClearFix"></div>
                        </div>                        

                        <div class="row SignUp_space_top SignUp_space_bottom"></div>


                        <div class="row SignUp_space_bottom">
                            <div class="col-md-offset-4 col-md-2">
                                <input type="submit" value="Save" name="Command" class="btn log_Btn_sm" id="BtnSave" tabindex = 13 />
                            </div>
                            <div class="col-md-2">
                                <input type="button" id="btnReset" class="btn log_Btn_sm" value="Reset" onclick="ResetMovement()" tabindex = 14 />
                            </div>
                        </div>
                        <div class="logSuccMsg" id="DivSucMsg" style="background-color:transparent"></div>
                        <div class="logErrMsg Form_Space_top" id="DivErrMsg" style="background-color:transparent"></div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="DivMovementList"></div>
                        </div>
                    </div>                   
                </div>                
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jqueryval")
}

