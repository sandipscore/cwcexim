    function SearchContainerWise() {
        //debugger;
        var OBLn = $('#OBLNo').val();
        if ($('#ContainerWiseSearch').val() == "1") {
            OBLn = '';
        }
        $.ajax({
            url: '/Import/DSR_CWCImport/SearchContainerWise',
            type: 'GET',
            dataType: 'JSON',
            data: { OBL: OBLn },
            success: function (data) {
                //-------------------------------------------------------------
                //debugger;
                if (ArrayOfCustomAppraisement.length > 0) {

                    data.filter(myFunc);

                    function myFunc() {
                        //debugger;
                        for (var i = 0; i < ArrayOfCustomAppraisement.length; ++i) {
                            // to remove every occurrence of the matched value
                            for (var j = data.length; j--;) {
                                if (data[j].ContainerNo == ArrayOfCustomAppraisement[i].ContainerNo) {
                                    // remove the element
                                    data.splice(j, 1);
                                }
                            }
                        }
                    }

                }
                //-------------------------------------------------------------

                var html = '';
                oblcount = 0;
                $.each(data, function (item, elem) {
                    oblcount = oblcount + 1;
                    html += '<li id="' + elem.CFSCode + '"  onclick="SelectContainer(&quot;' + elem.ContainerNo + '&quot;,&quot;' + elem.CFSCode + '&quot;)">' + elem.ContainerNo + '</li>';
                });

                $('#ContainerList').html(html);
                $('#ContainerModal').find('#ContainerBox').focus();

            }
        });
    }
    function SearchOBLWise() {
        //debugger;
