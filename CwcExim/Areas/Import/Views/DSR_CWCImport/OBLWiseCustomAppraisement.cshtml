                $.each(data, function (item, elem) {
                    var tabvalue = 2;
                    oblcount = oblcount + 1;
                    html += '<li tabindex=' + tabvalue + '_'+ item +' id="' + elem.OBLDate + '"onclick="GetOBldtl(&quot;' + elem.OBLNo + '&quot;,&quot;' + elem.OBLDate + '&quot;,&quot;' + elem.NoOfPackages + '&quot;,&quot;' + elem.GrossWeight + '&quot;,&quot;' + elem.CIFValue + '&quot;,&quot;' + elem.ImporterId + '&quot;,&quot;' + elem.Importer +'&quot;,&quot;' + elem.ShippingLineId + '&quot;,&quot;' + elem.ShippingLine +  '&quot;,&quot;' + elem.CargoDescription + '&quot;,&quot;' + "" + '&quot;,&quot;' + "" + '&quot;,&quot;'+elem.CHAId+'&quot;,&quot;'+elem.CHAName+'&quot;)" onkeypress="GetOBldtl(&quot;' + elem.OBLNo + '&quot;,&quot;' + elem.OBLDate + '&quot;,&quot;' + elem.NoOfPackages + '&quot;,&quot;' + elem.GrossWeight + '&quot;,&quot;' + elem.CIFValue + '&quot;,&quot;' + elem.ImporterId + '&quot;,&quot;' + elem.Importer + '&quot;,&quot;' + elem.CargoDescription + '&quot;,&quot;' + "" + '&quot;,&quot;' + "" + '&quot;,&quot;'+elem.CHAId+'&quot;,&quot;'+elem.CHAName+'&quot;)">' + elem.OBLNo + '</li>';
                });

                if ($('#OBLWiseSearch').val() == '1') {
                    oblcount = 1;
                }
                $('#ContainerOBLList').html(html);
