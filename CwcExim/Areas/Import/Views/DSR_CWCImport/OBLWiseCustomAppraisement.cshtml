                    function myFunc() {
                        //debugger;
                        for (var i = 0; i < ArrayOfCustomAppraisement.length; ++i) {
                            // to remove every occurrence of the matched value
                            for (var j = data.length; j--;) {
                                if (data[j].OBLNo == ArrayOfCustomAppraisement[i].OBLNo) {
                                    // remove the element
                                    data.splice(j, 1);
                                }
                            }
