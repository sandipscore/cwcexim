    $('#ShpngLinebox').keyup(function () {
        debugger;
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#tbltbodyShippingLine > tr').show();
        else {
            $('#tbltbodyShippingLine > tr').each(function () {
                debugger;
                var text = $(this).find('td').text().toLowerCase();
               
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });





    $('#TrainNobox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstTrainNoNo > li').show();
        else {
            $('#lstTrainNoNo > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
    $('#lstTrainNoNo > li').on("click", function () {
        debugger;
        $('#TrainNo').val($(this).text().split('(')[0]);
        $('#TrainSummaryID').val($(this).attr('id'));
        $("#blNomodal").modal("hide");
        angular.element('#JobOrderAppDiv').scope().GetTrainDetails();
        angular.element('#JobOrderAppDiv').scope().$apply();
        //GetTrainDetails();
    });

    function SearchByPartyCode() {
        if ($('#ShpngLinebox').val() != '') {
            $.ajax({
