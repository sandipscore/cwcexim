<style>
    #DivFormOneDet {
        height: 330px;
        overflow-x: hidden;
    }
    #tbleSline.dataTable tbody > tr > td {
        cursor: pointer;
    }
    .slimScrollDiv {
        height: 300px;
    }
    .table.tblhd {
        border-bottom: 0;
    }
    .slim_tble{
        width:100%;
        table-layout: fixed;
    }
    .txt_wrp{
         overflow:hidden; 
         word-wrap:break-word;
    }
   #ShippingLineModalBody {
        height: 300px;
    overflow-x: hidden;
    }
