    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.StringifyXML)
    <div id="JobOrderByRoadAppDiv" ng-controller="WFLDJobOrderByRoadCtrl">
        <div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-offset-0-5 col-md-11">
                        <div class="Head_h4">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4>Job Order By Road</h4>
                                </div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Job Order No:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.FormOneNo, new { @class = "form-control input-sm", @readonly = true })
                                        @Html.HiddenFor(m => m.FormOneId)
                                    </div>

                                    <div class="col-md-offset-4-5 col-md-1-5 col_cus_2 padding_rt">
                                        <label>Job Order Date:</label>
                                    </div>
                                    <div class="col-md-2 col_cus_2_5">
                                        @*@Html.TextBoxFor(m => m.JobOrderDate, DateTime.Now.ToString("dd/MMM/yyyy hh:mm"), new { @class = "form-control input-sm", @readonly = true })*@

                                        <input type="text" id="FormOneDate"
                                               name="date"
                                               placeholder="dd/mm/yyyy"
                                               onkeyup="var v = this.value;
                                                if (v.match(/^\d{2}$/) !== null) {
                                                this.value = v + '/';
                                                } else if (v.match(/^\d{2}\/\d{2}$/) !== null) {
                                                this.value = v + '/';
                                                }"
                                               maxlength="10" />

                                        <span style="color:red" id="ErrJobOrderDate"></span>
                                    </div>
                                </div>
                            </div>

                            <div class="row Form_Space_top" ng-hide="true">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Shipping Line Name:</label>
                                    </div>
                                    <div class="col-md-10-5 col_cus_10">
                                        @*<input type="text" ng-model="ShippingLineName" id="ShippingLineName" ng-disabled="true" required />
                                            <span class="search" style="top: 5px; right: 15px;"><i class="fa fa-search" aria-hidden="true" ng-model="Slmodal" ng-click="onShippingLineChange($index)" data-toggle="modal" data-target="#slmodal"></i></span>
                                            <input type="text" ng-hide="true" ng-model="ShippingLineId" />*@
                                        @Html.HiddenFor(m => m.ShippingLineId, new { @value = "0" })
                                        @Html.TextBoxFor(m => m.ShippingLineName, new { @class = "form-control input-sm", @readonly = true })
                                        <span class="search" id="ShippingLineSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#slmodal"></i></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2 padding_rt">
                                        <label>Transport By:</label>
                                    </div>
                                    <div class="col-md-2-5">
                                        @Html.TextBoxFor(m => m.TransportBy, new { @class = "form-control input-sm" , Value="Road" })
                                    </div>
                                    <div class="col-md-offset-4-5 col-md-1-5 col_cus_2 padding_rt">
                                        <label>Container / CBT:</label>
                                    </div>
                                    <div class="col-md-2">
                                        @Html.DropDownListFor(m => m.CONTCBT, new List<SelectListItem>
                                      {
                                      new SelectListItem { Text="Container",Value="CONT"},
                                      new SelectListItem { Text="CBT",Value="CBT"},
                                      }, "----Select----", new { @class = "form-control input-sm" })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-offset-5 col-md-2" style="text-align:center">
                                    <input type="button" value="Add Row" ng-show="ishide" class="btn log_Btn_sm" ng-click="AddJobOrderByRoad()" />
                                </div>
                            </div>

                            <div class="row">
                                <div class="SignUp_space_bottom SignUp_space_top"></div>
                            </div>

                            <div class="brdr_sec">
                                <div class="Form_Space_top" ng-hide="!JODetails.length">
                                    <div id="DivFormOneDet">
                                        <table class="table table-bordered table-striped dataTable" style="width:100%;">
                                            <thead>
                                                <tr>
                                                    <th width="5%">Sl No</th>
                                                    <th>Container / CBT No.</th>
                                                    <th>Size</th>
                                                    <th>Is ODC </th>
                                                    <th>Shipping Line</th>
                                                    <th>Container / CBT Load Type</th>
                                                    <th>Foreign Liner</th>
                                                    <th>Vessel Name</th>
                                                    <th>Vessel No</th>
                                                    <th width="8%" style="text-align:center;" ng-show="ishide">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody id="tblshippingMainTable">
                                                <tr ng-repeat="j in JODetails">
                                                    <td width="5%">{{$index+1}}<input type="hidden" ng-model="j.ID" /></td>
                                                    <td><input type="text" ng-model="j.ContainerNo" ng-disabled="isView" ng-blur="onchangetext(j);"  /></td>
                                                    <td>
                                                        <select ng-model="j.ContainerSize" ng-disabled="isView">
                                                            <option value="">--Select--</option>
                                                            <option value="{{t.id}}" ng-repeat="t in ContainerSizeList">{{t.ContainerSize}}</option>
                                                        </select>
                                                        <input type="hidden" ng-model="j.FormOneDetailID" />
                                                    </td>
                                                    <td>
                                                        <select ng-model="j.IsODC" ng-disabled="isView">
                                                            <option value="">--Select--</option>
                                                            <option value="{{t.id}}" ng-repeat="t in IsODCList">{{t.IsODC}}</option>
                                                        </select>
                                                    </td>
                                                    <td style="position:relative;">
                                                        <input type="text" ng-model="j.ShippingLineName"  readonly required id="ShippingLineName_{{$index}}" onkeypress="callmodal()" />
                                                        <input type="text" ng-hide="true" ng-model="j.ShippingLineId" />
                                                        <span class="search" style="top: 6px; right: 13px;" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" ng-model="j.Slmodal" ng-click="onShippingLineChange($index)" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                                    </td>
                                                    <td>
                                                        <select ng-model="j.LCLFCL" ng-disabled="isView" id="txtLCLFCLFocus">
                                                            <option value="">--Select--</option>
                                                            <option value="{{t.id}}" ng-repeat="t in ContainerLoadTypeList">{{t.ContainerLoadType}}</option>
                                                        </select>
                                                    </td>
                                                    <td><input type="text" ng-model="j.ForeignLiner" ng-disabled="isView" ng-blur="onchangetext(j);" /></td>
                                                    <td><input type="text" ng-model="j.VesselName" ng-disabled="isView" ng-blur="onchangetext(j);" /></td>
                                                    <td><input type="text" ng-model="j.VesselNo" ng-disabled="isView" ng-blur="onchangetext(j);" /></td>
                                                    <td width="8%" style="text-align:center;" ng-show="ishide">
                                                        <a href="#" ng-click="Delete($index)"><i class="fa fa-window-close-o Delete" aria-hidden="true"></i></a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="SignUp_space_bottom SignUp_space_top"></div>
                            </div>

                            <div class="row Form_Space_top">
                                <div class="form-group Form_Input">
                                    <div class="col-md-1-5 col_cus_2">
                                        <label>Remarks:</label>
                                    </div>
                                    <div class="col-md-10-5 col_cus_10">
                                        @Html.TextAreaFor(m => m.Remarks, new { @class = "form-control input-sm", @cols = "20", @maxlength = "250", @rows = "2" })
                                    </div>
                                </div>
                            </div>

                            <div class="SignUp_space_bottom SignUp_space_top"></div>

                            <div class="row">
                                <div class="col-md-offset-4 col-md-2" style="text-align:center">
                                    <input type="submit" id="BtnYardSubmit" value="Save" ng-show="isSave" class="btn log_Btn_sm" ng-click="OnJobOrderSave()" /> @*onclick="datevalidation();"*@
                                </div>
                                <div class="col-md-2">
                                    <input type="button" value="Reset" class="btn log_Btn_sm" ng-click="ResetImpJODetails()" />
                                </div>
                            </div>

                            <div class="logSuccMsg" style="background-color:transparent" id="DivJobOrder"></div>
                            <div class="logErrMsg Form_Space_bottom" style="background-color:transparent" id="DivJobOrderErrMsg"></div>


                            @*<div class="logSuccMsg Form_Space_top SignUp_space_bottom2" style="background-color:transparent">
                                    <div id="DivYardMsg"></div>
                                </div>

                                <div id="DivYardErrMsg" class="logErrMsg" style="background-color:transparent"></div>*@
                            <div class="row Form_Space_top Form_Space_bottom">
                                <div class="form-group Form_Input">
                                    <div class="col-md-offset-9 col-md-3">
                                        @Html.TextBox("ContainerNoSearch", "", new { placeholder = "Search By Container / CBT" })
                                        <span class="search"><i class="fa fa-search" aria-hidden="true" onclick="SearchByContainerNo()"></i></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2 SignUp_space_bottom">
                                    <input type="button" id="btnJobLoad" value="Show List" ng-show="isShow" class="btn log_Btn_sm" onclick="LoadJobEntryList()"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 SignUp_space_bottom">
                                    <div id="DivYardList"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            @Scripts.Render("~/bundles/jqueryval")

            <script type="text/javascript" src="/scripts/jquery.nicescroll.min.js"></script>
            <script type="text/javascript">
                $(function () {
                    //$('#slim_scroll').slimScroll({
                    //   // allowPageScroll: true
                    //});
                    $("#CONTCBT").focus();
                    //$("#ShpngLinebox").focus();
                    $("#TrainNobox").focus();

                   
                   
                });

                function callmodal()
                {
                    
                    $("#ShippingLineModal").modal('show');
                    $('#ShippingLineModal').on('shown.bs.modal', function () {
                        $('#ShpngLinebox').focus();
                    })
                    var tabindex = 102;
                    debugger;
                    $('#tbleSline  tr').each(function () {
                        if (this.type != "hidden") {
                            var $input = $(this);
                            $input.attr("tabindex", tabindex);
                            tabindex++;
                        }
                    });
                
                }


               
            </script>

            <div id="blNomodal" class="modal fade" role="dialog">
                <div class="modal-dialog">
                    <!-- Modal content-->
                    <div class="modal-content popup_modal">
                        <div class="modal-header popup_header">
                            <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                            <h4 class="modal-title">List of Train No.</h4>
                        </div>
                        <div class="modal-body popup_body">
                            <input placeholder="Search Me" id="TrainNobox" type="text" />
                            <ul class="navList" id="lstTrainNoNo" ng-click="GetTrainDetails()">
                                @{
                                    if (ViewBag.ListOfTrain != null)
                                    {
                                        foreach (var item in ViewBag.ListOfTrain)
                                        {
                                            <li id="@item.TrainSummaryID">@item.TrainNo</li>
                                        }
                                    }
                                }
                            </ul>
                        </div>
                        <div class="modal-footer">
                            <div class="row">
                                <div class="col-md-offset-6 col-md-6">
                                    <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="ShippingLineModal" class="modal fade" role="dialog">
                <div class="modal-dialog">
                    <!-- Modal content-->
                    <div class="modal-content popup_modal">
                        <div class="modal-header popup_header">
                            <button type="button" class="close" onclick="CloseEximTrader()"><i class="fa fa-times" aria-hidden="true"></i></button>
                            <h4 class="modal-title">List of Shipping Line</h4>
                        </div>
                        <div class="modal-body popup_body" style="position:relative;" id="ShippingLineModalBody">
                            <input placeholder="Search By Shipping Line Name Or Party Code" id="ShpngLinebox" type="text" autofocus tabindex="101"/>
                            <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick="SearchByPartyCode()"></i></span>

                            <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;" tabindex="101">
                                <thead>                                    
                                    <tr>
                                        <th colspan="8" width="80%">Shipping Line Name</th>
                                        <th colspan="4" width="20%">Party Code</th>
                                    </tr>
                                </thead>
                            </table>
                            <div id="slim_scroll">
                                <table class="table dataTable table-bordered table-striped slim_tble" id="tbleSline">
                                    <tbody id="tbltbodyShippingLine">
                                        @{
                                            if (ViewBag.lstShippingLine != null)
                                            {
                                                foreach (var item in ViewBag.lstShippingLine)
                                                {
                                                    <tr onkeypress="SelectShippingLineEnter(@item.ShippingLineId,'@item.ShippingLineName')" onclick="SelectShippingLine(@item.ShippingLineId,'@item.ShippingLineName')">
                                                        <td colspan="8" width="80%" class="txt_wrp">@item.ShippingLineName</td>
                                                        <td colspan="4" width="20%" class="txt_wrp">@item.PartyCode</td>
                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <div class="row">
                                <div class="col-md-6">
                                    @if (ViewBag.State != null && Convert.ToBoolean(ViewBag.State) == true)
                                    {
                                        <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreEximTrader()" class="btn log_Btn_sm" id="btnLoadMore">Load More Data</button>
                                    }
                                    else
                                    {
                                        <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreEximTrader()" class="btn log_Btn_sm" id="btnLoadMore" disabled>Load More Data</button>
                                    }
                                </div>
                                <div class="col-md-6">
                                    <button type="button" class="btn log_Btn_sm" onclick="CloseEximTrader()">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

}
