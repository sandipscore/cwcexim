    function SearchImporterByPartyCode() {
        debugger;
        if ($('#lstImp').val() != '') {
            $.ajax({
                url: '/Import/VRN_CWCImport/ImporterSearchByPartyCode',
                type: 'GET',
                data: { PartyCode: $('#lstImp').val() },
                success: function (data) {
                    debugger;
                    if (data.Status == 1) {
                        var html = '';
                        var tab = 401;
                        $.each(data.Data.lstImporter, function (item, elem) {
                            html += '<tr tabindex=' + tab + '_' + item + ' onkeypress="SelectImporter(' + elem.ImporterId + ',&quot;' + elem.ImporterName + '&quot;)" onclick="SelectImporter(' + elem.ImporterId + ',&quot;' + elem.ImporterName + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.ImporterName + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.ImporterCode + '</td></tr>';
                        });
                        $('#tbleImporterDtl tbody').html('');
                        $('#tbleImporterDtl tbody').html(html);
                        $('#btnLoadImpdtl').prop('disabled', false);
                        Page = -1;
                    }
                    else {
                        $('#tbleImporterDtl tbody').html('');
                        Page = -1;
                        $('#btnLoadImpdtl').prop('disabled', false);
                    }

                }
            });
        }
       // else
           // LoadCHA();
    }
    function ImporterModalClose() {
        $('#ImporterModal').modal('hide');
    }
