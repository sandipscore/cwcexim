    var oblcount = 0;
    function GetContainerOBL(CFSCode) {
        debugger;
        //var CFSCode = $('#CFSCode').val();
        if (CFSCode == '' || CFSCode == null) {
            alert('Please Enter Container');
            return false;
        }
        $.ajax({
            url: '/Import/VRN_CWCImport/GetContainerBOL',
            type: 'GET',
            dataType: 'JSON',
            data: { CFSCode: $('#CFSCode').val() },
            success: function (data) {
                debugger;
                var html = '';
                var tab = 401;
                oblcount = 0;
                $.each(data, function (item, elem) {
                    oblcount = oblcount + 1;
                    html += '<li tabindex=' + tab + '_' + item + ' id="' + elem.OBLDate + '" " onkeypress="GetOBldtl(&quot;' + elem.OBLNo + '&quot;,&quot;' + elem.OBLDate + '&quot;,&quot;' + elem.NoOfPackages + '&quot;,&quot;' + elem.GrossWeight + '&quot;,&quot;' + elem.CIFValue + '&quot;)" onclick="GetOBldtl(&quot;' + elem.OBLNo + '&quot;,&quot;' + elem.OBLDate + '&quot;,&quot;' + elem.NoOfPackages + '&quot;,&quot;' + elem.GrossWeight + '&quot;,&quot;' + elem.CIFValue + '&quot;)">' + elem.OBLNo + '</li>';
                });
