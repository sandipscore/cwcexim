    function SearchSAC()
    {
        var SacBox = $('#SacBox').val();
        $.ajax({
            url: '/Import/Wlj_CWCImport/GetSacNo',
            type: 'GET',
            dataType: 'JSON',
            data: { "SacNo": SacBox },
            success: function (data) {
                if (data.Status == 1) {
                    var Html = '';
                    $.each(data.Data, function (i, item) {
                        Html += '<tr onclick="SelectSac(' + item.SACId + ',&quot;' + item.SACNo + '&quot;,&quot;' + item.SACDate + '&quot;)"><td colspan="12" width="100%">' + item.SACNo + '</td></tr>';
                    });
                    $('#tbleSac').html(Html);
                }
            }
        });
    }
