    function GetOBLDet(DestuffingEntryId,OBLNo) {
        $.ajax({
            url: '/Import/Wlj_CWCImport/GetOBLDetailsByYard',
            type: 'GET',
            async: false,
            datatype: JSON,
            data: { "DestuffingEntryId": DestuffingEntryId, "OBLNo": OBLNo },
            success: function (data) {
                if (data.Status == 1) {
                    $('#FromGodownId').val(data.Data.FromGodownId);
                    $('#FromGodownName').val(data.Data.FromGodownName);
                    $('#FromLocationId').val(data.Data.FromLocationId);
                    $('#FromLocationName').val(data.Data.FromLocationName);
                    $('#NoOfPkg').val(data.Data.NoOfPkg);
                    $('#Weight').val(data.Data.Weight);
                    $('#SQM').val(data.Data.SQM);
                    $('#CUM').val(data.Data.CUM);
                    $('#CIF').val(data.Data.CIF);
                    $('#Duty').val(data.Data.Duty);
                }
            }
        });
    }


