    function CalcTax(elem) {
        debugger;
        var rawJson = JSON.parse($('#PaymentSheetModelJson').val());
        var MAmt = Number($('#tax' + elem).val());
        var IP = 0;
        var CP = 0;
        var SP = 0;
        //if (rawJson.InvoiceType == "Tax") {
            if (TaxType == "Tax") {
           
         
                if ($('#IsPartyStateInCompState').val() == "true") {
                    CP = Number($('#CGSTP' + elem).val());
                    SP = Number($('#SGSTP' + elem).val());
                }
                else {
                    IP = Number($('#IGSTP' + elem).val());
                }
                //if (parseFloat(rawJson.lstPostPaymentChrg[0].CGSTAmt) > 0 || parseFloat(rawJson.lstPostPaymentChrg[0].SGSTAmt) > 0)
                //{
                //    CP = Number($('#CGSTP' + elem).val());
                //    SP = Number($('#SGSTP' + elem).val());
                //}
                //else if (parseFloat(rawJson.lstPostPaymentChrg[0].IGSTAmt) > 0)
                //{
                //    IP = Number($('#IGSTP' + elem).val());
                //}

        }
        var CWCCharge = $.grep(rawJson.lstPostPaymentChrg, function (item) { return item.ChargeType == "CWC"; });

        //$('#IGSTA' + elem).val((MAmt * (IP / 100)).toFixed(2));
        //$('#CGSTA' + elem).val((MAmt * (CP / 100)).toFixed(2));
        //$('#SGSTA' + elem).val((MAmt * (SP / 100)).toFixed(2));
        //$('#TOT' + elem).val((MAmt + (MAmt * (IP / 100)) + (MAmt * (CP / 100)) + (MAmt * (SP / 100))).toFixed(2));

        $('#IGSTA' + elem).val(parseFloat(MAmt * (IP / 100)).toFixed(2));
        $('#CGSTA' + elem).val(parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2).toFixed(2));
        $('#SGSTA' + elem).val(parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2).toFixed(2));
        $('#TOT' + elem).val(parseFloat((MAmt + (MAmt * (IP / 100)) + parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2) + parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2))).toFixed(2));
        var selectedItem = $.grep(rawJson.lstPostPaymentChrg, function (item) { return item.ChargeId == elem; })[0];
        if (selectedItem == undefined)
            selectedItem = $.grep(rawJson.lstPostPaymentChrg, function (item) { return item.ChargeId + '_' + (indx - 1) == elem; })[0];
        if(selectedItem != undefined)
        {

      
       // selectedItem.Amount = MAmt;
            selectedItem.Taxable = MAmt;
            selectedItem.Discount = parseFloat($('#dis' + elem).val() == '' ? '0' : $('#dis' + elem).val());
        selectedItem.IGSTPer = IP;
        selectedItem.IGSTAmt = MAmt * (IP / 100);
        selectedItem.CGSTPer = CP;
        selectedItem.CGSTAmt = MAmt * (CP / 100);
        selectedItem.SGSTPer = SP;
        selectedItem.SGSTAmt = MAmt * (SP / 100);
        selectedItem.Total = parseFloat((MAmt + (MAmt * (IP / 100)) + parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2) + parseFloat(Math.ceil(MAmt * (CP + SP) / 100) / 2)));
        
        }

        var TotalCWC = 0;
        //var CWCCharge = $.grep(rawJson.lstPostPaymentChrg, function (item) { return item.ChargeType == "CWC"; });

        rawJson.TotalCGST = 0;
        rawJson.TotalSGST = 0;
        rawJson.TotalIGST = 0;
        rawJson.TotalAmt = 0;

        $.each(CWCCharge, function (i, item) {
            rawJson.TotalAmt += item.Amount;
            rawJson.TotalCGST += item.CGSTAmt;
            rawJson.TotalSGST += item.SGSTAmt;
            rawJson.TotalIGST += item.IGSTAmt;
            TotalCWC += item.Total;
        });
        $('#TOTCWCChrg').val((TotalCWC).toFixed(2));
        //$('#TOTCWCTDS').val(((TotalCWC * 10) / 100).toFixed(2));
        rawJson.CWCTotal = TotalCWC;
        var TotalHT = 0;
        var HTCharge = $.grep(rawJson.lstPostPaymentChrg, function (item) { return item.ChargeType == "HT"; });

        $.each(HTCharge, function (i, item) {
            rawJson.TotalAmt += item.Amount;
            rawJson.TotalCGST += item.CGSTAmt;
            rawJson.TotalSGST += item.SGSTAmt;
            rawJson.TotalIGST += item.IGSTAmt;
            TotalHT += item.Total;
        });
        $('#TOTHTChrg').val((TotalHT).toFixed(2));
        //$('#TOTHTTDS').val(((TotalHT * 2) / 100).toFixed(2));
        rawJson.HTTotal = TotalHT;
        $('#AllTotal').val((TotalCWC + TotalHT).toFixed(2));
        rawJson.AllTotal = (TotalCWC + TotalHT);
        $('#PaymentSheetModelJson').val(JSON.stringify(rawJson));
        CalcInvoice();
    }

