@model CwcExim.Areas.Import.Models.OBLWiseContainerEntryV2
<script src="~/Areas/Import/Scripts/App/Services/OBLWiseContainerServiceV2.js"></script>
<script src="~/Areas/Import/Scripts/App/Controllers/OBLWiseContainerCtrlV2.js"></script>
<script>
    angular.bootstrap(document.getElementById('OBLWiseContainerEntryAppDiv'), ['CWCApp']);
</script>
<style>
    #DivFormOneDet {
        height: 396px;
        overflow-x: hidden;
    }

    #tbleCommodity.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }

    .slim_tble {
        width: 100%;
        table-layout: fixed;
    }

    .txt_wrp {
        overflow: hidden;
        word-wrap: break-word;
    }

    #tbleSline.dataTable tbody > tr > td {
        cursor: pointer;
    }

    #tbleImp.dataTable tbody > tr > td {
        cursor: pointer;
    }
</style>
<div id="OBLWiseContainerEntryAppDiv" ng-controller="OBLWiseContainerEntryCtrl">
    <div class="row">
        <div class="col-md-12">
            <div class="Head_h4">
                <div class="row">
                    <div class="col-md-12">
                        <h4>OBL Wise Container / CBT Entry</h4>
                    </div>
                </div>
                @{
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(m => m.StringifiedText)
                    @Html.HiddenFor(m => m.impobldtlId)
                    @*@Html.HiddenFor(m => m.IsAlreadyUsed)*@
                if (ViewBag.ListOfContainerNo != null)
                {
                    <input type="hidden" id="hdnListOfContainerNo" value="@ViewBag.ListOfContainerNo" />
                }
                else
                {
                    <input type="hidden" id="hdnListOfContainerNo" value="" />
                }
                <div class="content_wrp">
                    <div id="DivJOHdr" class="Form_Space_bottom">
                        @Html.HiddenFor(m => m.IcesData, new { @value = '0' })

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>OBL/HBL No:</label>
                                    @Html.TextBoxFor(m => m.OBL_No, new { placeholder = "Enter OBL/HBL No" })
                                </div>
                            </div>
                            <div class="col-md-2 UploadFileBtn">
                                <input type="button" id="btnShowDetails" class="btn btn-primary" ng-model="btnGet" ng-hide="Action" value="Show Details" ng-click="GetOBLDetails()" />
                            </div>
                            <div class="col-md-3 col-md-offset-4">
                                <div class="form-group">
                                    <label>OBL/HBL Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.OBL_Date, new { @size = 10, @maxlength = 10, @onkeyup = "check()", @onblur = "checkdate()", placeholder = "Enter OBL/HBL Date" })
                                        <div class="field-validation-error" id="DivMsgs" style="background-color:transparent"></div>
                                        @Html.ValidationMessageFor(m => m.OBL_Date, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>SMTP No:</label>
                                    @Html.TextBoxFor(m => m.SMTPNo, new { placeholder = "Enter SMTP No" })
                                    @Html.ValidationMessageFor(m => m.SMTPNo, "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>SMTP Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.SMTP_Date, new { @size = 10, @maxlength = 10, @onkeyup = "SMTPcheck()", @onblur = "checkSMTPdate()", placeholder = "Enter SMTP Date" })
                                        <div class="field-validation-error" id="DivMsgs" style="background-color:transparent"></div>
                                        @Html.ValidationMessageFor(m => m.SMTP_Date, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Cargo Description:</label>
                                    @Html.TextAreaFor(m => m.CargoDescription, new { @rows = "3", @maxlength = "1000", placeholder = "Enter Cargo Description" })
                                    @Html.ValidationMessageFor(m => m.CargoDescription, "", new { @class = "Error_Msg" })
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Commodity:</label>
                                    <div class="position-relative">
                                        @Html.TextBoxFor(m => m.Commodity, "", new { @readonly = true, placeholder = "Enter Commodity" })
                                        <span class="input_icon"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#CommodityModal"></i></span>
                                        @Html.HiddenFor(m => m.CommodityId)
                                        @Html.ValidationMessageFor(m => m.Commodity, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Cargo Type:</label>
                                    @Html.HiddenFor(m => m.CargoType)
                                    @Html.DropDownList("ddlCargoType", new List<SelectListItem>
                                   {
                                       new SelectListItem {Text="HAZ",Value="1" },
                                       new SelectListItem {Text="Non-HAZ",Value="2" }
                                   }, "----Select----", new { @class = "dropdown", @onchange = "$('#CargoType').val($('#ddlCargoType').val())" })
                                    @Html.ValidationMessageFor(m => m.CargoType, "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>No Of Pkg.:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.NoOfPkg, new { placeholder = "Enter No Of Pkg" })
                                        @Html.ValidationMessageFor(m => m.NoOfPkg, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Pkg Type:</label>
                                    @Html.TextBoxFor(m => m.PkgType, new { placeholder = "Enter Pkg Type" })
                                    @Html.ValidationMessageFor(m => m.PkgType, "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Gross Wt(Kg).:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.GR_WT, new { placeholder = "Enter Gross Wt(Kg)" })
                                        @Html.ValidationMessageFor(m => m.GR_WT, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>ICE Gate Importer:</label>
                                    @Html.TextBoxFor(m => m.ICEGateImporterName, new { placeholder = "Enter ICE Gate Importer" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Movement Type:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.DropDownListFor(m => m.MovementType, new List<SelectListItem>
                               {
                                   new SelectListItem {Text="FCL",Value="FCL" },
                               }, new { @class = "dropdown" })
                                        @Html.ValidationMessageFor(m => m.MovementType, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Importer Name:</label>
                                    <div class="position-relative">
                                        @Html.HiddenFor(m => m.ImporterId)
                                        @Html.TextBoxFor(m => m.ImporterName, new { @readonly = true, placeholder = "Enter Importer Name" })
                                        <span class="input_icon" id="ImporterSerach"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#ImporterModal"></i></span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>IGM Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.IGM_Date, new { @size = 10, @maxlength = 10, @onkeyup = "checks()", @onblur = "checkdates()", placeholder = "Enter IGM Date" })
                                        <div class="field-validation-error" id="DivMsges" style="background-color:transparent"></div>
                                        @Html.ValidationMessageFor(m => m.IGM_Date, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>IGM No:</label>
                                    @Html.TextBoxFor(m => m.IGM_No, new { placeholder = "Enter IGM No" })
                                    @Html.ValidationMessageFor(m => m.IGM_No, "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>TP Date:</label>
                                    <div class="position-relative Date_Img">
                                        @Html.TextBoxFor(m => m.TPDate, new { @size = 10, @maxlength = 10, @onkeyup = "chec()", @onblur = "checkdat()", placeholder = "Enter TP Date" })
                                        <div class="field-validation-error" id="DivMs" style="background-color:transparent"></div>
                                        @Html.ValidationMessageFor(m => m.TPDate, "", new { @class = "Error_Msg" })
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>TP No:</label>
                                    @Html.TextBoxFor(m => m.TPNo, new { placeholder = "Enter TP No" })
                                    @Html.ValidationMessageFor(m => m.TPNo, "", new { @class = "Error_Msg" })
                                </div>
                            </div>

                            <div class="col-md-3 col-md-offset-6">
                                <div class="form-group">
                                    <label>Port Name:</label>
                                    @Html.DropDownListFor(m => m.PortId, new List<SelectListItem>
                               {
                               }, "----Select----", new { @class = "dropdown" })

                                    @Html.ValidationMessageFor(m => m.PortId, "", new { @class = "Error_Msg", placeholder = "Enter Port Name" })
                                    @Html.HiddenFor(m => m.PortName)
                                    @Html.HiddenFor(m => m.SelectPortId)
                                </div>
                            </div>
                        </div>

                        <div class="row Form_Space_top">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Country:</label>
                                    @Html.DropDownListFor(m => m.CountryId, new List<SelectListItem>
                               {
                               }, "----Select----", new { @class = "dropdown" })
                                    @Html.ValidationMessageFor(m => m.CountryId, "", new { @class = "Error_Msg", placeholder = "Enter Country" })
                                    @Html.HiddenFor(m => m.CountryName)
                                    @Html.HiddenFor(m => m.SelectCountryId)
                                </div>
                            </div>
                        </div>

                    </div>


                    <div class="row">
                        <div class="col-md-12">
                            <div id="DivFormOneDet" ng-hide="OblEntryDetails.length==0">
                                <table class="table table-striped table-bordered dataTable">
                                    <thead>
                                        <tr>
                                            <th width="3%" class="text-center">Action</th>
                                            <th width="2%">Sl No</th>
                                            <th width="5%">Container / CBT No</th>
                                            <th width="2%">Container / CBT Size</th>
                                            <th width="5%">No. of PKG</th>
                                            <th width="5%">GR WT</th>
                                            <th width="10%">Shipping Line</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="j in OblEntryDetails">
                                            <td style="text-align:center;">
                                                <input type="hidden" ng-model="j.IsProcessed" />
                                                <div class="table_icon"><a href="#" ng-click="Delete($index,j)"><i class="fa fa-trash-o" aria-hidden="true"></i></a></div>
                                            </td>
                                            <td>
                                                {{$index+1}}<input type="hidden" ng-model="j.ID" /><input type="hidden" ng-model="j.AddID" />
                                                <input type="hidden" ng-model="j.DetailsID" />
                                            </td>
                                            <td><input type="text" ng-model="j.ContainerNo" ng-readonly="j.ContIcesData==1" ng-blur="onchangetext(j);" maxlength="15" /></td>
                                            <td>
                                                <select ng-model="j.ContainerSize" ng-disabled="j.ContIcesData==1" required>
                                                    <option value="">--Select--</option>
                                                    <option value="{{s.id}}" ng-repeat="s in ContainerSizeList">{{s.ContainerSize}}</option>
                                                </select>
                                            </td>
                                            <td>
                                                <input type="text" ng-model="j.NoOfPkg" ng-readonly="j.ContIcesData==1" onkeypress="return isNumber(event)" />
                                            </td>
                                            <td><input type="text" ng-model="j.GR_WT" ng-readonly="j.ContIcesData==1" onkeypress="return isDecimalNumber(event)" /></td>

                                            <td>
                                                <div class="position-relative">
                                                    <input type="text" ng-model="j.ShippingLineName" id="ShippingLineName_{{$index}}" ng-disabled="true" required />
                                                    <input type="text" ng-hide="true" ng-model="j.ShippingLineId" />
                                                    <span class="input_icon" id="ShippingLineSerach"><i class="fa fa-search" aria-hidden="true" ng-model="j.ShippingLineModal" ng-click="onShippingLineChange($index)" data-toggle="modal" data-target="#ShippingLineModal"></i></span>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                        <div class="logErrMsg" id="DivTblContrErrMsg" style="background-color:transparent"></div>
                    </div>

                    <div class="d-flex justify-content-left">
                        <input type="button" id="btnAddJO" class="btn btn-primary mr-1 btn-100px" value="Add" ng-click="AddOblEntry()">
                        <input type="button" id="btnResetJO" class="btn btn-primary-border btn-100px" value="Reset" ng-click="ResetJODet()">
                    </div>

                    <div class="row">
                        <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                    </div>

                    <div class="d-flex justify-content-left">
                        <input type="submit" id="btnSave" class="btn btn-primary mr-1 btn-100px" value="Save" ng-click="OnOBLEntrySave()">
                        <input type="button" id="btnReset" class="btn btn-primary-border btn-100px" value="Reset" onclick="ResetAllField()">
                    </div>


                    <div class="logSuccMsg Form_Space_top" id="DivSucMsg" style="background-color:transparent"></div>
                    <div class="logErrMsg Form_Space_bottom" id="DivErrMsg" style="background-color:transparent"></div>


                    <div class="row mt10 mb10">
                        <div class="col-md-12">
                            <input type="button" id="BtnCommodityLoad" value="Show List" class="btn btn-primary btn-100px" onclick="LoadOblEntryList()" />
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="col-md-12">
                            <div class="form-group mb-0 position-relative">
                                @Html.TextBox("OBLIGMNO","", new { @placeholder = "OBL/IGMNO" })
                                <span class="input_icon"><i class="fa fa-search" aria-hidden="true" onclick="GetListOfOBLEntryOBL()"></i></span>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <div id="OblEntryList"></div>
                        </div>
                    </div>
                </div>
                }
            </div>

        </div>
    </div>

    <div class="modal fade" id="ContainerNoModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content popup_modal">
                <div class="modal-header popup_header">
                    <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i></button>
                    <h4 class="modal-title">List of Container / CBT No</h4>
                </div>
                <div class="modal-body popup_body">
                    <input placeholder="Search Me" id="ContainerNobox" type="text" />
                    <ul class="navList" id="lstContainerNo">
                        @{
                            if (ViewBag.ListOfContainerNo != null)
                            {
                                foreach (var item in ViewBag.ListOfContainerNo)
                                {
                                    <li id="@item.Size">@item.ContainerNo</li>
                                }
                            }
                            else
                            {
                                <li></li>
                            }

                        }
                    </ul>
                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-md-offset-6 col-md-6">
                            <button type="button" class="btn log_Btn_sm" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
