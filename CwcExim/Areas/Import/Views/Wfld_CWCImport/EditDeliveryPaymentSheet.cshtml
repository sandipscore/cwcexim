                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2 padding_rt">
                                <label>No of vehicles(Loading):</label>
                            </div>
                            <div class="col-md-1-5">
                                @Html.TextBox("VehicleNos", "", new { @class = "form-control input-sm" })
                            </div>

                            <div class="col-md-2-5 col_cus_2 padding_rt">
                                <label>No of vehicles(UnLoading):</label>
                            </div>
                            <div class="col-md-1-5">
                                @Html.TextBox("VehicleNoUn", "", new { @class = "form-control input-sm" })
                            </div>
                            @*<div class="col-md-1">
                                    <label>Bill To Party:</label>
                                </div>
                                <div class="col-md-1">

                                    <input type="checkbox" id="BillToParty" class="domCheckbox" />
                                            </div>*@
                            <div class="col-md-1">
                                <label>OT Hours:</label>
                            </div>
                            <div class="col-md-2">
                                @Html.TextBox("OTHours", "0")
                            </div>


                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-2 col_cus_2">
                                @Html.CheckBox("InsuredParty", false, new { id = "InsuredParty", @checked = false })
