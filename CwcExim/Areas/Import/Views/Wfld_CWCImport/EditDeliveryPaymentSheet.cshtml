                            </div>
                            <div class="col-md-2">

                                @Html.CheckBox("IsTransporter", false, new { id = "IsTransporter", @checked = false })
                                <label for="IsTransporter"><i class="square"></i><span> Own movement</span></label>

                            </div>
                            <div class="col-md-3">
                                @Html.CheckBox("ImportTobond", false, new { id = "ImportTobond", @checked = false })
                                <label for="ImportTobond"><i class="square"></i><span> Import To Bond Conversion</span></label>

                            </div>
                            <div class="col-md-2">
                                <div class="boolean-container">
                                    @Html.Hidden("SEZValue")
                                    @Html.CheckBox("SEZ", false, new {Id= "SEZ", @class = "myCheckbox", @tabindex = 16 })
                                    <label for="SEZ"><i class="square"></i><span>SEZ</span></label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">

                            <div class="col-md-1-5 padding_rt">
                                <label>Payment Mode:</label>
                            </div>
                            <div class="col-md-1 col_cus_2">
                                <div class="col-md-1" style="padding-top: 6px;">
                                    <span id="PMode">

                                    </span>

                                </div>

                            </div>

                            <div class="col-md-offset-2 col-md-3">
                                <input id="btnBOEDetails" class="btn log_Btn_sm" aria-hidden="true" type="button" value="OBL Details" onclick="BOESelect()" />
                            </div>
                        </div>
                    </div>

                    @*<div class="row Form_Space_top Form_Space_bottom">
                            <div class="col-md-12">
                                <table id="tblContainer" class="table table-bordered table-striped dataTable">
                                    <thead>
                                        <tr>
                                            <th style="width:8%;" class="text-center">Sl No</th>
                                            <th>ICD No</th>
                                            <th>Container No</th>
                                            <th>Size</th>
