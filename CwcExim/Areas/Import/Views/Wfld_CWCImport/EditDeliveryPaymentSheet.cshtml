<script>



    function PrintInvoice() {

        debugger;
        var invoiceno = $('#InvoiceNo').val();        

        var Token = $('input[name="__RequestVerificationToken"]').val();
        $.ajax({           
            url: '@Url.Action("GetBulkInvoiceReport", "Wfld_ReportCWC", new { Area = "Report" }) ',
            headers: { "__RequestVerificationToken": Token },
            dataType: 'JSON',
            type: 'POST',
            data: {
                InvoiceNumber: invoiceno,
                InvoiceModule: 'IMPDeli',
                PeriodFrom: '@DateTime.Now.ToString("dd/MM/yyyy")',
                PeriodTo: '@DateTime.Now.ToString("dd/MM/yyyy")',
                InvoiceModuleName: 'ASSESSMENT SHEET LCL',
            },
            success: function (data) {
                if (data.Status == 1)
                    window.open(data.Data + "?_t=" + (new Date().getTime()), "_blank");
                else
                    alert(data.Data);
            }
        });
    }

    

   

    function ShowList() {
        $('#SearchInvoiceNo').val("");
        $('#SearchInvoiceDate').val("");
        $('#DivListOfInvoice').load('/Import/WFLD_CWCImport/ListOfEditExpInvoice?Module=IMPDeli');
    }
    function SearchInvoice() {
        var InvoiceNo = $('#SearchInvoiceNo').val().trim();
        var InvoiceDate = $('#SearchInvoiceDate').val().trim();
        if ($('#SearchInvoiceNo').val() == "" && $('#SearchInvoiceDate').val() == "")
            alert("Enter Invoice No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate() == true)
            $('#DivListOfInvoice').load('/Import/WFLD_CWCImport/ListOfEditExpInvoice?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate + "&Module=IMPDeli");
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Import/WFLD_CWCImport/ListOfEditExpInvoice?InvoiceNo=' + InvoiceNo + "&Module=IMPDeli");
        else if (Validate() == true)
            $('#DivListOfInvoice').load('/Import/WFLD_CWCImport/ListOfEditExpInvoice?InvoiceDate=' + InvoiceDate + "&Module=IMPDeli");
    }





