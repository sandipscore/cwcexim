    function SelectInvoice(InvoiceId, InvoiceNo) {
        var InvNo = InvoiceNo.split('-');
        $('#InvoiceNo').val(InvNo[0]);
        $('#InvoiceId').val(InvoiceId);

        debugger;
        $.ajax({
            url: '/Import/Wfld_CWCImport/GetGodownInvoiceDetails',
            type: 'GET',
            data: { InvoiceId: InvoiceId },
            dataType: 'JSON',
            success: function (data) {
                //alert(data);
                debugger;
                var InvoiceDtl = data;

                $('#StuffingReqId').val(InvoiceDtl.StuffingReqId);
                $('#StuffingReqNo').val(InvoiceDtl.StuffingReqNo);
                $('#StuffingReqDate').val(InvoiceDtl.StuffingReqDate);
                $('#InvoiceDate').val(InvoiceDtl.InvoiceDate);
                $('#DeliveryId').val(InvoiceDtl.StuffingReqId);
                
                //var dt = InvoiceDtl.DeliveryDate.split("/");
                //var dt1 = dt[1] + "/" + dt[0] + "/" + dt[2];
                $('#DeliveryDate').val(InvoiceDtl.DeliveryDate);
                $('#CHAName').val(InvoiceDtl.CHAName);
                $('#PartyId').val(InvoiceDtl.PartyId);
                $('#PartyName').val(InvoiceDtl.PartyName);
                $('#PayeeId').val(InvoiceDtl.PayeeId);
                $('#PayeeName').val(InvoiceDtl.PayeeName);
                $('#GSTNo').val(InvoiceDtl.GSTNo);
                $('#hdnAddress').val(InvoiceDtl.Address);
                $('#hdnState').val(InvoiceDtl.State);
                $('#PlaceOfSupply').val(InvoiceDtl.State);

                $('#hdnStateCode').val(InvoiceDtl.StateCode);
                $('#hdnDeliveryType').val(InvoiceDtl.DeliveryType);
                $('#InvoiceType').val(InvoiceDtl.InvoiceType);
                $('#VehicleNoUn').val(InvoiceDtl.VehicleNoUn);
                $('#VehicleNos').val(InvoiceDtl.NoOfVehicle);
                $('#OTHours').val(InvoiceDtl.OTHours);
                $('#Cargo').val(InvoiceDtl.Cargo);
                if (InvoiceDtl.SEZ == "1")
                {
                    $('#SEZ').prop("checked", true);
                    $('#SEZValue').val('1');
                }
                if (InvoiceDtl.IsTransporter == "1") {
                    $('#IsTransporter').prop("checked", true);
                }
                if (InvoiceDtl.IsBond == "1") {
                    $('#ImportTobond').prop("checked", true);
                }
                if (InvoiceDtl.IsInsured == "1") {
                    $('#InsuredParty').prop("checked", true);
                }
               
                $('#IsTransporter').attr('disabled', 'disabled');
                $('#ImportTobond').attr('disabled', 'disabled');
                $('#InsuredParty').attr('disabled', 'disabled');
                $('#OTHours').prop('readonly', true);
                $('#VehicleNos').prop('readonly', true);
                $('#VehicleNoUn').prop('readonly', true);                                
                $("#InvoiceModal").modal("hide");

            }
        });
    }

