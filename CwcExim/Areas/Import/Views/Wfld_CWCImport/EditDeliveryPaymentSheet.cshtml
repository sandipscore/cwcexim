
@model  CwcExim.Areas.Import.Models.WFLD_DeliverApplication

@if (ViewBag.InvoiceList != null)
{
    <input type="hidden" id="hdnInvoice" value="@ViewBag.InvoiceList" />
}
else
{
    <input type="hidden" id="hdnInvoice" value="" />
}


<style>
    #tbleParty.dataTable tbody > tr > td {
        cursor: pointer;
    }

    #tblePayee.dataTable tbody > tr > td {
        cursor: pointer;
    }

    .slimScrollDiv {
        height: 300px;
    }

    .table.tblhd {
        border-bottom: 0;
    }
</style>
@using (Ajax.BeginForm("UpdateSingleMergeDeliveryApplication", "Wfld_CWCImport", new AjaxOptions { HttpMethod = "POST", OnSuccess = "PaymentOnSuccess", OnFailure = "PaymentOnFailure" }, new { @Id = "PaymentSheetForm" }))
{
    @Html.AntiForgeryToken()
    @Html.Hidden("ContainerJson")
    @Html.Hidden("BOEJson")
    @Html.Hidden("hdnAddress")
    @Html.Hidden("hdnState")
    @Html.Hidden("hdnStateCode")
    @Html.Hidden("hdnDeliveryType")
    @Html.Hidden("PaymentSheetModelJson")

    @Html.HiddenFor(m => m.BillToParty)
    //@Html.HiddenFor(m => m.IsInsured)
    //@Html.HiddenFor(m => m.ImportToBondCon)
 

   // @Html.HiddenFor(m => m.IsTransporter)
    @Html.HiddenFor(m => m.DeliveryAppDtlXml)
    @Html.HiddenFor(m => m.DeliveryOrdDtlXml)
    @Html.HiddenFor(m => m.DeliveryGodownDtlXml)

    @Html.HiddenFor(m => m.DestuffingId)
    @Html.HiddenFor(m => m.DeliveryId)
    //@Html.HiddenFor(m => m.InvoiceDate)
    @Html.HiddenFor(m => m.ObleFreeFlag)

    @Html.HiddenFor(m => m.lstContWiseAmountXML)
    @Html.HiddenFor(m => m.lstInvoiceCargoXML)
