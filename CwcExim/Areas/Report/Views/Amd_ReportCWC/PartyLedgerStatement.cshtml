    var Page=0;

    function LoadMoreParty() {
        debugger;
        $.ajax({
            url: '/Report/VRN_ReportCWC/OALoadPartyLedgerParty',
            type: 'GET',
            data: { PartyCode: "", Page: Page + 1 },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabindex=101;
                    $.each(data.Data.LstParty, function (item, elem) {
                        debugger;
                        html += '<tr tabindex='+ tabindex +'  onclick="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)" onkeypress="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.Party + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PartyCode + '</td></tr>';
                        tabindex++;
                    });
                    if (Page == -1)
                        $('#tblParty tbody').html(html);
                    else
                        $('#tblParty tbody').append(html);
                    Page++;

                    debugger;
                    if (data.Data.State == true)
                        $('#btnLoadMoreParty').prop('disabled', false);
                    else $('#btnLoadMoreParty').prop('disabled', true);
                }
            }
        });
    }


    function LoadParty() {
        debugger;
        Page = 0;
        $.ajax({
            url: '/Report/VRN_ReportCWC/OALoadPartyLedgerParty',
            type: 'GET',
            data: { PartyCode: "", Page: Page },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabindex=101;
                    $.each(data.Data.LstParty, function (item, elem) {
                        debugger;
                        html += '<tr tabindex='+ tabindex +' onclick="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)" onkeypress="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.Party + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PartyCode + '</td></tr>';
                        tabindex++;
                    });

                    $('#tblParty tbody').html(html);

                    if (data.Data.State == true)
                        $('#btnLoadMoreParty').prop('disabled', false);
                    else $('#btnLoadMoreParty').prop('disabled', true);
                }
            }
        });
    }

    function SelectParty(PartyId, Party) {
        debugger;
        $('#Party').val(Party);
        $('#PartyId').val(PartyId);
        $("#PartyModal").modal("hide");
        $('#PartyBox').val('');
        LoadParty();
    }

    function SearchParty() {
        debugger;
        if ($('#PartyBox').val() != '') {
            $.ajax({
                url: '/Report/VRN_ReportCWC/OASearchPartyLedgerParty',
                type: 'GET',
                data: { PartyCode: $('#PartyBox').val() },
                success: function (data) {
                    if (data.Status == 1) {
                        debugger;
                        var html = '';
                        var tabindex = 101;
                        $.each(data.Data.LstParty, function (item, elem) {
                            debugger;
                            html += '<tr tabindex=' + tabindex + '  onclick="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)" onkeypress="SelectParty(' + elem.PartyId + ',&quot;' + elem.Party + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.Party + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.PartyCode + '</td></tr>';
                            tabindex++;
                        });
                        $('#tblParty tbody').html('');
                        $('#tblParty tbody').html(html);
                        $('#btnLoadMoreParty').prop('disabled', false);
                        Page = -1;
                    }
                    else {
                        $('#tblParty tbody').html('');
                        Page = -1;
                        $('#btnLoadMoreParty').prop('disabled', false);
                    }

                }
            });
        }
        else
            LoadParty();
    }

    function CloseParty() {
        debugger;
        $('#PartyBox').val('');
        $("#PartyModal").modal("hide");
        LoadParty();
    }


    function PopupActivate(Id) {
        debugger;
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }

        if (CharCode == 32) {

            if (Id == "stuffingModal") {
                debugger;
                $("#stuffingModal").modal('show');
            }

            if (Id == "CHAModal") {
                debugger;
                angular.element('#YardInvoiceController').scope().LoadCHAList();


                $("#CHAModal").modal('show');
            }




            $('#' + Id).modal('show');
            if (Id == "MyModal") {
                debugger;
                $('#box').focus();
            }

            if (Id == "PartyModal") {
                debugger;



                $("#PartyModal").modal('show');
            }


            if (Id == "PayeeModal") {
                debugger;

                angular.element('#YardInvoiceController').scope().LoadPayeeList();


                $("PayeeModal").modal('show');
            }

            if (Id == "CHAModal") {
                debugger;
                $('#SearchCHAText').focus();
            }

            if (Id == "PortDestModal") {
                debugger;
                $('#PortDestbox').focus();
            }


            if (Id == "PortModal") {
                debugger;
                $('#Portbox').focus();
            }

            if (Id == "CommodityModal") {
                debugger;
                $('#CommodityBox').focus();
            }
            if (Id == "SBNoModal") {
                debugger;
                $('#SBNoBox').focus();
            }
            if (Id == "ExporterModal") {
                debugger;
                $('#Exporterbox').focus();
            }
        }




    }




    $('#PartyModal').on('shown.bs.modal', function () {

        $('#PartyBox').focus();

    });


    $('#PartyBox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstCHA > li').show();
        else {
            $('#lstCHA > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });

            SearchParty();
        }
    });
</script>

