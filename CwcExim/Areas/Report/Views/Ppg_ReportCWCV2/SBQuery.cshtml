
@model CwcExim.Areas.Report.Models.VRN_SBQuery

<style>
    .Table_center {
        text-align: center;
    }

    .table-striped > tfoot > tr:nth-child(odd) {
        background-color: #ffd4a9;
    }

    .table > tfoot > tr > th {
        font-size: 0.85em !important;
    }
</style>

@{

    AjaxOptions options = new AjaxOptions();
    options.HttpMethod = "POST";
    options.OnFailure = "PVFailure";
    options.OnSuccess = "CargoSuccess";
    options.InsertionMode = InsertionMode.InsertAfter;

}
@using (Ajax.BeginForm("GetDailyInvoiceReport", "Ppg_ReportCWC", options))
{
    @Html.AntiForgeryToken()

    @Html.HiddenFor(m => m.Id)
