        var sbdt = sbnosp[1];
        var dbdtsp = sbdt.split("/");
        var sbdtorg = dbdtsp[2] + "-" + dbdtsp[1] + "-" + dbdtsp[0];
        $('#Shipbill').val(sbn);
        $('#id').val($(this).attr('id'));
