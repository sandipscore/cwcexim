
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DayToDayDestuffingReport</title>
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link href="https://kendo.cdn.telerik.com/2022.3.913/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/2022.3.913/styles/kendo.blueopal.min.css" rel="stylesheet" />

    @*<script src="~/Scripts/js/js/telerikReportViewer.kendo-16.2.22.1109.min.js"></script><script src="~/Scripts/jquery-3.6.1.min.js"></script>
    <script src="~/Scripts/js/js/telerikReportViewer-16.2.22.1109.js"></script>*@

    <script src="~/Scripts/Telerik/js/telerikReportViewer.kendo-17.0.23.118.min.js"></script>
    <script src="~/Scripts/Telerik/js/telerikReportViewer-17.0.23.118.js"></script>
    <script type="text/javascript">


        $(document).ready(function () {
            debugger;

            var vStartdt = '@ViewBag.FromDt'; //new Date('@ViewBag.FromDt');
            var vEnddt = '@ViewBag.ToDt';//new Date('@ViewBag.ToDt');
            //var FinalStDt=(vStartdt.getMonth()+1)+"/"+vStartdt.getDate()+"/"+vStartdt.getFullYear();
           // var FinalEndDt=(vEnddt.getMonth()+1)+"/"+vEnddt.getDate()+"/"+vEnddt.getFullYear();
            //let formatted_date = current_datetime.getFullYear() + "-" + (current_datetime.getMonth() + 1) + "-" + current_datetime.getDate() + " " + current_datetime.getHours() + ":" + current_datetime.getMinutes() + ":" + current_datetime.getSeconds()





            $("#reportViewer1")
           .telerik_ReportViewer({
               serviceUrl: "https://cwc-cfs.com:99/api/Reports",
               reportSource: {
                   // The report can be set to a report file name (trdx report definition)
                   // or CLR type name (report class definition).
                   report: "DayToDayDestuffingReport.trdx", parameters: {
                       RPFromDt: vStartdt,
                       RPToDt: vEnddt,
                       Landscape: true
                   }


               },
               viewMode: telerikReportViewer.ViewModes.INTERACTIVE,
               scaleMode: telerikReportViewer.ScaleModes.SPECIFIC,
               scale: 1.0,
               enableAccessibility: false,
               sendEmail: { enabled: true }
           });



        }


        );
    </script>

    <style>
        #reportViewer1 {
            position: absolute;
            left: 5px;
            right: 5px;
            top: 50px;
            bottom: 5px;
            overflow: hidden;
            font-family: Verdana, Arial;
        }
    </style>

</head>
<body>
    <div id="reportViewer1">
        loading...
    </div>
</body>
</html>
