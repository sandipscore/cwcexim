<div class="modal fade mTabindex" id="SBNoModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content popup_modal">
            <div class="modal-header popup_header">
                <button type="button" class="close" onclick="CloseSBNo()"><i class="fa fa-times" aria-hidden="true"></i></button>
                <h4 class="modal-title">Choose SB No</h4>
            </div>
            <div class="modal-body popup_body" style="position:relative;">
                <input placeholder="Search By SBNo " id="SBbox" type="text" tabindex="201" autofocus />
                <span class="search" style="top: 18px;"><i class="fa fa-search" aria-hidden="true" onclick=SearchBySbNo()></i></span>

                <table class="table table-striped table-bordered dataTable tblhd" style="width:100%; margin:0 !important; border-bottom:0; padding:0;">
                    <thead>
                        <tr>
                            <th colspan="8" width="80%">SB NO</th>
                            <th colspan="4" width="20%">SB Date</th>
                        </tr>
                    </thead>
                </table>

                <div id="slim_scroll_3">
                    <table tabindex="202" class="table dataTable table-bordered table-striped slim_tble" id="tbleSB">
                       
                        @{
                            if (ViewBag.lstSB != null)
                            {
                                var tabvalue = 202;
                                foreach (var item in ViewBag.lstSB)
                                {
                                    <tr tabindex="@tabvalue" onclick="SelectSB(@item.Id,'@item.SBNO','@item.Date')" onkeypress="SelectSB(@item.Id,'@item.SBNO','@item.Date')">
                                        <td colspan="8" width="80%" class="txt_wrp">@item.SBNO</td>
                                        <td colspan="4" width="20%" class="txt_wrp">@item.Date</td>
                                    </tr>
                                    tabvalue++;
                                }
                            }
                        }
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-6">
                        @if (ViewBag.SBState != null && Convert.ToBoolean(ViewBag.SBState) == true)
                        {
                            <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreSB()" class="btn log_Btn_sm" id="btnLoadSB">Load More Data</button>
                        }
                        else
                        {
                            <button type="button" style="width:100%; margin-left:0;" onclick="LoadMoreSB()" class="btn log_Btn_sm" id="btnLoadSB" disabled>Load More Data</button>
                        }
                    </div>
                    <div class="col-md-6">
                        <button type="button" class="btn log_Btn_sm" onclick="CloseSB()">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/jqueryval")
<script>
    
    var Page = 0;
    function LoadSB() {
        debugger;
        Page = 0;
        $.ajax({
            url: '/Report/WFLD_ReportCWC/LoadSBList',
            type: 'GET',
            data: { PartyCode: "", Page: Page },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    $.each(data.Data.lstSB, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue +  ' onclick="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot,&quot;' + elem.Date + '&quot;)" onkeypress="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot,&quot;' + elem.Date + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.SBNO + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.Date + '</td></tr>';
                        tabvalue++;
                    });

                    $('#tbleSB tbody').html(html);

                    if (data.Data.State == true)
                        $('#btnLoadSB').prop('disabled', false);
                    else $('#btnLoadSB').prop('disabled', true);
                }
            }
        });
    }
    var Page = 0;
    function LoadMoreSB() {
        debugger;
        $.ajax({
            url: '/Report/WFLD_ReportCWC/LoadSBList',
            type: 'GET',
            data: { PartyCode: "", Page: Page + 1 },
            success: function (data) {
                if (data.Status == 1) {
                    debugger;
                    var html = '';
                    var tabvalue = 202;
                    //  Id, SBNO, SBDate
                    $.each(data.Data.lstSB, function (item, elem) {
                        html += '<tr tabindex=' + tabvalue + ' onclick="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot,&quot;' + elem.Date + '&quot;)" onkeypress="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot,&quot;' + elem.Date + '&quot;)"><td colspan="8" width="80%" class="txt_wrp">' + elem.SBNO + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.Date + '</td></tr>';

                        tabvalue++;
                    });
                    if (Page == -1)
                        $('#tbleSB tbody').html(html);
                    else
                        $('#tbleSB tbody').append(html);
                    Page++;

                    debugger;
                    if (data.Data.State == true)
                        $('#btnLoadSB').prop('disabled', false);
                    else $('#btnLoadSB').prop('disabled', true);
                }
            }
        });
    }
    function SearchBySbNo() {
        debugger;
        if ($('#SBbox').val() != '') {
            $.ajax({
                url: '/Report/WFLD_ReportCWC/SearchSB',
                type: 'GET',
                data: { PartyCode: $('#SBbox').val() },
                success: function (data) {
                    debugger;
                    if (data.Status == 1) {
                        var html = '';
                        var tabvalue = 204;
                        $.each(data.Data.lstSB, function (item, elem) {
                            html += '<tr tabindex=' + tabvalue + ' onclick="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot;,&quot;' + elem.Date + '&quot;)" onkeypress="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot;,&quot;' + elem.Date + '&quot;);"><td colspan="8" width="80%" class="txt_wrp">' + elem.SBNO + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.Date + '</td></tr>';
                          //  html += '<tr tabindex=' + tabvalue + ' onkeypress="SelectCommodity(' + item.CommodityId + ',&quot;' + item.CommodityName + '&quot;,&quot;' + item.CommodityType + '&quot;);" onclick="SelectCommodity(' + elem.CommodityId + ',&quot;' + elem.CommodityName + '&quot;,&quot;' + elem.CommodityType + '&quot;)"><td colspan="8" width="80%">' + elem.CommodityName + '</td></tr>';
                            // html += '<tr tabindex=' + tabvalue +  ' onkeypress="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot;,&quot;' + elem.Date + '&quot;)" onclick="SelectSB(' + elem.Id + ',&quot;' + elem.SBNO + '&quot;,&quot;' + elem.Date + '&quot;)" ><td colspan="8" width="80%" class="txt_wrp">' + elem.SBNO + '</td><td colspan="4" width="20%" class="txt_wrp">' + elem.Date + '</td></tr>';
                           // html += '<tr tabindex=' + tabvalue + ' onclick="SelectCommodity(' + elem.CommodityId + ',&quot;' + elem.CommodityName + '&quot;,&quot;' + item.CommodityType + '&quot;)" onkeypress="SelectCommodity(' + elem.CommodityId + ',&quot;' + elem.CommodityName + '&quot;,&quot;' + elem.CommodityType + '&quot;)"><td colspan="8" width="80%">' + elem.CommodityName + '</td></tr>';
                            tabvalue++;
                        });
                        $('#tbleSB tbody').html('');
                        $('#tbleSB tbody').html(html);
                        $('#btnLoadSB').prop('disabled', false);
                        Page = -1;
                    }
                    else {
                        $('#tbleSB tbody').html('');
                        Page = -1;
                        $('#btnLoadSB').prop('disabled', false);
                    }

                }
            });
        }
        else
            LoadSB();
    }
