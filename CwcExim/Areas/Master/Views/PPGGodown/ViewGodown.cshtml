@model CwcExim.Areas.Master.Models.PPGGodownVM
<script>
    function LoadGodownPage() {
        $('#DivBody').load('/Master/PPGGodown/CreateGodown');
    }
</script>

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>Godown Master</h4>
                </div>
            </div>

            <div class="content_wrp">
                <div class="row Form_Space_top">
                    <div class="col-md-5">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            <label>Godown Name:</label>
                            @Html.TextBoxFor(m => m.MstGodwon.GodownName, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top">
                    </div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary mr-1 btn-100px" value="Back" onclick="LoadGodownPage()">
                </div>

                <div class="row Form_Space_top">
                    <div id="DivTblLocation" class="col-sm-12 col-md-12">
                        <table class="table table-striped table-bordered dataTable Form_Input">
                            <thead>
                                <tr>
                                    <th>Location Name:</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    foreach (var item in Model.LstLocation)
                                    {
                                        <tr>
                                            <td>@Html.DisplayFor(m => item.LocationName)</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
</div>
@Scripts.Render("~/bundles/jqueryval")
