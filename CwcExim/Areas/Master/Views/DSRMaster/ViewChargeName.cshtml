@model CwcExim.Areas.Master.Models.DSRChargeName

<script>

    function LoadChargeNamePage() {
        $('#DivBody').load('/Master/DSRMaster/CreateChargeName');
    }

</script>

@Html.AntiForgeryToken()
<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                <div class="row SignUp_space_bottom">
                    <div class="col-md-12">
                        <h4>Charge Name Master</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group Form_Input">
                        <div class="col-md-1-5 col_cus_2">
                            Charge Name:
                        </div>
                        <div class="col-md-4">
                            <span class="ReQSimbol">*</span>
                            @Html.TextBoxFor(m => m.ChargeName, "", new { @class = "form-control input-sm",@readonly="readonly" })
                            @Html.ValidationMessageFor(m => m.ChargeName, "", new { @class = "Error_Msg" })
                        </div>
                        <div class="col-md-offset-1 col-md-1-5 col_cus_2">
                            Charge Code:
                        </div>
                        <div class="col-md-2">
                            <span class="ReQSimbol">*</span>
                            @Html.TextBoxFor(m => m.ChargeCode, "", new { @class = "form-control input-sm", @readonly = "readonly" })

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group Form_Input">
                        <div class="col-md-1-5 col_cus_1">
                            <label>Misc. Invoice :</label>
                        </div>
                        <div class="col-md-1">
                            <div class="boolean-container">
                                @Html.CheckBoxFor(m => m.IsMisc, htmlAttributes: new { @disabled = true })
                                <label for="IsMisc"><i class="square"></i></label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top ClearFix"></div>
                </div>
                <div class="row SignUp_space_bottom">
                    <div class="col-md-offset-5 col-md-2">
                        <input type="button" id="BtnChargeNameBack" value="Back" class="btn log_Btn_sm" onclick="LoadChargeNamePage()" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
