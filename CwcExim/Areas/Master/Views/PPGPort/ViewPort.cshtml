@model CwcExim.Areas.Master.Models.PPGPost

<script>
    function LoadPortPage() {
        debugger;
        $('#DivBody').load('/Master/PPGPort/CreatePort');
    }
</script>

<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>Port Master</h4>
                </div>
            </div>
            <div class="content_wrp">

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            <label>Port Name:</label>
                            @Html.TextBoxFor(m => m.PortName, new { @class = "", @readonly = "readonly" })
                            @Html.ValidationMessageFor(m => m.PortName, "", new { @class = "Error_Msg" })
                        </div>
                    </div>
                    <div class="col-md-2">
                        <span class="ReQSimbol">*</span>
                        <label>Port Alias:</label>
                        @Html.TextBoxFor(m => m.PortAlias, new { @class = "", @readonly = "readonly" })
                        @Html.ValidationMessageFor(m => m.PortAlias, "", new { @class = "Error_Msg" })
                    </div>
                    <div class="col-md-1">
                        <span class="ReQSimbol">*</span>
                        <label>POD/POA:</label>
                        <div class="form-group">
                            <label class="Radiocontainer">
                                @Html.CheckBoxFor(m => m.POD, new { @disabled = "disabled" })
                                <span class="checkmark"></span>
                            </label>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            <label>Country Name:</label>
                            @Html.DropDownListFor(m => m.CountryId, new List<SelectListItem> { new SelectListItem { Value = Model.CountryId.ToString(), Text = Model.CountryName.ToString() } }, new { @class = " dropdown", @disabled = "disabled" })
                        </div>
                    </div>

                    <div class="col-md-3">
                        <span class="ReQSimbol">*</span>
                        <label>State Name:</label>
                        @Html.DropDownListFor(m => m.StateId, new List<SelectListItem> { new SelectListItem { Value = Model.StateId.ToString(), Text = Model.StateName.ToString() } }, new { @class = " dropdown", @disabled = "disabled" })
                    </div>
                </div>

                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary mr-1 btn-100px" value="Back" onclick="LoadPortPage()">
                </div>

            </div>
        </div>
    </div>
</div>
