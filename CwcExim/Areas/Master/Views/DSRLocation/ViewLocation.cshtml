@model CwcExim.Areas.Master.Models.DSRLocation
<script>

        function LoadLocationPage() {
            $('#DivBody').load('/Master/DSRLocation/CreateLocation');
        }

</script>

@Html.AntiForgeryToken()
<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-0-5 col-md-11">
            <div class="Head_h4">
                <div class="row SignUp_space_bottom">
                    <div class="col-md-12">
                        <h4>Country Master</h4>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group Form_Input">
                        <div class="col-md-1-5 col_cus_2">
                            @Html.LabelFor(m => m.LocationName, new { @class = "InputLabel" })
                        </div>
                        <div class="col-md-4">
                            <span class="ReQSimbol">*</span>
                            @Html.TextBoxFor(m => m.LocationName, "", new { @class = "form-control input-sm", @readonly = "readonly" })
                            @Html.ValidationMessageFor(m => m.LocationName, "", new { @class = "Error_Msg" })
                        </div>
                        <div class="col-md-offset-1 col-md-1-5 col_cus_2">
                            @Html.LabelFor(m => m.LocationAlias)
                        </div>
                        <div class="col-md-2">
                            <span class="ReQSimbol">*</span>
                            @Html.TextBoxFor(m => m.LocationAlias, "", new { @class = "form-control input-sm", @readonly = "readonly" })
                            @Html.ValidationMessageFor(m => m.LocationAlias, "", new { @class = "Error_Msg" })
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top ClearFix"></div>
                </div>
                <div class="row SignUp_space_bottom">
                    <div class="col-md-offset-5 col-md-2">
                        <input type="button" id="BtnCountryBack" value="Back" class="btn log_Btn_sm" onclick="LoadLocationPage()" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
