@model CwcExim.Areas.Master.Models.PPGSac


<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>GST Against SAC</h4>
                </div>
            </div>
            <div class="content_wrp">
                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            @Html.LabelFor(m => m.SACCode, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.SACCode, new { @class = "" })
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            @Html.LabelFor(m => m.GST, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.GST, new { @class = "" })
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group">
                            <span class="ReQSimbol">*</span>
                            @Html.LabelFor(m => m.CESS, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.CESS, new { @class = "" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Description:</label>
                            @Html.TextAreaFor(m => m.Description, new { @style = "width:100%;height:75px;" })
                        </div>
                    </div>
                </div>

                <div class="row ">
                    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary btn-100px" id="BtnBack" value="Back" onclick="LoadSACPage()">
                </div>

            </div>
        </div>
    </div>
</div>
<script>
    $(function () { $('input[type="text"],textarea').prop('readonly', true); });
    function LoadSACPage() {
        $('#DivBody').load('/Master/PPGSac/CreateSAC');
    }
</script>
