@model CwcExim.Areas.Master.Models.PPGRentReservationModel

<script>
    $(document).ready(function () {
        $('#PasswordVEximTrader').val('@Model.Password');
        $('#ConfrmPasswordVEximTrader').val('@Model.Password');
    });
    function LoadRentReservationPage() {
        $('#DivBody').load('/Master/PPGRentReservationParty/CreateRentReservationParty');
    }

</script>
<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>Exim Trader Master</h4>
                </div>
            </div>
            <div class="content_wrp">
                <div class="row">
                    <div class="form-group">
                        <div class="col-md-1">
                            <div class="boolean-container">
                                @Html.CheckBoxFor(m => m.Rent, new { @disabled = "disabled" })
                                <label for="Rent"><i class="square"></i><span>Rent</span></label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <span class="ReQSimbol">*</span>
                                @Html.LabelFor(m => m.PartyName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                                @Html.TextBoxFor(m => m.PartyName, new { @class = "", @readonly = "readonly" })
                            </div>
                        </div>
                        <div class="col-md-3">
                            @Html.LabelFor(m => m.PartyAlias, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.PartyAlias, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-12">
                        <div class="form-group">
                            @Html.LabelFor(m => m.Address, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextAreaFor(m => m.Address, new { @style = "width:100%;height:75px;", @disabled = "disabled" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.CountryId)<span class="LabelColon">:</span>
                            @if (Model.CountryId > 0)
                            {
                                @Html.DropDownListFor(m => m.CountryId, new List<SelectListItem> { new SelectListItem { Value = Model.CountryId.ToString(), Text = Model.CountryName.ToString() } }, new { @class = "", @disabled = "disabled" })
                            }
                            else
                            {
                                @Html.DropDownListFor(m => m.CountryId, new SelectList("", "Value", "Text"), "---Select---", new { @class = "dropdown", @disabled = "disabled" })
                            }
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.StateId, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @if (Model.StateId > 0)
                        {
                            @Html.DropDownListFor(m => m.StateId, new List<SelectListItem> { new SelectListItem { Value = Model.StateId.ToString(), Text = Model.StateName.ToString() } }, new { @class = "dropdown", @disabled = "disabled" })
                        }
                        else
                        {
                            @Html.DropDownListFor(m => m.StateId, new SelectList("", "Value", "Text"), "---Select---", new { @class = "dropdown", @disabled = "disabled" })
                        }
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.CityId, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @if (Model.CityId > 0)
                            {
                                @Html.DropDownListFor(m => m.CityId, new List<SelectListItem> { new SelectListItem { Value = Model.CityId.ToString(), Text = Model.CityName.ToString() } }, new { @class = "dropdown", @disabled = "disabled" })
                            }
                            else
                            {
                                @Html.DropDownListFor(m => m.CityId, new SelectList("", "Value", "Text"), "---Select---", new { @class = "dropdown", @disabled = "disabled" })
                            }
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.PinCode, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.PinCode, new { @class = "", @readonly = "readonly" })
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.PhoneNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.PhoneNo, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.FaxNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.FaxNo, new { @class = "", @readonly = "readonly" })
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>Contact Person:</label>
                            @Html.TextBoxFor(m => m.ContactPerson, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.Email, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.Email, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.MobileNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.MobileNo, new { @class = "", @readonly = "readonly" })
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.Pan, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.Pan, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label>Aadhaar No:</label>
                        @Html.TextBoxFor(m => m.AadhaarNo, new { @class = "", @readonly = "readonly" })
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.GSTNo, new { @class = "InputLabel", @readonly = "readonly" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.GSTNo, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                </div>

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        @Html.LabelFor(m => m.Tan, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                        @Html.TextBoxFor(m => m.Tan, new { @class = "", @readonly = "readonly" })
                    </div>
                </div>

                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary btn-100px" value="Back" onclick="LoadRentReservationPage()">
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/jqueryval")
<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });
</script>
