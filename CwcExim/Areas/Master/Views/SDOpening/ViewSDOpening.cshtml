@model CwcExim.Areas.Master.Models.SDOpening
<script>
    function LoadSDPage() {
        $('#DivBody').load('/Master/SDOpening/CreateSDOpening');
    }
</script>
<div class="row">
    <div class="col-md-12">
        <div class="Head_h4">
            <div class="row SignUp_space_bottom">
                <div class="col-md-12">
                    <h4>View SD Opening</h4>
                </div>
            </div>
            <div class="content_wrp">

                <div class="row Form_Space_top">
                    <div class="col-md-3">
                        <div class="form-group">
                            @Html.LabelFor(m => m.ReceiptNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.FolioNo, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                    <div class="col-md-3 col-md-offset-6">
                        <div class="form-group">
                            @Html.LabelFor(m => m.EximTraderName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                            @Html.TextBoxFor(m => m.EximTraderName, new { @class = "", @readonly = "readonly" })
                        </div>
                    </div>
                </div>


                <div class="Head_h5 Form_Space_top">
                    <div class="row">
                        <div class="col-md-12">
                            <h5>Accounts Opening</h5>
                        </div>
                    </div>
                    <div class="row Form_Space_bottom Form_Space_top">
                        <div class="col-md-3">
                            <div class="form-group">
                                @Html.LabelFor(m => m.Date, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                                @Html.TextBoxFor(m => m.Date, new { @class = "", placeholder = "dd/mm/yyyy", @readonly = true })
                            </div>
                        </div>
                        <div class="col-md-3 col-md-offset-6">
                            <div class="form-group">
                                @Html.LabelFor(m => m.Amount, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                                @Html.TextBoxFor(m => m.Amount, new { @class = "", @readonly = "readonly" })
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="stoke SignUp_space_bottom SignUp_space_top"></div>
                </div>

                <div class="d-flex justify-content-left">
                    <input type="button" class="btn btn-primary mr-1 btn-100px" value="Back" onclick="LoadSDPage()">
                </div>
            </div>
        </div>
    </div>
</div>




