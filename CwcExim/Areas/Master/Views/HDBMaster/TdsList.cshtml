@model IEnumerable<CwcExim.Areas.Master.Models.HDBCWCTds>
<table class="table table-bordered table-striped" id="TblTdsList" style="width:100%;">
    <thead>
        <tr>
            <th style="text-align:center;">@Html.DisplayName("SL No")</th>
            <th>@Html.DisplayNameFor(m => m.EffectiveDate)</th>
            <th>@Html.DisplayNameFor(m => m.CWCTdsPrcnt)</th>
            <th>@Html.DisplayNameFor(m => m.HTTdsPrcnt)</th>
            <th>@Html.DisplayNameFor(m => m.SacCode)</th>
            <th style="text-align:center;">@Html.DisplayName("Edit")</th>
        </tr>
    </thead>
    <tbody>
        @{
            var SerialNo = 1;
            foreach (var item in Model)
            {
                <tr>
                    <td style="text-align:center;">@SerialNo.ToString()</td>
                    <td>@item.EffectiveDate</td>
                    <td>@item.CWCTdsPrcnt</td>
                    <td>@item.HTTdsPrcnt</td>
                    <td>@item.SacCode</td>
                    <td style="text-align:center;"><a href="#" onclick="EditGroundRent(@item.TdsId)"><i class="fa fa-pencil-square Edit"></i></a></td>
                </tr>
                SerialNo++;
            }
        }
    </tbody>
</table>
<script>
    $(document).ready(function () {
        var TblTdsList = $('#TblTdsList').DataTable({
            "bFilter": false,
            "bInfo": false,
            "pageLength": 50,
            "aoColumns": [
                    { "sName": "SL No", "width": "10%" },
                    { "sName": "Effective Date", "bSortable": false, },
                    { "sName": "CWCTdsPrcnt", "bSortable": false, },
                    { "sName": "HTTdsPrcnt", "bSortable": false, },
                    { "sName": "SacCode", "bSortable": false, },
                    { "sName": "Edit", "bSortable": false, "width": "10%" }
            ]
        });
        new $.fn.dataTable.FixedHeader(TblTdsList);
    });
    function EditGroundRent(TdsId)
    {
        $('#DivTds').load('/Master/HDBMaster/EditTds?TdsId=' + TdsId);
    }
</script>
