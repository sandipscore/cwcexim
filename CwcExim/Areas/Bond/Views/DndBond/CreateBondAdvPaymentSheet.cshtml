    function ShowList() {
        $('#SearchInvoiceNo').val("");
        $('#SearchInvoiceDate').val("");
        $('#DivListOfInvoice').load('/Bond/DndBond/ListOfInvoiceBondAdvance');
    }
    function SearchInvoice() {
        var InvoiceNo = $('#SearchInvoiceNo').val().trim();
        var InvoiceDate = $('#SearchInvoiceDate').val().trim();
        if ($('#SearchInvoiceNo').val() == "" && $('#SearchInvoiceDate').val() == "")
            alert("Enter Invoice No. or Invoice Date to Search");
        else if ($('#SearchInvoiceNo').val() != "" && $('#SearchInvoiceDate').val() != "" && Validate() == true) {
            $('#DivListOfInvoice').load('/Bond/DndBond/ListOfInvoiceBondAdvance?InvoiceNo=' + InvoiceNo + '&InvoiceDate=' + InvoiceDate);
        }
        else if ($('#SearchInvoiceNo').val() != "")
            $('#DivListOfInvoice').load('/Bond/DndBond/ListOfInvoiceBondAdvance?InvoiceNo=' + InvoiceNo);
        else if (Validate() == true)
            $('#DivListOfInvoice').load('/Bond/DndBond/ListOfInvoiceBondAdvance?InvoiceDate=' + InvoiceDate);
    }

