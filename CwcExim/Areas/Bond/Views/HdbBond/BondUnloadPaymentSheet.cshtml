            onClose: function () {
                $(".Date_Img .Error_Msg").text("");
                $('[data-valmsg-for="Data"]').html('<span></span>');
            }
        });

        StuffingReqBinding();
        PartyBinding();
        PayeeBinding();
    });

    function StuffingReqBinding() {
        debugger;
        if ($('#hdnSACList').val() != '') {
            var StuffingJson = JSON.parse($('#hdnSACList').val());
            var html = '';
            $.each(StuffingJson, function (i, item) {
                //html += '<li id="' + item.DepositAppId + '" onclick="selectStuffing(' + item.DepositAppId + ');">' + item.DepositNo + '</li>';
                html += '<li id="' + item.DepositAppId + '" onclick="selectStuffing(' + item.UnloadingId + ');">' + item.BondNo + ' - '+ item.AreaReserved+' SQM'+' - '+item.Units+' Nos -'+item.Weight+' Qtl'+ '</li>';
            });
            $('#lststuffing').html(html);
        }
    }
    $('#stuffingbox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lststuffing > li').show();
        else {
