                                </div>
                            </div>
                            @*<div class="col-md-1-5">
                                <label> Seal Charge:</label>
                            </div>
                            <div class="col-md-1">
                                <input type="checkbox" id="IsSealCharge" class="domCheckbox" />
                                @Html.Hidden("IsSealCharge")
                            </div>*@
                            
                        </div>
                    </div>
                    <div class="row SignUp_space_bottom2 SignUp_space_top2">
                        <div class="stoke"></div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-offset-4-5 col-md-3">
                                <input type="button" id="btnCalCharge" class="btn log_Btn_sm" onclick="ContainerSelect()" value="Show Charges" />
                            </div>
                        </div>
                    </div>
