                                <span class="ReQSimbol">*</span>
                                @Html.TextBox("StuffingReqNo", "", new { @readonly = true })
                                <span class="search" id="stuffingsearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#stuffingModal"></i></span>
                                @*@Html.ValidationMessageFor(m => m.StuffingReqNo)*@
                            </div>
                            <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2">
                                <label>Date:</label>
                            </div>
                            <div class="col-md-2 col_cus_2_5">
                                @*@Html.TextBoxFor(m => m.StuffingReqDate, new { @readonly = true })*@
                                @Html.TextBox("StuffingReqDate", "", new { @readonly = true })
                            </div>
                        </div>
                    </div>
                    

                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5">
                                <label>Printing:</label>
                            </div>
                            <div class="col-md-2">
                                @Html.DropDownList("Printing", new List<SelectListItem>
               {
                   new SelectListItem {Text="No",Value="No" },
                   new SelectListItem {Text="Yes",Value="Yes" }
               }, new { @onchange = "HideTextBox()" })

                            </div>
                            <div class="col-md-2">
                                @Html.TextBox("NoOfPrinting", "0", new { @class = "form-control input-sm", @style = "display:none;", @onkeypress = "return AllowOnlyInteger(event)", @onblur = "ClearErrMsg(this)", maxlength = "10", @onfocus = "hideMsg(this)" })
                            </div>
                            <div class="col-md-1-5 col_cus_2 col_cus_offset_3 col-md-offset-2-5">
                                <label>Discount :</label>
                            </div>
                            <div class="col-md-2">
                                @*@Html.TextBoxFor(m => m.GSTNo, new { @readonly = true })*@
                                @Html.TextBox("Discount", "0", new { @class = "form-control input-sm" })
                            </div>
                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Party Name:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">
                              
                                @Html.Hidden("PartyId")                               
                                @Html.TextBox("PartyName", "", new { @readonly = true })
                                <span class="search" id="PartySearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PartyModal"></i></span>
                              
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>Payee Name:</label>
                            </div>
                            <div class="col-md-10-5 col_cus_10">                              
                                @Html.Hidden("PayeeId")                              
                                @Html.TextBox("PayeeName", "", new { @readonly = true })
                                <span class="search" id="PayeeSearch"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" data-target="#PayeeModal"></i></span>
                               
                            </div>
                        </div>
                    </div>


                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label>SEZ:</label>
                            </div>
                            <div class="col-md-2-5">
                                @Html.DropDownList("SEZ", new List<SelectListItem>
                                        {
                                        new SelectListItem { Text="SEZWP",Value="SEZWP"},
                                        new SelectListItem {Text="SEZWOP",Value="SEZWOP" }

                                        }, "---Select---", new { @class = "form-control input-sm dropdown" })
                                @Html.ValidationMessage("SEZ", new { @class = "Error_Msg" })
                            </div>

                        </div>
                    </div>

                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label> GST No:</label>
                            </div>
                            <div class="col-md-2-5">                               
                                @Html.Hidden("GSTNo", "", new { @readonly = true })
                                @Html.TextBox("GSTNo2", "", new { @readonly = true })
                            </div>
                           
                            <div class="col-md-1-5 col-md-offset-4 col_cus_offset_3 col_cus_2">
                                <label>Delivery Area:</label>
                            </div>
                            <div class="col-md-2-5">                               
                                @Html.TextBox("AreaReserved", "", new { @readonly = true })
                            </div>
                        </div>
                    </div>
                    <div class="row Form_Space_top">
                        <div class="form-group Form_Input">
                            <div class="col-md-1-5 col_cus_2">
                                <label> Parking Days:</label>
                            </div>
                            <div class="col-md-2">
                                @Html.TextBox("ParkingDays", "0", new { Value = "0", @onkeypress = "return AllowOnlyInteger(event)" })
                            </div>
                            <div class="col-md-2">
                                <div class="boolean-container">
                                    @Html.CheckBox("SealCharge")
