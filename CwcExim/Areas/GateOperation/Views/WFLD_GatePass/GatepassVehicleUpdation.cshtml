    function LoadGatePass() {
        debugger;
        Page = 0;
        $.ajax({
            url: '/GateOperation/WFLD_GatePass/LoadGateList',
            type: 'GET',
            data: { PartyCode: "", Page: Page },
            success: function (data) {
