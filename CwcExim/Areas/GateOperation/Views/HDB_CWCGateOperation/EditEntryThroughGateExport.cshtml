          $('#divshiptext').hide();
          $('#divline').hide();
          $('#linesealtext').hide();
          $('#divref').hide();
          $('#divforwarder').show();
          $("#DepositorName").attr("readonly", true);
          $('#lblCBT').show();
          $('#lblCONT').hide();

      }
      else if (Radio == "Loaded") {
          $('#ReferenceSearch').show();
          $('#ContainerSearch').show();
          $("#StuffType").attr("disabled", true);
          GenerateLstLoadReferenceNumber();
          $('#ReferenceSearch').show();
          $('#ContainerSearch').show();
          $('#divcustext').show();
          $('#divcustom').show();
          $('#divsize').show();
          $('#divsizetext').show();
