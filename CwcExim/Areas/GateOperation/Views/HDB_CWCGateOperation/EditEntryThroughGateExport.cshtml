        var Radio = $('input[name=ContainerType]:checked').val();
        if (Radio == "Empty") {
            $('#ReferenceSearch').hide();
            $('#ContainerSearch').hide();
            $("#ContainerNo1").attr("disabled", false);
            $("#StuffType").attr("disabled", true);
