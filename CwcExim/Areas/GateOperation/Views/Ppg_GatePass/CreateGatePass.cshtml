            ContainerDetails[k].VehicleNo = $('#VehicleNo').val();
            ContainerDetails[k].CargoType = ($('#CargoType').val() == '') ? 0 : $('#CargoType').val();
            ContainerDetails[k].Location = $('#Location').val();
            ContainerDetails[k].PortOfDispatch = $('#PortOfDispatch').val();
            ResetDtlDetails();
            GenerateTable();
            $('#Index').val('');
        }
    }
