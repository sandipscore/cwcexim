    function SelectPort(PortId,PortName)
    {
      
        var CharCode;
        if (window.event) {
            CharCode = window.event.keyCode;
        }
        else {
            CharCode = evt.which;
        }
        if(CharCode==13)
        {
            $('#TransportFrom').val(PortName);
            $('#TransPortFromID').val(PortId);
            $("#TransportModal").modal("hide");
            $('#CustomSealNo').focus();
        }
        else
        {
            $('#TransportFrom').val(PortName);
            $('#TransPortFromID').val(PortId);
            $("#TransportModal").modal("hide");
            $('#CustomSealNo').focus();
        }
    }

