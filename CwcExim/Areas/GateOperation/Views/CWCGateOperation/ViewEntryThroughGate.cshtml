@model CwcExim.Areas.GateOperation.Models.EntryThroughGate
<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-1 col-md-10">
            <div class="row">
                <div class="Con_Heading SignUp_space_bottom col-md-12">
                    <h4>Entry Through Gate</h4>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.ContainerNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ContainerNo, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.CFSCode, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.CFSCode, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.GateInNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.GateInNo, new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.EntryDateTime, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.EntryDateTime, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.ReferenceNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ReferenceNo, new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.ReferenceDate, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ReferenceDate, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2" style="padding-right:0px;">
                        @Html.LabelFor(m => m.ShippingLine, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-10">
                        @Html.TextBoxFor(m => m.ShippingLine, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2" style="padding-right:0px;">
                        @Html.LabelFor(m => m.CHAName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-10">
                        @Html.TextBoxFor(m => m.CHAName, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2" style="padding-right:0px;">
                        @Html.LabelFor(m => m.ContainerNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ContainerNo, new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-1" style="padding-right:0px;">
                        @Html.LabelFor(m => m.Size, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-2">
                        @Html.TextBoxFor(m => m.Size, new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-1">
                        @Html.CheckBoxFor(m => m.Reefer, new { @class = "RegCheck" })
                    </div>
                    <div class="col-md-2" style="padding-right:0px;">
                        @Html.LabelFor(m => m.Reefer, new { @class = "InputLabel" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.CustomSealNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.CustomSealNo, new { @class = "form-cotrol input-sm" })
                    </div>
                    <div class="col-md-2" style="padding-right:0px;">
                        @Html.LabelFor(m => m.ShippingLineSealNo, new { @class = "form-cotrol input-sm" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ShippingLineSealNo, new { @class = "form-cotrol input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.VehicleNo, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.VehicleNo, new { @class = "form-cotrol input-sm" })
                    </div>
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.ChallanNo, new { @class = "form-cotrol input-sm" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.ChallanNo, new { @class = "form-cotrol input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.CargoDescription, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-10">
                        @Html.TextAreaFor(m => m.CargoDescription, new { @class = "form-cotrol input-sm", @style = "width:100%;height:100px;" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.CargoType, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-3">
                        @Html.DropDownListFor(m => m.CargoType, new List<SelectListItem>
                                {
                                    new SelectListItem { Text="A",Value="A"}
                                }, "----Select----", new { @class = "form-control input-sm dropdown" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.NoOfPackages, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.NoOfPackages, new { @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.GrossWeight, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(m => m.GrossWeight, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2">
                        @Html.LabelFor(m => m.DepositorName, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-10">
                        @Html.TextBoxFor(m => m.DepositorName, new { @class = "form-control input-sm" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-group Form_Input">
                    <div class="col-md-2 ">
                        @Html.LabelFor(m => m.Remarks, new { @class = "InputLabel" })<span class="LabelColon">:</span>
                    </div>
                    <div class="col-md-10">
                        @Html.TextAreaFor(m => m.Remarks, new { @class = "form-control input-sm", @style = "width:100%;height:100px;" })
                        @Html.ValidationMessageFor(m => m.Remarks, "", new { @class = "Error_Msg" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/jqueryval")
<script>
    $(function () {
        $('input[type="text"],textarea').prop('readonly', true);
        $('input[type="checkbox"]').prop('disabled', true);
    });
</script>


