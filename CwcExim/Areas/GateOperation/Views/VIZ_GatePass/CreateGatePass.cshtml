@model CwcExim.Areas.GateOperation.Models.VIZGatePass
<script src="~/Areas/GateOperation/Scripts/App/Controllers/VIZ_GatePassCtrl.js"></script>

<style>
    .add_btn {
        background: #fff;
        padding: 3px 10px;
        color: #333;
        border: 1px solid #333;
    }
</style>
<div id="gatepass" ng-controller="VIZ_GatePassCtrl">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-offset-0-5 col-md-11">
                <div class="Head_h4">
                    <div class="row">
                        <div class="col-md-12">
                            <h4>Gate Pass</h4>
                        </div>
                    </div>
                    @using (Ajax.BeginForm("AddEditGatePass", "VIZ_GatePass", new AjaxOptions
                    {
                        Confirm = "Are you sure you want to save?",
                        HttpMethod = "POST",
                        OnSuccess = "GatePassOnSuccess",
                        OnFailure = "GatePassOnFailure",
                    }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(m => m.StringifyData)
                        @Html.HiddenFor(m => m.VehicleXml)
                        @Html.Hidden("Module")
                        @Html.Hidden("GPID")

                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Gate Pass No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.TextBoxFor(m => m.GatePassNo, new { @class = "form-control input-sm", @readonly = true })
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col-md-1-5 col_cus_2 padding_rt">
                                    <label>Gate Pass Date:</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5 Date_Img">
                                    @Html.TextBoxFor(m => m.GatePassDate, new { @class = "form-control input-sm", @size = 10, @maxlength = 10, @onkeyup = "check()", @onblur = "checkdate()" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top Form_Space_bottom">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Invoice No:</label>
                                </div>
                                <div class="col-md-2-5">
                                    @Html.HiddenFor(m => m.InvoiceId)
                                    @Html.TextBoxFor(m => m.InvoiceNo, new { @readonly = true, @onkeydown = "PopupActivate('InvoiceModal');" })
                                    <span class="search"><i class="fa fa-search" aria-hidden="true" data-toggle="modal" onclick="GetInvoiceNoForGatePass();" data-target="#InvoiceModal"></i></span>
                                    @Html.ValidationMessageFor(m => m.InvoiceNo)
                                </div>
                                <div class="col-md-offset-4 col_cus_offset_3 col-md-1-5 col_cus_2 padding_rt">
                                    <label>Delivery Date:</label>
                                </div>
                                <div class="col-md-2 col_cus_2_5 Date_Img">
                                    @Html.TextBoxFor(m => m.DeliveryDate, new { @class = "form-control input-sm", @readonly = true })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>Remarks:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextAreaFor(m => m.Remarks, new { @style = "width:100%;height:50px;" })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2">
                                    <label>CHA Name:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextBoxFor(m => m.CHAName, new { @readonly = true })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Importer / Exporter Name:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextBoxFor(m => m.ImpExpName, new { @readonly = true })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Shipping Line Name:</label>
                                </div>
                                <div class="col-md-10-5 col_cus_10">
                                    @Html.TextBoxFor(m => m.ShippingLineName, new { @readonly = true })
                                </div>
                            </div>
                        </div>
                        <div class="row Form_Space_top Form_Space_bottom">
                            <div class="form-group Form_Input">
                                <div class="col-md-1-5 col_cus_2 padding_rt">
                                    <label>Excepted date and time of departure:</label>
                                </div>
