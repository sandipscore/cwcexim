<script type="text/javascript" src="/scripts/jquery.nicescroll.min.js"></script>
<script>
    var ContainerDetails = [];
    var Module = '';
    //******************************************************************************************
    var VehicleList = [];
    angular.bootstrap(document.getElementById('gatepass'), ['CWCApp']);
    $('#btnMultiVehicles').on('click', function () {
        if ($('#Index').val() != '') {
            $('#VehicleModal').modal();
        };

    });
    //******************************************************************************************
    $('#GatePassDate').focus();
    $(function () { GenerateBlankTable(); });
    $('#GatePassDate').datepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
            $(".Date_Img .Error_Msg").text("");
            $('[data-valmsg-for="Data"]').html('<span></span>');
        }
    });
    $('#DepartureDate').datetimepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    $('#ArrivalDate').datetimepicker({
        dateFormat: "dd/mm/yy",
        showOn: "button",
        buttonImage: "/Content/images/calendar.png",
        buttonImageOnly: true,
        buttonText: "Select date",
        changeMonth: true,
        changeYear: true,
        showAnima: "fadein",
        onClose: function () {
        }
    });
    function checkdate() {
        debugger;
        var result = moment($('#GatePassDate').val(), "DD/MM/YYYY", true).isValid();
        if (!result) {
            $('#DivMsgs').html('Incorrect Date');
            setTimeout(function () { $('#DivMsgs').html(''); }, 5000);
            $('#GatePassDate').val('');
        }
    }
    function check() {
        debugger;
        //alert(elm);
        $('#GatePassDate').val($('#GatePassDate').val().replace(/^(\d\d)(\d)$/g, '$1/$2').replace(/^(\d\d\/\d\d)(\d+)$/g, '$1/$2').replace(/[^\d\/]/g, ''));
        //  elm = elm.replace(/^(\d\d)(\d)$/g, '$1/$2').replace(/^(\d\d\/\d\d)(\d+)$/g, '$1/$2').replace(/[^\d\/]/g, '');
    }
    function GenerateBlankTable() {
        tablehtml = '';
        var tablehtml = '<table class="table table-bordered table-striped dataTable" id="tblEntryDet"><thead><tr><th class="text-center" style="width:10%;">SL No</th><th>Gate Pass No</th><th>Gate Pass Date</th><th style="display:none;">Invoice No</th><th>Is Cancelled</th><th class="text-center" style="width:10%;">Edit</th><th class="text-center" style="width:10%;">View</th><th class="text-center" style="width:10%;">Delete</th><th class="text-center" style="width:10%;">Preview</th><th class="text-center" style="width:10%;">Cancel</th></tr></thead><tbody>';
        $('#tblEntryDet').html(tablehtml);
    }
    function LoadGPList() {
        $('#tblEntryDet').html('');
        $('#DivGPList').load('/GateOperation/VIZ_GatePass/ListOfGatePass');
    }
    function GatePassOnSuccess(data) {
        if (data.Status == 1) {
            debugger;
            $('#GPID').val(data.Data);
            $('#DivSucMsg').html(data.Message);
            var Module = $('#Module').val();
           // $('#GatepassId').val(data.Data);
            if (Module.substring(0, 3) == 'EXP') {
                $('#btnDT').prop('disabled', false);
            }

            $('#BtnPrint').removeAttr("disabled");
            //setTimeout(function () { $('#DivBody').load('/GateOperation/WFLD_GatePass/CreateGatePass'); }, 5000);
        }
        else {
            $('#DivErrMsg').html(data.Message);
            setTimeout(function () { $('#DivSucMsg').html(''); }, 5000);
        }
    }
    function impJOOnFailure() { }
    $('#InvoiceNobox').keyup(function () {
        var val = $(this).val().toLowerCase();
        if (val == "")
            $('#lstInvoice > li').show();
        else {
            $('#lstInvoice > li').each(function () {
                var text = $(this).text().toLowerCase();
                (text.indexOf(val) >= 0 ? $(this).show() : $(this).hide());
            });
        }
    });
  /*  function SelectInvoice(elem, Module) {
        debugger;
        var invoice = elem.innerHTML.split('-');
        var inv=invoice[0]
        $('#InvoiceId').val(elem.id);
        $('#InvoiceNo').val(inv);
        $('#Module').val(Module);
        $("#InvoiceModal").modal("hide");
        GetDetailsAgainstInvoiceId(elem.id);
    }*/

    function GetDetailsAgainstInvoiceId(InvoiceId) {
        $.ajax({
            url: '/GateOperation/VIZ_GatePass/GetDetAgainstInvoice',
            type: 'GET',
