    function Print() {
        var Token = $('input[name="__RequestVerificationToken"]').val();
        var GPID = $('#GPID').val();
        $.ajax({
            url: '/GateOperation/VIZ_GatePass/GatePassPrint',
            type: 'POST',
            data: { GatePassId: GPID },
            dataType: 'JSON',
            headers: { '__RequestVerificationToken': Token },
            success: function (data) {
                if (data.Status == 1) {
                    window.open(data.Message + "?_t=" + (new Date().getTime()), "_blank");
                    setTimeout(function () { $('#DivBody').load('/GateOperation/VIZ_GatePass/CreateGatePass'); }, 5000);

                }
                else
                    alert(data.Message);
            }
        });

    }

