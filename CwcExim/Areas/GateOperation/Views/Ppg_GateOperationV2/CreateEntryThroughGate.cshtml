    function AutoPopulate(ContainerName) {
        // alert("blank");
        //$("#EntryDateTime").val('');
        //$("#ReferenceDate").val('');

        //$("#time").val('');
        $("#ReferenceNo").val('');
        $("#ShippingLineId").val(0);
